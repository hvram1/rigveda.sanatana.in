{
  "mandala": 5,
  "sukta": 78,
  "verses": [
    {
      "id": "5.78.1",
      "mandala": 5,
      "sukta": 78,
      "rik": 1,
      "padas": [
        {
          "text": "अश्वि᳚ना॒वेह ग᳚च्छतं॒ नास᳚त्या॒ मा वि वे᳚नतम् ।",
          "begin": 0.0,
          "end": 7.867
        },
        {
          "text": "हं॒सावि᳚व पतत॒मा सु॒ताँ उप᳚ ॥",
          "begin": 7.867,
          "end": 11.68
        }
      ]
    },
    {
      "id": "5.78.2",
      "mandala": 5,
      "sukta": 78,
      "rik": 2,
      "padas": [
        {
          "text": "अश्वि᳚ना हरि॒णावि᳚व गौ॒रावि॒वानु॒ यव᳚सम् ।",
          "begin": 0.0,
          "end": 5.553
        },
        {
          "text": "हं॒सावि᳚व पतत॒मा सु॒ताँ उप᳚ ॥",
          "begin": 5.553,
          "end": 9.16
        }
      ]
    },
    {
      "id": "5.78.3",
      "mandala": 5,
      "sukta": 78,
      "rik": 3,
      "padas": [
        {
          "text": "अश्वि᳚ना वाजिनीवसू जु॒षेथां᳚ य॒ज्ञमि॒ष्टये᳚ ।",
          "begin": 0.0,
          "end": 5.674
        },
        {
          "text": "हं॒सावि᳚व पतत॒मा सु॒ताँ उप᳚ ॥",
          "begin": 5.674,
          "end": 10.28
        }
      ]
    },
    {
      "id": "5.78.4",
      "mandala": 5,
      "sukta": 78,
      "rik": 4,
      "padas": [
        {
          "text": "अत्रि॒र्यद्वा᳚मव॒रोह᳚न्नृ॒बीस॒मजो᳚हवी॒न्नाध᳚मानेव॒ योषा᳚ ।",
          "begin": 0.0,
          "end": 8.685
        },
        {
          "text": "श्ये॒नस्य᳚ चि॒ज्जव᳚सा॒ नूत᳚ने॒नाग᳚च्छतमश्विना॒ शंत᳚मेन ॥",
          "begin": 8.685,
          "end": 15.0
        }
      ]
    },
    {
      "id": "5.78.5",
      "mandala": 5,
      "sukta": 78,
      "rik": 5,
      "padas": [
        {
          "text": "वि जि᳚हीष्व वनस्पते॒ योनि॒ः सूष्य᳚न्त्या इव ।",
          "begin": 0.0,
          "end": 5.911
        },
        {
          "text": "श्रु॒तं मे᳚ अश्विना॒ हवं᳚ स॒प्तव᳚ध्रिं च मुञ्चतम् ॥",
          "begin": 5.911,
          "end": 11.44
        }
      ]
    },
    {
      "id": "5.78.6",
      "mandala": 5,
      "sukta": 78,
      "rik": 6,
      "padas": [
        {
          "text": "भी॒ताय॒ नाध᳚मानाय॒ ऋष᳚ये स॒प्तव᳚ध्रये ।",
          "begin": 0.0,
          "end": 3.975
        },
        {
          "text": "मा॒याभि᳚रश्विना यु॒वं वृ॒क्षं सं च॒ वि चा᳚चथः ॥",
          "begin": 3.975,
          "end": 10.56
        }
      ]
    },
    {
      "id": "5.78.7",
      "mandala": 5,
      "sukta": 78,
      "rik": 7,
      "padas": [
        {
          "text": "यथा॒ वातः᳚ पुष्क॒रिणीं᳚ समि॒ङ्गय᳚ति स॒र्वतः᳚ ।",
          "begin": 0.0,
          "end": 6.112
        },
        {
          "text": "ए॒वा ते॒ गर्भ᳚ एजतु नि॒रैतु॒ दश᳚मास्यः ॥",
          "begin": 6.112,
          "end": 10.36
        }
      ]
    },
    {
      "id": "5.78.8",
      "mandala": 5,
      "sukta": 78,
      "rik": 8,
      "padas": [
        {
          "text": "यथा॒ वातो॒ यथा॒ वनं॒ यथा᳚ समु॒द्र एज᳚ति ।",
          "begin": 0.0,
          "end": 4.801
        },
        {
          "text": "ए॒वा त्वं द᳚शमास्य स॒हावे᳚हि ज॒रायु᳚णा ॥",
          "begin": 4.801,
          "end": 10.72
        }
      ]
    },
    {
      "id": "5.78.9",
      "mandala": 5,
      "sukta": 78,
      "rik": 9,
      "padas": [
        {
          "text": "दश॒ मासा᳚ञ्छशया॒नः कु᳚मा॒रो अधि᳚ मा॒तरि᳚ ।",
          "begin": 0.0,
          "end": 6.071
        },
        {
          "text": "नि॒रैतु᳚ जी॒वो अक्ष᳚तो जी॒वो जीव᳚न्त्या॒ अधि᳚ ॥",
          "begin": 6.071,
          "end": 12.8
        }
      ]
    }
  ]
}