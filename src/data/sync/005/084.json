      "rik": 1,
      "padas": [
        {
          "text": "बळि॒त्था पर्व᳚तानां खि॒द्रं बि᳚भर्षि पृथिवि ।",
          "begin": 0.0,
          "end": 6.515
        },
        {
          "text": "प्र या भूमिं᳚ प्रवत्वति म॒ह्ना जि॒नोषि᳚ महिनि ॥",
          "begin": 6.515,
          "end": 11.84
        }
      ]
    },
    {
      "id": "5.84.2",
      "mandala": 5,
      "sukta": 84,
      "rik": 2,
      "padas": [
        {
          "text": "स्तोमा᳚सस्त्वा विचारिणि॒ प्रति᳚ ष्टोभन्त्य॒क्तुभि᳚ः ।",
          "begin": 0.0,
          "end": 6.631
        },
        {
          "text": "प्र या वाजं॒ न हेष᳚न्तं पे॒रुमस्य᳚स्यर्जुनि ॥",
          "begin": 6.631,
          "end": 11.84
        }
      ]
    },
    {
      "id": "5.84.3",
      "mandala": 5,
      "sukta": 84,
      "rik": 3,
      "padas": [
        {
          "text": "दृ॒ळ्हा चि॒द्या वन॒स्पती᳚न्क्ष्म॒या दर्ध॒र्ष्योज᳚सा ।",
          "begin": 0.0,
          "end": 7.254
        },
        {
          "text": "यत्ते᳚ अ॒भ्रस्य᳚ वि॒द्युतो᳚ दि॒वो वर्ष᳚न्ति वृ॒ष्टयः᳚ ॥",
          "begin": 7.254,
          "end": 15.08
        }
      ]
    }
  ]
}