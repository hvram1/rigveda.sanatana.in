{
  "mandala": 5,
  "sukta": 50,
  "verses": [
    {
      "id": "5.50.1",
      "mandala": 5,
      "sukta": 50,
      "rik": 1,
      "padas": [
        {
          "text": "विश्वो᳚ दे॒वस्य᳚ ने॒तुर्मर्तो᳚ वुरीत स॒ख्यम् ।",
          "begin": 0.0,
          "end": 7.586
        },
        {
          "text": "विश्वो᳚ रा॒य इ᳚षुध्यति द्यु॒म्नं वृ᳚णीत पु॒ष्यसे᳚ ॥",
          "begin": 7.586,
          "end": 13.48
        }
      ]
    },
    {
      "id": "5.50.2",
      "mandala": 5,
      "sukta": 50,
      "rik": 2,
      "padas": [
        {
          "text": "ते ते᳚ देव नेत॒र्ये चे॒माँ अ᳚नु॒शसे᳚ ।",
          "begin": 0.0,
          "end": 5.555
        },
        {
          "text": "ते रा॒या ते ह्या॒३॒᳚पृचे॒ सचे᳚महि सच॒थ्यै᳚ः ॥",
          "begin": 5.555,
          "end": 14.6
        }
      ]
    },
    {
      "id": "5.50.3",
      "mandala": 5,
      "sukta": 50,
      "rik": 3,
      "padas": [
        {
          "text": "अतो᳚ न॒ आ नॄनति᳚थी॒नत॒ः पत्नी᳚र्दशस्यत ।",
          "begin": 0.0,
          "end": 5.4
        },
        {
          "text": "आ॒रे विश्वं᳚ पथे॒ष्ठां द्वि॒षो यु᳚योतु॒ यूयु᳚विः ॥",
          "begin": 5.4,
          "end": 12.28
        }
      ]
    },
    {
      "id": "5.50.4",
      "mandala": 5,
      "sukta": 50,
      "rik": 4,
      "padas": [
        {
          "text": "यत्र॒ वह्नि᳚र॒भिहि᳚तो दु॒द्रव॒द्द्रोण्य᳚ः प॒शुः ।",
          "begin": 0.0,
          "end": 5.243
        },
        {
          "text": "नृ॒मणा᳚ वी॒रप॒स्त्यो॑र्णा॒ धीरे᳚व॒ सनि᳚ता ॥",
          "begin": 5.243,
          "end": 10.88
        }
      ]
    },
    {
      "id": "5.50.5",
      "mandala": 5,
      "sukta": 50,
      "rik": 5,
      "padas": [
        {
          "text": "ए॒ष ते᳚ देव नेता॒ रथ॒स्पति॒ः शं र॒यिः ।",
          "begin": 0.0,
          "end": 6.151
        },
        {
          "text": "शं रा॒ये शं स्व॒स्तय᳚ इष॒ःस्तुतो᳚ मनामहे देव॒स्तुतो᳚ मनामहे ॥",
          "begin": 6.151,
          "end": 16.16
        }
      ]
    }
  ]
}