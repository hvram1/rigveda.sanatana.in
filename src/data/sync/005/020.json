{
  "mandala": 5,
  "sukta": 20,
  "verses": [
    {
      "id": "5.20.1",
      "mandala": 5,
      "sukta": 20,
      "rik": 1,
      "padas": [
        {
          "text": "यम᳚ग्ने वाजसातम॒ त्वं चि॒न्मन्य᳚से र॒यिम् ।",
          "begin": 0.0,
          "end": 6.583
        },
        {
          "text": "तं नो᳚ गी॒र्भिः श्र॒वाय्यं᳚ देव॒त्रा प᳚नया॒ युज᳚म् ॥",
          "begin": 6.583,
          "end": 13.0
        }
      ]
    },
    {
      "id": "5.20.2",
      "mandala": 5,
      "sukta": 20,
      "rik": 2,
      "padas": [
        {
          "text": "ये अ᳚ग्ने॒ नेरय᳚न्ति ते वृ॒द्धा उ॒ग्रस्य॒ शव᳚सः ।",
          "begin": 0.0,
          "end": 7.226
        },
        {
          "text": "अप॒ द्वेषो॒ अप॒ ह्वरो॒॑न्यव्र᳚तस्य सश्चिरे ॥",
          "begin": 7.226,
          "end": 12.08
        }
      ]
    },
    {
      "id": "5.20.3",
      "mandala": 5,
      "sukta": 20,
      "rik": 3,
      "padas": [
        {
          "text": "होता᳚रं त्वा वृणीम॒हे॑ग्ने॒ दक्ष᳚स्य॒ साध᳚नम् ।",
          "begin": 0.0,
          "end": 6.71
        },
        {
          "text": "य॒ज्ञेषु᳚ पू॒र्व्यं गि॒रा प्रय᳚स्वन्तो हवामहे ॥",
          "begin": 6.71,
          "end": 11.76
        }
      ]
    },
    {
      "id": "5.20.4",
      "mandala": 5,
      "sukta": 20,
      "rik": 4,
      "padas": [
        {
          "text": "इ॒त्था यथा᳚ त ऊ॒तये॒ सह᳚सावन्दि॒वेदि᳚वे ।",
          "begin": 0.0,
          "end": 4.873
        },
        {
          "text": "रा॒य ऋ॒ताय᳚ सुक्रतो॒ गोभि᳚ः ष्याम सध॒मादो᳚ वी॒रैः स्या᳚म सध॒मादः᳚ ॥",
          "begin": 4.873,
          "end": 16.28
        }
      ]
    }
  ]
}