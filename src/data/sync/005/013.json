{
  "mandala": 5,
  "sukta": 13,
  "verses": [
    {
      "id": "5.13.1",
      "mandala": 5,
      "sukta": 13,
      "rik": 1,
      "padas": [
        {
          "text": "अर्च᳚न्तस्त्वा हवाम॒हे॑र्च᳚न्त॒ः समि᳚धीमहि ।",
          "begin": 0.0,
          "end": 7.155
        },
        {
          "text": "अग्ने॒ अर्च᳚न्त ऊ॒तये᳚ ॥",
          "begin": 7.155,
          "end": 11.88
        }
      ]
    },
    {
      "id": "5.13.2",
      "mandala": 5,
      "sukta": 13,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒ग्नेः स्तोमं᳚ मनामहे सि॒ध्रम॒द्य दि᳚वि॒स्पृशः᳚ ।",
          "begin": 0.0,
          "end": 7.091
        },
        {
          "text": "दे॒वस्य᳚ द्रविण॒स्यवः᳚ ॥",
          "begin": 7.091,
          "end": 10.16
        }
      ]
    },
    {
      "id": "5.13.3",
      "mandala": 5,
      "sukta": 13,
      "rik": 3,
      "padas": [
        {
          "text": "अ॒ग्निर्जु᳚षत नो॒ गिरो॒ होता॒ यो मानु᳚षे॒ष्वा ।",
          "begin": 0.0,
          "end": 6.096
        },
        {
          "text": "स य᳚क्ष॒द्दैव्यं॒ जन᳚म् ॥",
          "begin": 6.096,
          "end": 9.24
        }
      ]
    },
    {
      "id": "5.13.4",
      "mandala": 5,
      "sukta": 13,
      "rik": 4,
      "padas": [
        {
          "text": "त्वम᳚ग्ने स॒प्रथा᳚ असि॒ जुष्टो॒ होता॒ वरे᳚ण्यः ।",
          "begin": 0.0,
          "end": 4.646
        },
        {
          "text": "त्वया᳚ य॒ज्ञं वि त᳚न्वते ॥",
          "begin": 4.646,
          "end": 10.56
        }
      ]
    },
    {
      "id": "5.13.5",
      "mandala": 5,
      "sukta": 13,
      "rik": 5,
      "padas": [
        {
          "text": "त्वाम᳚ग्ने वाज॒सात᳚मं॒ विप्रा᳚ वर्धन्ति॒ सुष्टु᳚तम् ।",
          "begin": 0.0,
          "end": 6.945
        },
        {
          "text": "स नो᳚ रास्व सु॒वीर्य᳚म् ॥",
          "begin": 6.945,
          "end": 10.68
        }
      ]
    },
    {
      "id": "5.13.6",
      "mandala": 5,
      "sukta": 13,
      "rik": 6,
      "padas": [
        {
          "text": "अग्ने᳚ ने॒मिर॒राँ इ᳚व दे॒वाँस्त्वं प᳚रि॒भूर᳚सि ।",
          "begin": 0.0,
          "end": 7.776
        },
        {
          "text": "आ राध᳚श्चि॒त्रमृ᳚ञ्जसे ॥",
          "begin": 7.776,
          "end": 11.64
        }
      ]
    }
  ]
}