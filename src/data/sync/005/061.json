{
  "mandala": 5,
  "sukta": 61,
  "verses": [
    {
      "id": "5.61.1",
      "mandala": 5,
      "sukta": 61,
      "rik": 1,
      "padas": [
        {
          "text": "के ष्ठा᳚ नर॒ः श्रेष्ठ᳚तमा॒ य एक᳚एक आय॒य ।",
          "begin": 0.0,
          "end": 8.137
        },
        {
          "text": "प॒र॒मस्या᳚ः परा॒वतः᳚ ॥",
          "begin": 8.137,
          "end": 11.68
        }
      ]
    },
    {
      "id": "5.61.2",
      "mandala": 5,
      "sukta": 61,
      "rik": 2,
      "padas": [
        {
          "text": "क्व१॒᳚ वो॑श्वा॒ः क्वा॒३॒᳚भीश᳚वः क॒थं शे᳚क क॒था य᳚य ।",
          "begin": 0.0,
          "end": 10.384
        },
        {
          "text": "पृ॒ष्ठे सदो᳚ न॒सोर्यमः᳚ ॥",
          "begin": 10.384,
          "end": 14.36
        }
      ]
    },
    {
      "id": "5.61.3",
      "mandala": 5,
      "sukta": 61,
      "rik": 3,
      "padas": [
        {
          "text": "ज॒घने॒ चोद᳚ एषां॒ वि स॒क्थानि॒ नरो᳚ यमुः ।",
          "begin": 0.0,
          "end": 6.308
        },
        {
          "text": "पु॒त्र॒कृ॒थे न जन᳚यः ॥",
          "begin": 6.308,
          "end": 9.48
        }
      ]
    },
    {
      "id": "5.61.4",
      "mandala": 5,
      "sukta": 61,
      "rik": 4,
      "padas": [
        {
          "text": "परा᳚ वीरास एतन॒ मर्या᳚सो॒ भद्र᳚जानयः ।",
          "begin": 0.0,
          "end": 6.938
        },
        {
          "text": "अ॒ग्नि॒तपो॒ यथास᳚थ ॥",
          "begin": 6.938,
          "end": 10.04
        }
      ]
    },
    {
      "id": "5.61.5",
      "mandala": 5,
      "sukta": 61,
      "rik": 5,
      "padas": [
        {
          "text": "सन॒त्साश्व्यं᳚ प॒शुमु॒त गव्यं᳚ श॒ताव᳚यम् ।",
          "begin": 0.0,
          "end": 6.333
        },
        {
          "text": "श्या॒वाश्व᳚स्तुताय॒ या दोर्वी॒रायो᳚प॒बर्बृ᳚हत् ॥",
          "begin": 6.333,
          "end": 12.76
        }
      ]
    },
    {
      "id": "5.61.6",
      "mandala": 5,
      "sukta": 61,
      "rik": 6,
      "padas": [
        {
          "text": "उ॒त त्वा॒ स्त्री शशी᳚यसी पुं॒सो भ᳚वति॒ वस्य᳚सी ।",
          "begin": 0.0,
          "end": 6.115
        },
        {
          "text": "अदे᳚वत्रादरा॒धसः᳚ ॥",
          "begin": 6.115,
          "end": 9.8
        }
      ]
    },
    {
      "id": "5.61.7",
      "mandala": 5,
      "sukta": 61,
      "rik": 7,
      "padas": [
        {
          "text": "वि या जा॒नाति॒ जसु᳚रिं॒ वि तृष्य᳚न्तं॒ वि का॒मिन᳚म् ।",
          "begin": 0.0,
          "end": 4.967
        },
        {
          "text": "दे॒व॒त्रा कृ᳚णु॒ते मनः᳚ ॥",
          "begin": 4.967,
          "end": 9.64
        }
      ]
    },
    {
      "id": "5.61.8",
      "mandala": 5,
      "sukta": 61,
      "rik": 8,
      "padas": [
        {
          "text": "उ॒त घा॒ नेमो॒ अस्तु᳚त॒ः पुमाँ॒ इति᳚ ब्रुवे प॒णिः ।",
          "begin": 0.0,
          "end": 6.683
        },
        {
          "text": "स वैर᳚देय॒ इत्स॒मः ॥",
          "begin": 6.683,
          "end": 9.48
        }
      ]
    },
    {
      "id": "5.61.9",
      "mandala": 5,
      "sukta": 61,
      "rik": 9,
      "padas": [
        {
          "text": "उ॒त मे᳚॑रपद्युव॒तिर्म᳚म॒न्दुषी॒ प्रति᳚ श्या॒वाय᳚ वर्त॒निम् ।",
          "begin": 0.0,
          "end": 7.047
        },
        {
          "text": "वि रोहि᳚ता पुरुमी॒ळ्हाय᳚ येमतु॒र्विप्रा᳚य दी॒र्घय᳚शसे ॥",
          "begin": 7.047,
          "end": 14.04
        }
      ]
    },
    {
      "id": "5.61.10",
      "mandala": 5,
      "sukta": 61,
      "rik": 10,
      "padas": [
        {
          "text": "यो मे᳚ धेनू॒नां श॒तं वैद᳚दश्वि॒र्यथा॒ दद᳚त् ।",
          "begin": 0.0,
          "end": 7.069
        },
        {
          "text": "त॒र॒न्त इ᳚व मं॒हना᳚ ॥",
          "begin": 7.069,
          "end": 10.08
        }
      ]
    },
    {
      "id": "5.61.11",
      "mandala": 5,
      "sukta": 61,
      "rik": 11,
      "padas": [
        {
          "text": "य ईं॒ वह᳚न्त आ॒शुभि॒ः पिब᳚न्तो मदि॒रं मधु᳚ ।",
          "begin": 0.0,
          "end": 6.875
        },
        {
          "text": "अत्र॒ श्रवां᳚सि दधिरे ॥",
          "begin": 6.875,
          "end": 9.96
        }
      ]
    },
    {
      "id": "5.61.12",
      "mandala": 5,
      "sukta": 61,
      "rik": 12,
      "padas": [
        {
          "text": "येषां᳚ श्रि॒याधि॒ रोद᳚सी वि॒भ्राज᳚न्ते॒ रथे॒ष्वा ।",
          "begin": 0.0,
          "end": 6.951
        },
        {
          "text": "दि॒वि रु॒क्म इ᳚वो॒परि᳚ ॥",
          "begin": 6.951,
          "end": 9.92
        }
      ]
    },
    {
      "id": "5.61.13",
      "mandala": 5,
      "sukta": 61,
      "rik": 13,
      "padas": [
        {
          "text": "युवा॒ स मारु᳚तो ग॒णस्त्वे॒षर᳚थो॒ अने᳚द्यः ।",
          "begin": 0.0,
          "end": 6.421
        },
        {
          "text": "शु॒भं॒यावाप्र᳚तिष्कुतः ॥",
          "begin": 6.421,
          "end": 9.6
        }
      ]
    },
    {
      "id": "5.61.14",
      "mandala": 5,
      "sukta": 61,
      "rik": 14,
      "padas": [
        {
          "text": "को वे᳚द नू॒नमे᳚षां॒ यत्रा॒ मद᳚न्ति॒ धूत᳚यः ।",
          "begin": 0.0,
          "end": 9.405
        },
        {
          "text": "ऋ॒तजा᳚ता अरे॒पसः᳚ ॥",
          "begin": 9.405,
          "end": 11.28
        }
      ]
    },
    {
      "id": "5.61.15",
      "mandala": 5,
      "sukta": 61,
      "rik": 15,
      "padas": [
        {
          "text": "यू॒यं मर्तं᳚ विपन्यवः प्रणे॒तार᳚ इ॒त्था धि॒या ।",
          "begin": 0.0,
          "end": 6.91
        },
        {
          "text": "श्रोता᳚रो॒ याम᳚हूतिषु ॥",
          "begin": 6.91,
          "end": 10.28
        }
      ]
    },
    {
      "id": "5.61.16",
      "mandala": 5,
      "sukta": 61,
      "rik": 16,
      "padas": [
        {
          "text": "ते नो॒ वसू᳚नि॒ काम्या᳚ पुरुश्च॒न्द्रा रि᳚शादसः ।",
          "begin": 0.0,
          "end": 7.438
        },
        {
          "text": "आ य᳚ज्ञियासो ववृत्तन ॥",
          "begin": 7.438,
          "end": 10.08
        }
      ]
    },
    {
      "id": "5.61.17",
      "mandala": 5,
      "sukta": 61,
      "rik": 17,
      "padas": [
        {
          "text": "ए॒तं मे॒ स्तोम᳚मूर्म्ये दा॒र्भ्याय॒ परा᳚ वह ।",
          "begin": 0.0,
          "end": 6.948
        },
        {
          "text": "गिरो᳚ देवि र॒थीरि᳚व ॥",
          "begin": 6.948,
          "end": 9.92
        }
      ]
    },
    {
      "id": "5.61.18",
      "mandala": 5,
      "sukta": 61,
      "rik": 18,
      "padas": [
        {
          "text": "उ॒त मे᳚ वोचता॒दिति᳚ सु॒तसो᳚मे॒ रथ᳚वीतौ ।",
          "begin": 0.0,
          "end": 6.404
        },
        {
          "text": "न कामो॒ अप᳚ वेति मे ॥",
          "begin": 6.404,
          "end": 9.44
        }
      ]
    },
    {
      "id": "5.61.19",
      "mandala": 5,
      "sukta": 61,
      "rik": 19,
      "padas": [
        {
          "text": "ए॒ष क्षे᳚ति॒ रथ᳚वीतिर्म॒घवा॒ गोम᳚ती॒रनु᳚ ।",
          "begin": 0.0,
          "end": 6.45
        },
        {
          "text": "पर्व᳚ते॒ष्वप᳚श्रितः ॥",
          "begin": 6.45,
          "end": 10.92
        }
      ]
    }
  ]
}