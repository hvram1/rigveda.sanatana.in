{
  "mandala": 5,
  "sukta": 70,
  "verses": [
    {
      "id": "5.70.1",
      "mandala": 5,
      "sukta": 70,
      "rik": 1,
      "padas": [
        {
          "text": "पु॒रू॒रुणा᳚ चि॒द्ध्यस्त्यवो᳚ नू॒नं वां᳚ वरुण ।",
          "begin": 0.0,
          "end": 8.253
        },
        {
          "text": "मित्र॒ वंसि᳚ वां सुम॒तिम् ॥",
          "begin": 8.253,
          "end": 11.44
        }
      ]
    },
    {
      "id": "5.70.2",
      "mandala": 5,
      "sukta": 70,
      "rik": 2,
      "padas": [
        {
          "text": "ता वां᳚ स॒म्यग᳚द्रुह्वा॒णेष᳚मश्याम॒ धाय᳚से ।",
          "begin": 0.0,
          "end": 6.539
        },
        {
          "text": "व॒यं ते रु᳚द्रा स्याम ॥",
          "begin": 6.539,
          "end": 9.52
        }
      ]
    },
    {
      "id": "5.70.3",
      "mandala": 5,
      "sukta": 70,
      "rik": 3,
      "padas": [
        {
          "text": "पा॒तं नो᳚ रुद्रा पा॒युभि᳚रु॒त त्रा᳚येथां सुत्रा॒त्रा ।",
          "begin": 0.0,
          "end": 8.749
        },
        {
          "text": "तु॒र्याम॒ दस्यू᳚न्त॒नूभि᳚ः ॥",
          "begin": 8.749,
          "end": 11.56
        }
      ]
    },
    {
      "id": "5.70.4",
      "mandala": 5,
      "sukta": 70,
      "rik": 4,
      "padas": [
        {
          "text": "मा कस्या᳚द्भुतक्रतू य॒क्षं भु᳚जेमा त॒नूभि᳚ः ।",
          "begin": 0.0,
          "end": 7.385
        },
        {
          "text": "मा शेष᳚सा॒ मा तन᳚सा ॥",
          "begin": 7.385,
          "end": 11.76
        }
      ]
    }
  ]
}