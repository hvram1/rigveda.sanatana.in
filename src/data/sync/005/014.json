{
  "mandala": 5,
  "sukta": 14,
  "verses": [
    {
      "id": "5.14.1",
      "mandala": 5,
      "sukta": 14,
      "rik": 1,
      "padas": [
        {
          "text": "अ॒ग्निं स्तोमे᳚न बोधय समिधा॒नो अम᳚र्त्यम् ।",
          "begin": 0.0,
          "end": 7.964
        },
        {
          "text": "ह॒व्या दे॒वेषु᳚ नो दधत् ॥",
          "begin": 7.964,
          "end": 11.24
        }
      ]
    },
    {
      "id": "5.14.2",
      "mandala": 5,
      "sukta": 14,
      "rik": 2,
      "padas": [
        {
          "text": "तम᳚ध्व॒रेष्वी᳚ळते दे॒वं मर्ता॒ अम᳚र्त्यम् ।",
          "begin": 0.0,
          "end": 6.212
        },
        {
          "text": "यजि᳚ष्ठं॒ मानु᳚षे॒ जने᳚ ॥",
          "begin": 6.212,
          "end": 9.8
        }
      ]
    },
    {
      "id": "5.14.3",
      "mandala": 5,
      "sukta": 14,
      "rik": 3,
      "padas": [
        {
          "text": "तं हि शश्व᳚न्त॒ ईळ᳚ते स्रु॒चा दे॒वं घृ᳚त॒श्चुता᳚ ।",
          "begin": 0.0,
          "end": 7.779
        },
        {
          "text": "अ॒ग्निं ह॒व्याय॒ वोळ्ह᳚वे ॥",
          "begin": 7.779,
          "end": 10.4
        }
      ]
    },
    {
      "id": "5.14.4",
      "mandala": 5,
      "sukta": 14,
      "rik": 4,
      "padas": [
        {
          "text": "अ॒ग्निर्जा॒तो अ᳚रोचत॒ घ्नन्दस्यू॒ञ्ज्योति᳚षा॒ तमः᳚ ।",
          "begin": 0.0,
          "end": 7.239
        },
        {
          "text": "अवि᳚न्द॒द्गा अ॒पः स्व᳚ः ॥",
          "begin": 7.239,
          "end": 10.76
        }
      ]
    },
    {
      "id": "5.14.5",
      "mandala": 5,
      "sukta": 14,
      "rik": 5,
      "padas": [
        {
          "text": "अ॒ग्निमी॒ळेन्यं᳚ क॒विं घृ॒तपृ᳚ष्ठं सपर्यत ।",
          "begin": 0.0,
          "end": 6.018
        },
        {
          "text": "वेतु᳚ मे शृ॒णव॒द्धव᳚म् ॥",
          "begin": 6.018,
          "end": 9.08
        }
      ]
    },
    {
      "id": "5.14.6",
      "mandala": 5,
      "sukta": 14,
      "rik": 6,
      "padas": [
        {
          "text": "अ॒ग्निं घृ॒तेन᳚ वावृधु॒ः स्तोमे᳚भिर्वि॒श्वच᳚र्षणिम् ।",
          "begin": 0.0,
          "end": 6.712
        },
        {
          "text": "स्वा॒धीभि᳚र्वच॒स्युभि᳚ः ॥",
          "begin": 6.712,
          "end": 11.2
        }
      ]
    }
  ]
}