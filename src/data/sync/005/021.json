{
  "mandala": 5,
  "sukta": 21,
  "verses": [
    {
      "id": "5.21.1",
      "mandala": 5,
      "sukta": 21,
      "rik": 1,
      "padas": [
        {
          "text": "म॒नु॒ष्वत्त्वा॒ नि धी᳚महि मनु॒ष्वत्समि᳚धीमहि ।",
          "begin": 0.0,
          "end": 5.386
        },
        {
          "text": "अग्ने᳚ मनु॒ष्वद᳚ङ्गिरो दे॒वान्दे᳚वय॒ते य᳚ज ॥",
          "begin": 5.386,
          "end": 14.08
        }
      ]
    },
    {
      "id": "5.21.2",
      "mandala": 5,
      "sukta": 21,
      "rik": 2,
      "padas": [
        {
          "text": "त्वं हि मानु᳚षे॒ जने॑ग्ने॒ सुप्री᳚त इ॒ध्यसे᳚ ।",
          "begin": 0.0,
          "end": 6.399
        },
        {
          "text": "स्रुच᳚स्त्वा यन्त्यानु॒षक्सुजा᳚त॒ सर्पि᳚रासुते ॥",
          "begin": 6.399,
          "end": 11.72
        }
      ]
    },
    {
      "id": "5.21.3",
      "mandala": 5,
      "sukta": 21,
      "rik": 3,
      "padas": [
        {
          "text": "त्वां विश्वे᳚ स॒जोष᳚सो दे॒वासो᳚ दू॒तम᳚क्रत ।",
          "begin": 0.0,
          "end": 7.066
        },
        {
          "text": "स॒प॒र्यन्त᳚स्त्वा कवे य॒ज्ञेषु᳚ दे॒वमी᳚ळते ॥",
          "begin": 7.066,
          "end": 12.32
        }
      ]
    },
    {
      "id": "5.21.4",
      "mandala": 5,
      "sukta": 21,
      "rik": 4,
      "padas": [
        {
          "text": "दे॒वं वो᳚ देवय॒ज्यया॒ग्निमी᳚ळीत॒ मर्त्य᳚ः ।",
          "begin": 0.0,
          "end": 6.886
        },
        {
          "text": "समि᳚द्धः शुक्र दीदिह्यृ॒तस्य॒ योनि॒मास᳚दः स॒सस्य॒ योनि॒मास᳚दः ॥",
          "begin": 6.886,
          "end": 14.68
        }
      ]
    }
  ]
}