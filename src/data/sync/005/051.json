{
  "mandala": 5,
  "sukta": 51,
  "verses": [
    {
      "id": "5.51.1",
      "mandala": 5,
      "sukta": 51,
      "rik": 1,
      "padas": [
        {
          "text": "अग्ने᳚ सु॒तस्य᳚ पी॒तये॒ विश्वै॒रूमे᳚भि॒रा ग᳚हि ।",
          "begin": 0.0,
          "end": 7.18
        },
        {
          "text": "दे॒वेभि᳚र्ह॒व्यदा᳚तये ॥",
          "begin": 7.18,
          "end": 11.08
        }
      ]
    },
    {
      "id": "5.51.2",
      "mandala": 5,
      "sukta": 51,
      "rik": 2,
      "padas": [
        {
          "text": "ऋत᳚धीतय॒ आ ग᳚त॒ सत्य᳚धर्माणो अध्व॒रम् ।",
          "begin": 0.0,
          "end": 5.813
        },
        {
          "text": "अ॒ग्नेः पि᳚बत जि॒ह्वया᳚ ॥",
          "begin": 5.813,
          "end": 8.92
        }
      ]
    },
    {
      "id": "5.51.3",
      "mandala": 5,
      "sukta": 51,
      "rik": 3,
      "padas": [
        {
          "text": "विप्रे᳚भिर्विप्र सन्त्य प्रात॒र्याव᳚भि॒रा ग᳚हि ।",
          "begin": 0.0,
          "end": 6.092
        },
        {
          "text": "दे॒वेभि॒ः सोम᳚पीतये ॥",
          "begin": 6.092,
          "end": 8.6
        }
      ]
    },
    {
      "id": "5.51.4",
      "mandala": 5,
      "sukta": 51,
      "rik": 4,
      "padas": [
        {
          "text": "अ॒यं सोम᳚श्च॒मू सु॒तो॑म᳚त्रे॒ परि᳚ षिच्यते ।",
          "begin": 0.0,
          "end": 4.971
        },
        {
          "text": "प्रि॒य इन्द्रा᳚य वा॒यवे᳚ ॥",
          "begin": 4.971,
          "end": 8.2
        }
      ]
    },
    {
      "id": "5.51.5",
      "mandala": 5,
      "sukta": 51,
      "rik": 5,
      "padas": [
        {
          "text": "वाय॒वा या᳚हि वी॒तये᳚ जुषा॒णो ह॒व्यदा᳚तये ।",
          "begin": 0.0,
          "end": 6.47
        },
        {
          "text": "पिबा᳚ सु॒तस्यान्ध᳚सो अ॒भि प्रयः᳚ ॥",
          "begin": 6.47,
          "end": 10.84
        }
      ]
    },
    {
      "id": "5.51.6",
      "mandala": 5,
      "sukta": 51,
      "rik": 6,
      "padas": [
        {
          "text": "इन्द्र᳚श्च वायवेषां सु॒तानां᳚ पी॒तिम᳚र्हथः ।",
          "begin": 0.0,
          "end": 5.666
        },
        {
          "text": "ताञ्जु᳚षेथामरे॒पसा᳚व॒भि प्रयः᳚ ॥",
          "begin": 5.666,
          "end": 10.16
        }
      ]
    },
    {
      "id": "5.51.7",
      "mandala": 5,
      "sukta": 51,
      "rik": 7,
      "padas": [
        {
          "text": "सु॒ता इन्द्रा᳚य वा॒यवे॒ सोमा᳚सो॒ दध्या᳚शिरः ।",
          "begin": 0.0,
          "end": 6.366
        },
        {
          "text": "नि॒म्नं न य᳚न्ति॒ सिन्ध᳚वो॒॑भि प्रयः᳚ ॥",
          "begin": 6.366,
          "end": 10.92
        }
      ]
    },
    {
      "id": "5.51.8",
      "mandala": 5,
      "sukta": 51,
      "rik": 8,
      "padas": [
        {
          "text": "स॒जूर्विश्वे᳚भिर्दे॒वेभि᳚र॒श्विभ्या᳚मु॒षसा᳚ स॒जूः ।",
          "begin": 0.0,
          "end": 8.469
        },
        {
          "text": "आ या᳚ह्यग्ने अत्रि॒वत्सु॒ते र᳚ण ॥",
          "begin": 8.469,
          "end": 11.6
        }
      ]
    },
    {
      "id": "5.51.9",
      "mandala": 5,
      "sukta": 51,
      "rik": 9,
      "padas": [
        {
          "text": "स॒जूर्मि॒त्रावरु᳚णाभ्यां स॒जूः सोमे᳚न॒ विष्णु᳚ना ।",
          "begin": 0.0,
          "end": 4.943
        },
        {
          "text": "आ या᳚ह्यग्ने अत्रि॒वत्सु॒ते र᳚ण ॥",
          "begin": 4.943,
          "end": 11.08
        }
      ]
    },
    {
      "id": "5.51.10",
      "mandala": 5,
      "sukta": 51,
      "rik": 10,
      "padas": [
        {
          "text": "स॒जूरा᳚दि॒त्यैर्वसु᳚भिः स॒जूरिन्द्रे᳚ण वा॒युना᳚ ।",
          "begin": 0.0,
          "end": 7.148
        },
        {
          "text": "आ या᳚ह्यग्ने अत्रि॒वत्सु॒ते र᳚ण ॥",
          "begin": 7.148,
          "end": 11.2
        }
      ]
    },
    {
      "id": "5.51.11",
      "mandala": 5,
      "sukta": 51,
      "rik": 11,
      "padas": [
        {
          "text": "स्व॒स्ति नो᳚ मिमीताम॒श्विना॒ भगः᳚ स्व॒स्ति दे॒व्यदि᳚तिरन॒र्वणः᳚ ।",
          "begin": 0.0,
          "end": 7.329
        },
        {
          "text": "स्व॒स्ति पू॒षा असु᳚रो दधातु नः स्व॒स्ति द्यावा᳚पृथि॒वी सु᳚चे॒तुना᳚ ॥",
          "begin": 7.329,
          "end": 14.64
        }
      ]
    },
    {
      "id": "5.51.12",
      "mandala": 5,
      "sukta": 51,
      "rik": 12,
      "padas": [
        {
          "text": "स्व॒स्तये᳚ वा॒युमुप᳚ ब्रवामहै॒ सोमं᳚ स्व॒स्ति भुव᳚नस्य॒ यस्पति᳚ः ।",
          "begin": 0.0,
          "end": 7.971
        },
        {
          "text": "बृह॒स्पतिं॒ सर्व᳚गणं स्व॒स्तये᳚ स्व॒स्तय᳚ आदि॒त्यासो᳚ भवन्तु नः ॥",
          "begin": 7.971,
          "end": 15.64
        }
      ]
    },
    {
      "id": "5.51.13",
      "mandala": 5,
      "sukta": 51,
      "rik": 13,
      "padas": [
        {
          "text": "विश्वे᳚ दे॒वा नो᳚ अ॒द्या स्व॒स्तये᳚ वैश्वान॒रो वसु᳚र॒ग्निः स्व॒स्तये᳚ ।",
          "begin": 0.0,
          "end": 9.474
        },
        {
          "text": "दे॒वा अ᳚वन्त्वृ॒भवः᳚ स्व॒स्तये᳚ स्व॒स्ति नो᳚ रु॒द्रः पा॒त्वंह᳚सः ॥",
          "begin": 9.474,
          "end": 17.36
        }
      ]
    },
    {
      "id": "5.51.14",
      "mandala": 5,
      "sukta": 51,
      "rik": 14,
      "padas": [
        {
          "text": "स्व॒स्ति मि᳚त्रावरुणा स्व॒स्ति प᳚थ्ये रेवति ।",
          "begin": 0.0,
          "end": 5.365
        },
        {
          "text": "स्व॒स्ति न॒ इन्द्र᳚श्चा॒ग्निश्च᳚ स्व॒स्ति नो᳚ अदिते कृधि ॥",
          "begin": 5.365,
          "end": 11.16
        }
      ]
    },
    {
      "id": "5.51.15",
      "mandala": 5,
      "sukta": 51,
      "rik": 15,
      "padas": [
        {
          "text": "स्व॒स्ति पन्था॒मनु᳚ चरेम सूर्याचन्द्र॒मसा᳚विव ।",
          "begin": 0.0,
          "end": 7.447
        },
        {
          "text": "पुन॒र्दद॒ताघ्न᳚ता जान॒ता सं ग᳚मेमहि ॥",
          "begin": 7.447,
          "end": 15.64
        }
      ]
    }
  ]
}