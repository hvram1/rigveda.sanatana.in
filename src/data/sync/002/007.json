{
  "mandala": 2,
  "sukta": 7,
  "verses": [
    {
      "id": "2.7.1",
      "mandala": 2,
      "sukta": 7,
      "rik": 1,
      "padas": [
        {
          "text": "श्रेष्ठं᳚ यविष्ठ भार॒ताग्ने᳚ द्यु॒मन्त॒मा भ᳚र ।",
          "begin": 0.0,
          "end": 9.05
        },
        {
          "text": "वसो᳚ पुरु॒स्पृहं᳚ र॒यिम् ॥",
          "begin": 9.05,
          "end": 12.84
        }
      ]
    },
    {
      "id": "2.7.2",
      "mandala": 2,
      "sukta": 7,
      "rik": 2,
      "padas": [
        {
          "text": "मा नो॒ अरा᳚तिरीशत दे॒वस्य॒ मर्त्य᳚स्य च ।",
          "begin": 0.0,
          "end": 6.031
        },
        {
          "text": "पर्षि॒ तस्या᳚ उ॒त द्वि॒षः ॥",
          "begin": 6.031,
          "end": 10.4
        }
      ]
    },
    {
      "id": "2.7.3",
      "mandala": 2,
      "sukta": 7,
      "rik": 3,
      "padas": [
        {
          "text": "विश्वा᳚ उ॒त त्वया᳚ व॒यं धारा᳚ उद॒न्या᳚ इव ।",
          "begin": 0.0,
          "end": 8.362
        },
        {
          "text": "अति᳚ गाहेमहि॒ द्विषः᳚ ॥",
          "begin": 8.362,
          "end": 11.32
        }
      ]
    },
    {
      "id": "2.7.4",
      "mandala": 2,
      "sukta": 7,
      "rik": 4,
      "padas": [
        {
          "text": "शुचि᳚ः पावक॒ वन्द्यो॑ग्ने᳚ बृ॒हद्वि रो᳚चसे ।",
          "begin": 0.0,
          "end": 5.17
        },
        {
          "text": "त्वं घृ॒तेभि॒राहु᳚तः ॥",
          "begin": 5.17,
          "end": 9.36
        }
      ]
    },
    {
      "id": "2.7.5",
      "mandala": 2,
      "sukta": 7,
      "rik": 5,
      "padas": [
        {
          "text": "त्वं नो᳚ असि भार॒ताग्ने᳚ व॒शाभि᳚रु॒क्षभि᳚ः ।",
          "begin": 0.0,
          "end": 7.236
        },
        {
          "text": "अ॒ष्टाप᳚दीभि॒राहु᳚तः ॥",
          "begin": 7.236,
          "end": 10.24
        }
      ]
    },
    {
      "id": "2.7.6",
      "mandala": 2,
      "sukta": 7,
      "rik": 6,
      "padas": [
        {
          "text": "द्र्व᳚न्नः स॒र्पिरा᳚सुतिः प्र॒त्नो होता॒ वरे᳚ण्यः ।",
          "begin": 0.0,
          "end": 8.156
        },
        {
          "text": "सह᳚सस्पु॒त्रो अद्भु᳚तः ॥",
          "begin": 8.156,
          "end": 12.6
        }
      ]
    }
  ]
}