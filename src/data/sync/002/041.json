{
  "mandala": 2,
  "sukta": 41,
  "verses": [
    {
      "id": "2.41.1",
      "mandala": 2,
      "sukta": 41,
      "rik": 1,
      "padas": [
        {
          "text": "वायो॒ ये ते᳚ सह॒स्रिणो॒ रथा᳚स॒स्तेभि॒रा ग᳚हि ।",
          "begin": 0.0,
          "end": 7.281
        },
        {
          "text": "नि॒युत्वा॒न्त्सोम᳚पीतये ॥",
          "begin": 7.281,
          "end": 10.48
        }
      ]
    },
    {
      "id": "2.41.2",
      "mandala": 2,
      "sukta": 41,
      "rik": 2,
      "padas": [
        {
          "text": "नि॒युत्वा᳚न्वाय॒वा ग᳚ह्य॒यं शु॒क्रो अ᳚यामि ते ।",
          "begin": 0.0,
          "end": 6.436
        },
        {
          "text": "गन्ता᳚सि सुन्व॒तो गृ॒हम् ॥",
          "begin": 6.436,
          "end": 10.08
        }
      ]
    },
    {
      "id": "2.41.3",
      "mandala": 2,
      "sukta": 41,
      "rik": 3,
      "padas": [
        {
          "text": "शु॒क्रस्या॒द्य गवा᳚शिर॒ इन्द्र᳚वायू नि॒युत्व᳚तः ।",
          "begin": 0.0,
          "end": 6.572
        },
        {
          "text": "आ या᳚तं॒ पिब᳚तं नरा ॥",
          "begin": 6.572,
          "end": 9.6
        }
      ]
    },
    {
      "id": "2.41.4",
      "mandala": 2,
      "sukta": 41,
      "rik": 4,
      "padas": [
        {
          "text": "अ॒यं वां᳚ मित्रावरुणा सु॒तः सोम᳚ ऋतावृधा ।",
          "begin": 0.0,
          "end": 5.889
        },
        {
          "text": "ममेदि॒ह श्रु᳚तं॒ हव᳚म् ॥",
          "begin": 5.889,
          "end": 9.4
        }
      ]
    },
    {
      "id": "2.41.5",
      "mandala": 2,
      "sukta": 41,
      "rik": 5,
      "padas": [
        {
          "text": "राजा᳚ना॒वन᳚भिद्रुहा ध्रु॒वे सद᳚स्युत्त॒मे ।",
          "begin": 0.0,
          "end": 6.684
        },
        {
          "text": "स॒हस्र᳚स्थूण आसाते ॥",
          "begin": 6.684,
          "end": 9.84
        }
      ]
    },
    {
      "id": "2.41.6",
      "mandala": 2,
      "sukta": 41,
      "rik": 6,
      "padas": [
        {
          "text": "ता स॒म्राजा᳚ घृ॒तासु᳚ती आदि॒त्या दानु᳚न॒स्पती᳚ ।",
          "begin": 0.0,
          "end": 7.586
        },
        {
          "text": "सचे᳚ते॒ अन᳚वह्वरम् ॥",
          "begin": 7.586,
          "end": 10.92
        }
      ]
    },
    {
      "id": "2.41.7",
      "mandala": 2,
      "sukta": 41,
      "rik": 7,
      "padas": [
        {
          "text": "गोम᳚दू॒ षु ना᳚स॒त्याश्वा᳚वद्यातमश्विना ।",
          "begin": 0.0,
          "end": 6.152
        },
        {
          "text": "व॒र्ती रु᳚द्रा नृ॒पाय्य᳚म् ॥",
          "begin": 6.152,
          "end": 10.2
        }
      ]
    },
    {
      "id": "2.41.8",
      "mandala": 2,
      "sukta": 41,
      "rik": 8,
      "padas": [
        {
          "text": "न यत्परो॒ नान्त᳚र आद॒धर्ष᳚द्वृषण्वसू ।",
          "begin": 0.0,
          "end": 5.268
        },
        {
          "text": "दु॒ःशंसो॒ मर्त्यो᳚ रि॒पुः ॥",
          "begin": 5.268,
          "end": 9.16
        }
      ]
    },
    {
      "id": "2.41.9",
      "mandala": 2,
      "sukta": 41,
      "rik": 9,
      "padas": [
        {
          "text": "ता न॒ आ वो᳚ळ्हमश्विना र॒यिं पि॒शङ्ग᳚संदृशम् ।",
          "begin": 0.0,
          "end": 6.548
        },
        {
          "text": "धिष्ण्या᳚ वरिवो॒विद᳚म् ॥",
          "begin": 6.548,
          "end": 9.76
        }
      ]
    },
    {
      "id": "2.41.10",
      "mandala": 2,
      "sukta": 41,
      "rik": 10,
      "padas": [
        {
          "text": "इन्द्रो᳚ अ॒ङ्ग म॒हद्भ॒यम॒भी षदप᳚ चुच्यवत् ।",
          "begin": 0.0,
          "end": 6.435
        },
        {
          "text": "स हि स्थि॒रो विच᳚र्षणिः ॥",
          "begin": 6.435,
          "end": 8.56
        }
      ]
    },
    {
      "id": "2.41.11",
      "mandala": 2,
      "sukta": 41,
      "rik": 11,
      "padas": [
        {
          "text": "इन्द्र᳚श्च मृ॒ळया᳚ति नो॒ न नः᳚ प॒श्चाद॒घं न᳚शत् ।",
          "begin": 0.0,
          "end": 3.863
        },
        {
          "text": "भ॒द्रं भ᳚वाति नः पु॒रः ॥",
          "begin": 3.863,
          "end": 9.16
        }
      ]
    },
    {
      "id": "2.41.12",
      "mandala": 2,
      "sukta": 41,
      "rik": 12,
      "padas": [
        {
          "text": "इन्द्र॒ आशा᳚भ्य॒स्परि॒ सर्वा᳚भ्यो॒ अभ᳚यं करत् ।",
          "begin": 0.0,
          "end": 7.203
        },
        {
          "text": "जेता॒ शत्रू॒न्विच᳚र्षणिः ॥",
          "begin": 7.203,
          "end": 10.44
        }
      ]
    },
    {
      "id": "2.41.13",
      "mandala": 2,
      "sukta": 41,
      "rik": 13,
      "padas": [
        {
          "text": "विश्वे᳚ देवास॒ आ ग᳚त शृणु॒ता म᳚ इ॒मं हव᳚म् ।",
          "begin": 0.0,
          "end": 6.762
        },
        {
          "text": "एदं ब॒र्हिर्नि षी᳚दत ॥",
          "begin": 6.762,
          "end": 10.08
        }
      ]
    },
    {
      "id": "2.41.14",
      "mandala": 2,
      "sukta": 41,
      "rik": 14,
      "padas": [
        {
          "text": "ती॒व्रो वो॒ मधु᳚माँ अ॒यं शु॒नहो᳚त्रेषु मत्स॒रः ।",
          "begin": 0.0,
          "end": 7.171
        },
        {
          "text": "ए॒तं पि᳚बत॒ काम्य᳚म् ॥",
          "begin": 7.171,
          "end": 10.52
        }
      ]
    },
    {
      "id": "2.41.15",
      "mandala": 2,
      "sukta": 41,
      "rik": 15,
      "padas": [
        {
          "text": "इन्द्र᳚ज्येष्ठा॒ मरु᳚द्गणा॒ देवा᳚स॒ः पूष᳚रातयः ।",
          "begin": 0.0,
          "end": 7.083
        },
        {
          "text": "विश्वे॒ मम᳚ श्रुता॒ हव᳚म् ॥",
          "begin": 7.083,
          "end": 10.32
        }
      ]
    },
    {
      "id": "2.41.16",
      "mandala": 2,
      "sukta": 41,
      "rik": 16,
      "padas": [
        {
          "text": "अम्बि᳚तमे॒ नदी᳚तमे॒ देवि᳚तमे॒ सर᳚स्वति ।",
          "begin": 0.0,
          "end": 5.375
        },
        {
          "text": "अ॒प्र॒श॒स्ता इ᳚व स्मसि॒ प्रश᳚स्तिमम्ब नस्कृधि ॥",
          "begin": 5.375,
          "end": 11.04
        }
      ]
    },
    {
      "id": "2.41.17",
      "mandala": 2,
      "sukta": 41,
      "rik": 17,
      "padas": [
        {
          "text": "त्वे विश्वा᳚ सरस्वति श्रि॒तायूं᳚षि दे॒व्याम् ।",
          "begin": 0.0,
          "end": 6.481
        },
        {
          "text": "शु॒नहो᳚त्रेषु मत्स्व प्र॒जां दे᳚वि दिदिड्ढि नः ॥",
          "begin": 6.481,
          "end": 12.92
        }
      ]
    },
    {
      "id": "2.41.18",
      "mandala": 2,
      "sukta": 41,
      "rik": 18,
      "padas": [
        {
          "text": "इ॒मा ब्रह्म᳚ सरस्वति जु॒षस्व᳚ वाजिनीवति ।",
          "begin": 0.0,
          "end": 4.427
        },
        {
          "text": "या ते॒ मन्म᳚ गृत्सम॒दा ऋ᳚तावरि प्रि॒या दे॒वेषु॒ जुह्व᳚ति ॥",
          "begin": 4.427,
          "end": 11.64
        }
      ]
    },
    {
      "id": "2.41.19",
      "mandala": 2,
      "sukta": 41,
      "rik": 19,
      "padas": [
        {
          "text": "प्रेतां᳚ य॒ज्ञस्य᳚ श॒म्भुवा᳚ यु॒वामिदा वृ᳚णीमहे ।",
          "begin": 0.0,
          "end": 7.147
        },
        {
          "text": "अ॒ग्निं च᳚ हव्य॒वाह᳚नम् ॥",
          "begin": 7.147,
          "end": 10.08
        }
      ]
    },
    {
      "id": "2.41.20",
      "mandala": 2,
      "sukta": 41,
      "rik": 20,
      "padas": [
        {
          "text": "द्यावा᳚ नः पृथि॒वी इ॒मं सि॒ध्रम॒द्य दि᳚वि॒स्पृश᳚म् ।",
          "begin": 0.0,
          "end": 5.789
        },
        {
          "text": "य॒ज्ञं दे॒वेषु᳚ यच्छताम् ॥",
          "begin": 5.789,
          "end": 10.52
        }
      ]
    },
    {
      "id": "2.41.21",
      "mandala": 2,
      "sukta": 41,
      "rik": 21,
      "padas": [
        {
          "text": "आ वा᳚मु॒पस्थ᳚मद्रुहा दे॒वाः सी᳚दन्तु य॒ज्ञिया᳚ः ।",
          "begin": 0.0,
          "end": 8.866
        },
        {
          "text": "इ॒हाद्य सोम᳚पीतये ॥",
          "begin": 8.866,
          "end": 13.08
        }
      ]
    }
  ]
}