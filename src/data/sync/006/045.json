{
  "mandala": 6,
  "sukta": 45,
  "verses": [
    {
      "id": "6.45.1",
      "mandala": 6,
      "sukta": 45,
      "rik": 1,
      "padas": [
        {
          "text": "य आन᳚यत्परा॒वत॒ः सुनी᳚ती तु॒र्वशं॒ यदु᳚म् ।",
          "begin": 0.0,
          "end": 7.268
        },
        {
          "text": "इन्द्र॒ः स नो॒ युवा॒ सखा᳚ ॥",
          "begin": 7.268,
          "end": 11.32
        }
      ]
    },
    {
      "id": "6.45.2",
      "mandala": 6,
      "sukta": 45,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒वि॒प्रे चि॒द्वयो॒ दध᳚दना॒शुना᳚ चि॒दर्व᳚ता ।",
          "begin": 0.0,
          "end": 6.504
        },
        {
          "text": "इन्द्रो॒ जेता᳚ हि॒तं धन᳚म् ॥",
          "begin": 6.504,
          "end": 10.44
        }
      ]
    },
    {
      "id": "6.45.3",
      "mandala": 6,
      "sukta": 45,
      "rik": 3,
      "padas": [
        {
          "text": "म॒हीर᳚स्य॒ प्रणी᳚तयः पू॒र्वीरु॒त प्रश᳚स्तयः ।",
          "begin": 0.0,
          "end": 7.35
        },
        {
          "text": "नास्य᳚ क्षीयन्त ऊ॒तयः᳚ ॥",
          "begin": 7.35,
          "end": 11.2
        }
      ]
    },
    {
      "id": "6.45.4",
      "mandala": 6,
      "sukta": 45,
      "rik": 4,
      "padas": [
        {
          "text": "सखा᳚यो॒ ब्रह्म᳚वाह॒से॑र्च᳚त॒ प्र च᳚ गायत ।",
          "begin": 0.0,
          "end": 6.977
        },
        {
          "text": "स हि न॒ः प्रम᳚तिर्म॒ही ॥",
          "begin": 6.977,
          "end": 9.92
        }
      ]
    },
    {
      "id": "6.45.5",
      "mandala": 6,
      "sukta": 45,
      "rik": 5,
      "padas": [
        {
          "text": "त्वमेक᳚स्य वृत्रहन्नवि॒ता द्वयो᳚रसि ।",
          "begin": 0.0,
          "end": 5.828
        },
        {
          "text": "उ॒तेदृशे॒ यथा᳚ व॒यम् ॥",
          "begin": 5.828,
          "end": 9.84
        }
      ]
    },
    {
      "id": "6.45.6",
      "mandala": 6,
      "sukta": 45,
      "rik": 6,
      "padas": [
        {
          "text": "नय॒सीद्वति॒ द्विषः᳚ कृ॒णोष्यु᳚क्थशं॒सिनः᳚ ।",
          "begin": 0.0,
          "end": 6.369
        },
        {
          "text": "नृभि᳚ः सु॒वीर᳚ उच्यसे ॥",
          "begin": 6.369,
          "end": 9.32
        }
      ]
    },
    {
      "id": "6.45.7",
      "mandala": 6,
      "sukta": 45,
      "rik": 7,
      "padas": [
        {
          "text": "ब्र॒ह्माणं॒ ब्रह्म᳚वाहसं गी॒र्भिः सखा᳚यमृ॒ग्मिय᳚म् ।",
          "begin": 0.0,
          "end": 8.136
        },
        {
          "text": "गां न दो॒हसे᳚ हुवे ॥",
          "begin": 8.136,
          "end": 11.48
        }
      ]
    },
    {
      "id": "6.45.8",
      "mandala": 6,
      "sukta": 45,
      "rik": 8,
      "padas": [
        {
          "text": "यस्य॒ विश्वा᳚नि॒ हस्त᳚योरू॒चुर्वसू᳚नि॒ नि द्वि॒ता ।",
          "begin": 0.0,
          "end": 7.214
        },
        {
          "text": "वी॒रस्य᳚ पृतना॒षहः᳚ ॥",
          "begin": 7.214,
          "end": 11.0
        }
      ]
    },
    {
      "id": "6.45.9",
      "mandala": 6,
      "sukta": 45,
      "rik": 9,
      "padas": [
        {
          "text": "वि दृ॒ळ्हानि᳚ चिदद्रिवो॒ जना᳚नां शचीपते ।",
          "begin": 0.0,
          "end": 6.95
        },
        {
          "text": "वृ॒ह मा॒या अ᳚नानत ॥",
          "begin": 6.95,
          "end": 9.84
        }
      ]
    },
    {
      "id": "6.45.10",
      "mandala": 6,
      "sukta": 45,
      "rik": 10,
      "padas": [
        {
          "text": "तमु᳚ त्वा सत्य सोमपा॒ इन्द्र᳚ वाजानां पते ।",
          "begin": 0.0,
          "end": 6.069
        },
        {
          "text": "अहू᳚महि श्रव॒स्यवः᳚ ॥",
          "begin": 6.069,
          "end": 10.76
        }
      ]
    },
    {
      "id": "6.45.11",
      "mandala": 6,
      "sukta": 45,
      "rik": 11,
      "padas": [
        {
          "text": "तमु᳚ त्वा॒ यः पु॒रासि᳚थ॒ यो वा᳚ नू॒नं हि॒ते धने᳚ ।",
          "begin": 0.0,
          "end": 7.406
        },
        {
          "text": "हव्य॒ः स श्रु᳚धी॒ हव᳚म् ॥",
          "begin": 7.406,
          "end": 10.6
        }
      ]
    },
    {
      "id": "6.45.12",
      "mandala": 6,
      "sukta": 45,
      "rik": 12,
      "padas": [
        {
          "text": "धी॒भिरर्व᳚द्भि॒रर्व᳚तो॒ वाजाँ᳚ इन्द्र श्र॒वाय्या᳚न् ।",
          "begin": 0.0,
          "end": 7.904
        },
        {
          "text": "त्वया᳚ जेष्म हि॒तं धन᳚म् ॥",
          "begin": 7.904,
          "end": 11.88
        }
      ]
    },
    {
      "id": "6.45.13",
      "mandala": 6,
      "sukta": 45,
      "rik": 13,
      "padas": [
        {
          "text": "अभू᳚रु वीर गिर्वणो म॒हाँ इ᳚न्द्र॒ धने᳚ हि॒ते ।",
          "begin": 0.0,
          "end": 9.389
        },
        {
          "text": "भरे᳚ वितन्त॒साय्य᳚ः ॥",
          "begin": 9.389,
          "end": 12.48
        }
      ]
    },
    {
      "id": "6.45.14",
      "mandala": 6,
      "sukta": 45,
      "rik": 14,
      "padas": [
        {
          "text": "या त᳚ ऊ॒तिर᳚मित्रहन्म॒क्षूज᳚वस्त॒मास᳚ति ।",
          "begin": 0.0,
          "end": 6.102
        },
        {
          "text": "तया᳚ नो हिनुही॒ रथ᳚म् ॥",
          "begin": 6.102,
          "end": 10.52
        }
      ]
    },
    {
      "id": "6.45.15",
      "mandala": 6,
      "sukta": 45,
      "rik": 15,
      "padas": [
        {
          "text": "स रथे᳚न र॒थीत᳚मो॒॑स्माके᳚नाभि॒युग्व᳚ना ।",
          "begin": 0.0,
          "end": 7.159
        },
        {
          "text": "जेषि᳚ जिष्णो हि॒तं धन᳚म् ॥",
          "begin": 7.159,
          "end": 11.4
        }
      ]
    },
    {
      "id": "6.45.16",
      "mandala": 6,
      "sukta": 45,
      "rik": 16,
      "padas": [
        {
          "text": "य एक॒ इत्तमु᳚ ष्टुहि कृष्टी॒नां विच᳚र्षणिः ।",
          "begin": 0.0,
          "end": 4.257
        },
        {
          "text": "पति᳚र्ज॒ज्ञे वृष᳚क्रतुः ॥",
          "begin": 4.257,
          "end": 9.32
        }
      ]
    },
    {
      "id": "6.45.17",
      "mandala": 6,
      "sukta": 45,
      "rik": 17,
      "padas": [
        {
          "text": "यो गृ᳚ण॒तामिदासि᳚था॒पिरू॒ती शि॒वः सखा᳚ ।",
          "begin": 0.0,
          "end": 6.007
        },
        {
          "text": "स त्वं न᳚ इन्द्र मृळय ॥",
          "begin": 6.007,
          "end": 8.96
        }
      ]
    },
    {
      "id": "6.45.18",
      "mandala": 6,
      "sukta": 45,
      "rik": 18,
      "padas": [
        {
          "text": "धि॒ष्व वज्रं॒ गभ᳚स्त्यो रक्षो॒हत्या᳚य वज्रिवः ।",
          "begin": 0.0,
          "end": 6.683
        },
        {
          "text": "सा॒स॒ही॒ष्ठा अ॒भि स्पृधः᳚ ॥",
          "begin": 6.683,
          "end": 10.44
        }
      ]
    },
    {
      "id": "6.45.19",
      "mandala": 6,
      "sukta": 45,
      "rik": 19,
      "padas": [
        {
          "text": "प्र॒त्नं र᳚यी॒णां युजं॒ सखा᳚यं कीरि॒चोद᳚नम् ।",
          "begin": 0.0,
          "end": 6.613
        },
        {
          "text": "ब्रह्म᳚वाहस्तमं हुवे ॥",
          "begin": 6.613,
          "end": 9.6
        }
      ]
    },
    {
      "id": "6.45.20",
      "mandala": 6,
      "sukta": 45,
      "rik": 20,
      "padas": [
        {
          "text": "स हि विश्वा᳚नि॒ पार्थि᳚वाँ॒ एको॒ वसू᳚नि॒ पत्य᳚ते ।",
          "begin": 0.0,
          "end": 7.364
        },
        {
          "text": "गिर्व᳚णस्तमो॒ अध्रि᳚गुः ॥",
          "begin": 7.364,
          "end": 11.24
        }
      ]
    },
    {
      "id": "6.45.21",
      "mandala": 6,
      "sukta": 45,
      "rik": 21,
      "padas": [
        {
          "text": "स नो᳚ नि॒युद्भि॒रा पृ᳚ण॒ कामं॒ वाजे᳚भिर॒श्विभि᳚ः ।",
          "begin": 0.0,
          "end": 6.518
        },
        {
          "text": "गोम᳚द्भिर्गोपते धृ॒षत् ॥",
          "begin": 6.518,
          "end": 11.04
        }
      ]
    },
    {
      "id": "6.45.22",
      "mandala": 6,
      "sukta": 45,
      "rik": 22,
      "padas": [
        {
          "text": "तद्वो᳚ गाय सु॒ते सचा᳚ पुरुहू॒ताय॒ सत्व᳚ने ।",
          "begin": 0.0,
          "end": 7.332
        },
        {
          "text": "शं यद्गवे॒ न शा॒किने᳚ ॥",
          "begin": 7.332,
          "end": 10.76
        }
      ]
    },
    {
      "id": "6.45.23",
      "mandala": 6,
      "sukta": 45,
      "rik": 23,
      "padas": [
        {
          "text": "न घा॒ वसु॒र्नि य᳚मते दा॒नं वाज᳚स्य॒ गोम᳚तः ।",
          "begin": 0.0,
          "end": 6.428
        },
        {
          "text": "यत्सी॒मुप॒ श्रव॒द्गिरः᳚ ॥",
          "begin": 6.428,
          "end": 9.52
        }
      ]
    },
    {
      "id": "6.45.24",
      "mandala": 6,
      "sukta": 45,
      "rik": 24,
      "padas": [
        {
          "text": "कु॒वित्स᳚स्य॒ प्र हि व्र॒जं गोम᳚न्तं दस्यु॒हा गम᳚त् ।",
          "begin": 0.0,
          "end": 7.237
        },
        {
          "text": "शची᳚भि॒रप᳚ नो वरत् ॥",
          "begin": 7.237,
          "end": 10.36
        }
      ]
    },
    {
      "id": "6.45.25",
      "mandala": 6,
      "sukta": 45,
      "rik": 25,
      "padas": [
        {
          "text": "इ॒मा उ᳚ त्वा शतक्रतो॒॑भि प्र णो᳚नुवु॒र्गिरः᳚ ।",
          "begin": 0.0,
          "end": 6.661
        },
        {
          "text": "इन्द्र᳚ व॒त्सं न मा॒तरः᳚ ॥",
          "begin": 6.661,
          "end": 9.84
        }
      ]
    },
    {
      "id": "6.45.26",
      "mandala": 6,
      "sukta": 45,
      "rik": 26,
      "padas": [
        {
          "text": "दू॒णाशं᳚ स॒ख्यं तव॒ गौर᳚सि वीर गव्य॒ते ।",
          "begin": 0.0,
          "end": 6.697
        },
        {
          "text": "अश्वो᳚ अश्वाय॒ते भ᳚व ॥",
          "begin": 6.697,
          "end": 10.2
        }
      ]
    },
    {
      "id": "6.45.27",
      "mandala": 6,
      "sukta": 45,
      "rik": 27,
      "padas": [
        {
          "text": "स म᳚न्दस्वा॒ ह्यन्ध᳚सो॒ राध᳚से त॒न्वा᳚ म॒हे ।",
          "begin": 0.0,
          "end": 7.461
        },
        {
          "text": "न स्तो॒तारं᳚ नि॒दे क᳚रः ॥",
          "begin": 7.461,
          "end": 11.12
        }
      ]
    },
    {
      "id": "6.45.28",
      "mandala": 6,
      "sukta": 45,
      "rik": 28,
      "padas": [
        {
          "text": "इ॒मा उ᳚ त्वा सु॒तेसु᳚ते॒ नक्ष᳚न्ते गिर्वणो॒ गिरः᳚ ।",
          "begin": 0.0,
          "end": 7.558
        },
        {
          "text": "व॒त्सं गावो॒ न धे॒नवः᳚ ॥",
          "begin": 7.558,
          "end": 10.84
        }
      ]
    },
    {
      "id": "6.45.29",
      "mandala": 6,
      "sukta": 45,
      "rik": 29,
      "padas": [
        {
          "text": "पु॒रू॒तमं᳚ पुरू॒णां स्तो᳚तॄ॒णां विवा᳚चि ।",
          "begin": 0.0,
          "end": 5.314
        },
        {
          "text": "वाजे᳚भिर्वाजय॒ताम् ॥",
          "begin": 5.314,
          "end": 11.52
        }
      ]
    },
    {
      "id": "6.45.30",
      "mandala": 6,
      "sukta": 45,
      "rik": 30,
      "padas": [
        {
          "text": "अ॒स्माक᳚मिन्द्र भूतु ते॒ स्तोमो॒ वाहि᳚ष्ठो॒ अन्त᳚मः ।",
          "begin": 0.0,
          "end": 7.673
        },
        {
          "text": "अ॒स्मान्रा॒ये म॒हे हि᳚नु ॥",
          "begin": 7.673,
          "end": 11.04
        }
      ]
    },
    {
      "id": "6.45.31",
      "mandala": 6,
      "sukta": 45,
      "rik": 31,
      "padas": [
        {
          "text": "अधि᳚ बृ॒बुः प᳚णी॒नां वर्षि᳚ष्ठे मू॒र्धन्न᳚स्थात् ।",
          "begin": 0.0,
          "end": 7.082
        },
        {
          "text": "उ॒रुः कक्षो॒ न गा॒ङ्ग्यः ॥",
          "begin": 7.082,
          "end": 10.44
        }
      ]
    },
    {
      "id": "6.45.32",
      "mandala": 6,
      "sukta": 45,
      "rik": 32,
      "padas": [
        {
          "text": "यस्य᳚ वा॒योरि᳚व द्र॒वद्भ॒द्रा रा॒तिः स᳚ह॒स्रिणी᳚ ।",
          "begin": 0.0,
          "end": 6.976
        },
        {
          "text": "स॒द्यो दा॒नाय॒ मंह᳚ते ॥",
          "begin": 6.976,
          "end": 10.56
        }
      ]
    },
    {
      "id": "6.45.33",
      "mandala": 6,
      "sukta": 45,
      "rik": 33,
      "padas": [
        {
          "text": "तत्सु नो॒ विश्वे᳚ अ॒र्य आ सदा᳚ गृणन्ति का॒रवः᳚ ।",
          "begin": 0.0,
          "end": 8.034
        },
        {
          "text": "बृ॒बुं स᳚हस्र॒दात᳚मं सू॒रिं स᳚हस्र॒सात᳚मम् ॥",
          "begin": 8.034,
          "end": 14.28
        }
      ]
    }
  ]
}