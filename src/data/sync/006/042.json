{
  "mandala": 6,
  "sukta": 42,
  "verses": [
    {
      "id": "6.42.1",
      "mandala": 6,
      "sukta": 42,
      "rik": 1,
      "padas": [
        {
          "text": "प्रत्य᳚स्मै॒ पिपी᳚षते॒ विश्वा᳚नि वि॒दुषे᳚ भर ।",
          "begin": 0.0,
          "end": 8.463
        },
        {
          "text": "अ॒रं॒ग॒माय॒ जग्म॒ये॑प᳚श्चाद्दघ्वने॒ नरे᳚ ॥",
          "begin": 8.463,
          "end": 14.08
        }
      ]
    },
    {
      "id": "6.42.2",
      "mandala": 6,
      "sukta": 42,
      "rik": 2,
      "padas": [
        {
          "text": "एमे᳚नं प्र॒त्येत᳚न॒ सोमे᳚भिः सोम॒पात᳚मम् ।",
          "begin": 0.0,
          "end": 7.213
        },
        {
          "text": "अम᳚त्रेभिरृजी॒षिण॒मिन्द्रं᳚ सु॒तेभि॒रिन्दु᳚भिः ॥",
          "begin": 7.213,
          "end": 12.72
        }
      ]
    },
    {
      "id": "6.42.3",
      "mandala": 6,
      "sukta": 42,
      "rik": 3,
      "padas": [
        {
          "text": "यदी᳚ सु॒तेभि॒रिन्दु᳚भि॒ः सोमे᳚भिः प्रति॒भूष᳚थ ।",
          "begin": 0.0,
          "end": 6.053
        },
        {
          "text": "वेदा॒ विश्व᳚स्य॒ मेधि᳚रो धृ॒षत्तंत॒मिदेष᳚ते ॥",
          "begin": 6.053,
          "end": 12.36
        }
      ]
    },
    {
      "id": "6.42.4",
      "mandala": 6,
      "sukta": 42,
      "rik": 4,
      "padas": [
        {
          "text": "अ॒स्माअ᳚स्मा॒ इदन्ध॒सो॑ध्व᳚र्यो॒ प्र भ᳚रा सु॒तम् ।",
          "begin": 0.0,
          "end": 8.157
        },
        {
          "text": "कु॒वित्स᳚मस्य॒ जेन्य᳚स्य॒ शर्ध᳚तो॒॑भिश᳚स्तेरव॒स्पर᳚त् ॥",
          "begin": 8.157,
          "end": 16.16
        }
      ]
    }
  ]
}