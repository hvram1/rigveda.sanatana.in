{
  "mandala": 6,
  "sukta": 56,
  "verses": [
    {
      "id": "6.56.1",
      "mandala": 6,
      "sukta": 56,
      "rik": 1,
      "padas": [
        {
          "text": "य ए᳚नमा॒दिदे᳚शति कर॒म्भादिति᳚ पू॒षण᳚म् ।",
          "begin": 0.0,
          "end": 7.14
        },
        {
          "text": "न तेन᳚ दे॒व आ॒दिशे᳚ ॥",
          "begin": 7.14,
          "end": 11.44
        }
      ]
    },
    {
      "id": "6.56.2",
      "mandala": 6,
      "sukta": 56,
      "rik": 2,
      "padas": [
        {
          "text": "उ॒त घा॒ स र॒थीत᳚म॒ः सख्या॒ सत्प᳚तिर्यु॒जा ।",
          "begin": 0.0,
          "end": 5.365
        },
        {
          "text": "इन्द्रो᳚ वृ॒त्राणि᳚ जिघ्नते ॥",
          "begin": 5.365,
          "end": 9.76
        }
      ]
    },
    {
      "id": "6.56.3",
      "mandala": 6,
      "sukta": 56,
      "rik": 3,
      "padas": [
        {
          "text": "उ॒तादः प᳚रु॒षे गवि॒ सूर᳚श्च॒क्रं हि᳚र॒ण्यय᳚म् ।",
          "begin": 0.0,
          "end": 6.416
        },
        {
          "text": "न्यै᳚रयद्र॒थीत᳚मः ॥",
          "begin": 6.416,
          "end": 10.2
        }
      ]
    },
    {
      "id": "6.56.4",
      "mandala": 6,
      "sukta": 56,
      "rik": 4,
      "padas": [
        {
          "text": "यद॒द्य त्वा᳚ पुरुष्टुत॒ ब्रवा᳚म दस्र मन्तुमः ।",
          "begin": 0.0,
          "end": 7.207
        },
        {
          "text": "तत्सु नो॒ मन्म᳚ साधय ॥",
          "begin": 7.207,
          "end": 10.2
        }
      ]
    },
    {
      "id": "6.56.5",
      "mandala": 6,
      "sukta": 56,
      "rik": 5,
      "padas": [
        {
          "text": "इ॒मं च᳚ नो ग॒वेष᳚णं सा॒तये᳚ सीषधो ग॒णम् ।",
          "begin": 0.0,
          "end": 8.132
        },
        {
          "text": "आ॒रात्पू᳚षन्नसि श्रु॒तः ॥",
          "begin": 8.132,
          "end": 11.08
        }
      ]
    },
    {
      "id": "6.56.6",
      "mandala": 6,
      "sukta": 56,
      "rik": 6,
      "padas": [
        {
          "text": "आ ते᳚ स्व॒स्तिमी᳚मह आ॒रेअ᳚घा॒मुपा᳚वसुम् ।",
          "begin": 0.0,
          "end": 8.511
        },
        {
          "text": "अ॒द्या च᳚ स॒र्वता᳚तये॒ श्वश्च᳚ स॒र्वता᳚तये ॥",
          "begin": 8.511,
          "end": 15.68
        }
      ]
    }
  ]
}