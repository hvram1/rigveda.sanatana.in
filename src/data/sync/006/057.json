{
  "mandala": 6,
  "sukta": 57,
  "verses": [
    {
      "id": "6.57.1",
      "mandala": 6,
      "sukta": 57,
      "rik": 1,
      "padas": [
        {
          "text": "इन्द्रा॒ नु पू॒षणा᳚ व॒यं स॒ख्याय᳚ स्व॒स्तये᳚ ।",
          "begin": 0.0,
          "end": 7.343
        },
        {
          "text": "हु॒वेम॒ वाज᳚सातये ॥",
          "begin": 7.343,
          "end": 11.04
        }
      ]
    },
    {
      "id": "6.57.2",
      "mandala": 6,
      "sukta": 57,
      "rik": 2,
      "padas": [
        {
          "text": "सोम᳚म॒न्य उपा᳚सद॒त्पात᳚वे च॒म्वो᳚ः सु॒तम् ।",
          "begin": 0.0,
          "end": 6.31
        },
        {
          "text": "क॒र॒म्भम॒न्य इ᳚च्छति ॥",
          "begin": 6.31,
          "end": 10.28
        }
      ]
    },
    {
      "id": "6.57.3",
      "mandala": 6,
      "sukta": 57,
      "rik": 3,
      "padas": [
        {
          "text": "अ॒जा अ॒न्यस्य॒ वह्न᳚यो॒ हरी᳚ अ॒न्यस्य॒ सम्भृ᳚ता ।",
          "begin": 0.0,
          "end": 6.742
        },
        {
          "text": "ताभ्यां᳚ वृ॒त्राणि᳚ जिघ्नते ॥",
          "begin": 6.742,
          "end": 11.48
        }
      ]
    },
    {
      "id": "6.57.4",
      "mandala": 6,
      "sukta": 57,
      "rik": 4,
      "padas": [
        {
          "text": "यदिन्द्रो॒ अन᳚य॒द्रितो᳚ म॒हीर॒पो वृष᳚न्तमः ।",
          "begin": 0.0,
          "end": 5.97
        },
        {
          "text": "तत्र᳚ पू॒षाभ᳚व॒त्सचा᳚ ॥",
          "begin": 5.97,
          "end": 11.16
        }
      ]
    },
    {
      "id": "6.57.5",
      "mandala": 6,
      "sukta": 57,
      "rik": 5,
      "padas": [
        {
          "text": "तां पू॒ष्णः सु᳚म॒तिं व॒यं वृ॒क्षस्य॒ प्र व॒यामि᳚व ।",
          "begin": 0.0,
          "end": 7.177
        },
        {
          "text": "इन्द्र᳚स्य॒ चा र᳚भामहे ॥",
          "begin": 7.177,
          "end": 10.28
        }
      ]
    },
    {
      "id": "6.57.6",
      "mandala": 6,
      "sukta": 57,
      "rik": 6,
      "padas": [
        {
          "text": "उत्पू॒षणं᳚ युवामहे॒॑भीशूँ᳚रिव॒ सार᳚थिः ।",
          "begin": 0.0,
          "end": 7.52
        },
        {
          "text": "म॒ह्या इन्द्रं᳚ स्व॒स्तये᳚ ॥",
          "begin": 7.52,
          "end": 12.84
        }
      ]
    }
  ]
}