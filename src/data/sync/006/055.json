{
  "mandala": 6,
  "sukta": 55,
  "verses": [
    {
      "id": "6.55.1",
      "mandala": 6,
      "sukta": 55,
      "rik": 1,
      "padas": [
        {
          "text": "एहि॒ वां वि᳚मुचो नपा॒दाघृ᳚णे॒ सं स᳚चावहै ।",
          "begin": 0.0,
          "end": 7.937
        },
        {
          "text": "र॒थीरृ॒तस्य᳚ नो भव ॥",
          "begin": 7.937,
          "end": 10.84
        }
      ]
    },
    {
      "id": "6.55.2",
      "mandala": 6,
      "sukta": 55,
      "rik": 2,
      "padas": [
        {
          "text": "र॒थीत᳚मं कप॒र्दिन॒मीशा᳚नं॒ राध᳚सो म॒हः ।",
          "begin": 0.0,
          "end": 4.347
        },
        {
          "text": "रा॒यः सखा᳚यमीमहे ॥",
          "begin": 4.347,
          "end": 10.6
        }
      ]
    },
    {
      "id": "6.55.3",
      "mandala": 6,
      "sukta": 55,
      "rik": 3,
      "padas": [
        {
          "text": "रा॒यो धारा᳚स्याघृणे॒ वसो᳚ रा॒शिर᳚जाश्व ।",
          "begin": 0.0,
          "end": 7.925
        },
        {
          "text": "धीव᳚तोधीवत॒ः सखा᳚ ॥",
          "begin": 7.925,
          "end": 11.6
        }
      ]
    },
    {
      "id": "6.55.4",
      "mandala": 6,
      "sukta": 55,
      "rik": 4,
      "padas": [
        {
          "text": "पू॒षणं॒ न्व१॒᳚जाश्व॒मुप᳚ स्तोषाम वा॒जिन᳚म् ।",
          "begin": 0.0,
          "end": 6.882
        },
        {
          "text": "स्वसु॒र्यो जा॒र उ॒च्यते᳚ ॥",
          "begin": 6.882,
          "end": 10.68
        }
      ]
    },
    {
      "id": "6.55.5",
      "mandala": 6,
      "sukta": 55,
      "rik": 5,
      "padas": [
        {
          "text": "मा॒तुर्दि᳚धि॒षुम᳚ब्रवं॒ स्वसु᳚र्जा॒रः शृ᳚णोतु नः ।",
          "begin": 0.0,
          "end": 7.303
        },
        {
          "text": "भ्रातेन्द्र᳚स्य॒ सखा॒ मम᳚ ॥",
          "begin": 7.303,
          "end": 10.16
        }
      ]
    },
    {
      "id": "6.55.6",
      "mandala": 6,
      "sukta": 55,
      "rik": 6,
      "padas": [
        {
          "text": "आजासः᳚ पू॒षणं॒ रथे᳚ निशृ॒म्भास्ते ज᳚न॒श्रिय᳚म् ।",
          "begin": 0.0,
          "end": 7.364
        },
        {
          "text": "दे॒वं व᳚हन्तु॒ बिभ्र᳚तः ॥",
          "begin": 7.364,
          "end": 12.6
        }
      ]
    }
  ]
}