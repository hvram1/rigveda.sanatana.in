{
  "mandala": 6,
  "sukta": 44,
  "verses": [
    {
      "id": "6.44.1",
      "mandala": 6,
      "sukta": 44,
      "rik": 1,
      "padas": [
        {
          "text": "यो र᳚यिवो र॒यिंत᳚मो॒ यो द्यु॒म्नैर्द्यु॒म्नव᳚त्तमः ।",
          "begin": 0.0,
          "end": 7.892
        },
        {
          "text": "सोमः᳚ सु॒तः स इ᳚न्द्र॒ ते॑स्ति᳚ स्वधापते॒ मदः᳚ ॥",
          "begin": 7.892,
          "end": 13.88
        }
      ]
    },
    {
      "id": "6.44.2",
      "mandala": 6,
      "sukta": 44,
      "rik": 2,
      "padas": [
        {
          "text": "यः श॒ग्मस्तु᳚विशग्म ते रा॒यो दा॒मा म᳚ती॒नाम् ।",
          "begin": 0.0,
          "end": 6.842
        },
        {
          "text": "सोमः᳚ सु॒तः स इ᳚न्द्र॒ ते॑स्ति᳚ स्वधापते॒ मदः᳚ ॥",
          "begin": 6.842,
          "end": 12.8
        }
      ]
    },
    {
      "id": "6.44.3",
      "mandala": 6,
      "sukta": 44,
      "rik": 3,
      "padas": [
        {
          "text": "येन᳚ वृ॒द्धो न शव᳚सा तु॒रो न स्वाभि᳚रू॒तिभि᳚ः ।",
          "begin": 0.0,
          "end": 6.688
        },
        {
          "text": "सोमः᳚ सु॒तः स इ᳚न्द्र॒ ते॑स्ति᳚ स्वधापते॒ मदः᳚ ॥",
          "begin": 6.688,
          "end": 12.56
        }
      ]
    },
    {
      "id": "6.44.4",
      "mandala": 6,
      "sukta": 44,
      "rik": 4,
      "padas": [
        {
          "text": "त्यमु᳚ वो॒ अप्र᳚हणं गृणी॒षे शव᳚स॒स्पति᳚म् ।",
          "begin": 0.0,
          "end": 6.383
        },
        {
          "text": "इन्द्रं᳚ विश्वा॒साहं॒ नरं॒ मंहि᳚ष्ठं वि॒श्वच᳚र्षणिम् ॥",
          "begin": 6.383,
          "end": 12.32
        }
      ]
    },
    {
      "id": "6.44.5",
      "mandala": 6,
      "sukta": 44,
      "rik": 5,
      "padas": [
        {
          "text": "यं व॒र्धय॒न्तीद्गिर॒ः पतिं᳚ तु॒रस्य॒ राध᳚सः ।",
          "begin": 0.0,
          "end": 6.461
        },
        {
          "text": "तमिन्न्व᳚स्य॒ रोद᳚सी दे॒वी शुष्मं᳚ सपर्यतः ॥",
          "begin": 6.461,
          "end": 12.12
        }
      ]
    },
    {
      "id": "6.44.6",
      "mandala": 6,
      "sukta": 44,
      "rik": 6,
      "padas": [
        {
          "text": "तद्व᳚ उ॒क्थस्य᳚ ब॒र्हणेन्द्रा᳚योपस्तृणी॒षणि᳚ ।",
          "begin": 0.0,
          "end": 5.744
        },
        {
          "text": "विपो॒ न यस्यो॒तयो॒ वि यद्रोह᳚न्ति स॒क्षितः᳚ ॥",
          "begin": 5.744,
          "end": 12.12
        }
      ]
    },
    {
      "id": "6.44.7",
      "mandala": 6,
      "sukta": 44,
      "rik": 7,
      "padas": [
        {
          "text": "अवि᳚द॒द्दक्षं᳚ मि॒त्रो नवी᳚यान्पपा॒नो दे॒वेभ्यो॒ वस्यो᳚ अचैत् ।",
          "begin": 0.0,
          "end": 10.295
        },
        {
          "text": "स॒स॒वान्त्स्तौ॒लाभि᳚र्धौ॒तरी᳚भिरुरु॒ष्या पा॒युर᳚भव॒त्सखि᳚भ्यः ॥",
          "begin": 10.295,
          "end": 18.08
        }
      ]
    },
    {
      "id": "6.44.8",
      "mandala": 6,
      "sukta": 44,
      "rik": 8,
      "padas": [
        {
          "text": "ऋ॒तस्य᳚ प॒थि वे॒धा अ᳚पायि श्रि॒ये मनां᳚सि दे॒वासो᳚ अक्रन् ।",
          "begin": 0.0,
          "end": 9.279
        },
        {
          "text": "दधा᳚नो॒ नाम᳚ म॒हो वचो᳚भि॒र्वपु᳚र्दृ॒शये᳚ वे॒न्यो व्या᳚वः ॥",
          "begin": 9.279,
          "end": 18.04
        }
      ]
    },
    {
      "id": "6.44.9",
      "mandala": 6,
      "sukta": 44,
      "rik": 9,
      "padas": [
        {
          "text": "द्यु॒मत्त᳚मं॒ दक्षं᳚ धेह्य॒स्मे सेधा॒ जना᳚नां पू॒र्वीररा᳚तीः ।",
          "begin": 0.0,
          "end": 9.302
        },
        {
          "text": "वर्षी᳚यो॒ वयः᳚ कृणुहि॒ शची᳚भि॒र्धन᳚स्य सा॒ताव॒स्माँ अ᳚विड्ढि ॥",
          "begin": 9.302,
          "end": 18.4
        }
      ]
    },
    {
      "id": "6.44.10",
      "mandala": 6,
      "sukta": 44,
      "rik": 10,
      "padas": [
        {
          "text": "इन्द्र॒ तुभ्य॒मिन्म᳚घवन्नभूम व॒यं दा॒त्रे ह᳚रिवो॒ मा वि वे᳚नः ।",
          "begin": 0.0,
          "end": 5.123
        },
        {
          "text": "नकि᳚रा॒पिर्द᳚दृशे मर्त्य॒त्रा किम॒ङ्ग र᳚ध्र॒चोद᳚नं त्वाहुः ॥",
          "begin": 5.123,
          "end": 14.96
        }
      ]
    },
    {
      "id": "6.44.11",
      "mandala": 6,
      "sukta": 44,
      "rik": 11,
      "padas": [
        {
          "text": "मा जस्व᳚ने वृषभ नो ररीथा॒ मा ते᳚ रे॒वतः᳚ स॒ख्ये रि᳚षाम ।",
          "begin": 0.0,
          "end": 9.602
        },
        {
          "text": "पू॒र्वीष्ट᳚ इन्द्र नि॒ष्षिधो॒ जने᳚षु ज॒ह्यसु᳚ष्वी॒न्प्र वृ॒हापृ᳚णतः ॥",
          "begin": 9.602,
          "end": 16.16
        }
      ]
    },
    {
      "id": "6.44.12",
      "mandala": 6,
      "sukta": 44,
      "rik": 12,
      "padas": [
        {
          "text": "उद॒भ्राणी᳚व स्त॒नय᳚न्निय॒र्तीन्द्रो॒ राधां॒स्यश्व्या᳚नि॒ गव्या᳚ ।",
          "begin": 0.0,
          "end": 9.664
        },
        {
          "text": "त्वम᳚सि प्र॒दिवः᳚ का॒रुधा᳚या॒ मा त्वा᳚दा॒मान॒ आ द᳚भन्म॒घोनः᳚ ॥",
          "begin": 9.664,
          "end": 18.44
        }
      ]
    },
    {
      "id": "6.44.13",
      "mandala": 6,
      "sukta": 44,
      "rik": 13,
      "padas": [
        {
          "text": "अध्व᳚र्यो वीर॒ प्र म॒हे सु॒ताना॒मिन्द्रा᳚य भर॒ स ह्य᳚स्य॒ राजा᳚ ।",
          "begin": 0.0,
          "end": 8.624
        },
        {
          "text": "यः पू॒र्व्याभि᳚रु॒त नूत᳚नाभिर्गी॒र्भिर्वा᳚वृ॒धे गृ᳚ण॒तामृषी᳚णाम् ॥",
          "begin": 8.624,
          "end": 17.56
        }
      ]
    },
    {
      "id": "6.44.14",
      "mandala": 6,
      "sukta": 44,
      "rik": 14,
      "padas": [
        {
          "text": "अ॒स्य मदे᳚ पु॒रु वर्पां᳚सि वि॒द्वानिन्द्रो᳚ वृ॒त्राण्य᳚प्र॒ती ज᳚घान ।",
          "begin": 0.0,
          "end": 9.653
        },
        {
          "text": "तमु॒ प्र हो᳚षि॒ मधु᳚मन्तमस्मै॒ सोमं᳚ वी॒राय᳚ शि॒प्रिणे॒ पिब᳚ध्यै ॥",
          "begin": 9.653,
          "end": 17.52
        }
      ]
    },
    {
      "id": "6.44.15",
      "mandala": 6,
      "sukta": 44,
      "rik": 15,
      "padas": [
        {
          "text": "पाता᳚ सु॒तमिन्द्रो᳚ अस्तु॒ सोमं॒ हन्ता᳚ वृ॒त्रं वज्रे᳚ण मन्दसा॒नः ।",
          "begin": 0.0,
          "end": 10.002
        },
        {
          "text": "गन्ता᳚ य॒ज्ञं प᳚रा॒वत᳚श्चि॒दच्छा॒ वसु᳚र्धी॒नाम᳚वि॒ता का॒रुधा᳚याः ॥",
          "begin": 10.002,
          "end": 19.64
        }
      ]
    },
    {
      "id": "6.44.16",
      "mandala": 6,
      "sukta": 44,
      "rik": 16,
      "padas": [
        {
          "text": "इ॒दं त्यत्पात्र᳚मिन्द्र॒पान॒मिन्द्र᳚स्य प्रि॒यम॒मृत᳚मपायि ।",
          "begin": 0.0,
          "end": 7.257
        },
        {
          "text": "मत्स॒द्यथा᳚ सौमन॒साय᳚ दे॒वं व्य१॒᳚स्मद्द्वेषो᳚ यु॒यव॒द्व्यंहः᳚ ॥",
          "begin": 7.257,
          "end": 16.44
        }
      ]
    },
    {
      "id": "6.44.17",
      "mandala": 6,
      "sukta": 44,
      "rik": 17,
      "padas": [
        {
          "text": "ए॒ना म᳚न्दा॒नो ज॒हि शू᳚र॒ शत्रू᳚ञ्जा॒मिमजा᳚मिं मघवन्न॒मित्रा᳚न् ।",
          "begin": 0.0,
          "end": 10.854
        },
        {
          "text": "अ॒भि॒षे॒णाँ अ॒भ्या॒३॒᳚देदि᳚शाना॒न्परा᳚च इन्द्र॒ प्र मृ᳚णा ज॒ही च᳚ ॥",
          "begin": 10.854,
          "end": 21.24
        }
      ]
    },
    {
      "id": "6.44.18",
      "mandala": 6,
      "sukta": 44,
      "rik": 18,
      "padas": [
        {
          "text": "आ॒सु ष्मा᳚ णो मघवन्निन्द्र पृ॒त्स्व१॒᳚स्मभ्यं॒ महि॒ वरि᳚वः सु॒गं कः᳚ ।",
          "begin": 0.0,
          "end": 9.42
        },
        {
          "text": "अ॒पां तो॒कस्य॒ तन᳚यस्य जे॒ष इन्द्र᳚ सू॒रीन्कृ᳚णु॒हि स्मा᳚ नो अ॒र्धम् ॥",
          "begin": 9.42,
          "end": 17.76
        }
      ]
    },
    {
      "id": "6.44.19",
      "mandala": 6,
      "sukta": 44,
      "rik": 19,
      "padas": [
        {
          "text": "आ त्वा॒ हर᳚यो॒ वृष᳚णो युजा॒ना वृष᳚रथासो॒ वृष᳚रश्म॒यो॑त्या᳚ः ।",
          "begin": 0.0,
          "end": 8.908
        },
        {
          "text": "अ॒स्म॒त्राञ्चो॒ वृष᳚णो वज्र॒वाहो॒ वृष्णे॒ मदा᳚य सु॒युजो᳚ वहन्तु ॥",
          "begin": 8.908,
          "end": 16.76
        }
      ]
    },
    {
      "id": "6.44.20",
      "mandala": 6,
      "sukta": 44,
      "rik": 20,
      "padas": [
        {
          "text": "आ ते᳚ वृष॒न्वृष᳚णो॒ द्रोण᳚मस्थुर्घृत॒प्रुषो॒ नोर्मयो॒ मद᳚न्तः ।",
          "begin": 0.0,
          "end": 9.206
        },
        {
          "text": "इन्द्र॒ प्र तुभ्यं॒ वृष᳚भिः सु॒तानां॒ वृष्णे᳚ भरन्ति वृष॒भाय॒ सोम᳚म् ॥",
          "begin": 9.206,
          "end": 17.28
        }
      ]
    },
    {
      "id": "6.44.21",
      "mandala": 6,
      "sukta": 44,
      "rik": 21,
      "padas": [
        {
          "text": "वृषा᳚सि दि॒वो वृ᳚ष॒भः पृ᳚थि॒व्या वृषा॒ सिन्धू᳚नां वृष॒भः स्तिया᳚नाम् ।",
          "begin": 0.0,
          "end": 9.441
        },
        {
          "text": "वृष्णे᳚ त॒ इन्दु᳚र्वृषभ पीपाय स्वा॒दू रसो᳚ मधु॒पेयो॒ वरा᳚य ॥",
          "begin": 9.441,
          "end": 18.96
        }
      ]
    },
    {
      "id": "6.44.22",
      "mandala": 6,
      "sukta": 44,
      "rik": 22,
      "padas": [
        {
          "text": "अ॒यं दे॒वः सह᳚सा॒ जाय᳚मान॒ इन्द्रे᳚ण यु॒जा प॒णिम᳚स्तभायत् ।",
          "begin": 0.0,
          "end": 7.845
        },
        {
          "text": "अ॒यं स्वस्य᳚ पि॒तुरायु᳚धा॒नीन्दु᳚रमुष्णा॒दशि᳚वस्य मा॒याः ॥",
          "begin": 7.845,
          "end": 15.64
        }
      ]
    },
    {
      "id": "6.44.23",
      "mandala": 6,
      "sukta": 44,
      "rik": 23,
      "padas": [
        {
          "text": "अ॒यम᳚कृणोदु॒षसः᳚ सु॒पत्नी᳚र॒यं सूर्ये᳚ अदधा॒ज्ज्योति᳚र॒न्तः ।",
          "begin": 0.0,
          "end": 7.698
        },
        {
          "text": "अ॒यं त्रि॒धातु᳚ दि॒वि रो᳚च॒नेषु᳚ त्रि॒तेषु᳚ विन्दद॒मृतं॒ निगू᳚ळ्हम् ॥",
          "begin": 7.698,
          "end": 16.76
        }
      ]
    },
    {
      "id": "6.44.24",
      "mandala": 6,
      "sukta": 44,
      "rik": 24,
      "padas": [
        {
          "text": "अ॒यं द्यावा᳚पृथि॒वी वि ष्क᳚भायद॒यं रथ᳚मयुनक्स॒प्तर᳚श्मिम् ।",
          "begin": 0.0,
          "end": 8.944
        },
        {
          "text": "अ॒यं गोषु॒ शच्या᳚ प॒क्वम॒न्तः सोमो᳚ दाधार॒ दश᳚यन्त्र॒मुत्स᳚म् ॥",
          "begin": 8.944,
          "end": 17.52
        }
      ]
    }
  ]
}