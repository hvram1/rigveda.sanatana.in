{
  "mandala": 6,
  "sukta": 53,
  "verses": [
    {
      "id": "6.53.1",
      "mandala": 6,
      "sukta": 53,
      "rik": 1,
      "padas": [
        {
          "text": "व॒यमु᳚ त्वा पथस्पते॒ रथं॒ न वाज᳚सातये ।",
          "begin": 0.0,
          "end": 6.984
        },
        {
          "text": "धि॒ये पू᳚षन्नयुज्महि ॥",
          "begin": 6.984,
          "end": 10.92
        }
      ]
    },
    {
      "id": "6.53.2",
      "mandala": 6,
      "sukta": 53,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒भि नो॒ नर्यं॒ वसु᳚ वी॒रं प्रय᳚तदक्षिणम् ।",
          "begin": 0.0,
          "end": 6.162
        },
        {
          "text": "वा॒मं गृ॒हप᳚तिं नय ॥",
          "begin": 6.162,
          "end": 8.88
        }
      ]
    },
    {
      "id": "6.53.3",
      "mandala": 6,
      "sukta": 53,
      "rik": 3,
      "padas": [
        {
          "text": "अदि᳚त्सन्तं चिदाघृणे॒ पूष॒न्दाना᳚य चोदय ।",
          "begin": 0.0,
          "end": 6.592
        },
        {
          "text": "प॒णेश्चि॒द्वि म्र᳚दा॒ मनः᳚ ॥",
          "begin": 6.592,
          "end": 10.92
        }
      ]
    },
    {
      "id": "6.53.4",
      "mandala": 6,
      "sukta": 53,
      "rik": 4,
      "padas": [
        {
          "text": "वि प॒थो वाज᳚सातये चिनु॒हि वि मृधो᳚ जहि ।",
          "begin": 0.0,
          "end": 6.364
        },
        {
          "text": "साध᳚न्तामुग्र नो॒ धियः᳚ ॥",
          "begin": 6.364,
          "end": 10.36
        }
      ]
    },
    {
      "id": "6.53.5",
      "mandala": 6,
      "sukta": 53,
      "rik": 5,
      "padas": [
        {
          "text": "परि᳚ तृन्धि पणी॒नामार᳚या॒ हृद᳚या कवे ।",
          "begin": 0.0,
          "end": 6.407
        },
        {
          "text": "अथे᳚म॒स्मभ्यं᳚ रन्धय ॥",
          "begin": 6.407,
          "end": 10.24
        }
      ]
    },
    {
      "id": "6.53.6",
      "mandala": 6,
      "sukta": 53,
      "rik": 6,
      "padas": [
        {
          "text": "वि पू᳚ष॒न्नार᳚या तुद प॒णेरि᳚च्छ हृ॒दि प्रि॒यम् ।",
          "begin": 0.0,
          "end": 6.425
        },
        {
          "text": "अथे᳚म॒स्मभ्यं᳚ रन्धय ॥",
          "begin": 6.425,
          "end": 11.0
        }
      ]
    },
    {
      "id": "6.53.7",
      "mandala": 6,
      "sukta": 53,
      "rik": 7,
      "padas": [
        {
          "text": "आ रि᳚ख किकि॒रा कृ᳚णु पणी॒नां हृद᳚या कवे ।",
          "begin": 0.0,
          "end": 7.594
        },
        {
          "text": "अथे᳚म॒स्मभ्यं᳚ रन्धय ॥",
          "begin": 7.594,
          "end": 10.88
        }
      ]
    },
    {
      "id": "6.53.8",
      "mandala": 6,
      "sukta": 53,
      "rik": 8,
      "padas": [
        {
          "text": "यां पू᳚षन्ब्रह्म॒चोद᳚नी॒मारां॒ बिभ᳚र्ष्याघृणे ।",
          "begin": 0.0,
          "end": 8.093
        },
        {
          "text": "तया᳚ समस्य॒ हृद᳚य॒मा रि᳚ख किकि॒रा कृ᳚णु ॥",
          "begin": 8.093,
          "end": 13.4
        }
      ]
    },
    {
      "id": "6.53.9",
      "mandala": 6,
      "sukta": 53,
      "rik": 9,
      "padas": [
        {
          "text": "या ते॒ अष्ट्रा॒ गोओ᳚प॒शाघृ᳚णे पशु॒साध᳚नी ।",
          "begin": 0.0,
          "end": 9.974
        },
        {
          "text": "तस्या᳚स्ते सु॒म्नमी᳚महे ॥",
          "begin": 9.974,
          "end": 12.36
        }
      ]
    },
    {
      "id": "6.53.10",
      "mandala": 6,
      "sukta": 53,
      "rik": 10,
      "padas": [
        {
          "text": "उ॒त नो᳚ गो॒षणिं॒ धिय᳚मश्व॒सां वा᳚ज॒सामु॒त ।",
          "begin": 0.0,
          "end": 7.568
        },
        {
          "text": "नृ॒वत्कृ᳚णुहि वी॒तये᳚ ॥",
          "begin": 7.568,
          "end": 12.96
        }
      ]
    }
  ]
}