  "verses": [
    {
      "id": "6.73.1",
      "mandala": 6,
      "sukta": 73,
      "rik": 1,
      "padas": [
        {
          "text": "यो अ᳚द्रि॒भित्प्र᳚थम॒जा ऋ॒तावा॒ बृह॒स्पति᳚राङ्गिर॒सो ह॒विष्मा᳚न् ।",
          "begin": 0.0,
          "end": 9.234
        },
        {
          "text": "द्वि॒बर्ह᳚ज्मा प्राघर्म॒सत्पि॒ता न॒ आ रोद᳚सी वृष॒भो रो᳚रवीति ॥",
          "begin": 9.234,
          "end": 16.84
        }
      ]
    },
    {
      "id": "6.73.2",
      "mandala": 6,
      "sukta": 73,
      "rik": 2,
      "padas": [
        {
          "text": "जना᳚य चि॒द्य ईव᳚त उ लो॒कं बृह॒स्पति᳚र्दे॒वहू᳚तौ च॒कार᳚ ।",
          "begin": 0.0,
          "end": 8.793
        },
        {
          "text": "घ्नन्वृ॒त्राणि॒ वि पुरो᳚ दर्दरीति॒ जय॒ञ्छत्रूँ᳚र॒मित्रा᳚न्पृ॒त्सु साह᳚न् ॥",
          "begin": 8.793,
          "end": 18.2
        }
      ]
    },
    {
      "id": "6.73.3",
      "mandala": 6,
      "sukta": 73,
      "rik": 3,
      "padas": [
        {
          "text": "बृह॒स्पति॒ः सम᳚जय॒द्वसू᳚नि म॒हो व्र॒जान्गोम᳚तो दे॒व ए॒षः ।",
          "begin": 0.0,
          "end": 8.051
        },
        {
          "text": "अ॒पः सिषा᳚स॒न्त्स्व१॒᳚रप्र᳚तीतो॒ बृह॒स्पति॒र्हन्त्य॒मित्र᳚म॒र्कैः ॥",
          "begin": 8.051,
          "end": 18.68
        }
      ]
    }
  ]
}