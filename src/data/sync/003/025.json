{
  "mandala": 3,
  "sukta": 25,
  "verses": [
    {
      "id": "3.25.1",
      "mandala": 3,
      "sukta": 25,
      "rik": 1,
      "padas": [
        {
          "text": "अग्ने᳚ दि॒वः सू॒नुर᳚सि॒ प्रचे᳚ता॒स्तना᳚ पृथि॒व्या उ॒त वि॒श्ववे᳚दाः ।",
          "begin": 0.0,
          "end": 10.64
        },
        {
          "text": "ऋध᳚ग्दे॒वाँ इ॒ह य᳚जा चिकित्वः ॥",
          "begin": 10.64,
          "end": 15.48
        }
      ]
    },
    {
      "id": "3.25.2",
      "mandala": 3,
      "sukta": 25,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒ग्निः स᳚नोति वी॒र्या᳚णि वि॒द्वान्त्स॒नोति॒ वाज᳚म॒मृता᳚य॒ भूष᳚न् ।",
          "begin": 0.0,
          "end": 9.585
        },
        {
          "text": "स नो᳚ दे॒वाँ एह व᳚हा पुरुक्षो ॥",
          "begin": 9.585,
          "end": 14.48
        }
      ]
    },
    {
      "id": "3.25.3",
      "mandala": 3,
      "sukta": 25,
      "rik": 3,
      "padas": [
        {
          "text": "अ॒ग्निर्द्यावा᳚पृथि॒वी वि॒श्वज᳚न्ये॒ आ भा᳚ति दे॒वी अ॒मृते॒ अमू᳚रः ।",
          "begin": 0.0,
          "end": 11.113
        },
        {
          "text": "क्षय॒न्वाजै᳚ः पुरुश्च॒न्द्रो नमो᳚भिः ॥",
          "begin": 11.113,
          "end": 16.56
        }
      ]
    },
    {
      "id": "3.25.4",
      "mandala": 3,
      "sukta": 25,
      "rik": 4,
      "padas": [
        {
          "text": "अग्न॒ इन्द्र᳚श्च दा॒शुषो᳚ दुरो॒णे सु॒ताव᳚तो य॒ज्ञमि॒होप᳚ यातम् ।",
          "begin": 0.0,
          "end": 8.496
        },
        {
          "text": "अम᳚र्धन्ता सोम॒पेया᳚य देवा ॥",
          "begin": 8.496,
          "end": 12.8
        }
      ]
    },
    {
      "id": "3.25.5",
      "mandala": 3,
      "sukta": 25,
      "rik": 5,
      "padas": [
        {
          "text": "अग्ने᳚ अ॒पां समि᳚ध्यसे दुरो॒णे नित्य᳚ः सूनो सहसो जातवेदः ।",
          "begin": 0.0,
          "end": 9.612
        },
        {
          "text": "स॒धस्था᳚नि म॒हय᳚मान ऊ॒ती ॥",
          "begin": 9.612,
          "end": 14.88
        }
      ]
    }
  ]
}