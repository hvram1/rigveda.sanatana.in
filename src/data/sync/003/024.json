{
  "mandala": 3,
  "sukta": 24,
  "verses": [
    {
      "id": "3.24.1",
      "mandala": 3,
      "sukta": 24,
      "rik": 1,
      "padas": [
        {
          "text": "अग्ने॒ सह᳚स्व॒ पृत᳚ना अ॒भिमा᳚ती॒रपा᳚स्य ।",
          "begin": 0.0,
          "end": 6.923
        },
        {
          "text": "दु॒ष्टर॒स्तर॒न्नरा᳚ती॒र्वर्चो᳚ धा य॒ज्ञवा᳚हसे ॥",
          "begin": 6.923,
          "end": 13.88
        }
      ]
    },
    {
      "id": "3.24.2",
      "mandala": 3,
      "sukta": 24,
      "rik": 2,
      "padas": [
        {
          "text": "अग्न᳚ इ॒ळा समि᳚ध्यसे वी॒तिहो᳚त्रो॒ अम᳚र्त्यः ।",
          "begin": 0.0,
          "end": 6.709
        },
        {
          "text": "जु॒षस्व॒ सू नो᳚ अध्व॒रम् ॥",
          "begin": 6.709,
          "end": 10.24
        }
      ]
    },
    {
      "id": "3.24.3",
      "mandala": 3,
      "sukta": 24,
      "rik": 3,
      "padas": [
        {
          "text": "अग्ने᳚ द्यु॒म्नेन᳚ जागृवे॒ सह᳚सः सूनवाहुत ।",
          "begin": 0.0,
          "end": 6.887
        },
        {
          "text": "एदं ब॒र्हिः स᳚दो॒ मम᳚ ॥",
          "begin": 6.887,
          "end": 10.2
        }
      ]
    },
    {
      "id": "3.24.4",
      "mandala": 3,
      "sukta": 24,
      "rik": 4,
      "padas": [
        {
          "text": "अग्ने॒ विश्वे᳚भिर॒ग्निभि᳚र्दे॒वेभि᳚र्महया॒ गिरः᳚ ।",
          "begin": 0.0,
          "end": 7.626
        },
        {
          "text": "य॒ज्ञेषु॒ य उ᳚ चा॒यवः᳚ ॥",
          "begin": 7.626,
          "end": 10.76
        }
      ]
    },
    {
      "id": "3.24.5",
      "mandala": 3,
      "sukta": 24,
      "rik": 5,
      "padas": [
        {
          "text": "अग्ने॒ दा दा॒शुषे᳚ र॒यिं वी॒रव᳚न्तं॒ परी᳚णसम् ।",
          "begin": 0.0,
          "end": 7.818
        },
        {
          "text": "शि॒शी॒हि नः᳚ सूनु॒मतः᳚ ॥",
          "begin": 7.818,
          "end": 11.96
        }
      ]
    }
  ]
}