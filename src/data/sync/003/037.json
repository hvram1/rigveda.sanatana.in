{
  "mandala": 3,
  "sukta": 37,
  "verses": [
    {
      "id": "3.37.1",
      "mandala": 3,
      "sukta": 37,
      "rik": 1,
      "padas": [
        {
          "text": "वार्त्र᳚हत्याय॒ शव᳚से पृतना॒षाह्या᳚य च ।",
          "begin": 0.0,
          "end": 7.63
        },
        {
          "text": "इन्द्र॒ त्वा व᳚र्तयामसि ॥",
          "begin": 7.63,
          "end": 10.92
        }
      ]
    },
    {
      "id": "3.37.2",
      "mandala": 3,
      "sukta": 37,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒र्वा॒चीनं॒ सु ते॒ मन᳚ उ॒त चक्षु᳚ः शतक्रतो ।",
          "begin": 0.0,
          "end": 5.886
        },
        {
          "text": "इन्द्र᳚ कृ॒ण्वन्तु᳚ वा॒घतः᳚ ॥",
          "begin": 5.886,
          "end": 9.56
        }
      ]
    },
    {
      "id": "3.37.3",
      "mandala": 3,
      "sukta": 37,
      "rik": 3,
      "padas": [
        {
          "text": "नामा᳚नि ते शतक्रतो॒ विश्वा᳚भिर्गी॒र्भिरी᳚महे ।",
          "begin": 0.0,
          "end": 8.485
        },
        {
          "text": "इन्द्रा᳚भिमाति॒षाह्ये᳚ ॥",
          "begin": 8.485,
          "end": 12.48
        }
      ]
    },
    {
      "id": "3.37.4",
      "mandala": 3,
      "sukta": 37,
      "rik": 4,
      "padas": [
        {
          "text": "पु॒रु॒ष्टु॒तस्य॒ धाम᳚भिः श॒तेन᳚ महयामसि ।",
          "begin": 0.0,
          "end": 5.335
        },
        {
          "text": "इन्द्र᳚स्य चर्षणी॒धृतः᳚ ॥",
          "begin": 5.335,
          "end": 9.32
        }
      ]
    },
    {
      "id": "3.37.5",
      "mandala": 3,
      "sukta": 37,
      "rik": 5,
      "padas": [
        {
          "text": "इन्द्रं᳚ वृ॒त्राय॒ हन्त᳚वे पुरुहू॒तमुप᳚ ब्रुवे ।",
          "begin": 0.0,
          "end": 7.082
        },
        {
          "text": "भरे᳚षु॒ वाज᳚सातये ॥",
          "begin": 7.082,
          "end": 10.08
        }
      ]
    },
    {
      "id": "3.37.6",
      "mandala": 3,
      "sukta": 37,
      "rik": 6,
      "padas": [
        {
          "text": "वाजे᳚षु सास॒हिर्भ᳚व॒ त्वामी᳚महे शतक्रतो ।",
          "begin": 0.0,
          "end": 7.477
        },
        {
          "text": "इन्द्र᳚ वृ॒त्राय॒ हन्त᳚वे ॥",
          "begin": 7.477,
          "end": 10.68
        }
      ]
    },
    {
      "id": "3.37.7",
      "mandala": 3,
      "sukta": 37,
      "rik": 7,
      "padas": [
        {
          "text": "द्यु॒म्नेषु᳚ पृत॒नाज्ये᳚ पृत्सु॒तूर्षु॒ श्रव᳚स्सु च ।",
          "begin": 0.0,
          "end": 7.226
        },
        {
          "text": "इन्द्र॒ साक्ष्वा॒भिमा᳚तिषु ॥",
          "begin": 7.226,
          "end": 10.8
        }
      ]
    },
    {
      "id": "3.37.8",
      "mandala": 3,
      "sukta": 37,
      "rik": 8,
      "padas": [
        {
          "text": "शु॒ष्मिन्त᳚मं न ऊ॒तये᳚ द्यु॒म्निनं᳚ पाहि॒ जागृ᳚विम् ।",
          "begin": 0.0,
          "end": 7.955
        },
        {
          "text": "इन्द्र॒ सोमं᳚ शतक्रतो ॥",
          "begin": 7.955,
          "end": 11.28
        }
      ]
    },
    {
      "id": "3.37.9",
      "mandala": 3,
      "sukta": 37,
      "rik": 9,
      "padas": [
        {
          "text": "इ॒न्द्रि॒याणि᳚ शतक्रतो॒ या ते॒ जने᳚षु प॒ञ्चसु᳚ ।",
          "begin": 0.0,
          "end": 6.798
        },
        {
          "text": "इन्द्र॒ तानि᳚ त॒ आ वृ᳚णे ॥",
          "begin": 6.798,
          "end": 9.72
        }
      ]
    },
    {
      "id": "3.37.10",
      "mandala": 3,
      "sukta": 37,
      "rik": 10,
      "padas": [
        {
          "text": "अग᳚न्निन्द्र॒ श्रवो᳚ बृ॒हद्द्यु॒म्नं द᳚धिष्व दु॒ष्टर᳚म् ।",
          "begin": 0.0,
          "end": 7.542
        },
        {
          "text": "उत्ते॒ शुष्मं᳚ तिरामसि ॥",
          "begin": 7.542,
          "end": 11.16
        }
      ]
    },
    {
      "id": "3.37.11",
      "mandala": 3,
      "sukta": 37,
      "rik": 11,
      "padas": [
        {
          "text": "अ॒र्वा॒वतो᳚ न॒ आ ग॒ह्यथो᳚ शक्र परा॒वतः᳚ ।",
          "begin": 0.0,
          "end": 8.097
        },
        {
          "text": "उ॒ लो॒को यस्ते᳚ अद्रिव॒ इन्द्रे॒ह तत॒ आ ग᳚हि ॥",
          "begin": 8.097,
          "end": 14.2
        }
      ]
    }
  ]
}