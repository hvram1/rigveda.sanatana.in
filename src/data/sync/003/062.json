{
  "mandala": 3,
  "sukta": 62,
  "verses": [
    {
      "id": "3.62.1",
      "mandala": 3,
      "sukta": 62,
      "rik": 1,
      "padas": [
        {
          "text": "इ॒मा उ᳚ वां भृ॒मयो॒ मन्य᳚माना यु॒वाव᳚ते॒ न तुज्या᳚ अभूवन् ।",
          "begin": 0.0,
          "end": 8.47
        },
        {
          "text": "क्व१॒᳚ त्यदि᳚न्द्रावरुणा॒ यशो᳚ वां॒ येन᳚ स्मा॒ सिनं॒ भर᳚थ॒ः सखि᳚भ्यः ॥",
          "begin": 8.47,
          "end": 18.48
        }
      ]
    },
    {
      "id": "3.62.2",
      "mandala": 3,
      "sukta": 62,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒यमु᳚ वां पुरु॒तमो᳚ रयी॒यञ्छ᳚श्वत्त॒ममव᳚से जोहवीति ।",
          "begin": 0.0,
          "end": 8.455
        },
        {
          "text": "स॒जोषा᳚विन्द्रावरुणा म॒रुद्भि᳚र्दि॒वा पृ᳚थि॒व्या शृ᳚णुतं॒ हवं᳚ मे ॥",
          "begin": 8.455,
          "end": 16.44
        }
      ]
    },
    {
      "id": "3.62.3",
      "mandala": 3,
      "sukta": 62,
      "rik": 3,
      "padas": [
        {
          "text": "अ॒स्मे तदि᳚न्द्रावरुणा॒ वसु᳚ ष्याद॒स्मे र॒यिर्म᳚रुत॒ः सर्व᳚वीरः ।",
          "begin": 0.0,
          "end": 8.27
        },
        {
          "text": "अ॒स्मान्वरू᳚त्रीः शर॒णैर᳚वन्त्व॒स्मान्होत्रा॒ भार᳚ती॒ दक्षि᳚णाभिः ॥",
          "begin": 8.27,
          "end": 16.64
        }
      ]
    },
    {
      "id": "3.62.4",
      "mandala": 3,
      "sukta": 62,
      "rik": 4,
      "padas": [
        {
          "text": "बृह᳚स्पते जु॒षस्व᳚ नो ह॒व्यानि᳚ विश्वदेव्य ।",
          "begin": 0.0,
          "end": 6.03
        },
        {
          "text": "रास्व॒ रत्ना᳚नि दा॒शुषे᳚ ॥",
          "begin": 6.03,
          "end": 10.44
        }
      ]
    },
    {
      "id": "3.62.5",
      "mandala": 3,
      "sukta": 62,
      "rik": 5,
      "padas": [
        {
          "text": "शुचि᳚म॒र्कैर्बृह॒स्पति᳚मध्व॒रेषु᳚ नमस्यत ।",
          "begin": 0.0,
          "end": 5.099
        },
        {
          "text": "अना॒म्योज॒ आ च᳚के ॥",
          "begin": 5.099,
          "end": 8.84
        }
      ]
    },
    {
      "id": "3.62.6",
      "mandala": 3,
      "sukta": 62,
      "rik": 6,
      "padas": [
        {
          "text": "वृ॒ष॒भं च᳚र्षणी॒नां वि॒श्वरू᳚प॒मदा᳚भ्यम् ।",
          "begin": 0.0,
          "end": 6.646
        },
        {
          "text": "बृह॒स्पतिं॒ वरे᳚ण्यम् ॥",
          "begin": 6.646,
          "end": 9.92
        }
      ]
    },
    {
      "id": "3.62.7",
      "mandala": 3,
      "sukta": 62,
      "rik": 7,
      "padas": [
        {
          "text": "इ॒यं ते᳚ पूषन्नाघृणे सुष्टु॒तिर्दे᳚व॒ नव्य᳚सी ।",
          "begin": 0.0,
          "end": 7.642
        },
        {
          "text": "अ॒स्माभि॒स्तुभ्यं᳚ शस्यते ॥",
          "begin": 7.642,
          "end": 11.32
        }
      ]
    },
    {
      "id": "3.62.8",
      "mandala": 3,
      "sukta": 62,
      "rik": 8,
      "padas": [
        {
          "text": "तां जु᳚षस्व॒ गिरं॒ मम᳚ वाज॒यन्ती᳚मवा॒ धिय᳚म् ।",
          "begin": 0.0,
          "end": 6.636
        },
        {
          "text": "व॒धू॒युरि᳚व॒ योष᳚णाम् ॥",
          "begin": 6.636,
          "end": 10.0
        }
      ]
    },
    {
      "id": "3.62.9",
      "mandala": 3,
      "sukta": 62,
      "rik": 9,
      "padas": [
        {
          "text": "यो विश्वा॒भि वि॒पश्य᳚ति॒ भुव᳚ना॒ सं च॒ पश्य᳚ति ।",
          "begin": 0.0,
          "end": 5.997
        },
        {
          "text": "स नः᳚ पू॒षावि॒ता भु᳚वत् ॥",
          "begin": 5.997,
          "end": 9.28
        }
      ]
    },
    {
      "id": "3.62.10",
      "mandala": 3,
      "sukta": 62,
      "rik": 10,
      "padas": [
        {
          "text": "तत्स᳚वि॒तुर्वरे᳚ण्यं॒ भर्गो᳚ दे॒वस्य᳚ धीमहि ।",
          "begin": 0.0,
          "end": 7.159
        },
        {
          "text": "धियो॒ यो नः᳚ प्रचो॒दया᳚त् ॥",
          "begin": 7.159,
          "end": 10.96
        }
      ]
    },
    {
      "id": "3.62.11",
      "mandala": 3,
      "sukta": 62,
      "rik": 11,
      "padas": [
        {
          "text": "दे॒वस्य᳚ सवि॒तुर्व॒यं वा᳚ज॒यन्त॒ः पुरं᳚ध्या ।",
          "begin": 0.0,
          "end": 7.108
        },
        {
          "text": "भग᳚स्य रा॒तिमी᳚महे ॥",
          "begin": 7.108,
          "end": 10.28
        }
      ]
    },
    {
      "id": "3.62.12",
      "mandala": 3,
      "sukta": 62,
      "rik": 12,
      "padas": [
        {
          "text": "दे॒वं नरः᳚ सवि॒तारं॒ विप्रा᳚ य॒ज्ञैः सु᳚वृ॒क्तिभि᳚ः ।",
          "begin": 0.0,
          "end": 7.429
        },
        {
          "text": "न॒म॒स्यन्ति᳚ धि॒येषि॒ताः ॥",
          "begin": 7.429,
          "end": 10.56
        }
      ]
    },
    {
      "id": "3.62.13",
      "mandala": 3,
      "sukta": 62,
      "rik": 13,
      "padas": [
        {
          "text": "सोमो᳚ जिगाति गातु॒विद्दे॒वाना᳚मेति निष्कृ॒तम् ।",
          "begin": 0.0,
          "end": 7.637
        },
        {
          "text": "ऋ॒तस्य॒ योनि᳚मा॒सद᳚म् ॥",
          "begin": 7.637,
          "end": 10.8
        }
      ]
    },
    {
      "id": "3.62.14",
      "mandala": 3,
      "sukta": 62,
      "rik": 14,
      "padas": [
        {
          "text": "सोमो᳚ अ॒स्मभ्यं᳚ द्वि॒पदे॒ चतु᳚ष्पदे च प॒शवे᳚ ।",
          "begin": 0.0,
          "end": 7.391
        },
        {
          "text": "अ॒न॒मी॒वा इष᳚स्करत् ॥",
          "begin": 7.391,
          "end": 11.0
        }
      ]
    },
    {
      "id": "3.62.15",
      "mandala": 3,
      "sukta": 62,
      "rik": 15,
      "padas": [
        {
          "text": "अ॒स्माक॒मायु᳚र्व॒र्धय᳚न्न॒भिमा᳚ती॒ः सह᳚मानः ।",
          "begin": 0.0,
          "end": 7.953
        },
        {
          "text": "सोमः᳚ स॒धस्थ॒मास᳚दत् ॥",
          "begin": 7.953,
          "end": 11.16
        }
      ]
    },
    {
      "id": "3.62.16",
      "mandala": 3,
      "sukta": 62,
      "rik": 16,
      "padas": [
        {
          "text": "आ नो᳚ मित्रावरुणा घृ॒तैर्गव्यू᳚तिमुक्षतम् ।",
          "begin": 0.0,
          "end": 6.848
        },
        {
          "text": "मध्वा॒ रजां᳚सि सुक्रतू ॥",
          "begin": 6.848,
          "end": 11.04
        }
      ]
    },
    {
      "id": "3.62.17",
      "mandala": 3,
      "sukta": 62,
      "rik": 17,
      "padas": [
        {
          "text": "उ॒रु॒शंसा᳚ नमो॒वृधा᳚ म॒ह्ना दक्ष᳚स्य राजथः ।",
          "begin": 0.0,
          "end": 7.275
        },
        {
          "text": "द्राघि᳚ष्ठाभिः शुचिव्रता ॥",
          "begin": 7.275,
          "end": 10.76
        }
      ]
    },
    {
      "id": "3.62.18",
      "mandala": 3,
      "sukta": 62,
      "rik": 18,
      "padas": [
        {
          "text": "गृ॒णा॒ना ज॒मद᳚ग्निना॒ योना᳚वृ॒तस्य᳚ सीदतम् ।",
          "begin": 0.0,
          "end": 6.085
        },
        {
          "text": "पा॒तं सोम᳚मृतावृधा ॥",
          "begin": 6.085,
          "end": 10.0
        }
      ]
    }
  ]
}