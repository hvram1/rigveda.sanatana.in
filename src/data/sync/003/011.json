{
  "mandala": 3,
  "sukta": 11,
  "verses": [
    {
      "id": "3.11.1",
      "mandala": 3,
      "sukta": 11,
      "rik": 1,
      "padas": [
        {
          "text": "अ॒ग्निर्होता᳚ पु॒रोहि᳚तो॑ध्व॒रस्य॒ विच᳚र्षणिः ।",
          "begin": 0.0,
          "end": 8.067
        },
        {
          "text": "स वे᳚द य॒ज्ञमा᳚नु॒षक् ॥",
          "begin": 8.067,
          "end": 11.88
        }
      ]
    },
    {
      "id": "3.11.2",
      "mandala": 3,
      "sukta": 11,
      "rik": 2,
      "padas": [
        {
          "text": "स ह᳚व्य॒वाळम᳚र्त्य उ॒शिग्दू॒तश्चनो᳚हितः ।",
          "begin": 0.0,
          "end": 4.764
        },
        {
          "text": "अ॒ग्निर्धि॒या समृ᳚ण्वति ॥",
          "begin": 4.764,
          "end": 9.72
        }
      ]
    },
    {
      "id": "3.11.3",
      "mandala": 3,
      "sukta": 11,
      "rik": 3,
      "padas": [
        {
          "text": "अ॒ग्निर्धि॒या स चे᳚तति के॒तुर्य॒ज्ञस्य᳚ पू॒र्व्यः ।",
          "begin": 0.0,
          "end": 5.58
        },
        {
          "text": "अर्थं॒ ह्य᳚स्य त॒रणि᳚ ॥",
          "begin": 5.58,
          "end": 9.0
        }
      ]
    },
    {
      "id": "3.11.4",
      "mandala": 3,
      "sukta": 11,
      "rik": 4,
      "padas": [
        {
          "text": "अ॒ग्निं सू॒नुं सन᳚श्रुतं॒ सह᳚सो जा॒तवे᳚दसम् ।",
          "begin": 0.0,
          "end": 6.858
        },
        {
          "text": "वह्निं᳚ दे॒वा अ᳚कृण्वत ॥",
          "begin": 6.858,
          "end": 10.16
        }
      ]
    },
    {
      "id": "3.11.5",
      "mandala": 3,
      "sukta": 11,
      "rik": 5,
      "padas": [
        {
          "text": "अदा᳚भ्यः पुरए॒ता वि॒शाम॒ग्निर्मानु᳚षीणाम् ।",
          "begin": 0.0,
          "end": 6.277
        },
        {
          "text": "तूर्णी॒ रथ॒ः सदा॒ नवः᳚ ॥",
          "begin": 6.277,
          "end": 10.08
        }
      ]
    },
    {
      "id": "3.11.6",
      "mandala": 3,
      "sukta": 11,
      "rik": 6,
      "padas": [
        {
          "text": "सा॒ह्वान्विश्वा᳚ अभि॒युज॒ः क्रतु᳚र्दे॒वाना॒ममृ᳚क्तः ।",
          "begin": 0.0,
          "end": 7.494
        },
        {
          "text": "अ॒ग्निस्तु॒विश्र᳚वस्तमः ॥",
          "begin": 7.494,
          "end": 10.12
        }
      ]
    },
    {
      "id": "3.11.7",
      "mandala": 3,
      "sukta": 11,
      "rik": 7,
      "padas": [
        {
          "text": "अ॒भि प्रयां᳚सि॒ वाह᳚सा दा॒श्वाँ अ᳚श्नोति॒ मर्त्य᳚ः ।",
          "begin": 0.0,
          "end": 7.388
        },
        {
          "text": "क्षयं᳚ पाव॒कशो᳚चिषः ॥",
          "begin": 7.388,
          "end": 10.84
        }
      ]
    },
    {
      "id": "3.11.8",
      "mandala": 3,
      "sukta": 11,
      "rik": 8,
      "padas": [
        {
          "text": "परि॒ विश्वा᳚नि॒ सुधि᳚ता॒ग्नेर᳚श्याम॒ मन्म᳚भिः ।",
          "begin": 0.0,
          "end": 6.577
        },
        {
          "text": "विप्रा᳚सो जा॒तवे᳚दसः ॥",
          "begin": 6.577,
          "end": 10.28
        }
      ]
    },
    {
      "id": "3.11.9",
      "mandala": 3,
      "sukta": 11,
      "rik": 9,
      "padas": [
        {
          "text": "अग्ने॒ विश्वा᳚नि॒ वार्या॒ वाजे᳚षु सनिषामहे ।",
          "begin": 0.0,
          "end": 7.142
        },
        {
          "text": "त्वे दे॒वास॒ एरि᳚रे ॥",
          "begin": 7.142,
          "end": 11.32
        }
      ]
    }
  ]
}