{
  "mandala": 3,
  "sukta": 42,
  "verses": [
    {
      "id": "3.42.1",
      "mandala": 3,
      "sukta": 42,
      "rik": 1,
      "padas": [
        {
          "text": "उप᳚ नः सु॒तमा ग᳚हि॒ सोम᳚मिन्द्र॒ गवा᳚शिरम् ।",
          "begin": 0.0,
          "end": 8.44
        },
        {
          "text": "हरि᳚भ्यां॒ यस्ते᳚ अस्म॒युः ॥",
          "begin": 8.44,
          "end": 12.6
        }
      ]
    },
    {
      "id": "3.42.2",
      "mandala": 3,
      "sukta": 42,
      "rik": 2,
      "padas": [
        {
          "text": "तमि᳚न्द्र॒ मद॒मा ग᳚हि बर्हि॒ःष्ठां ग्राव᳚भिः सु॒तम् ।",
          "begin": 0.0,
          "end": 6.448
        },
        {
          "text": "कु॒विन्न्व᳚स्य तृ॒प्णवः᳚ ॥",
          "begin": 6.448,
          "end": 9.8
        }
      ]
    },
    {
      "id": "3.42.3",
      "mandala": 3,
      "sukta": 42,
      "rik": 3,
      "padas": [
        {
          "text": "इन्द्र᳚मि॒त्था गिरो॒ ममाच्छा᳚गुरिषि॒ता इ॒तः ।",
          "begin": 0.0,
          "end": 7.143
        },
        {
          "text": "आ॒वृते॒ सोम᳚पीतये ॥",
          "begin": 7.143,
          "end": 9.88
        }
      ]
    },
    {
      "id": "3.42.4",
      "mandala": 3,
      "sukta": 42,
      "rik": 4,
      "padas": [
        {
          "text": "इन्द्रं॒ सोम᳚स्य पी॒तये॒ स्तोमै᳚रि॒ह ह᳚वामहे ।",
          "begin": 0.0,
          "end": 6.803
        },
        {
          "text": "उ॒क्थेभि᳚ः कु॒विदा॒गम᳚त् ॥",
          "begin": 6.803,
          "end": 9.68
        }
      ]
    },
    {
      "id": "3.42.5",
      "mandala": 3,
      "sukta": 42,
      "rik": 5,
      "padas": [
        {
          "text": "इन्द्र॒ सोमा᳚ः सु॒ता इ॒मे तान्द᳚धिष्व शतक्रतो ।",
          "begin": 0.0,
          "end": 6.999
        },
        {
          "text": "ज॒ठरे᳚ वाजिनीवसो ॥",
          "begin": 6.999,
          "end": 10.08
        }
      ]
    },
    {
      "id": "3.42.6",
      "mandala": 3,
      "sukta": 42,
      "rik": 6,
      "padas": [
        {
          "text": "वि॒द्मा हि त्वा᳚ धनंज॒यं वाजे᳚षु दधृ॒षं क᳚वे ।",
          "begin": 0.0,
          "end": 7.323
        },
        {
          "text": "अधा᳚ ते सु॒म्नमी᳚महे ॥",
          "begin": 7.323,
          "end": 10.88
        }
      ]
    },
    {
      "id": "3.42.7",
      "mandala": 3,
      "sukta": 42,
      "rik": 7,
      "padas": [
        {
          "text": "इ॒ममि᳚न्द्र॒ गवा᳚शिरं॒ यवा᳚शिरं च नः पिब ।",
          "begin": 0.0,
          "end": 5.375
        },
        {
          "text": "आ॒गत्या॒ वृष᳚भिः सु॒तम् ॥",
          "begin": 5.375,
          "end": 10.16
        }
      ]
    },
    {
      "id": "3.42.8",
      "mandala": 3,
      "sukta": 42,
      "rik": 8,
      "padas": [
        {
          "text": "तुभ्येदि᳚न्द्र॒ स्व ओ॒क्ये॒३॒᳚ सोमं᳚ चोदामि पी॒तये᳚ ।",
          "begin": 0.0,
          "end": 10.396
        },
        {
          "text": "ए॒ष रा᳚रन्तु ते हृ॒दि ॥",
          "begin": 10.396,
          "end": 13.84
        }
      ]
    },
    {
      "id": "3.42.9",
      "mandala": 3,
      "sukta": 42,
      "rik": 9,
      "padas": [
        {
          "text": "त्वां सु॒तस्य᳚ पी॒तये᳚ प्र॒त्नमि᳚न्द्र हवामहे ।",
          "begin": 0.0,
          "end": 7.343
        },
        {
          "text": "कु॒शि॒कासो᳚ अव॒स्यवः᳚ ॥",
          "begin": 7.343,
          "end": 12.88
        }
      ]
    }
  ]
}