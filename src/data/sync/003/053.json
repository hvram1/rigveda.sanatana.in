{
  "mandala": 3,
  "sukta": 53,
  "verses": [
    {
      "id": "3.53.1",
      "mandala": 3,
      "sukta": 53,
      "rik": 1,
      "padas": [
        {
          "text": "इन्द्रा᳚पर्वता बृह॒ता रथे᳚न वा॒मीरिष॒ आ व᳚हतं सु॒वीरा᳚ः ।",
          "begin": 0.0,
          "end": 10.658
        },
        {
          "text": "वी॒तं ह॒व्यान्य᳚ध्व॒रेषु᳚ देवा॒ वर्धे᳚थां गी॒र्भिरिळ᳚या॒ मद᳚न्ता ॥",
          "begin": 10.658,
          "end": 19.4
        }
      ]
    },
    {
      "id": "3.53.2",
      "mandala": 3,
      "sukta": 53,
      "rik": 2,
      "padas": [
        {
          "text": "तिष्ठा॒ सु कं᳚ मघव॒न्मा परा᳚ गा॒ः सोम᳚स्य॒ नु त्वा॒ सुषु᳚तस्य यक्षि ।",
          "begin": 0.0,
          "end": 8.038
        },
        {
          "text": "पि॒तुर्न पु॒त्रः सिच॒मा र᳚भे त॒ इन्द्र॒ स्वादि᳚ष्ठया गि॒रा श᳚चीवः ॥",
          "begin": 8.038,
          "end": 15.36
        }
      ]
    },
    {
      "id": "3.53.3",
      "mandala": 3,
      "sukta": 53,
      "rik": 3,
      "padas": [
        {
          "text": "शंसा᳚वाध्वर्यो॒ प्रति᳚ मे गृणी॒हीन्द्रा᳚य॒ वाहः᳚ कृणवाव॒ जुष्ट᳚म् ।",
          "begin": 0.0,
          "end": 10.052
        },
        {
          "text": "एदं ब॒र्हिर्यज᳚मानस्य सी॒दाथा᳚ च भूदु॒क्थमिन्द्रा᳚य श॒स्तम् ॥",
          "begin": 10.052,
          "end": 17.56
        }
      ]
    },
    {
      "id": "3.53.4",
      "mandala": 3,
      "sukta": 53,
      "rik": 4,
      "padas": [
        {
          "text": "जा॒येदस्तं᳚ मघव॒न्त्सेदु॒ योनि॒स्तदित्त्वा᳚ यु॒क्ता हर᳚यो वहन्तु ।",
          "begin": 0.0,
          "end": 9.373
        },
        {
          "text": "य॒दा क॒दा च᳚ सु॒नवा᳚म॒ सोम᳚म॒ग्निष्ट्वा᳚ दू॒तो ध᳚न्वा॒त्यच्छ᳚ ॥",
          "begin": 9.373,
          "end": 17.16
        }
      ]
    },
    {
      "id": "3.53.5",
      "mandala": 3,
      "sukta": 53,
      "rik": 5,
      "padas": [
        {
          "text": "परा᳚ याहि मघव॒न्ना च᳚ या॒हीन्द्र᳚ भ्रातरुभ॒यत्रा᳚ ते॒ अर्थ᳚म् ।",
          "begin": 0.0,
          "end": 9.468
        },
        {
          "text": "यत्रा॒ रथ᳚स्य बृह॒तो नि॒धानं᳚ वि॒मोच᳚नं वा॒जिनो॒ रास᳚भस्य ॥",
          "begin": 9.468,
          "end": 16.68
        }
      ]
    },
    {
      "id": "3.53.6",
      "mandala": 3,
      "sukta": 53,
      "rik": 6,
      "padas": [
        {
          "text": "अपा॒ः सोम॒मस्त᳚मिन्द्र॒ प्र या᳚हि कल्या॒णीर्जा॒या सु॒रणं᳚ गृ॒हे ते᳚ ।",
          "begin": 0.0,
          "end": 9.846
        },
        {
          "text": "यत्रा॒ रथ᳚स्य बृह॒तो नि॒धानं᳚ वि॒मोच᳚नं वा॒जिनो॒ दक्षि᳚णावत् ॥",
          "begin": 9.846,
          "end": 17.08
        }
      ]
    },
    {
      "id": "3.53.7",
      "mandala": 3,
      "sukta": 53,
      "rik": 7,
      "padas": [
        {
          "text": "इ॒मे भो॒जा अङ्गि᳚रसो॒ विरू᳚पा दि॒वस्पु॒त्रासो॒ असु᳚रस्य वी॒राः ।",
          "begin": 0.0,
          "end": 9.547
        },
        {
          "text": "वि॒श्वामि᳚त्राय॒ दद᳚तो म॒घानि᳚ सहस्रसा॒वे प्र ति᳚रन्त॒ आयु᳚ः ॥",
          "begin": 9.547,
          "end": 16.72
        }
      ]
    },
    {
      "id": "3.53.8",
      "mandala": 3,
      "sukta": 53,
      "rik": 8,
      "padas": [
        {
          "text": "रू॒पंरू᳚पं म॒घवा᳚ बोभवीति मा॒याः कृ᳚ण्वा॒नस्त॒न्वं१॒᳚ परि॒ स्वाम् ।",
          "begin": 0.0,
          "end": 10.241
        },
        {
          "text": "त्रिर्यद्दि॒वः परि᳚ मुहू॒र्तमागा॒त्स्वैर्मन्त्रै॒रनृ᳚तुपा ऋ॒तावा᳚ ॥",
          "begin": 10.241,
          "end": 18.68
        }
      ]
    },
    {
      "id": "3.53.9",
      "mandala": 3,
      "sukta": 53,
      "rik": 9,
      "padas": [
        {
          "text": "म॒हाँ ऋषि᳚र्देव॒जा दे॒वजू॒तो॑स्त᳚भ्ना॒त्सिन्धु᳚मर्ण॒वं नृ॒चक्षा᳚ः ।",
          "begin": 0.0,
          "end": 9.347
        },
        {
          "text": "वि॒श्वामि᳚त्रो॒ यदव᳚हत्सु॒दास॒मप्रि᳚यायत कुशि॒केभि॒रिन्द्र᳚ः ॥",
          "begin": 9.347,
          "end": 16.08
        }
      ]
    },
    {
      "id": "3.53.10",
      "mandala": 3,
      "sukta": 53,
      "rik": 10,
      "padas": [
        {
          "text": "हं॒सा इ᳚व कृणुथ॒ श्लोक॒मद्रि᳚भि॒र्मद᳚न्तो गी॒र्भिर᳚ध्व॒रे सु॒ते सचा᳚ ।",
          "begin": 0.0,
          "end": 9.068
        },
        {
          "text": "दे॒वेभि᳚र्विप्रा ऋषयो नृचक्षसो॒ वि पि᳚बध्वं कुशिकाः सो॒म्यं मधु᳚ ॥",
          "begin": 9.068,
          "end": 17.2
        }
      ]
    },
    {
      "id": "3.53.11",
      "mandala": 3,
      "sukta": 53,
      "rik": 11,
      "padas": [
        {
          "text": "उप॒ प्रेत᳚ कुशिकाश्चे॒तय᳚ध्व॒मश्वं᳚ रा॒ये प्र मु᳚ञ्चता सु॒दासः᳚ ।",
          "begin": 0.0,
          "end": 9.186
        },
        {
          "text": "राजा᳚ वृ॒त्रं ज᳚ङ्घन॒त्प्रागपा॒गुद॒गथा᳚ यजाते॒ वर॒ आ पृ᳚थि॒व्याः ॥",
          "begin": 9.186,
          "end": 17.96
        }
      ]
    },
    {
      "id": "3.53.12",
      "mandala": 3,
      "sukta": 53,
      "rik": 12,
      "padas": [
        {
          "text": "य इ॒मे रोद᳚सी उ॒भे अ॒हमिन्द्र॒मतु᳚ष्टवम् ।",
          "begin": 0.0,
          "end": 5.89
        },
        {
          "text": "वि॒श्वामि᳚त्रस्य रक्षति॒ ब्रह्मे॒दं भार᳚तं॒ जन᳚म् ॥",
          "begin": 5.89,
          "end": 12.24
        }
      ]
    },
    {
      "id": "3.53.13",
      "mandala": 3,
      "sukta": 53,
      "rik": 13,
      "padas": [
        {
          "text": "वि॒श्वामि᳚त्रा अरासत॒ ब्रह्मेन्द्रा᳚य व॒ज्रिणे᳚ ।",
          "begin": 0.0,
          "end": 7.317
        },
        {
          "text": "कर॒दिन्न᳚ः सु॒राध᳚सः ॥",
          "begin": 7.317,
          "end": 10.08
        }
      ]
    },
    {
      "id": "3.53.14",
      "mandala": 3,
      "sukta": 53,
      "rik": 14,
      "padas": [
        {
          "text": "किं ते᳚ कृण्वन्ति॒ कीक᳚टेषु॒ गावो॒ नाशिरं᳚ दु॒ह्रे न त᳚पन्ति घ॒र्मम् ।",
          "begin": 0.0,
          "end": 9.245
        },
        {
          "text": "आ नो᳚ भर॒ प्रम᳚गन्दस्य॒ वेदो᳚ नैचाशा॒खं म᳚घवन्रन्धया नः ॥",
          "begin": 9.245,
          "end": 17.08
        }
      ]
    },
    {
      "id": "3.53.15",
      "mandala": 3,
      "sukta": 53,
      "rik": 15,
      "padas": [
        {
          "text": "स॒स॒र्प॒रीरम᳚तिं॒ बाध᳚माना बृ॒हन्मि᳚माय ज॒मद᳚ग्निदत्ता ।",
          "begin": 0.0,
          "end": 7.922
        },
        {
          "text": "आ सूर्य᳚स्य दुहि॒ता त᳚तान॒ श्रवो᳚ दे॒वेष्व॒मृत᳚मजु॒र्यम् ॥",
          "begin": 7.922,
          "end": 14.84
        }
      ]
    },
    {
      "id": "3.53.16",
      "mandala": 3,
      "sukta": 53,
      "rik": 16,
      "padas": [
        {
          "text": "स॒स॒र्प॒रीर᳚भर॒त्तूय᳚मे॒भ्यो॑धि॒ श्रव॒ः पाञ्च᳚जन्यासु कृ॒ष्टिषु᳚ ।",
          "begin": 0.0,
          "end": 8.322
        },
        {
          "text": "सा प॒क्ष्या॒३॒᳚ नव्य॒मायु॒र्दधा᳚ना॒ यां मे᳚ पलस्तिजमद॒ग्नयो᳚ द॒दुः ॥",
          "begin": 8.322,
          "end": 18.92
        }
      ]
    },
    {
      "id": "3.53.17",
      "mandala": 3,
      "sukta": 53,
      "rik": 17,
      "padas": [
        {
          "text": "स्थि॒रौ गावौ᳚ भवतां वी॒ळुरक्षो॒ मेषा वि व᳚र्हि॒ मा यु॒गं वि शा᳚रि ।",
          "begin": 0.0,
          "end": 9.243
        },
        {
          "text": "इन्द्र᳚ः पात॒ल्ये᳚ ददतां॒ शरी᳚तो॒ररि᳚ष्टनेमे अ॒भि नः᳚ सचस्व ॥",
          "begin": 9.243,
          "end": 17.36
        }
      ]
    },
    {
      "id": "3.53.18",
      "mandala": 3,
      "sukta": 53,
      "rik": 18,
      "padas": [
        {
          "text": "बलं᳚ धेहि त॒नूषु᳚ नो॒ बल᳚मिन्द्रान॒ळुत्सु᳚ नः ।",
          "begin": 0.0,
          "end": 4.966
        },
        {
          "text": "बलं᳚ तो॒काय॒ तन᳚याय जी॒वसे॒ त्वं हि ब᳚ल॒दा असि᳚ ॥",
          "begin": 4.966,
          "end": 13.28
        }
      ]
    },
    {
      "id": "3.53.19",
      "mandala": 3,
      "sukta": 53,
      "rik": 19,
      "padas": [
        {
          "text": "अ॒भि व्य᳚यस्व खदि॒रस्य॒ सार॒मोजो᳚ धेहि स्पन्द॒ने शिं॒शपा᳚याम् ।",
          "begin": 0.0,
          "end": 9.156
        },
        {
          "text": "अक्ष᳚ वीळो वीळित वी॒ळय᳚स्व॒ मा यामा᳚द॒स्मादव᳚ जीहिपो नः ॥",
          "begin": 9.156,
          "end": 16.56
        }
      ]
    },
    {
      "id": "3.53.20",
      "mandala": 3,
      "sukta": 53,
      "rik": 20,
      "padas": [
        {
          "text": "अ॒यम॒स्मान्वन॒स्पति॒र्मा च॒ हा मा च᳚ रीरिषत् ।",
          "begin": 0.0,
          "end": 6.727
        },
        {
          "text": "स्व॒स्त्या गृ॒हेभ्य॒ आव॒सा आ वि॒मोच᳚नात् ॥",
          "begin": 6.727,
          "end": 12.12
        }
      ]
    },
    {
      "id": "3.53.21",
      "mandala": 3,
      "sukta": 53,
      "rik": 21,
      "padas": [
        {
          "text": "इन्द्रो॒तिभि᳚र्बहु॒लाभि᳚र्नो अ॒द्य या᳚च्छ्रे॒ष्ठाभि᳚र्मघवञ्छूर जिन्व ।",
          "begin": 0.0,
          "end": 9.585
        },
        {
          "text": "यो नो॒ द्वेष्ट्यध᳚र॒ः सस्प᳚दीष्ट॒ यमु᳚ द्वि॒ष्मस्तमु᳚ प्रा॒णो ज᳚हातु ॥",
          "begin": 9.585,
          "end": 17.64
        }
      ]
    },
    {
      "id": "3.53.22",
      "mandala": 3,
      "sukta": 53,
      "rik": 22,
      "padas": [
        {
          "text": "प॒र॒शुं चि॒द्वि त᳚पति शिम्ब॒लं चि॒द्वि वृ᳚श्चति ।",
          "begin": 0.0,
          "end": 5.777
        },
        {
          "text": "उ॒खा चि᳚दिन्द्र॒ येष᳚न्ती॒ प्रय᳚स्ता॒ फेन᳚मस्यति ॥",
          "begin": 5.777,
          "end": 11.4
        }
      ]
    },
    {
      "id": "3.53.23",
      "mandala": 3,
      "sukta": 53,
      "rik": 23,
      "padas": [
        {
          "text": "न साय᳚कस्य चिकिते जनासो लो॒धं न᳚यन्ति॒ पशु॒ मन्य᳚मानाः ।",
          "begin": 0.0,
          "end": 8.02
        },
        {
          "text": "नावा᳚जिनं वा॒जिना᳚ हासयन्ति॒ न ग᳚र्द॒भं पु॒रो अश्वा᳚न्नयन्ति ॥",
          "begin": 8.02,
          "end": 16.24
        }
      ]
    },
    {
      "id": "3.53.24",
      "mandala": 3,
      "sukta": 53,
      "rik": 24,
      "padas": [
        {
          "text": "इ॒म इ᳚न्द्र भर॒तस्य᳚ पु॒त्रा अ᳚पपि॒त्वं चि᳚कितु॒र्न प्र᳚पि॒त्वम् ।",
          "begin": 0.0,
          "end": 8.187
        },
        {
          "text": "हि॒न्वन्त्यश्व॒मर᳚णं॒ न नित्यं॒ ज्या᳚वाजं॒ परि᳚ णयन्त्या॒जौ ॥",
          "begin": 8.187,
          "end": 17.16
        }
      ]
    }
  ]
}