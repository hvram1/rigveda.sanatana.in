{
  "mandala": 3,
  "sukta": 12,
  "verses": [
    {
      "id": "3.12.1",
      "mandala": 3,
      "sukta": 12,
      "rik": 1,
      "padas": [
        {
          "text": "इन्द्रा᳚ग्नी॒ आ ग᳚तं सु॒तं गी॒र्भिर्नभो॒ वरे᳚ण्यम् ।",
          "begin": 0.0,
          "end": 8.809
        },
        {
          "text": "अ॒स्य पा᳚तं धि॒येषि॒ता ॥",
          "begin": 8.809,
          "end": 12.2
        }
      ]
    },
    {
      "id": "3.12.2",
      "mandala": 3,
      "sukta": 12,
      "rik": 2,
      "padas": [
        {
          "text": "इन्द्रा᳚ग्नी जरि॒तुः सचा᳚ य॒ज्ञो जि᳚गाति॒ चेत᳚नः ।",
          "begin": 0.0,
          "end": 7.244
        },
        {
          "text": "अ॒या पा᳚तमि॒मं सु॒तम् ॥",
          "begin": 7.244,
          "end": 10.32
        }
      ]
    },
    {
      "id": "3.12.3",
      "mandala": 3,
      "sukta": 12,
      "rik": 3,
      "padas": [
        {
          "text": "इन्द्र᳚म॒ग्निं क᳚वि॒च्छदा᳚ य॒ज्ञस्य᳚ जू॒त्या वृ᳚णे ।",
          "begin": 0.0,
          "end": 6.774
        },
        {
          "text": "ता सोम᳚स्ये॒ह तृ᳚म्पताम् ॥",
          "begin": 6.774,
          "end": 10.36
        }
      ]
    },
    {
      "id": "3.12.4",
      "mandala": 3,
      "sukta": 12,
      "rik": 4,
      "padas": [
        {
          "text": "तो॒शा वृ᳚त्र॒हणा᳚ हुवे स॒जित्वा॒नाप᳚राजिता ।",
          "begin": 0.0,
          "end": 6.744
        },
        {
          "text": "इ॒न्द्रा॒ग्नी वा᳚ज॒सात᳚मा ॥",
          "begin": 6.744,
          "end": 10.44
        }
      ]
    },
    {
      "id": "3.12.5",
      "mandala": 3,
      "sukta": 12,
      "rik": 5,
      "padas": [
        {
          "text": "प्र वा᳚मर्चन्त्यु॒क्थिनो᳚ नीथा॒विदो᳚ जरि॒तारः᳚ ।",
          "begin": 0.0,
          "end": 7.372
        },
        {
          "text": "इन्द्रा᳚ग्नी॒ इष॒ आ वृ᳚णे ॥",
          "begin": 7.372,
          "end": 11.28
        }
      ]
    },
    {
      "id": "3.12.6",
      "mandala": 3,
      "sukta": 12,
      "rik": 6,
      "padas": [
        {
          "text": "इन्द्रा᳚ग्नी नव॒तिं पुरो᳚ दा॒सप᳚त्नीरधूनुतम् ।",
          "begin": 0.0,
          "end": 7.444
        },
        {
          "text": "सा॒कमेके᳚न॒ कर्म᳚णा ॥",
          "begin": 7.444,
          "end": 10.76
        }
      ]
    },
    {
      "id": "3.12.7",
      "mandala": 3,
      "sukta": 12,
      "rik": 7,
      "padas": [
        {
          "text": "इन्द्रा᳚ग्नी॒ अप᳚स॒स्पर्युप॒ प्र य᳚न्ति धी॒तयः᳚ ।",
          "begin": 0.0,
          "end": 6.464
        },
        {
          "text": "ऋ॒तस्य᳚ प॒थ्या॒३॒᳚ अनु᳚ ॥",
          "begin": 6.464,
          "end": 13.04
        }
      ]
    },
    {
      "id": "3.12.8",
      "mandala": 3,
      "sukta": 12,
      "rik": 8,
      "padas": [
        {
          "text": "इन्द्रा᳚ग्नी तवि॒षाणि᳚ वां स॒धस्था᳚नि॒ प्रयां᳚सि च ।",
          "begin": 0.0,
          "end": 9.584
        },
        {
          "text": "यु॒वोर॒प्तूर्यं᳚ हि॒तम् ॥",
          "begin": 9.584,
          "end": 11.64
        }
      ]
    },
    {
      "id": "3.12.9",
      "mandala": 3,
      "sukta": 12,
      "rik": 9,
      "padas": [
        {
          "text": "इन्द्रा᳚ग्नी रोच॒ना दि॒वः परि॒ वाजे᳚षु भूषथः ।",
          "begin": 0.0,
          "end": 6.539
        },
        {
          "text": "तद्वां᳚ चेति॒ प्र वी॒र्य᳚म् ॥",
          "begin": 6.539,
          "end": 13.0
        }
      ]
    }
  ]
}