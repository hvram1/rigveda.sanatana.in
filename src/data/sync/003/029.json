{
  "mandala": 3,
  "sukta": 29,
  "verses": [
    {
      "id": "3.29.1",
      "mandala": 3,
      "sukta": 29,
      "rik": 1,
      "padas": [
        {
          "text": "अस्ती॒दम᳚धि॒मन्थ᳚न॒मस्ति᳚ प्र॒जन᳚नं कृ॒तम् ।",
          "begin": 0.0,
          "end": 6.406
        },
        {
          "text": "ए॒तां वि॒श्पत्नी॒मा भ᳚रा॒ग्निं म᳚न्थाम पू॒र्वथा᳚ ॥",
          "begin": 6.406,
          "end": 13.4
        }
      ]
    },
    {
      "id": "3.29.2",
      "mandala": 3,
      "sukta": 29,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒रण्यो॒र्निहि᳚तो जा॒तवे᳚दा॒ गर्भ᳚ इव॒ सुधि᳚तो ग॒र्भिणी᳚षु ।",
          "begin": 0.0,
          "end": 9.735
        },
        {
          "text": "दि॒वेदि᳚व॒ ईड्यो᳚ जागृ॒वद्भि᳚र्ह॒विष्म᳚द्भिर्मनु॒ष्ये᳚भिर॒ग्निः ॥",
          "begin": 9.735,
          "end": 16.56
        }
      ]
    },
    {
      "id": "3.29.3",
      "mandala": 3,
      "sukta": 29,
      "rik": 3,
      "padas": [
        {
          "text": "उ॒त्ता॒नाया॒मव᳚ भरा चिकि॒त्वान्त्स॒द्यः प्रवी᳚ता॒ वृष᳚णं जजान ।",
          "begin": 0.0,
          "end": 8.456
        },
        {
          "text": "अ॒रु॒षस्तू᳚पो॒ रुश᳚दस्य॒ पाज॒ इळा᳚यास्पु॒त्रो व॒युने᳚॑जनिष्ट ॥",
          "begin": 8.456,
          "end": 17.4
        }
      ]
    },
    {
      "id": "3.29.4",
      "mandala": 3,
      "sukta": 29,
      "rik": 4,
      "padas": [
        {
          "text": "इळा᳚यास्त्वा प॒दे व॒यं नाभा᳚ पृथि॒व्या अधि᳚ ।",
          "begin": 0.0,
          "end": 6.113
        },
        {
          "text": "जात᳚वेदो॒ नि धी᳚म॒ह्यग्ने᳚ ह॒व्याय॒ वोळ्ह᳚वे ॥",
          "begin": 6.113,
          "end": 14.48
        }
      ]
    },
    {
      "id": "3.29.5",
      "mandala": 3,
      "sukta": 29,
      "rik": 5,
      "padas": [
        {
          "text": "मन्थ᳚ता नरः क॒विमद्व᳚यन्तं॒ प्रचे᳚तसम॒मृतं᳚ सु॒प्रती᳚कम् ।",
          "begin": 0.0,
          "end": 8.969
        },
        {
          "text": "य॒ज्ञस्य᳚ के॒तुं प्र᳚थ॒मं पु॒रस्ता᳚द॒ग्निं न᳚रो जनयता सु॒शेव᳚म् ॥",
          "begin": 8.969,
          "end": 17.0
        }
      ]
    },
    {
      "id": "3.29.6",
      "mandala": 3,
      "sukta": 29,
      "rik": 6,
      "padas": [
        {
          "text": "यदी॒ मन्थ᳚न्ति बा॒हुभि॒र्वि रो᳚च॒ते॑श्वो॒ न वा॒ज्य᳚रु॒षो वने॒ष्वा ।",
          "begin": 0.0,
          "end": 9.684
        },
        {
          "text": "चि॒त्रो न याम᳚न्न॒श्विनो॒रनि᳚वृत॒ः परि᳚ वृण॒क्त्यश्म᳚न॒स्तृणा॒ दह᳚न् ॥",
          "begin": 9.684,
          "end": 17.6
        }
      ]
    },
    {
      "id": "3.29.7",
      "mandala": 3,
      "sukta": 29,
      "rik": 7,
      "padas": [
        {
          "text": "जा॒तो अ॒ग्नी रो᳚चते॒ चेकि᳚तानो वा॒जी विप्र᳚ः कविश॒स्तः सु॒दानु᳚ः ।",
          "begin": 0.0,
          "end": 9.839
        },
        {
          "text": "यं दे॒वास॒ ईड्यं᳚ विश्व॒विदं᳚ हव्य॒वाह॒मद᳚धुरध्व॒रेषु᳚ ॥",
          "begin": 9.839,
          "end": 18.2
        }
      ]
    },
    {
      "id": "3.29.8",
      "mandala": 3,
      "sukta": 29,
      "rik": 8,
      "padas": [
        {
          "text": "सीद᳚ होत॒ः स्व उ᳚ लो॒के चि᳚कि॒त्वान्त्सा॒दया᳚ य॒ज्ञं सु᳚कृ॒तस्य॒ योनौ᳚ ।",
          "begin": 0.0,
          "end": 10.185
        },
        {
          "text": "दे॒वा॒वीर्दे॒वान्ह॒विषा᳚ यजा॒स्यग्ने᳚ बृ॒हद्यज᳚माने॒ वयो᳚ धाः ॥",
          "begin": 10.185,
          "end": 19.6
        }
      ]
    },
    {
      "id": "3.29.9",
      "mandala": 3,
      "sukta": 29,
      "rik": 9,
      "padas": [
        {
          "text": "कृ॒णोत᳚ धू॒मं वृष᳚णं सखा॒यो॑स्रे᳚धन्त इतन॒ वाज॒मच्छ᳚ ।",
          "begin": 0.0,
          "end": 7.226
        },
        {
          "text": "अ॒यम॒ग्निः पृ᳚तना॒षाट् सु॒वीरो॒ येन᳚ दे॒वासो॒ अस᳚हन्त॒ दस्यू᳚न् ॥",
          "begin": 7.226,
          "end": 16.68
        }
      ]
    },
    {
      "id": "3.29.10",
      "mandala": 3,
      "sukta": 29,
      "rik": 10,
      "padas": [
        {
          "text": "अ॒यं ते॒ योनि᳚रृ॒त्वियो॒ यतो᳚ जा॒तो अरो᳚चथाः ।",
          "begin": 0.0,
          "end": 7.795
        },
        {
          "text": "तं जा॒नन्न᳚ग्न॒ आ सी॒दाथा᳚ नो वर्धया॒ गिरः᳚ ॥",
          "begin": 7.795,
          "end": 14.92
        }
      ]
    },
    {
      "id": "3.29.11",
      "mandala": 3,
      "sukta": 29,
      "rik": 11,
      "padas": [
        {
          "text": "तनू॒नपा᳚दुच्यते॒ गर्भ᳚ आसु॒रो नरा॒शंसो᳚ भवति॒ यद्वि॒जाय᳚ते ।",
          "begin": 0.0,
          "end": 9.675
        },
        {
          "text": "मा॒त॒रिश्वा॒ यदमि᳚मीत मा॒तरि॒ वात᳚स्य॒ सर्गो᳚ अभव॒त्सरी᳚मणि ॥",
          "begin": 9.675,
          "end": 17.68
        }
      ]
    },
    {
      "id": "3.29.12",
      "mandala": 3,
      "sukta": 29,
      "rik": 12,
      "padas": [
        {
          "text": "सु॒नि॒र्मथा॒ निर्म᳚थितः सुनि॒धा निहि᳚तः क॒विः ।",
          "begin": 0.0,
          "end": 6.724
        },
        {
          "text": "अग्ने᳚ स्वध्व॒रा कृ᳚णु दे॒वान्दे᳚वय॒ते य᳚ज ॥",
          "begin": 6.724,
          "end": 13.0
        }
      ]
    },
    {
      "id": "3.29.13",
      "mandala": 3,
      "sukta": 29,
      "rik": 13,
      "padas": [
        {
          "text": "अजी᳚जनन्न॒मृतं॒ मर्त्या᳚सो॑स्रे॒माणं᳚ त॒रणिं᳚ वी॒ळुज᳚म्भम् ।",
          "begin": 0.0,
          "end": 10.302
        },
        {
          "text": "दश॒ स्वसा᳚रो अ॒ग्रुवः᳚ समी॒चीः पुमां᳚सं जा॒तम॒भि सं र᳚भन्ते ॥",
          "begin": 10.302,
          "end": 18.88
        }
      ]
    },
    {
      "id": "3.29.14",
      "mandala": 3,
      "sukta": 29,
      "rik": 14,
      "padas": [
        {
          "text": "प्र स॒प्तहो᳚ता सन॒काद᳚रोचत मा॒तुरु॒पस्थे॒ यदशो᳚च॒दूध᳚नि ।",
          "begin": 0.0,
          "end": 8.285
        },
        {
          "text": "न नि मि᳚षति सु॒रणो᳚ दि॒वेदि᳚वे॒ यदसु᳚रस्य ज॒ठरा॒दजा᳚यत ॥",
          "begin": 8.285,
          "end": 15.6
        }
      ]
    },
    {
      "id": "3.29.15",
      "mandala": 3,
      "sukta": 29,
      "rik": 15,
      "padas": [
        {
          "text": "अ॒मि॒त्रा॒युधो᳚ म॒रुता᳚मिव प्र॒याः प्र᳚थम॒जा ब्रह्म᳚णो॒ विश्व॒मिद्वि᳚दुः ।",
          "begin": 0.0,
          "end": 9.468
        },
        {
          "text": "द्यु॒म्नव॒द्ब्रह्म᳚ कुशि॒कास॒ एरि᳚र॒ एक᳚एको॒ दमे᳚ अ॒ग्निं समी᳚धिरे ॥",
          "begin": 9.468,
          "end": 17.6
        }
      ]
    },
    {
      "id": "3.29.16",
      "mandala": 3,
      "sukta": 29,
      "rik": 16,
      "padas": [
        {
          "text": "यद॒द्य त्वा᳚ प्रय॒ति य॒ज्ञे अ॒स्मिन्होत᳚श्चिकि॒त्वो॑वृ᳚णीमही॒ह ।",
          "begin": 0.0,
          "end": 8.866
        },
        {
          "text": "ध्रु॒वम᳚या ध्रु॒वमु॒ताश᳚मिष्ठाः प्रजा॒नन्वि॒द्वाँ उप᳚ याहि॒ सोम᳚म् ॥",
          "begin": 8.866,
          "end": 16.92
        }
      ]
    }
  ]
}