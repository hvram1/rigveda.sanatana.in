{
  "mandala": 9,
  "sukta": 43,
  "verses": [
    {
      "id": "9.43.1",
      "mandala": 9,
      "sukta": 43,
      "rik": 1,
      "padas": [
        {
          "text": "यो अत्य᳚ इव मृ॒ज्यते॒ गोभि॒र्मदा᳚य हर्य॒तः ।",
          "begin": 0.0,
          "end": 7.498
        },
        {
          "text": "तं गी॒र्भिर्वा᳚सयामसि ॥",
          "begin": 7.498,
          "end": 10.6
        }
      ]
    },
    {
      "id": "9.43.2",
      "mandala": 9,
      "sukta": 43,
      "rik": 2,
      "padas": [
        {
          "text": "तं नो॒ विश्वा᳚ अव॒स्युवो॒ गिरः᳚ शुम्भन्ति पू॒र्वथा᳚ ।",
          "begin": 0.0,
          "end": 6.104
        },
        {
          "text": "इन्दु॒मिन्द्रा᳚य पी॒तये᳚ ॥",
          "begin": 6.104,
          "end": 8.92
        }
      ]
    },
    {
      "id": "9.43.3",
      "mandala": 9,
      "sukta": 43,
      "rik": 3,
      "padas": [
        {
          "text": "पु॒ना॒नो या᳚ति हर्य॒तः सोमो᳚ गी॒र्भिः परि᳚ष्कृतः ।",
          "begin": 0.0,
          "end": 6.106
        },
        {
          "text": "विप्र᳚स्य॒ मेध्या᳚तिथेः ॥",
          "begin": 6.106,
          "end": 9.08
        }
      ]
    },
    {
      "id": "9.43.4",
      "mandala": 9,
      "sukta": 43,
      "rik": 4,
      "padas": [
        {
          "text": "पव᳚मान वि॒दा र॒यिम॒स्मभ्यं᳚ सोम सु॒श्रिय᳚म् ।",
          "begin": 0.0,
          "end": 4.855
        },
        {
          "text": "इन्दो᳚ स॒हस्र᳚वर्चसम् ॥",
          "begin": 4.855,
          "end": 8.04
        }
      ]
    },
    {
      "id": "9.43.5",
      "mandala": 9,
      "sukta": 43,
      "rik": 5,
      "padas": [
        {
          "text": "इन्दु॒रत्यो॒ न वा᳚ज॒सृत्कनि᳚क्रन्ति प॒वित्र॒ आ ।",
          "begin": 0.0,
          "end": 5.451
        },
        {
          "text": "यदक्षा॒रति᳚ देव॒युः ॥",
          "begin": 5.451,
          "end": 7.8
        }
      ]
    },
    {
      "id": "9.43.6",
      "mandala": 9,
      "sukta": 43,
      "rik": 6,
      "padas": [
        {
          "text": "पव᳚स्व॒ वाज᳚सातये॒ विप्र᳚स्य गृण॒तो वृ॒धे ।",
          "begin": 0.0,
          "end": 5.206
        },
        {
          "text": "सोम॒ रास्व᳚ सु॒वीर्य᳚म् ॥",
          "begin": 5.206,
          "end": 9.48
        }
      ]
    }
  ]
}