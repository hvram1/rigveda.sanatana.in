{
  "mandala": 9,
  "sukta": 65,
  "verses": [
    {
      "id": "9.65.1",
      "mandala": 9,
      "sukta": 65,
      "rik": 1,
      "padas": [
        {
          "text": "हि॒न्वन्ति॒ सूर॒मुस्र᳚य॒ः स्वसा᳚रो जा॒मय॒स्पति᳚म् ।",
          "begin": 0.0,
          "end": 6.3
        },
        {
          "text": "म॒हामिन्दुं᳚ मही॒युवः᳚ ॥",
          "begin": 6.3,
          "end": 9.16
        }
      ]
    },
    {
      "id": "9.65.2",
      "mandala": 9,
      "sukta": 65,
      "rik": 2,
      "padas": [
        {
          "text": "पव᳚मान रु॒चारु᳚चा दे॒वो दे॒वेभ्य॒स्परि᳚ ।",
          "begin": 0.0,
          "end": 4.774
        },
        {
          "text": "विश्वा॒ वसू॒न्या वि᳚श ॥",
          "begin": 4.774,
          "end": 7.2
        }
      ]
    },
    {
      "id": "9.65.3",
      "mandala": 9,
      "sukta": 65,
      "rik": 3,
      "padas": [
        {
          "text": "आ प᳚वमान सुष्टु॒तिं वृ॒ष्टिं दे॒वेभ्यो॒ दुवः᳚ ।",
          "begin": 0.0,
          "end": 4.867
        },
        {
          "text": "इ॒षे प᳚वस्व सं॒यत᳚म् ॥",
          "begin": 4.867,
          "end": 7.8
        }
      ]
    },
    {
      "id": "9.65.4",
      "mandala": 9,
      "sukta": 65,
      "rik": 4,
      "padas": [
        {
          "text": "वृषा॒ ह्यसि᳚ भा॒नुना᳚ द्यु॒मन्तं᳚ त्वा हवामहे ।",
          "begin": 0.0,
          "end": 5.23
        },
        {
          "text": "पव᳚मान स्वा॒ध्य᳚ः ॥",
          "begin": 5.23,
          "end": 8.08
        }
      ]
    },
    {
      "id": "9.65.5",
      "mandala": 9,
      "sukta": 65,
      "rik": 5,
      "padas": [
        {
          "text": "आ प᳚वस्व सु॒वीर्यं॒ मन्द᳚मानः स्वायुध ।",
          "begin": 0.0,
          "end": 4.753
        },
        {
          "text": "इ॒हो ष्वि᳚न्द॒वा ग᳚हि ॥",
          "begin": 4.753,
          "end": 7.24
        }
      ]
    },
    {
      "id": "9.65.6",
      "mandala": 9,
      "sukta": 65,
      "rik": 6,
      "padas": [
        {
          "text": "यद॒द्भिः प᳚रिषि॒च्यसे᳚ मृ॒ज्यमा᳚नो॒ गभ᳚स्त्योः ।",
          "begin": 0.0,
          "end": 5.443
        },
        {
          "text": "द्रुणा᳚ स॒धस्थ᳚मश्नुषे ॥",
          "begin": 5.443,
          "end": 8.72
        }
      ]
    },
    {
      "id": "9.65.7",
      "mandala": 9,
      "sukta": 65,
      "rik": 7,
      "padas": [
        {
          "text": "प्र सोमा᳚य व्यश्व॒वत्पव᳚मानाय गायत ।",
          "begin": 0.0,
          "end": 5.189
        },
        {
          "text": "म॒हे स॒हस्र᳚चक्षसे ॥",
          "begin": 5.189,
          "end": 7.36
        }
      ]
    },
    {
      "id": "9.65.8",
      "mandala": 9,
      "sukta": 65,
      "rik": 8,
      "padas": [
        {
          "text": "यस्य॒ वर्णं᳚ मधु॒श्चुतं॒ हरिं᳚ हि॒न्वन्त्यद्रि᳚भिः ।",
          "begin": 0.0,
          "end": 5.379
        },
        {
          "text": "इन्दु॒मिन्द्रा᳚य पी॒तये᳚ ॥",
          "begin": 5.379,
          "end": 8.44
        }
      ]
    },
    {
      "id": "9.65.9",
      "mandala": 9,
      "sukta": 65,
      "rik": 9,
      "padas": [
        {
          "text": "तस्य᳚ ते वा॒जिनो᳚ व॒यं विश्वा॒ धना᳚नि जि॒ग्युषः᳚ ।",
          "begin": 0.0,
          "end": 5.617
        },
        {
          "text": "स॒खि॒त्वमा वृ᳚णीमहे ॥",
          "begin": 5.617,
          "end": 7.8
        }
      ]
    },
    {
      "id": "9.65.10",
      "mandala": 9,
      "sukta": 65,
      "rik": 10,
      "padas": [
        {
          "text": "वृषा᳚ पवस्व॒ धार᳚या म॒रुत्व᳚ते च मत्स॒रः ।",
          "begin": 0.0,
          "end": 5.208
        },
        {
          "text": "विश्वा॒ दधा᳚न॒ ओज᳚सा ॥",
          "begin": 5.208,
          "end": 7.88
        }
      ]
    },
    {
      "id": "9.65.11",
      "mandala": 9,
      "sukta": 65,
      "rik": 11,
      "padas": [
        {
          "text": "तं त्वा᳚ ध॒र्तार᳚मो॒ण्यो॒३॒᳚ः पव᳚मान स्व॒र्दृश᳚म् ।",
          "begin": 0.0,
          "end": 7.53
        },
        {
          "text": "हि॒न्वे वाजे᳚षु वा॒जिन᳚म् ॥",
          "begin": 7.53,
          "end": 11.2
        }
      ]
    },
    {
      "id": "9.65.12",
      "mandala": 9,
      "sukta": 65,
      "rik": 12,
      "padas": [
        {
          "text": "अ॒या चि॒त्तो वि॒पानया॒ हरि᳚ः पवस्व॒ धार᳚या ।",
          "begin": 0.0,
          "end": 4.94
        },
        {
          "text": "युजं॒ वाजे᳚षु चोदय ॥",
          "begin": 4.94,
          "end": 7.56
        }
      ]
    },
    {
      "id": "9.65.13",
      "mandala": 9,
      "sukta": 65,
      "rik": 13,
      "padas": [
        {
          "text": "आ न᳚ इन्दो म॒हीमिषं॒ पव᳚स्व वि॒श्वद᳚र्शतः ।",
          "begin": 0.0,
          "end": 5.013
        },
        {
          "text": "अ॒स्मभ्यं᳚ सोम गातु॒वित् ॥",
          "begin": 5.013,
          "end": 7.8
        }
      ]
    },
    {
      "id": "9.65.14",
      "mandala": 9,
      "sukta": 65,
      "rik": 14,
      "padas": [
        {
          "text": "आ क॒लशा᳚ अनूष॒तेन्दो॒ धारा᳚भि॒रोज᳚सा ।",
          "begin": 0.0,
          "end": 6.181
        },
        {
          "text": "एन्द्र᳚स्य पी॒तये᳚ विश ॥",
          "begin": 6.181,
          "end": 8.36
        }
      ]
    },
    {
      "id": "9.65.15",
      "mandala": 9,
      "sukta": 65,
      "rik": 15,
      "padas": [
        {
          "text": "यस्य᳚ ते॒ मद्यं॒ रसं᳚ ती॒व्रं दु॒हन्त्यद्रि᳚भिः ।",
          "begin": 0.0,
          "end": 5.101
        },
        {
          "text": "स प᳚वस्वाभिमाति॒हा ॥",
          "begin": 5.101,
          "end": 7.32
        }
      ]
    },
    {
      "id": "9.65.16",
      "mandala": 9,
      "sukta": 65,
      "rik": 16,
      "padas": [
        {
          "text": "राजा᳚ मे॒धाभि᳚रीयते॒ पव᳚मानो म॒नावधि᳚ ।",
          "begin": 0.0,
          "end": 5.579
        },
        {
          "text": "अ॒न्तरि᳚क्षेण॒ यात᳚वे ॥",
          "begin": 5.579,
          "end": 7.88
        }
      ]
    },
    {
      "id": "9.65.17",
      "mandala": 9,
      "sukta": 65,
      "rik": 17,
      "padas": [
        {
          "text": "आ न᳚ इन्दो शत॒ग्विनं॒ गवां॒ पोषं॒ स्वश्व्य᳚म् ।",
          "begin": 0.0,
          "end": 5.459
        },
        {
          "text": "वहा॒ भग᳚त्तिमू॒तये᳚ ॥",
          "begin": 5.459,
          "end": 8.12
        }
      ]
    },
    {
      "id": "9.65.18",
      "mandala": 9,
      "sukta": 65,
      "rik": 18,
      "padas": [
        {
          "text": "आ नः᳚ सोम॒ सहो॒ जुवो᳚ रू॒पं न वर्च᳚से भर ।",
          "begin": 0.0,
          "end": 5.355
        },
        {
          "text": "सु॒ष्वा॒णो दे॒ववी᳚तये ॥",
          "begin": 5.355,
          "end": 8.08
        }
      ]
    },
    {
      "id": "9.65.19",
      "mandala": 9,
      "sukta": 65,
      "rik": 19,
      "padas": [
        {
          "text": "अर्षा᳚ सोम द्यु॒मत्त᳚मो॒॑भि द्रोणा᳚नि॒ रोरु᳚वत् ।",
          "begin": 0.0,
          "end": 5.932
        },
        {
          "text": "सीद᳚ञ्छ्ये॒नो न योनि॒मा ॥",
          "begin": 5.932,
          "end": 9.0
        }
      ]
    },
    {
      "id": "9.65.20",
      "mandala": 9,
      "sukta": 65,
      "rik": 20,
      "padas": [
        {
          "text": "अ॒प्सा इन्द्रा᳚य वा॒यवे॒ वरु᳚णाय म॒रुद्भ्य᳚ः ।",
          "begin": 0.0,
          "end": 5.953
        },
        {
          "text": "सोमो᳚ अर्षति॒ विष्ण᳚वे ॥",
          "begin": 5.953,
          "end": 8.76
        }
      ]
    },
    {
      "id": "9.65.21",
      "mandala": 9,
      "sukta": 65,
      "rik": 21,
      "padas": [
        {
          "text": "इषं᳚ तो॒काय᳚ नो॒ दध᳚द॒स्मभ्यं᳚ सोम वि॒श्वतः᳚ ।",
          "begin": 0.0,
          "end": 6.319
        },
        {
          "text": "आ प᳚वस्व सह॒स्रिण᳚म् ॥",
          "begin": 6.319,
          "end": 8.72
        }
      ]
    },
    {
      "id": "9.65.22",
      "mandala": 9,
      "sukta": 65,
      "rik": 22,
      "padas": [
        {
          "text": "ये सोमा᳚सः परा॒वति॒ ये अ᳚र्वा॒वति᳚ सुन्वि॒रे ।",
          "begin": 0.0,
          "end": 6.289
        },
        {
          "text": "ये वा॒दः श᳚र्य॒णाव᳚ति ॥",
          "begin": 6.289,
          "end": 9.0
        }
      ]
    },
    {
      "id": "9.65.23",
      "mandala": 9,
      "sukta": 65,
      "rik": 23,
      "padas": [
        {
          "text": "य आ᳚र्जी॒केषु॒ कृत्व᳚सु॒ ये मध्ये᳚ प॒स्त्या᳚नाम् ।",
          "begin": 0.0,
          "end": 6.584
        },
        {
          "text": "ये वा॒ जने᳚षु प॒ञ्चसु᳚ ॥",
          "begin": 6.584,
          "end": 9.24
        }
      ]
    },
    {
      "id": "9.65.24",
      "mandala": 9,
      "sukta": 65,
      "rik": 24,
      "padas": [
        {
          "text": "ते नो᳚ वृ॒ष्टिं दि॒वस्परि॒ पव᳚न्ता॒मा सु॒वीर्य᳚म् ।",
          "begin": 0.0,
          "end": 6.204
        },
        {
          "text": "सु॒वा॒ना दे॒वास॒ इन्द᳚वः ॥",
          "begin": 6.204,
          "end": 8.88
        }
      ]
    },
    {
      "id": "9.65.25",
      "mandala": 9,
      "sukta": 65,
      "rik": 25,
      "padas": [
        {
          "text": "पव᳚ते हर्य॒तो हरि᳚र्गृणा॒नो ज॒मद᳚ग्निना ।",
          "begin": 0.0,
          "end": 4.908
        },
        {
          "text": "हि॒न्वा॒नो गोरधि᳚ त्व॒चि ॥",
          "begin": 4.908,
          "end": 7.4
        }
      ]
    },
    {
      "id": "9.65.26",
      "mandala": 9,
      "sukta": 65,
      "rik": 26,
      "padas": [
        {
          "text": "प्र शु॒क्रासो᳚ वयो॒जुवो᳚ हिन्वा॒नासो॒ न सप्त᳚यः ।",
          "begin": 0.0,
          "end": 5.915
        },
        {
          "text": "श्री॒णा॒ना अ॒प्सु मृ᳚ञ्जत ॥",
          "begin": 5.915,
          "end": 8.88
        }
      ]
    },
    {
      "id": "9.65.27",
      "mandala": 9,
      "sukta": 65,
      "rik": 27,
      "padas": [
        {
          "text": "तं त्वा᳚ सु॒तेष्वा॒भुवो᳚ हिन्वि॒रे दे॒वता᳚तये ।",
          "begin": 0.0,
          "end": 6.199
        },
        {
          "text": "स प᳚वस्वा॒नया᳚ रु॒चा ॥",
          "begin": 6.199,
          "end": 8.88
        }
      ]
    },
    {
      "id": "9.65.28",
      "mandala": 9,
      "sukta": 65,
      "rik": 28,
      "padas": [
        {
          "text": "आ ते॒ दक्षं᳚ मयो॒भुवं॒ वह्नि᳚म॒द्या वृ᳚णीमहे ।",
          "begin": 0.0,
          "end": 5.422
        },
        {
          "text": "पान्त॒मा पु᳚रु॒स्पृह᳚म् ॥",
          "begin": 5.422,
          "end": 8.24
        }
      ]
    },
    {
      "id": "9.65.29",
      "mandala": 9,
      "sukta": 65,
      "rik": 29,
      "padas": [
        {
          "text": "आ म॒न्द्रमा वरे᳚ण्य॒मा विप्र॒मा म᳚नी॒षिण᳚म् ।",
          "begin": 0.0,
          "end": 5.454
        },
        {
          "text": "पान्त॒मा पु᳚रु॒स्पृह᳚म् ॥",
          "begin": 5.454,
          "end": 8.04
        }
      ]
    },
    {
      "id": "9.65.30",
      "mandala": 9,
      "sukta": 65,
      "rik": 30,
      "padas": [
        {
          "text": "आ र॒यिमा सु᳚चे॒तुन॒मा सु᳚क्रतो त॒नूष्वा ।",
          "begin": 0.0,
          "end": 5.412
        },
        {
          "text": "पान्त॒मा पु᳚रु॒स्पृह᳚म् ॥",
          "begin": 5.412,
          "end": 9.68
        }
      ]
    }
  ]
}