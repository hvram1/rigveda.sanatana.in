{
  "mandala": 9,
  "sukta": 59,
  "verses": [
    {
      "id": "9.59.1",
      "mandala": 9,
      "sukta": 59,
      "rik": 1,
      "padas": [
        {
          "text": "पव᳚स्व गो॒जिद᳚श्व॒जिद्वि᳚श्व॒जित्सो᳚म रण्य॒जित् ।",
          "begin": 0.0,
          "end": 7.571
        },
        {
          "text": "प्र॒जाव॒द्रत्न॒मा भ᳚र ॥",
          "begin": 7.571,
          "end": 10.0
        }
      ]
    },
    {
      "id": "9.59.2",
      "mandala": 9,
      "sukta": 59,
      "rik": 2,
      "padas": [
        {
          "text": "पव᳚स्वा॒द्भ्यो अदा᳚भ्य॒ः पव॒स्वौष᳚धीभ्यः ।",
          "begin": 0.0,
          "end": 4.466
        },
        {
          "text": "पव᳚स्व धि॒षणा᳚भ्यः ॥",
          "begin": 4.466,
          "end": 7.6
        }
      ]
    },
    {
      "id": "9.59.3",
      "mandala": 9,
      "sukta": 59,
      "rik": 3,
      "padas": [
        {
          "text": "त्वं सो᳚म॒ पव᳚मानो॒ विश्वा᳚नि दुरि॒ता त᳚र ।",
          "begin": 0.0,
          "end": 4.893
        },
        {
          "text": "क॒विः सी᳚द॒ नि ब॒र्हिषि᳚ ॥",
          "begin": 4.893,
          "end": 7.36
        }
      ]
    },
    {
      "id": "9.59.4",
      "mandala": 9,
      "sukta": 59,
      "rik": 4,
      "padas": [
        {
          "text": "पव᳚मान॒ स्व᳚र्विदो॒ जाय᳚मानो॑भवो म॒हान् ।",
          "begin": 0.0,
          "end": 6.986
        },
        {
          "text": "इन्दो॒ विश्वाँ᳚ अ॒भीद᳚सि ॥",
          "begin": 6.986,
          "end": 9.64
        }
      ]
    }
  ]
}