{
  "mandala": 9,
  "sukta": 3,
  "verses": [
    {
      "id": "9.3.1",
      "mandala": 9,
      "sukta": 3,
      "rik": 1,
      "padas": [
        {
          "text": "ए॒ष दे॒वो अम᳚र्त्यः पर्ण॒वीरि᳚व दीयति ।",
          "begin": 0.0,
          "end": 6.013
        },
        {
          "text": "अ॒भि द्रोणा᳚न्या॒सद᳚म् ॥",
          "begin": 6.013,
          "end": 9.56
        }
      ]
    },
    {
      "id": "9.3.2",
      "mandala": 9,
      "sukta": 3,
      "rik": 2,
      "padas": [
        {
          "text": "ए॒ष दे॒वो वि॒पा कृ॒तो॑ति॒ ह्वरां᳚सि धावति ।",
          "begin": 0.0,
          "end": 4.975
        },
        {
          "text": "पव᳚मानो॒ अदा᳚भ्यः ॥",
          "begin": 4.975,
          "end": 7.92
        }
      ]
    },
    {
      "id": "9.3.3",
      "mandala": 9,
      "sukta": 3,
      "rik": 3,
      "padas": [
        {
          "text": "ए॒ष दे॒वो वि᳚प॒न्युभि॒ः पव᳚मान ऋता॒युभि᳚ः ।",
          "begin": 0.0,
          "end": 4.668
        },
        {
          "text": "हरि॒र्वाजा᳚य मृज्यते ॥",
          "begin": 4.668,
          "end": 7.96
        }
      ]
    },
    {
      "id": "9.3.4",
      "mandala": 9,
      "sukta": 3,
      "rik": 4,
      "padas": [
        {
          "text": "ए॒ष विश्वा᳚नि॒ वार्या॒ शूरो॒ यन्नि᳚व॒ सत्व᳚भिः ।",
          "begin": 0.0,
          "end": 5.345
        },
        {
          "text": "पव᳚मानः सिषासति ॥",
          "begin": 5.345,
          "end": 8.04
        }
      ]
    },
    {
      "id": "9.3.5",
      "mandala": 9,
      "sukta": 3,
      "rik": 5,
      "padas": [
        {
          "text": "ए॒ष दे॒वो र᳚थर्यति॒ पव᳚मानो दशस्यति ।",
          "begin": 0.0,
          "end": 4.999
        },
        {
          "text": "आ॒विष्कृ᳚णोति वग्व॒नुम् ॥",
          "begin": 4.999,
          "end": 7.36
        }
      ]
    },
    {
      "id": "9.3.6",
      "mandala": 9,
      "sukta": 3,
      "rik": 6,
      "padas": [
        {
          "text": "ए॒ष विप्रै᳚र॒भिष्टु᳚तो॒॑पो दे॒वो वि गा᳚हते ।",
          "begin": 0.0,
          "end": 5.542
        },
        {
          "text": "दध॒द्रत्ना᳚नि दा॒शुषे᳚ ॥",
          "begin": 5.542,
          "end": 9.2
        }
      ]
    },
    {
      "id": "9.3.7",
      "mandala": 9,
      "sukta": 3,
      "rik": 7,
      "padas": [
        {
          "text": "ए॒ष दिवं॒ वि धा᳚वति ति॒रो रजां᳚सि॒ धार᳚या ।",
          "begin": 0.0,
          "end": 5.64
        },
        {
          "text": "पव᳚मान॒ः कनि᳚क्रदत् ॥",
          "begin": 5.64,
          "end": 7.96
        }
      ]
    },
    {
      "id": "9.3.8",
      "mandala": 9,
      "sukta": 3,
      "rik": 8,
      "padas": [
        {
          "text": "ए॒ष दिवं॒ व्यास᳚रत्ति॒रो रजां॒स्यस्पृ᳚तः ।",
          "begin": 0.0,
          "end": 4.786
        },
        {
          "text": "पव᳚मानः स्वध्व॒रः ॥",
          "begin": 4.786,
          "end": 7.36
        }
      ]
    },
    {
      "id": "9.3.9",
      "mandala": 9,
      "sukta": 3,
      "rik": 9,
      "padas": [
        {
          "text": "ए॒ष प्र॒त्नेन॒ जन्म᳚ना दे॒वो दे॒वेभ्य᳚ः सु॒तः ।",
          "begin": 0.0,
          "end": 5.082
        },
        {
          "text": "हरि᳚ः प॒वित्रे᳚ अर्षति ॥",
          "begin": 5.082,
          "end": 8.32
        }
      ]
    },
    {
      "id": "9.3.10",
      "mandala": 9,
      "sukta": 3,
      "rik": 10,
      "padas": [
        {
          "text": "ए॒ष उ॒ स्य पु᳚रुव्र॒तो ज᳚ज्ञा॒नो ज॒नय॒न्निषः᳚ ।",
          "begin": 0.0,
          "end": 5.792
        },
        {
          "text": "धार᳚या पवते सु॒तः ॥",
          "begin": 5.792,
          "end": 9.08
        }
      ]
    }
  ]
}