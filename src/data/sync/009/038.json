{
  "mandala": 9,
  "sukta": 38,
  "verses": [
    {
      "id": "9.38.1",
      "mandala": 9,
      "sukta": 38,
      "rik": 1,
      "padas": [
        {
          "text": "ए॒ष उ॒ स्य वृषा॒ रथो॑व्यो॒ वारे᳚भिरर्षति ।",
          "begin": 0.0,
          "end": 6.012
        },
        {
          "text": "गच्छ॒न्वाजं᳚ सह॒स्रिण᳚म् ॥",
          "begin": 6.012,
          "end": 9.08
        }
      ]
    },
    {
      "id": "9.38.2",
      "mandala": 9,
      "sukta": 38,
      "rik": 2,
      "padas": [
        {
          "text": "ए॒तं त्रि॒तस्य॒ योष᳚णो॒ हरिं᳚ हिन्व॒न्त्यद्रि᳚भिः ।",
          "begin": 0.0,
          "end": 5.367
        },
        {
          "text": "इन्दु॒मिन्द्रा᳚य पी॒तये᳚ ॥",
          "begin": 5.367,
          "end": 8.12
        }
      ]
    },
    {
      "id": "9.38.3",
      "mandala": 9,
      "sukta": 38,
      "rik": 3,
      "padas": [
        {
          "text": "ए॒तं त्यं ह॒रितो॒ दश᳚ मर्मृ॒ज्यन्ते᳚ अप॒स्युवः᳚ ।",
          "begin": 0.0,
          "end": 4.816
        },
        {
          "text": "याभि॒र्मदा᳚य॒ शुम्भ᳚ते ॥",
          "begin": 4.816,
          "end": 8.2
        }
      ]
    },
    {
      "id": "9.38.4",
      "mandala": 9,
      "sukta": 38,
      "rik": 4,
      "padas": [
        {
          "text": "ए॒ष स्य मानु᳚षी॒ष्वा श्ये॒नो न वि॒क्षु सी᳚दति ।",
          "begin": 0.0,
          "end": 5.452
        },
        {
          "text": "गच्छ᳚ञ्जा॒रो न यो॒षित᳚म् ॥",
          "begin": 5.452,
          "end": 9.04
        }
      ]
    },
    {
      "id": "9.38.5",
      "mandala": 9,
      "sukta": 38,
      "rik": 5,
      "padas": [
        {
          "text": "ए॒ष स्य मद्यो॒ रसो॑व᳚ चष्टे दि॒वः शिशु᳚ः ।",
          "begin": 0.0,
          "end": 4.835
        },
        {
          "text": "य इन्दु॒र्वार॒मावि᳚शत् ॥",
          "begin": 4.835,
          "end": 7.0
        }
      ]
    },
    {
      "id": "9.38.6",
      "mandala": 9,
      "sukta": 38,
      "rik": 6,
      "padas": [
        {
          "text": "ए॒ष स्य पी॒तये᳚ सु॒तो हरि᳚रर्षति धर्ण॒सिः ।",
          "begin": 0.0,
          "end": 5.017
        },
        {
          "text": "क्रन्द॒न्योनि᳚म॒भि प्रि॒यम् ॥",
          "begin": 5.017,
          "end": 9.56
        }
      ]
    }
  ]
}