{
  "mandala": 9,
  "sukta": 27,
  "verses": [
    {
      "id": "9.27.1",
      "mandala": 9,
      "sukta": 27,
      "rik": 1,
      "padas": [
        {
          "text": "ए॒ष क॒विर॒भिष्टु᳚तः प॒वित्रे॒ अधि᳚ तोशते ।",
          "begin": 0.0,
          "end": 6.339
        },
        {
          "text": "पु॒ना॒नो घ्नन्नप॒ स्रिधः᳚ ॥",
          "begin": 6.339,
          "end": 9.72
        }
      ]
    },
    {
      "id": "9.27.2",
      "mandala": 9,
      "sukta": 27,
      "rik": 2,
      "padas": [
        {
          "text": "ए॒ष इन्द्रा᳚य वा॒यवे᳚ स्व॒र्जित्परि᳚ षिच्यते ।",
          "begin": 0.0,
          "end": 5.509
        },
        {
          "text": "प॒वित्रे᳚ दक्ष॒साध᳚नः ॥",
          "begin": 5.509,
          "end": 8.12
        }
      ]
    },
    {
      "id": "9.27.3",
      "mandala": 9,
      "sukta": 27,
      "rik": 3,
      "padas": [
        {
          "text": "ए॒ष नृभि॒र्वि नी᳚यते दि॒वो मू॒र्धा वृषा᳚ सु॒तः ।",
          "begin": 0.0,
          "end": 5.674
        },
        {
          "text": "सोमो॒ वने᳚षु विश्व॒वित् ॥",
          "begin": 5.674,
          "end": 8.36
        }
      ]
    },
    {
      "id": "9.27.4",
      "mandala": 9,
      "sukta": 27,
      "rik": 4,
      "padas": [
        {
          "text": "ए॒ष ग॒व्युर᳚चिक्रद॒त्पव᳚मानो हिरण्य॒युः ।",
          "begin": 0.0,
          "end": 4.605
        },
        {
          "text": "इन्दु᳚ः सत्रा॒जिदस्तृ᳚तः ॥",
          "begin": 4.605,
          "end": 7.04
        }
      ]
    },
    {
      "id": "9.27.5",
      "mandala": 9,
      "sukta": 27,
      "rik": 5,
      "padas": [
        {
          "text": "ए॒ष सूर्ये᳚ण हासते॒ पव᳚मानो॒ अधि॒ द्यवि᳚ ।",
          "begin": 0.0,
          "end": 4.484
        },
        {
          "text": "प॒वित्रे᳚ मत्स॒रो मदः᳚ ॥",
          "begin": 4.484,
          "end": 8.28
        }
      ]
    },
    {
      "id": "9.27.6",
      "mandala": 9,
      "sukta": 27,
      "rik": 6,
      "padas": [
        {
          "text": "ए॒ष शु॒ष्म्य᳚सिष्यदद॒न्तरि᳚क्षे॒ वृषा॒ हरि᳚ः ।",
          "begin": 0.0,
          "end": 7.396
        },
        {
          "text": "पु॒ना॒न इन्दु॒रिन्द्र॒मा ॥",
          "begin": 7.396,
          "end": 9.56
        }
      ]
    }
  ]
}