{
  "mandala": 9,
  "sukta": 48,
  "verses": [
    {
      "id": "9.48.1",
      "mandala": 9,
      "sukta": 48,
      "rik": 1,
      "padas": [
        {
          "text": "तं त्वा᳚ नृ॒म्णानि॒ बिभ्र᳚तं स॒धस्थे᳚षु म॒हो दि॒वः ।",
          "begin": 0.0,
          "end": 7.795
        },
        {
          "text": "चारुं᳚ सुकृ॒त्यये᳚महे ॥",
          "begin": 7.795,
          "end": 10.12
        }
      ]
    },
    {
      "id": "9.48.2",
      "mandala": 9,
      "sukta": 48,
      "rik": 2,
      "padas": [
        {
          "text": "संवृ᳚क्तधृष्णुमु॒क्थ्यं᳚ म॒हाम᳚हिव्रतं॒ मद᳚म् ।",
          "begin": 0.0,
          "end": 5.084
        },
        {
          "text": "श॒तं पुरो᳚ रुरु॒क्षणि᳚म् ॥",
          "begin": 5.084,
          "end": 7.76
        }
      ]
    },
    {
      "id": "9.48.3",
      "mandala": 9,
      "sukta": 48,
      "rik": 3,
      "padas": [
        {
          "text": "अत᳚स्त्वा र॒यिम॒भि राजा᳚नं सुक्रतो दि॒वः ।",
          "begin": 0.0,
          "end": 4.557
        },
        {
          "text": "सु॒प॒र्णो अ᳚व्य॒थिर्भ᳚रत् ॥",
          "begin": 4.557,
          "end": 7.4
        }
      ]
    },
    {
      "id": "9.48.4",
      "mandala": 9,
      "sukta": 48,
      "rik": 4,
      "padas": [
        {
          "text": "विश्व᳚स्मा॒ इत्स्व᳚र्दृ॒शे साधा᳚रणं रज॒स्तुर᳚म् ।",
          "begin": 0.0,
          "end": 5.301
        },
        {
          "text": "गो॒पामृ॒तस्य॒ विर्भ᳚रत् ॥",
          "begin": 5.301,
          "end": 7.52
        }
      ]
    },
    {
      "id": "9.48.5",
      "mandala": 9,
      "sukta": 48,
      "rik": 5,
      "padas": [
        {
          "text": "अधा᳚ हिन्वा॒न इ᳚न्द्रि॒यं ज्यायो᳚ महि॒त्वमा᳚नशे ।",
          "begin": 0.0,
          "end": 6.134
        },
        {
          "text": "अ॒भि॒ष्टि॒कृद्विच᳚र्षणिः ॥",
          "begin": 6.134,
          "end": 9.36
        }
      ]
    }
  ]
}