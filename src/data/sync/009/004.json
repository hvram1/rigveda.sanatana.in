{
  "mandala": 9,
  "sukta": 4,
  "verses": [
    {
      "id": "9.4.1",
      "mandala": 9,
      "sukta": 4,
      "rik": 1,
      "padas": [
        {
          "text": "सना᳚ च सोम॒ जेषि᳚ च॒ पव᳚मान॒ महि॒ श्रवः᳚ ।",
          "begin": 0.0,
          "end": 7.632
        },
        {
          "text": "अथा᳚ नो॒ वस्य᳚सस्कृधि ॥",
          "begin": 7.632,
          "end": 10.68
        }
      ]
    },
    {
      "id": "9.4.2",
      "mandala": 9,
      "sukta": 4,
      "rik": 2,
      "padas": [
        {
          "text": "सना॒ ज्योति॒ः सना॒ स्व१॒᳚र्विश्वा᳚ च सोम॒ सौभ᳚गा ।",
          "begin": 0.0,
          "end": 6.725
        },
        {
          "text": "अथा᳚ नो॒ वस्य᳚सस्कृधि ॥",
          "begin": 6.725,
          "end": 9.64
        }
      ]
    },
    {
      "id": "9.4.3",
      "mandala": 9,
      "sukta": 4,
      "rik": 3,
      "padas": [
        {
          "text": "सना॒ दक्ष᳚मु॒त क्रतु॒मप᳚ सोम॒ मृधो᳚ जहि ।",
          "begin": 0.0,
          "end": 5.701
        },
        {
          "text": "अथा᳚ नो॒ वस्य᳚सस्कृधि ॥",
          "begin": 5.701,
          "end": 7.84
        }
      ]
    },
    {
      "id": "9.4.4",
      "mandala": 9,
      "sukta": 4,
      "rik": 4,
      "padas": [
        {
          "text": "पवी᳚तारः पुनी॒तन॒ सोम॒मिन्द्रा᳚य॒ पात᳚वे ।",
          "begin": 0.0,
          "end": 5.77
        },
        {
          "text": "अथा᳚ नो॒ वस्य᳚सस्कृधि ॥",
          "begin": 5.77,
          "end": 8.56
        }
      ]
    },
    {
      "id": "9.4.5",
      "mandala": 9,
      "sukta": 4,
      "rik": 5,
      "padas": [
        {
          "text": "त्वं सूर्ये᳚ न॒ आ भ᳚ज॒ तव॒ क्रत्वा॒ तवो॒तिभि᳚ः ।",
          "begin": 0.0,
          "end": 6.5
        },
        {
          "text": "अथा᳚ नो॒ वस्य᳚सस्कृधि ॥",
          "begin": 6.5,
          "end": 8.72
        }
      ]
    },
    {
      "id": "9.4.6",
      "mandala": 9,
      "sukta": 4,
      "rik": 6,
      "padas": [
        {
          "text": "तव॒ क्रत्वा॒ तवो॒तिभि॒र्ज्योक्प᳚श्येम॒ सूर्य᳚म् ।",
          "begin": 0.0,
          "end": 6.319
        },
        {
          "text": "अथा᳚ नो॒ वस्य᳚सस्कृधि ॥",
          "begin": 6.319,
          "end": 8.44
        }
      ]
    },
    {
      "id": "9.4.7",
      "mandala": 9,
      "sukta": 4,
      "rik": 7,
      "padas": [
        {
          "text": "अ॒भ्य᳚र्ष स्वायुध॒ सोम᳚ द्वि॒बर्ह᳚सं र॒यिम् ।",
          "begin": 0.0,
          "end": 5.479
        },
        {
          "text": "अथा᳚ नो॒ वस्य᳚सस्कृधि ॥",
          "begin": 5.479,
          "end": 7.6
        }
      ]
    },
    {
      "id": "9.4.8",
      "mandala": 9,
      "sukta": 4,
      "rik": 8,
      "padas": [
        {
          "text": "अ॒भ्य१॒᳚र्षान᳚पच्युतो र॒यिं स॒मत्सु᳚ सास॒हिः ।",
          "begin": 0.0,
          "end": 5.42
        },
        {
          "text": "अथा᳚ नो॒ वस्य᳚सस्कृधि ॥",
          "begin": 5.42,
          "end": 8.56
        }
      ]
    },
    {
      "id": "9.4.9",
      "mandala": 9,
      "sukta": 4,
      "rik": 9,
      "padas": [
        {
          "text": "त्वां य॒ज्ञैर᳚वीवृध॒न्पव᳚मान॒ विध᳚र्मणि ।",
          "begin": 0.0,
          "end": 5.893
        },
        {
          "text": "अथा᳚ नो॒ वस्य᳚सस्कृधि ॥",
          "begin": 5.893,
          "end": 8.44
        }
      ]
    },
    {
      "id": "9.4.10",
      "mandala": 9,
      "sukta": 4,
      "rik": 10,
      "padas": [
        {
          "text": "र॒यिं न᳚श्चि॒त्रम॒श्विन॒मिन्दो᳚ वि॒श्वायु॒मा भ᳚र ।",
          "begin": 0.0,
          "end": 6.738
        },
        {
          "text": "अथा᳚ नो॒ वस्य᳚सस्कृधि ॥",
          "begin": 6.738,
          "end": 13.0
        }
      ]
    }
  ]
}