{
  "mandala": 9,
  "sukta": 64,
  "verses": [
    {
      "id": "9.64.1",
      "mandala": 9,
      "sukta": 64,
      "rik": 1,
      "padas": [
        {
          "text": "वृषा᳚ सोम द्यु॒माँ अ᳚सि॒ वृषा᳚ देव॒ वृष᳚व्रतः ।",
          "begin": 0.0,
          "end": 7.438
        },
        {
          "text": "वृषा॒ धर्मा᳚णि दधिषे ॥",
          "begin": 7.438,
          "end": 10.16
        }
      ]
    },
    {
      "id": "9.64.2",
      "mandala": 9,
      "sukta": 64,
      "rik": 2,
      "padas": [
        {
          "text": "वृष्ण᳚स्ते॒ वृष्ण्यं॒ शवो॒ वृषा॒ वनं॒ वृषा॒ मदः᳚ ।",
          "begin": 0.0,
          "end": 4.65
        },
        {
          "text": "स॒त्यं वृ᳚ष॒न्वृषेद᳚सि ॥",
          "begin": 4.65,
          "end": 6.8
        }
      ]
    },
    {
      "id": "9.64.3",
      "mandala": 9,
      "sukta": 64,
      "rik": 3,
      "padas": [
        {
          "text": "अश्वो॒ न च᳚क्रदो॒ वृषा॒ सं गा इ᳚न्दो॒ समर्व᳚तः ।",
          "begin": 0.0,
          "end": 5.39
        },
        {
          "text": "वि नो᳚ रा॒ये दुरो᳚ वृधि ॥",
          "begin": 5.39,
          "end": 8.36
        }
      ]
    },
    {
      "id": "9.64.4",
      "mandala": 9,
      "sukta": 64,
      "rik": 4,
      "padas": [
        {
          "text": "असृ᳚क्षत॒ प्र वा॒जिनो᳚ ग॒व्या सोमा᳚सो अश्व॒या ।",
          "begin": 0.0,
          "end": 5.941
        },
        {
          "text": "शु॒क्रासो᳚ वीर॒याशवः᳚ ॥",
          "begin": 5.941,
          "end": 8.8
        }
      ]
    },
    {
      "id": "9.64.5",
      "mandala": 9,
      "sukta": 64,
      "rik": 5,
      "padas": [
        {
          "text": "शु॒म्भमा᳚ना ऋता॒युभि᳚र्मृ॒ज्यमा᳚ना॒ गभ᳚स्त्योः ।",
          "begin": 0.0,
          "end": 6.181
        },
        {
          "text": "पव᳚न्ते॒ वारे᳚ अ॒व्यये᳚ ॥",
          "begin": 6.181,
          "end": 9.52
        }
      ]
    },
    {
      "id": "9.64.6",
      "mandala": 9,
      "sukta": 64,
      "rik": 6,
      "padas": [
        {
          "text": "ते विश्वा᳚ दा॒शुषे॒ वसु॒ सोमा᳚ दि॒व्यानि॒ पार्थि᳚वा ।",
          "begin": 0.0,
          "end": 5.55
        },
        {
          "text": "पव᳚न्ता॒मान्तरि᳚क्ष्या ॥",
          "begin": 5.55,
          "end": 8.64
        }
      ]
    },
    {
      "id": "9.64.7",
      "mandala": 9,
      "sukta": 64,
      "rik": 7,
      "padas": [
        {
          "text": "पव᳚मानस्य विश्ववि॒त्प्र ते॒ सर्गा᳚ असृक्षत ।",
          "begin": 0.0,
          "end": 5.152
        },
        {
          "text": "सूर्य᳚स्येव॒ न र॒श्मयः᳚ ॥",
          "begin": 5.152,
          "end": 7.8
        }
      ]
    },
    {
      "id": "9.64.8",
      "mandala": 9,
      "sukta": 64,
      "rik": 8,
      "padas": [
        {
          "text": "के॒तुं कृ॒ण्वन्दि॒वस्परि॒ विश्वा᳚ रू॒पाभ्य᳚र्षसि ।",
          "begin": 0.0,
          "end": 5.508
        },
        {
          "text": "स॒मु॒द्रः सो᳚म पिन्वसे ॥",
          "begin": 5.508,
          "end": 8.16
        }
      ]
    },
    {
      "id": "9.64.9",
      "mandala": 9,
      "sukta": 64,
      "rik": 9,
      "padas": [
        {
          "text": "हि॒न्वा॒नो वाच᳚मिष्यसि॒ पव᳚मान॒ विध᳚र्मणि ।",
          "begin": 0.0,
          "end": 4.708
        },
        {
          "text": "अक्रा᳚न्दे॒वो न सूर्य᳚ः ॥",
          "begin": 4.708,
          "end": 7.8
        }
      ]
    },
    {
      "id": "9.64.10",
      "mandala": 9,
      "sukta": 64,
      "rik": 10,
      "padas": [
        {
          "text": "इन्दु᳚ः पविष्ट॒ चेत᳚नः प्रि॒यः क᳚वी॒नां म॒ती ।",
          "begin": 0.0,
          "end": 4.759
        },
        {
          "text": "सृ॒जदश्वं᳚ र॒थीरि᳚व ॥",
          "begin": 4.759,
          "end": 7.12
        }
      ]
    },
    {
      "id": "9.64.11",
      "mandala": 9,
      "sukta": 64,
      "rik": 11,
      "padas": [
        {
          "text": "ऊ॒र्मिर्यस्ते᳚ प॒वित्र॒ आ दे᳚वा॒वीः प॒र्यक्ष᳚रत् ।",
          "begin": 0.0,
          "end": 5.974
        },
        {
          "text": "सीद᳚न्नृ॒तस्य॒ योनि॒मा ॥",
          "begin": 5.974,
          "end": 8.56
        }
      ]
    },
    {
      "id": "9.64.12",
      "mandala": 9,
      "sukta": 64,
      "rik": 12,
      "padas": [
        {
          "text": "स नो᳚ अर्ष प॒वित्र॒ आ मदो॒ यो दे᳚व॒वीत᳚मः ।",
          "begin": 0.0,
          "end": 6.053
        },
        {
          "text": "इन्द॒विन्द्रा᳚य पी॒तये᳚ ॥",
          "begin": 6.053,
          "end": 8.0
        }
      ]
    },
    {
      "id": "9.64.13",
      "mandala": 9,
      "sukta": 64,
      "rik": 13,
      "padas": [
        {
          "text": "इ॒षे प᳚वस्व॒ धार᳚या मृ॒ज्यमा᳚नो मनी॒षिभि᳚ः ।",
          "begin": 0.0,
          "end": 5.017
        },
        {
          "text": "इन्दो᳚ रु॒चाभि गा इ᳚हि ॥",
          "begin": 5.017,
          "end": 7.8
        }
      ]
    },
    {
      "id": "9.64.14",
      "mandala": 9,
      "sukta": 64,
      "rik": 14,
      "padas": [
        {
          "text": "पु॒ना॒नो वरि᳚वस्कृ॒ध्यूर्जं॒ जना᳚य गिर्वणः ।",
          "begin": 0.0,
          "end": 5.109
        },
        {
          "text": "हरे᳚ सृजा॒न आ॒शिर᳚म् ॥",
          "begin": 5.109,
          "end": 7.8
        }
      ]
    },
    {
      "id": "9.64.15",
      "mandala": 9,
      "sukta": 64,
      "rik": 15,
      "padas": [
        {
          "text": "पु॒ना॒नो दे॒ववी᳚तय॒ इन्द्र᳚स्य याहि निष्कृ॒तम् ।",
          "begin": 0.0,
          "end": 4.586
        },
        {
          "text": "द्यु॒ता॒नो वा॒जिभि᳚र्य॒तः ॥",
          "begin": 4.586,
          "end": 7.44
        }
      ]
    },
    {
      "id": "9.64.16",
      "mandala": 9,
      "sukta": 64,
      "rik": 16,
      "padas": [
        {
          "text": "प्र हि᳚न्वा॒नास॒ इन्द॒वो॑च्छा᳚ समु॒द्रमा॒शवः᳚ ।",
          "begin": 0.0,
          "end": 5.666
        },
        {
          "text": "धि॒या जू॒ता अ᳚सृक्षत ॥",
          "begin": 5.666,
          "end": 8.0
        }
      ]
    },
    {
      "id": "9.64.17",
      "mandala": 9,
      "sukta": 64,
      "rik": 17,
      "padas": [
        {
          "text": "म॒र्मृ॒जा॒नास᳚ आ॒यवो॒ वृथा᳚ समु॒द्रमिन्द᳚वः ।",
          "begin": 0.0,
          "end": 4.977
        },
        {
          "text": "अग्म᳚न्नृ॒तस्य॒ योनि॒मा ॥",
          "begin": 4.977,
          "end": 7.52
        }
      ]
    },
    {
      "id": "9.64.18",
      "mandala": 9,
      "sukta": 64,
      "rik": 18,
      "padas": [
        {
          "text": "परि᳚ णो याह्यस्म॒युर्विश्वा॒ वसू॒न्योज᳚सा ।",
          "begin": 0.0,
          "end": 4.487
        },
        {
          "text": "पा॒हि न॒ः शर्म᳚ वी॒रव᳚त् ॥",
          "begin": 4.487,
          "end": 7.0
        }
      ]
    },
    {
      "id": "9.64.19",
      "mandala": 9,
      "sukta": 64,
      "rik": 19,
      "padas": [
        {
          "text": "मिमा᳚ति॒ वह्नि॒रेत᳚शः प॒दं यु᳚जा॒न ऋक्व᳚भिः ।",
          "begin": 0.0,
          "end": 4.335
        },
        {
          "text": "प्र यत्स᳚मु॒द्र आहि᳚तः ॥",
          "begin": 4.335,
          "end": 7.08
        }
      ]
    },
    {
      "id": "9.64.20",
      "mandala": 9,
      "sukta": 64,
      "rik": 20,
      "padas": [
        {
          "text": "आ यद्योनिं᳚ हिर॒ण्यय᳚मा॒शुरृ॒तस्य॒ सीद᳚ति ।",
          "begin": 0.0,
          "end": 4.616
        },
        {
          "text": "जहा॒त्यप्र᳚चेतसः ॥",
          "begin": 4.616,
          "end": 7.24
        }
      ]
    },
    {
      "id": "9.64.21",
      "mandala": 9,
      "sukta": 64,
      "rik": 21,
      "padas": [
        {
          "text": "अ॒भि वे॒ना अ᳚नूष॒तेय᳚क्षन्ति॒ प्रचे᳚तसः ।",
          "begin": 0.0,
          "end": 5.032
        },
        {
          "text": "मज्ज॒न्त्यवि᳚चेतसः ॥",
          "begin": 5.032,
          "end": 7.08
        }
      ]
    },
    {
      "id": "9.64.22",
      "mandala": 9,
      "sukta": 64,
      "rik": 22,
      "padas": [
        {
          "text": "इन्द्रा᳚येन्दो म॒रुत्व᳚ते॒ पव᳚स्व॒ मधु᳚मत्तमः ।",
          "begin": 0.0,
          "end": 5.005
        },
        {
          "text": "ऋ॒तस्य॒ योनि᳚मा॒सद᳚म् ॥",
          "begin": 5.005,
          "end": 7.44
        }
      ]
    },
    {
      "id": "9.64.23",
      "mandala": 9,
      "sukta": 64,
      "rik": 23,
      "padas": [
        {
          "text": "तं त्वा॒ विप्रा᳚ वचो॒विद॒ः परि᳚ ष्कृण्वन्ति वे॒धसः᳚ ।",
          "begin": 0.0,
          "end": 5.385
        },
        {
          "text": "सं त्वा᳚ मृजन्त्या॒यवः᳚ ॥",
          "begin": 5.385,
          "end": 7.96
        }
      ]
    },
    {
      "id": "9.64.24",
      "mandala": 9,
      "sukta": 64,
      "rik": 24,
      "padas": [
        {
          "text": "रसं᳚ ते मि॒त्रो अ᳚र्य॒मा पिब᳚न्ति॒ वरु᳚णः कवे ।",
          "begin": 0.0,
          "end": 5.821
        },
        {
          "text": "पव᳚मानस्य म॒रुतः᳚ ॥",
          "begin": 5.821,
          "end": 7.96
        }
      ]
    },
    {
      "id": "9.64.25",
      "mandala": 9,
      "sukta": 64,
      "rik": 25,
      "padas": [
        {
          "text": "त्वं सो᳚म विप॒श्चितं᳚ पुना॒नो वाच᳚मिष्यसि ।",
          "begin": 0.0,
          "end": 5.306
        },
        {
          "text": "इन्दो᳚ स॒हस्र᳚भर्णसम् ॥",
          "begin": 5.306,
          "end": 7.88
        }
      ]
    },
    {
      "id": "9.64.26",
      "mandala": 9,
      "sukta": 64,
      "rik": 26,
      "padas": [
        {
          "text": "उ॒तो स॒हस्र᳚भर्णसं॒ वाचं᳚ सोम मख॒स्युव᳚म् ।",
          "begin": 0.0,
          "end": 4.885
        },
        {
          "text": "पु॒ना॒न इ᳚न्द॒वा भ᳚र ॥",
          "begin": 4.885,
          "end": 7.72
        }
      ]
    },
    {
      "id": "9.64.27",
      "mandala": 9,
      "sukta": 64,
      "rik": 27,
      "padas": [
        {
          "text": "पु॒ना॒न इ᳚न्दवेषां॒ पुरु᳚हूत॒ जना᳚नाम् ।",
          "begin": 0.0,
          "end": 4.828
        },
        {
          "text": "प्रि॒यः स᳚मु॒द्रमा वि᳚श ॥",
          "begin": 4.828,
          "end": 7.24
        }
      ]
    },
    {
      "id": "9.64.28",
      "mandala": 9,
      "sukta": 64,
      "rik": 28,
      "padas": [
        {
          "text": "दवि᳚द्युतत्या रु॒चा प᳚रि॒ष्टोभ᳚न्त्या कृ॒पा ।",
          "begin": 0.0,
          "end": 5.054
        },
        {
          "text": "सोमा᳚ः शु॒क्रा गवा᳚शिरः ॥",
          "begin": 5.054,
          "end": 8.2
        }
      ]
    },
    {
      "id": "9.64.29",
      "mandala": 9,
      "sukta": 64,
      "rik": 29,
      "padas": [
        {
          "text": "हि॒न्वा॒नो हे॒तृभि᳚र्य॒त आ वाजं᳚ वा॒ज्य᳚क्रमीत् ।",
          "begin": 0.0,
          "end": 6.15
        },
        {
          "text": "सीद᳚न्तो व॒नुषो᳚ यथा ॥",
          "begin": 6.15,
          "end": 9.08
        }
      ]
    },
    {
      "id": "9.64.30",
      "mandala": 9,
      "sukta": 64,
      "rik": 30,
      "padas": [
        {
          "text": "ऋ॒धक्सो᳚म स्व॒स्तये᳚ संजग्मा॒नो दि॒वः क॒विः ।",
          "begin": 0.0,
          "end": 6.139
        },
        {
          "text": "पव᳚स्व॒ सूर्यो᳚ दृ॒शे ॥",
          "begin": 6.139,
          "end": 11.04
        }
      ]
    }
  ]
}