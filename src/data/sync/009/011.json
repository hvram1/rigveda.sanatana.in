{
  "mandala": 9,
  "sukta": 11,
  "verses": [
    {
      "id": "9.11.1",
      "mandala": 9,
      "sukta": 11,
      "rik": 1,
      "padas": [
        {
          "text": "उपा᳚स्मै गायता नर॒ः पव᳚माना॒येन्द᳚वे ।",
          "begin": 0.0,
          "end": 6.943
        },
        {
          "text": "अ॒भि दे॒वाँ इय᳚क्षते ॥",
          "begin": 6.943,
          "end": 10.32
        }
      ]
    },
    {
      "id": "9.11.2",
      "mandala": 9,
      "sukta": 11,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒भि ते॒ मधु᳚ना॒ पयो॑थ᳚र्वाणो अशिश्रयुः ।",
          "begin": 0.0,
          "end": 5.999
        },
        {
          "text": "दे॒वं दे॒वाय᳚ देव॒यु ॥",
          "begin": 5.999,
          "end": 8.6
        }
      ]
    },
    {
      "id": "9.11.3",
      "mandala": 9,
      "sukta": 11,
      "rik": 3,
      "padas": [
        {
          "text": "स नः᳚ पवस्व॒ शं गवे॒ शं जना᳚य॒ शमर्व᳚ते ।",
          "begin": 0.0,
          "end": 5.754
        },
        {
          "text": "शं रा᳚ज॒न्नोष᳚धीभ्यः ॥",
          "begin": 5.754,
          "end": 9.0
        }
      ]
    },
    {
      "id": "9.11.4",
      "mandala": 9,
      "sukta": 11,
      "rik": 4,
      "padas": [
        {
          "text": "ब॒भ्रवे॒ नु स्वत᳚वसे॑रु॒णाय᳚ दिवि॒स्पृशे᳚ ।",
          "begin": 0.0,
          "end": 5.596
        },
        {
          "text": "सोमा᳚य गा॒थम᳚र्चत ॥",
          "begin": 5.596,
          "end": 8.56
        }
      ]
    },
    {
      "id": "9.11.5",
      "mandala": 9,
      "sukta": 11,
      "rik": 5,
      "padas": [
        {
          "text": "हस्त᳚च्युतेभि॒रद्रि᳚भिः सु॒तं सोमं᳚ पुनीतन ।",
          "begin": 0.0,
          "end": 5.619
        },
        {
          "text": "मधा॒वा धा᳚वता॒ मधु᳚ ॥",
          "begin": 5.619,
          "end": 8.52
        }
      ]
    },
    {
      "id": "9.11.6",
      "mandala": 9,
      "sukta": 11,
      "rik": 6,
      "padas": [
        {
          "text": "नम॒सेदुप᳚ सीदत द॒ध्नेद॒भि श्री᳚णीतन ।",
          "begin": 0.0,
          "end": 5.562
        },
        {
          "text": "इन्दु॒मिन्द्रे᳚ दधातन ॥",
          "begin": 5.562,
          "end": 8.32
        }
      ]
    },
    {
      "id": "9.11.7",
      "mandala": 9,
      "sukta": 11,
      "rik": 7,
      "padas": [
        {
          "text": "अ॒मि॒त्र॒हा विच᳚र्षणि॒ः पव᳚स्व सोम॒ शं गवे᳚ ।",
          "begin": 0.0,
          "end": 5.006
        },
        {
          "text": "दे॒वेभ्यो᳚ अनुकाम॒कृत् ॥",
          "begin": 5.006,
          "end": 8.68
        }
      ]
    },
    {
      "id": "9.11.8",
      "mandala": 9,
      "sukta": 11,
      "rik": 8,
      "padas": [
        {
          "text": "इन्द्रा᳚य सोम॒ पात᳚वे॒ मदा᳚य॒ परि᳚ षिच्यसे ।",
          "begin": 0.0,
          "end": 6.343
        },
        {
          "text": "म॒न॒श्चिन्मन᳚स॒स्पति᳚ः ॥",
          "begin": 6.343,
          "end": 8.76
        }
      ]
    },
    {
      "id": "9.11.9",
      "mandala": 9,
      "sukta": 11,
      "rik": 9,
      "padas": [
        {
          "text": "पव᳚मान सु॒वीर्यं᳚ र॒यिं सो᳚म रिरीहि नः ।",
          "begin": 0.0,
          "end": 6.374
        },
        {
          "text": "इन्द॒विन्द्रे᳚ण नो यु॒जा ॥",
          "begin": 6.374,
          "end": 10.52
        }
      ]
    }
  ]
}