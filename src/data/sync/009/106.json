{
  "mandala": 9,
  "sukta": 106,
  "verses": [
    {
      "id": "9.106.1",
      "mandala": 9,
      "sukta": 106,
      "rik": 1,
      "padas": [
        {
          "text": "इन्द्र॒मच्छ᳚ सु॒ता इ॒मे वृष᳚णं यन्तु॒ हर᳚यः ।",
          "begin": 0.0,
          "end": 6.642
        },
        {
          "text": "श्रु॒ष्टी जा॒तास॒ इन्द᳚वः स्व॒र्विदः᳚ ॥",
          "begin": 6.642,
          "end": 10.52
        }
      ]
    },
    {
      "id": "9.106.2",
      "mandala": 9,
      "sukta": 106,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒यं भरा᳚य सान॒सिरिन्द्रा᳚य पवते सु॒तः ।",
          "begin": 0.0,
          "end": 5.663
        },
        {
          "text": "सोमो॒ जैत्र᳚स्य चेतति॒ यथा᳚ वि॒दे ॥",
          "begin": 5.663,
          "end": 9.6
        }
      ]
    },
    {
      "id": "9.106.3",
      "mandala": 9,
      "sukta": 106,
      "rik": 3,
      "padas": [
        {
          "text": "अ॒स्येदिन्द्रो॒ मदे॒ष्वा ग्रा॒भं गृ᳚भ्णीत सान॒सिम् ।",
          "begin": 0.0,
          "end": 5.454
        },
        {
          "text": "वज्रं᳚ च॒ वृष᳚णं भर॒त्सम᳚प्सु॒जित् ॥",
          "begin": 5.454,
          "end": 9.48
        }
      ]
    },
    {
      "id": "9.106.4",
      "mandala": 9,
      "sukta": 106,
      "rik": 4,
      "padas": [
        {
          "text": "प्र ध᳚न्वा सोम॒ जागृ᳚वि॒रिन्द्रा᳚येन्दो॒ परि᳚ स्रव ।",
          "begin": 0.0,
          "end": 5.97
        },
        {
          "text": "द्यु॒मन्तं॒ शुष्म॒मा भ᳚रा स्व॒र्विद᳚म् ॥",
          "begin": 5.97,
          "end": 9.8
        }
      ]
    },
    {
      "id": "9.106.5",
      "mandala": 9,
      "sukta": 106,
      "rik": 5,
      "padas": [
        {
          "text": "इन्द्रा᳚य॒ वृष᳚णं॒ मदं॒ पव᳚स्व वि॒श्वद᳚र्शतः ।",
          "begin": 0.0,
          "end": 5.436
        },
        {
          "text": "स॒हस्र᳚यामा पथि॒कृद्वि᳚चक्ष॒णः ॥",
          "begin": 5.436,
          "end": 8.8
        }
      ]
    },
    {
      "id": "9.106.6",
      "mandala": 9,
      "sukta": 106,
      "rik": 6,
      "padas": [
        {
          "text": "अ॒स्मभ्यं᳚ गातु॒वित्त᳚मो दे॒वेभ्यो॒ मधु᳚मत्तमः ।",
          "begin": 0.0,
          "end": 6.805
        },
        {
          "text": "स॒हस्रं᳚ याहि प॒थिभि॒ः कनि᳚क्रदत् ॥",
          "begin": 6.805,
          "end": 9.68
        }
      ]
    },
    {
      "id": "9.106.7",
      "mandala": 9,
      "sukta": 106,
      "rik": 7,
      "padas": [
        {
          "text": "पव᳚स्व दे॒ववी᳚तय॒ इन्दो॒ धारा᳚भि॒रोज᳚सा ।",
          "begin": 0.0,
          "end": 5.475
        },
        {
          "text": "आ क॒लशं॒ मधु᳚मान्त्सोम नः सदः ॥",
          "begin": 5.475,
          "end": 9.8
        }
      ]
    },
    {
      "id": "9.106.8",
      "mandala": 9,
      "sukta": 106,
      "rik": 8,
      "padas": [
        {
          "text": "तव᳚ द्र॒प्सा उ᳚द॒प्रुत॒ इन्द्रं॒ मदा᳚य वावृधुः ।",
          "begin": 0.0,
          "end": 5.937
        },
        {
          "text": "त्वां दे॒वासो᳚ अ॒मृता᳚य॒ कं प᳚पुः ॥",
          "begin": 5.937,
          "end": 10.16
        }
      ]
    },
    {
      "id": "9.106.9",
      "mandala": 9,
      "sukta": 106,
      "rik": 9,
      "padas": [
        {
          "text": "आ नः᳚ सुतास इन्दवः पुना॒ना धा᳚वता र॒यिम् ।",
          "begin": 0.0,
          "end": 5.82
        },
        {
          "text": "वृ॒ष्टिद्या᳚वो रीत्यापः स्व॒र्विदः᳚ ॥",
          "begin": 5.82,
          "end": 10.04
        }
      ]
    },
    {
      "id": "9.106.10",
      "mandala": 9,
      "sukta": 106,
      "rik": 10,
      "padas": [
        {
          "text": "सोमः᳚ पुना॒न ऊ॒र्मिणाव्यो॒ वारं॒ वि धा᳚वति ।",
          "begin": 0.0,
          "end": 5.384
        },
        {
          "text": "अग्रे᳚ वा॒चः पव᳚मान॒ः कनि᳚क्रदत् ॥",
          "begin": 5.384,
          "end": 9.92
        }
      ]
    },
    {
      "id": "9.106.11",
      "mandala": 9,
      "sukta": 106,
      "rik": 11,
      "padas": [
        {
          "text": "धी॒भिर्हि᳚न्वन्ति वा॒जिनं॒ वने॒ क्रीळ᳚न्त॒मत्य᳚विम् ।",
          "begin": 0.0,
          "end": 5.763
        },
        {
          "text": "अ॒भि त्रि᳚पृ॒ष्ठं म॒तय॒ः सम᳚स्वरन् ॥",
          "begin": 5.763,
          "end": 9.92
        }
      ]
    },
    {
      "id": "9.106.12",
      "mandala": 9,
      "sukta": 106,
      "rik": 12,
      "padas": [
        {
          "text": "अस᳚र्जि क॒लशाँ᳚ अ॒भि मी॒ळ्हे सप्ति॒र्न वा᳚ज॒युः ।",
          "begin": 0.0,
          "end": 5.908
        },
        {
          "text": "पु॒ना॒नो वाचं᳚ ज॒नय᳚न्नसिष्यदत् ॥",
          "begin": 5.908,
          "end": 10.0
        }
      ]
    },
    {
      "id": "9.106.13",
      "mandala": 9,
      "sukta": 106,
      "rik": 13,
      "padas": [
        {
          "text": "पव᳚ते हर्य॒तो हरि॒रति॒ ह्वरां᳚सि॒ रंह्या᳚ ।",
          "begin": 0.0,
          "end": 5.94
        },
        {
          "text": "अ॒भ्यर्ष᳚न्त्स्तो॒तृभ्यो᳚ वी॒रव॒द्यशः᳚ ॥",
          "begin": 5.94,
          "end": 10.56
        }
      ]
    },
    {
      "id": "9.106.14",
      "mandala": 9,
      "sukta": 106,
      "rik": 14,
      "padas": [
        {
          "text": "अ॒या प᳚वस्व देव॒युर्मधो॒र्धारा᳚ असृक्षत ।",
          "begin": 0.0,
          "end": 5.795
        },
        {
          "text": "रेभ᳚न्प॒वित्रं॒ पर्ये᳚षि वि॒श्वतः᳚ ॥",
          "begin": 5.795,
          "end": 14.88
        }
      ]
    }
  ]
}