{
  "mandala": 9,
  "sukta": 29,
  "verses": [
    {
      "id": "9.29.1",
      "mandala": 9,
      "sukta": 29,
      "rik": 1,
      "padas": [
        {
          "text": "प्रास्य॒ धारा᳚ अक्षर॒न्वृष्ण᳚ः सु॒तस्यौज᳚सा ।",
          "begin": 0.0,
          "end": 7.703
        },
        {
          "text": "दे॒वाँ अनु᳚ प्र॒भूष᳚तः ॥",
          "begin": 7.703,
          "end": 10.96
        }
      ]
    },
    {
      "id": "9.29.2",
      "mandala": 9,
      "sukta": 29,
      "rik": 2,
      "padas": [
        {
          "text": "सप्तिं᳚ मृजन्ति वे॒धसो᳚ गृ॒णन्त᳚ः का॒रवो᳚ गि॒रा ।",
          "begin": 0.0,
          "end": 5.478
        },
        {
          "text": "ज्योति᳚र्जज्ञा॒नमु॒क्थ्य᳚म् ॥",
          "begin": 5.478,
          "end": 8.56
        }
      ]
    },
    {
      "id": "9.29.3",
      "mandala": 9,
      "sukta": 29,
      "rik": 3,
      "padas": [
        {
          "text": "सु॒षहा᳚ सोम॒ तानि᳚ ते पुना॒नाय᳚ प्रभूवसो ।",
          "begin": 0.0,
          "end": 5.269
        },
        {
          "text": "वर्धा᳚ समु॒द्रमु॒क्थ्य᳚म् ॥",
          "begin": 5.269,
          "end": 8.2
        }
      ]
    },
    {
      "id": "9.29.4",
      "mandala": 9,
      "sukta": 29,
      "rik": 4,
      "padas": [
        {
          "text": "विश्वा॒ वसू᳚नि सं॒जय॒न्पव᳚स्व सोम॒ धार᳚या ।",
          "begin": 0.0,
          "end": 4.841
        },
        {
          "text": "इ॒नु द्वेषां᳚सि स॒ध्र्य᳚क् ॥",
          "begin": 4.841,
          "end": 7.84
        }
      ]
    },
    {
      "id": "9.29.5",
      "mandala": 9,
      "sukta": 29,
      "rik": 5,
      "padas": [
        {
          "text": "रक्षा॒ सु नो॒ अर᳚रुषः स्व॒नात्स᳚मस्य॒ कस्य᳚ चित् ।",
          "begin": 0.0,
          "end": 4.949
        },
        {
          "text": "नि॒दो यत्र᳚ मुमु॒च्महे᳚ ॥",
          "begin": 4.949,
          "end": 7.4
        }
      ]
    },
    {
      "id": "9.29.6",
      "mandala": 9,
      "sukta": 29,
      "rik": 6,
      "padas": [
        {
          "text": "एन्दो॒ पार्थि᳚वं र॒यिं दि॒व्यं प᳚वस्व॒ धार᳚या ।",
          "begin": 0.0,
          "end": 5.072
        },
        {
          "text": "द्यु॒मन्तं॒ शुष्म॒मा भ᳚र ॥",
          "begin": 5.072,
          "end": 9.52
        }
      ]
    }
  ]
}