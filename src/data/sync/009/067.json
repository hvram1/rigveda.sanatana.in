    {
      "id": "9.67.19",
      "mandala": 9,
      "sukta": 67,
      "rik": 19,
      "padas": [
        {
          "text": "ग्राव्णा᳚ तु॒न्नो अ॒भिष्टु᳚तः प॒वित्रं᳚ सोम गच्छसि ।",
          "begin": 0.0,
          "end": 6.473
        },
        {
          "text": "दध᳚त्स्तो॒त्रे सु॒वीर्य᳚म् ॥",
          "begin": 6.473,
          "end": 9.64
        }
      ]
    },
    {
      "id": "9.67.20",
      "mandala": 9,
      "sukta": 67,
      "rik": 20,
      "padas": [
        {
          "text": "ए॒ष तु॒न्नो अ॒भिष्टु᳚तः प॒वित्र॒मति᳚ गाहते ।",
          "begin": 0.0,
          "end": 4.966
        },
        {
          "text": "र॒क्षो॒हा वार᳚म॒व्यय᳚म् ॥",
          "begin": 4.966,
          "end": 7.52
        }
      ]
    },
    {
      "id": "9.67.21",
      "mandala": 9,
      "sukta": 67,
      "rik": 21,
      "padas": [
        {
          "text": "यदन्ति॒ यच्च᳚ दूर॒के भ॒यं वि॒न्दति॒ मामि॒ह ।",
          "begin": 0.0,
          "end": 4.689
        },
        {
          "text": "पव᳚मान॒ वि तज्ज᳚हि ॥",
          "begin": 4.689,
          "end": 6.72
        }
      ]
    },
    {
      "id": "9.67.22",
      "mandala": 9,
      "sukta": 67,
      "rik": 22,
      "padas": [
        {
          "text": "पव᳚मान॒ः सो अ॒द्य नः᳚ प॒वित्रे᳚ण॒ विच᳚र्षणिः ।",
          "begin": 0.0,
          "end": 5.449
        },
        {
          "text": "यः पो॒ता स पु᳚नातु नः ॥",
          "begin": 5.449,
          "end": 7.88
        }
      ]
    },
    {
      "id": "9.67.23",
      "mandala": 9,
      "sukta": 67,
      "rik": 23,
      "padas": [
        {
          "text": "यत्ते᳚ प॒वित्र᳚म॒र्चिष्यग्ने॒ वित᳚तम॒न्तरा ।",
          "begin": 0.0,
          "end": 5.135
        },
        {
          "text": "ब्रह्म॒ तेन᳚ पुनीहि नः ॥",
          "begin": 5.135,
          "end": 7.44
        }
      ]
    },
    {
      "id": "9.67.24",
      "mandala": 9,
      "sukta": 67,
      "rik": 24,
      "padas": [
        {
          "text": "यत्ते᳚ प॒वित्र᳚मर्चि॒वदग्ने॒ तेन᳚ पुनीहि नः ।",
          "begin": 0.0,
          "end": 2.754
        },
        {
          "text": "ब्र॒ह्म॒स॒वैः पु᳚नीहि नः ॥",
          "begin": 2.754,
          "end": 7.28
        }
      ]
    },
    {
      "id": "9.67.25",
      "mandala": 9,
      "sukta": 67,
      "rik": 25,
      "padas": [
        {
          "text": "उ॒भाभ्यां᳚ देव सवितः प॒वित्रे᳚ण स॒वेन᳚ च ।",
          "begin": 0.0,
          "end": 5.471
        },
        {
          "text": "मां पु᳚नीहि वि॒श्वतः᳚ ॥",
          "begin": 5.471,
          "end": 7.88
        }
      ]
    },
    {
      "id": "9.67.26",
      "mandala": 9,
      "sukta": 67,
      "rik": 26,
      "padas": [
        {
          "text": "त्रि॒भिष्ट्वं दे᳚व सवित॒र्वर्षि᳚ष्ठैः सोम॒ धाम᳚भिः ।",
          "begin": 0.0,
          "end": 5.96
        },
        {
          "text": "अग्ने॒ दक्षै᳚ः पुनीहि नः ॥",
          "begin": 5.96,
          "end": 8.36
        }
      ]
    },
    {
      "id": "9.67.27",
      "mandala": 9,
      "sukta": 67,
      "rik": 27,
      "padas": [
        {
          "text": "पु॒नन्तु॒ मां दे᳚वज॒नाः पु॒नन्तु॒ वस᳚वो धि॒या ।",
          "begin": 0.0,
          "end": 4.849
        },
        {
          "text": "विश्वे᳚ देवाः पुनी॒त मा॒ जात᳚वेदः पुनी॒हि मा᳚ ॥",
          "begin": 4.849,
          "end": 10.28
        }
      ]
    },
    {
      "id": "9.67.28",
      "mandala": 9,
      "sukta": 67,
      "rik": 28,
      "padas": [
        {
          "text": "प्र प्या᳚यस्व॒ प्र स्य᳚न्दस्व॒ सोम॒ विश्वे᳚भिरं॒शुभि᳚ः ।",
          "begin": 0.0,
          "end": 5.841
        },
        {
          "text": "दे॒वेभ्य᳚ उत्त॒मं ह॒विः ॥",
          "begin": 5.841,
          "end": 9.44
        }
      ]
    },
    {
      "id": "9.67.29",
      "mandala": 9,
      "sukta": 67,
      "rik": 29,
      "padas": [
        {
          "text": "उप᳚ प्रि॒यं पनि᳚प्नतं॒ युवा᳚नमाहुती॒वृध᳚म् ।",
          "begin": 0.0,
          "end": 3.865
        },
        {
          "text": "अग᳚न्म॒ बिभ्र᳚तो॒ नमः᳚ ॥",
          "begin": 3.865,
          "end": 8.12
        }
      ]
    },
    {
      "id": "9.67.30",
      "mandala": 9,
      "sukta": 67,
      "rik": 30,
      "padas": [
        {
          "text": "अ॒लाय्य᳚स्य पर॒शुर्न᳚नाश॒ तमा प᳚वस्व देव सोम ।",
          "begin": 0.0,
          "end": 4.544
        },
        {
          "text": "आ॒खुं चि᳚दे॒व दे᳚व सोम ॥",
          "begin": 4.544,
          "end": 8.44
        }
      ]
    },
    {
      "id": "9.67.31",
      "mandala": 9,
      "sukta": 67,
      "rik": 31,
      "padas": [
        {
          "text": "यः पा᳚वमा॒नीर॒ध्येत्यृषि᳚भि॒ः सम्भृ᳚तं॒ रस᳚म् ।",
          "begin": 0.0,
          "end": 5.726
        },
        {
          "text": "सर्वं॒ स पू॒तम᳚श्नाति स्वदि॒तं मा᳚त॒रिश्व᳚ना ॥",
          "begin": 5.726,
          "end": 10.08
        }
      ]
    },
    {
      "id": "9.67.32",
      "mandala": 9,
      "sukta": 67,
      "rik": 32,
      "padas": [
        {
          "text": "पा॒व॒मा॒नीर्यो अ॒ध्येत्यृषि᳚भि॒ः सम्भृ᳚तं॒ रस᳚म् ।",
          "begin": 0.0,
          "end": 6.465
        },
        {
          "text": "तस्मै॒ सर᳚स्वती दुहे क्षी॒रं स॒र्पिर्मधू᳚द॒कम् ॥",
          "begin": 6.465,
          "end": 14.2
        }
      ]
    }
  ]
