{
  "mandala": 9,
  "sukta": 35,
  "verses": [
    {
      "id": "9.35.1",
      "mandala": 9,
      "sukta": 35,
      "rik": 1,
      "padas": [
        {
          "text": "आ नः᳚ पवस्व॒ धार᳚या॒ पव᳚मान र॒यिं पृ॒थुम् ।",
          "begin": 0.0,
          "end": 6.566
        },
        {
          "text": "यया॒ ज्योति᳚र्वि॒दासि᳚ नः ॥",
          "begin": 6.566,
          "end": 9.6
        }
      ]
    },
    {
      "id": "9.35.2",
      "mandala": 9,
      "sukta": 35,
      "rik": 2,
      "padas": [
        {
          "text": "इन्दो᳚ समुद्रमीङ्खय॒ पव᳚स्व विश्वमेजय ।",
          "begin": 0.0,
          "end": 5.003
        },
        {
          "text": "रा॒यो ध॒र्ता न॒ ओज᳚सा ॥",
          "begin": 5.003,
          "end": 7.72
        }
      ]
    },
    {
      "id": "9.35.3",
      "mandala": 9,
      "sukta": 35,
      "rik": 3,
      "padas": [
        {
          "text": "त्वया᳚ वी॒रेण᳚ वीरवो॒॑भि ष्या᳚म पृतन्य॒तः ।",
          "begin": 0.0,
          "end": 5.658
        },
        {
          "text": "क्षरा᳚ णो अ॒भि वार्य᳚म् ॥",
          "begin": 5.658,
          "end": 8.84
        }
      ]
    },
    {
      "id": "9.35.4",
      "mandala": 9,
      "sukta": 35,
      "rik": 4,
      "padas": [
        {
          "text": "प्र वाज॒मिन्दु᳚रिष्यति॒ सिषा᳚सन्वाज॒सा ऋषि᳚ः ।",
          "begin": 0.0,
          "end": 4.811
        },
        {
          "text": "व्र॒ता वि᳚दा॒न आयु᳚धा ॥",
          "begin": 4.811,
          "end": 7.44
        }
      ]
    },
    {
      "id": "9.35.5",
      "mandala": 9,
      "sukta": 35,
      "rik": 5,
      "padas": [
        {
          "text": "तं गी॒र्भिर्वा᳚चमीङ्ख॒यं पु᳚ना॒नं वा᳚सयामसि ।",
          "begin": 0.0,
          "end": 5.998
        },
        {
          "text": "सोमं॒ जन᳚स्य॒ गोप᳚तिम् ॥",
          "begin": 5.998,
          "end": 8.48
        }
      ]
    },
    {
      "id": "9.35.6",
      "mandala": 9,
      "sukta": 35,
      "rik": 6,
      "padas": [
        {
          "text": "विश्वो॒ यस्य᳚ व्र॒ते जनो᳚ दा॒धार॒ धर्म᳚ण॒स्पते᳚ः ।",
          "begin": 0.0,
          "end": 6.912
        },
        {
          "text": "पु॒ना॒नस्य᳚ प्र॒भूव᳚सोः ॥",
          "begin": 6.912,
          "end": 11.52
        }
      ]
    }
  ]
}