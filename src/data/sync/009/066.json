{
  "mandala": 9,
  "sukta": 66,
  "verses": [
    {
      "id": "9.66.1",
      "mandala": 9,
      "sukta": 66,
      "rik": 1,
      "padas": [
        {
          "text": "पव᳚स्व विश्वचर्षणे॒॑भि विश्वा᳚नि॒ काव्या᳚ ।",
          "begin": 0.0,
          "end": 6.703
        },
        {
          "text": "सखा॒ सखि᳚भ्य॒ ईड्य᳚ः ॥",
          "begin": 6.703,
          "end": 11.64
        }
      ]
    },
    {
      "id": "9.66.2",
      "mandala": 9,
      "sukta": 66,
      "rik": 2,
      "padas": [
        {
          "text": "ताभ्यां॒ विश्व᳚स्य राजसि॒ ये प᳚वमान॒ धाम᳚नी ।",
          "begin": 0.0,
          "end": 5.003
        },
        {
          "text": "प्र॒ती॒ची सो᳚म त॒स्थतु᳚ः ॥",
          "begin": 5.003,
          "end": 7.84
        }
      ]
    },
    {
      "id": "9.66.3",
      "mandala": 9,
      "sukta": 66,
      "rik": 3,
      "padas": [
        {
          "text": "परि॒ धामा᳚नि॒ यानि᳚ ते॒ त्वं सो᳚मासि वि॒श्वतः᳚ ।",
          "begin": 0.0,
          "end": 5.485
        },
        {
          "text": "पव᳚मान ऋ॒तुभि᳚ः कवे ॥",
          "begin": 5.485,
          "end": 7.68
        }
      ]
    },
    {
      "id": "9.66.4",
      "mandala": 9,
      "sukta": 66,
      "rik": 4,
      "padas": [
        {
          "text": "पव᳚स्व ज॒नय॒न्निषो॒॑भि विश्वा᳚नि॒ वार्या᳚ ।",
          "begin": 0.0,
          "end": 4.993
        },
        {
          "text": "सखा॒ सखि᳚भ्य ऊ॒तये᳚ ॥",
          "begin": 4.993,
          "end": 7.36
        }
      ]
    },
    {
      "id": "9.66.5",
      "mandala": 9,
      "sukta": 66,
      "rik": 5,
      "padas": [
        {
          "text": "तव᳚ शु॒क्रासो᳚ अ॒र्चयो᳚ दि॒वस्पृ॒ष्ठे वि त᳚न्वते ।",
          "begin": 0.0,
          "end": 5.919
        },
        {
          "text": "प॒वित्रं᳚ सोम॒ धाम᳚भिः ॥",
          "begin": 5.919,
          "end": 8.72
        }
      ]
    },
    {
      "id": "9.66.6",
      "mandala": 9,
      "sukta": 66,
      "rik": 6,
      "padas": [
        {
          "text": "तवे॒मे स॒प्त सिन्ध᳚वः प्र॒शिषं᳚ सोम सिस्रते ।",
          "begin": 0.0,
          "end": 5.579
        },
        {
          "text": "तुभ्यं᳚ धावन्ति धे॒नवः᳚ ॥",
          "begin": 5.579,
          "end": 8.36
        }
      ]
    },
    {
      "id": "9.66.7",
      "mandala": 9,
      "sukta": 66,
      "rik": 7,
      "padas": [
        {
          "text": "प्र सो᳚म याहि॒ धार᳚या सु॒त इन्द्रा᳚य मत्स॒रः ।",
          "begin": 0.0,
          "end": 5.615
        },
        {
          "text": "दधा᳚नो॒ अक्षि᳚ति॒ श्रवः᳚ ॥",
          "begin": 5.615,
          "end": 8.72
        }
      ]
    },
    {
      "id": "9.66.8",
      "mandala": 9,
      "sukta": 66,
      "rik": 8,
      "padas": [
        {
          "text": "समु᳚ त्वा धी॒भिर᳚स्वरन्हिन्व॒तीः स॒प्त जा॒मयः᳚ ।",
          "begin": 0.0,
          "end": 5.298
        },
        {
          "text": "विप्र᳚मा॒जा वि॒वस्व᳚तः ॥",
          "begin": 5.298,
          "end": 8.0
        }
      ]
    },
    {
      "id": "9.66.9",
      "mandala": 9,
      "sukta": 66,
      "rik": 9,
      "padas": [
        {
          "text": "मृ॒जन्ति᳚ त्वा॒ सम॒ग्रुवो॑व्ये᳚ जी॒रावधि॒ ष्वणि᳚ ।",
          "begin": 0.0,
          "end": 4.966
        },
        {
          "text": "रे॒भो यद॒ज्यसे॒ वने᳚ ॥",
          "begin": 4.966,
          "end": 8.04
        }
      ]
    },
    {
      "id": "9.66.10",
      "mandala": 9,
      "sukta": 66,
      "rik": 10,
      "padas": [
        {
          "text": "पव᳚मानस्य ते कवे॒ वाजि॒न्त्सर्गा᳚ असृक्षत ।",
          "begin": 0.0,
          "end": 5.038
        },
        {
          "text": "अर्व᳚न्तो॒ न श्र᳚व॒स्यवः᳚ ॥",
          "begin": 5.038,
          "end": 8.68
        }
      ]
    },
    {
      "id": "9.66.11",
      "mandala": 9,
      "sukta": 66,
      "rik": 11,
      "padas": [
        {
          "text": "अच्छा॒ कोशं᳚ मधु॒श्चुत॒मसृ᳚ग्रं॒ वारे᳚ अ॒व्यये᳚ ।",
          "begin": 0.0,
          "end": 6.205
        },
        {
          "text": "अवा᳚वशन्त धी॒तयः᳚ ॥",
          "begin": 6.205,
          "end": 8.88
        }
      ]
    },
    {
      "id": "9.66.12",
      "mandala": 9,
      "sukta": 66,
      "rik": 12,
      "padas": [
        {
          "text": "अच्छा᳚ समु॒द्रमिन्द॒वो॑स्तं॒ गावो॒ न धे॒नवः᳚ ।",
          "begin": 0.0,
          "end": 5.294
        },
        {
          "text": "अग्म᳚न्नृ॒तस्य॒ योनि॒मा ॥",
          "begin": 5.294,
          "end": 8.2
        }
      ]
    },
    {
      "id": "9.66.13",
      "mandala": 9,
      "sukta": 66,
      "rik": 13,
      "padas": [
        {
          "text": "प्र ण᳚ इन्दो म॒हे रण॒ आपो᳚ अर्षन्ति॒ सिन्ध᳚वः ।",
          "begin": 0.0,
          "end": 5.529
        },
        {
          "text": "यद्गोभि᳚र्वासयि॒ष्यसे᳚ ॥",
          "begin": 5.529,
          "end": 8.64
        }
      ]
    },
    {
      "id": "9.66.14",
      "mandala": 9,
      "sukta": 66,
      "rik": 14,
      "padas": [
        {
          "text": "अस्य᳚ ते स॒ख्ये व॒यमिय᳚क्षन्त॒स्त्वोत᳚यः ।",
          "begin": 0.0,
          "end": 4.702
        },
        {
          "text": "इन्दो᳚ सखि॒त्वमु᳚श्मसि ॥",
          "begin": 4.702,
          "end": 7.36
        }
      ]
    },
    {
      "id": "9.66.15",
      "mandala": 9,
      "sukta": 66,
      "rik": 15,
      "padas": [
        {
          "text": "आ प᳚वस्व॒ गवि᳚ष्टये म॒हे सो᳚म नृ॒चक्ष᳚से ।",
          "begin": 0.0,
          "end": 6.056
        },
        {
          "text": "एन्द्र᳚स्य ज॒ठरे᳚ विश ॥",
          "begin": 6.056,
          "end": 7.88
        }
      ]
    },
    {
      "id": "9.66.16",
      "mandala": 9,
      "sukta": 66,
      "rik": 16,
      "padas": [
        {
          "text": "म॒हाँ अ᳚सि सोम॒ ज्येष्ठ᳚ उ॒ग्राणा᳚मिन्द॒ ओजि᳚ष्ठः ।",
          "begin": 0.0,
          "end": 6.099
        },
        {
          "text": "युध्वा॒ सञ्छश्व᳚ज्जिगेथ ॥",
          "begin": 6.099,
          "end": 8.88
        }
      ]
    },
    {
      "id": "9.66.17",
      "mandala": 9,
      "sukta": 66,
      "rik": 17,
      "padas": [
        {
          "text": "य उ॒ग्रेभ्य᳚श्चि॒दोजी᳚या॒ञ्छूरे᳚भ्यश्चि॒च्छूर᳚तरः ।",
          "begin": 0.0,
          "end": 6.679
        },
        {
          "text": "भू॒रि॒दाभ्य᳚श्चि॒न्मंही᳚यान् ॥",
          "begin": 6.679,
          "end": 10.08
        }
      ]
    },
    {
      "id": "9.66.18",
      "mandala": 9,
      "sukta": 66,
      "rik": 18,
      "padas": [
        {
          "text": "त्वं सो᳚म॒ सूर॒ एष᳚स्तो॒कस्य᳚ सा॒ता त॒नूना᳚म् ।",
          "begin": 0.0,
          "end": 5.158
        },
        {
          "text": "वृ॒णी॒महे᳚ स॒ख्याय᳚ वृणी॒महे॒ युज्या᳚य ॥",
          "begin": 5.158,
          "end": 9.76
        }
      ]
    },
    {
      "id": "9.66.19",
      "mandala": 9,
      "sukta": 66,
      "rik": 19,
      "padas": [
        {
          "text": "अग्न॒ आयूं᳚षि पवस॒ आ सु॒वोर्ज॒मिषं᳚ च नः ।",
          "begin": 0.0,
          "end": 5.134
        },
        {
          "text": "आ॒रे बा᳚धस्व दु॒च्छुना᳚म् ॥",
          "begin": 5.134,
          "end": 8.48
        }
      ]
    },
    {
      "id": "9.66.20",
      "mandala": 9,
      "sukta": 66,
      "rik": 20,
      "padas": [
        {
          "text": "अ॒ग्निरृषि॒ः पव᳚मान॒ः पाञ्च᳚जन्यः पु॒रोहि᳚तः ।",
          "begin": 0.0,
          "end": 5.163
        },
        {
          "text": "तमी᳚महे महाग॒यम् ॥",
          "begin": 5.163,
          "end": 7.64
        }
      ]
    },
    {
      "id": "9.66.21",
      "mandala": 9,
      "sukta": 66,
      "rik": 21,
      "padas": [
        {
          "text": "अग्ने॒ पव᳚स्व॒ स्वपा᳚ अ॒स्मे वर्च᳚ः सु॒वीर्य᳚म् ।",
          "begin": 0.0,
          "end": 5.66
        },
        {
          "text": "दध᳚द्र॒यिं मयि॒ पोष᳚म् ॥",
          "begin": 5.66,
          "end": 8.16
        }
      ]
    },
    {
      "id": "9.66.22",
      "mandala": 9,
      "sukta": 66,
      "rik": 22,
      "padas": [
        {
          "text": "पव᳚मानो॒ अति॒ स्रिधो॒॑भ्य᳚र्षति सुष्टु॒तिम् ।",
          "begin": 0.0,
          "end": 4.671
        },
        {
          "text": "सूरो॒ न वि॒श्वद᳚र्शतः ॥",
          "begin": 4.671,
          "end": 6.96
        }
      ]
    },
    {
      "id": "9.66.23",
      "mandala": 9,
      "sukta": 66,
      "rik": 23,
      "padas": [
        {
          "text": "स म᳚र्मृजा॒न आ॒युभि॒ः प्रय᳚स्वा॒न्प्रय᳚से हि॒तः ।",
          "begin": 0.0,
          "end": 5.445
        },
        {
          "text": "इन्दु॒रत्यो᳚ विचक्ष॒णः ॥",
          "begin": 5.445,
          "end": 8.0
        }
      ]
    },
    {
      "id": "9.66.24",
      "mandala": 9,
      "sukta": 66,
      "rik": 24,
      "padas": [
        {
          "text": "पव᳚मान ऋ॒तं बृ॒हच्छु॒क्रं ज्योति᳚रजीजनत् ।",
          "begin": 0.0,
          "end": 4.484
        },
        {
          "text": "कृ॒ष्णा तमां᳚सि॒ जङ्घ᳚नत् ॥",
          "begin": 4.484,
          "end": 7.36
        }
      ]
    },
    {
      "id": "9.66.25",
      "mandala": 9,
      "sukta": 66,
      "rik": 25,
      "padas": [
        {
          "text": "पव᳚मानस्य॒ जङ्घ्न᳚तो॒ हरे᳚श्च॒न्द्रा अ᳚सृक्षत ।",
          "begin": 0.0,
          "end": 5.768
        },
        {
          "text": "जी॒रा अ᳚जि॒रशो᳚चिषः ॥",
          "begin": 5.768,
          "end": 8.36
        }
      ]
    },
    {
      "id": "9.66.26",
      "mandala": 9,
      "sukta": 66,
      "rik": 26,
      "padas": [
        {
          "text": "पव᳚मानो र॒थीत᳚मः शु॒भ्रेभि᳚ः शु॒भ्रश᳚स्तमः ।",
          "begin": 0.0,
          "end": 4.864
        },
        {
          "text": "हरि᳚श्चन्द्रो म॒रुद्ग᳚णः ॥",
          "begin": 4.864,
          "end": 7.4
        }
      ]
    },
    {
      "id": "9.66.27",
      "mandala": 9,
      "sukta": 66,
      "rik": 27,
      "padas": [
        {
          "text": "पव᳚मानो॒ व्य᳚श्नवद्र॒श्मिभि᳚र्वाज॒सात᳚मः ।",
          "begin": 0.0,
          "end": 5.412
        },
        {
          "text": "दध᳚त्स्तो॒त्रे सु॒वीर्य᳚म् ॥",
          "begin": 5.412,
          "end": 8.64
        }
      ]
    },
    {
      "id": "9.66.28",
      "mandala": 9,
      "sukta": 66,
      "rik": 28,
      "padas": [
        {
          "text": "प्र सु᳚वा॒न इन्दु᳚रक्षाः प॒वित्र॒मत्य॒व्यय᳚म् ।",
          "begin": 0.0,
          "end": 5.023
        },
        {
          "text": "पु॒ना॒न इन्दु॒रिन्द्र॒मा ॥",
          "begin": 5.023,
          "end": 7.48
        }
      ]
    },
    {
      "id": "9.66.29",
      "mandala": 9,
      "sukta": 66,
      "rik": 29,
      "padas": [
        {
          "text": "ए॒ष सोमो॒ अधि᳚ त्व॒चि गवां᳚ क्रीळ॒त्यद्रि᳚भिः ।",
          "begin": 0.0,
          "end": 5.68
        },
        {
          "text": "इन्द्रं॒ मदा᳚य॒ जोहु᳚वत् ॥",
          "begin": 5.68,
          "end": 8.36
        }
      ]
    },
    {
      "id": "9.66.30",
      "mandala": 9,
      "sukta": 66,
      "rik": 30,
      "padas": [
        {
          "text": "यस्य᳚ ते द्यु॒म्नव॒त्पय॒ः पव᳚मा॒नाभृ᳚तं दि॒वः ।",
          "begin": 0.0,
          "end": 8.495
        },
        {
          "text": "तेन᳚ नो मृळ जी॒वसे᳚ ॥",
          "begin": 8.495,
          "end": 11.28
        }
      ]
    }
  ]
}