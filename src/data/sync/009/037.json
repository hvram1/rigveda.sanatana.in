{
  "mandala": 9,
  "sukta": 37,
  "verses": [
    {
      "id": "9.37.1",
      "mandala": 9,
      "sukta": 37,
      "rik": 1,
      "padas": [
        {
          "text": "स सु॒तः पी॒तये॒ वृषा॒ सोमः᳚ प॒वित्रे᳚ अर्षति ।",
          "begin": 0.0,
          "end": 6.368
        },
        {
          "text": "वि॒घ्नन्रक्षां᳚सि देव॒युः ॥",
          "begin": 6.368,
          "end": 9.76
        }
      ]
    },
    {
      "id": "9.37.2",
      "mandala": 9,
      "sukta": 37,
      "rik": 2,
      "padas": [
        {
          "text": "स प॒वित्रे᳚ विचक्ष॒णो हरि᳚रर्षति धर्ण॒सिः ।",
          "begin": 0.0,
          "end": 5.062
        },
        {
          "text": "अ॒भि योनिं॒ कनि᳚क्रदत् ॥",
          "begin": 5.062,
          "end": 7.2
        }
      ]
    },
    {
      "id": "9.37.3",
      "mandala": 9,
      "sukta": 37,
      "rik": 3,
      "padas": [
        {
          "text": "स वा॒जी रो᳚च॒ना दि॒वः पव᳚मानो॒ वि धा᳚वति ।",
          "begin": 0.0,
          "end": 5.512
        },
        {
          "text": "र॒क्षो॒हा वार᳚म॒व्यय᳚म् ॥",
          "begin": 5.512,
          "end": 8.16
        }
      ]
    },
    {
      "id": "9.37.4",
      "mandala": 9,
      "sukta": 37,
      "rik": 4,
      "padas": [
        {
          "text": "स त्रि॒तस्याधि॒ सान᳚वि॒ पव᳚मानो अरोचयत् ।",
          "begin": 0.0,
          "end": 4.842
        },
        {
          "text": "जा॒मिभि॒ः सूर्यं᳚ स॒ह ॥",
          "begin": 4.842,
          "end": 7.36
        }
      ]
    },
    {
      "id": "9.37.5",
      "mandala": 9,
      "sukta": 37,
      "rik": 5,
      "padas": [
        {
          "text": "स वृ᳚त्र॒हा वृषा᳚ सु॒तो व᳚रिवो॒विददा᳚भ्यः ।",
          "begin": 0.0,
          "end": 5.233
        },
        {
          "text": "सोमो॒ वाज᳚मिवासरत् ॥",
          "begin": 5.233,
          "end": 7.68
        }
      ]
    },
    {
      "id": "9.37.6",
      "mandala": 9,
      "sukta": 37,
      "rik": 6,
      "padas": [
        {
          "text": "स दे॒वः क॒विने᳚षि॒तो॒३॒᳚॑भि द्रोणा᳚नि धावति ।",
          "begin": 0.0,
          "end": 6.982
        },
        {
          "text": "इन्दु॒रिन्द्रा᳚य मं॒हना᳚ ॥",
          "begin": 6.982,
          "end": 11.64
        }
      ]
    }
  ]
}