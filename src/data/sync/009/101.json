{
  "mandala": 9,
  "sukta": 101,
  "verses": [
    {
      "id": "9.101.1",
      "mandala": 9,
      "sukta": 101,
      "rik": 1,
      "padas": [
        {
          "text": "पु॒रोजि᳚ती वो॒ अन्ध᳚सः सु॒ताय᳚ मादयि॒त्नवे᳚ ।",
          "begin": 0.0,
          "end": 5.718
        },
        {
          "text": "अप॒ श्वानं᳚ श्नथिष्टन॒ सखा᳚यो दीर्घजि॒ह्व्य᳚म् ॥",
          "begin": 5.718,
          "end": 10.88
        }
      ]
    },
    {
      "id": "9.101.2",
      "mandala": 9,
      "sukta": 101,
      "rik": 2,
      "padas": [
        {
          "text": "यो धार᳚या पाव॒कया᳚ परिप्र॒स्यन्द᳚ते सु॒तः ।",
          "begin": 0.0,
          "end": 5.894
        },
        {
          "text": "इन्दु॒रश्वो॒ न कृत्व्य᳚ः ॥",
          "begin": 5.894,
          "end": 8.44
        }
      ]
    },
    {
      "id": "9.101.3",
      "mandala": 9,
      "sukta": 101,
      "rik": 3,
      "padas": [
        {
          "text": "तं दु॒रोष᳚म॒भी नर॒ः सोमं᳚ वि॒श्वाच्या᳚ धि॒या ।",
          "begin": 0.0,
          "end": 6.098
        },
        {
          "text": "य॒ज्ञं हि᳚न्व॒न्त्यद्रि᳚भिः ॥",
          "begin": 6.098,
          "end": 9.0
        }
      ]
    },
    {
      "id": "9.101.4",
      "mandala": 9,
      "sukta": 101,
      "rik": 4,
      "padas": [
        {
          "text": "सु॒तासो॒ मधु᳚मत्तमा॒ः सोमा॒ इन्द्रा᳚य म॒न्दिनः᳚ ।",
          "begin": 0.0,
          "end": 6.123
        },
        {
          "text": "प॒वित्र᳚वन्तो अक्षरन्दे॒वान्ग᳚च्छन्तु वो॒ मदा᳚ः ॥",
          "begin": 6.123,
          "end": 11.32
        }
      ]
    },
    {
      "id": "9.101.5",
      "mandala": 9,
      "sukta": 101,
      "rik": 5,
      "padas": [
        {
          "text": "इन्दु॒रिन्द्रा᳚य पवत॒ इति᳚ दे॒वासो᳚ अब्रुवन् ।",
          "begin": 0.0,
          "end": 5.947
        },
        {
          "text": "वा॒चस्पति᳚र्मखस्यते॒ विश्व॒स्येशा᳚न॒ ओज᳚सा ॥",
          "begin": 5.947,
          "end": 10.84
        }
      ]
    },
    {
      "id": "9.101.6",
      "mandala": 9,
      "sukta": 101,
      "rik": 6,
      "padas": [
        {
          "text": "स॒हस्र᳚धारः पवते समु॒द्रो वा᳚चमीङ्ख॒यः ।",
          "begin": 0.0,
          "end": 5.704
        },
        {
          "text": "सोम॒ः पती᳚ रयी॒णां सखेन्द्र᳚स्य दि॒वेदि᳚वे ॥",
          "begin": 5.704,
          "end": 10.64
        }
      ]
    },
    {
      "id": "9.101.7",
      "mandala": 9,
      "sukta": 101,
      "rik": 7,
      "padas": [
        {
          "text": "अ॒यं पू॒षा र॒यिर्भग॒ः सोमः᳚ पुना॒नो अ᳚र्षति ।",
          "begin": 0.0,
          "end": 5.238
        },
        {
          "text": "पति॒र्विश्व᳚स्य॒ भूम᳚नो॒ व्य᳚ख्य॒द्रोद᳚सी उ॒भे ॥",
          "begin": 5.238,
          "end": 10.6
        }
      ]
    },
    {
      "id": "9.101.8",
      "mandala": 9,
      "sukta": 101,
      "rik": 8,
      "padas": [
        {
          "text": "समु᳚ प्रि॒या अ᳚नूषत॒ गावो॒ मदा᳚य॒ घृष्व᳚यः ।",
          "begin": 0.0,
          "end": 6.03
        },
        {
          "text": "सोमा᳚सः कृण्वते प॒थः पव᳚मानास॒ इन्द᳚वः ॥",
          "begin": 6.03,
          "end": 10.84
        }
      ]
    },
    {
      "id": "9.101.9",
      "mandala": 9,
      "sukta": 101,
      "rik": 9,
      "padas": [
        {
          "text": "य ओजि᳚ष्ठ॒स्तमा भ᳚र॒ पव᳚मान श्र॒वाय्य᳚म् ।",
          "begin": 0.0,
          "end": 5.274
        },
        {
          "text": "यः पञ्च᳚ चर्ष॒णीर॒भि र॒यिं येन॒ वना᳚महै ॥",
          "begin": 5.274,
          "end": 9.76
        }
      ]
    },
    {
      "id": "9.101.10",
      "mandala": 9,
      "sukta": 101,
      "rik": 10,
      "padas": [
        {
          "text": "सोमा᳚ः पवन्त॒ इन्द᳚वो॒॑स्मभ्यं᳚ गातु॒वित्त᳚माः ।",
          "begin": 0.0,
          "end": 4.06
        },
        {
          "text": "मि॒त्राः सु᳚वा॒ना अ᳚रे॒पसः᳚ स्वा॒ध्य᳚ः स्व॒र्विदः᳚ ॥",
          "begin": 4.06,
          "end": 11.52
        }
      ]
    },
    {
      "id": "9.101.11",
      "mandala": 9,
      "sukta": 101,
      "rik": 11,
      "padas": [
        {
          "text": "सु॒ष्वा॒णासो॒ व्यद्रि᳚भि॒श्चिता᳚ना॒ गोरधि᳚ त्व॒चि ।",
          "begin": 0.0,
          "end": 6.006
        },
        {
          "text": "इष᳚म॒स्मभ्य᳚म॒भित॒ः सम᳚स्वरन्वसु॒विदः᳚ ॥",
          "begin": 6.006,
          "end": 9.84
        }
      ]
    },
    {
      "id": "9.101.12",
      "mandala": 9,
      "sukta": 101,
      "rik": 12,
      "padas": [
        {
          "text": "ए॒ते पू॒ता वि᳚प॒श्चित॒ः सोमा᳚सो॒ दध्या᳚शिरः ।",
          "begin": 0.0,
          "end": 6.262
        },
        {
          "text": "सूर्या᳚सो॒ न द᳚र्श॒तासो᳚ जिग॒त्नवो᳚ ध्रु॒वा घृ॒ते ॥",
          "begin": 6.262,
          "end": 11.88
        }
      ]
    },
    {
      "id": "9.101.13",
      "mandala": 9,
      "sukta": 101,
      "rik": 13,
      "padas": [
        {
          "text": "प्र सु᳚न्वा॒नस्यान्ध᳚सो॒ मर्तो॒ न वृ᳚त॒ तद्वचः᳚ ।",
          "begin": 0.0,
          "end": 4.585
        },
        {
          "text": "अप॒ श्वान᳚मरा॒धसं᳚ ह॒ता म॒खं न भृग᳚वः ॥",
          "begin": 4.585,
          "end": 10.24
        }
      ]
    },
    {
      "id": "9.101.14",
      "mandala": 9,
      "sukta": 101,
      "rik": 14,
      "padas": [
        {
          "text": "आ जा॒मिरत्के᳚ अव्यत भु॒जे न पु॒त्र ओ॒ण्यो᳚ः ।",
          "begin": 0.0,
          "end": 6.504
        },
        {
          "text": "सर᳚ज्जा॒रो न योष᳚णां व॒रो न योनि᳚मा॒सद᳚म् ॥",
          "begin": 6.504,
          "end": 11.32
        }
      ]
    },
    {
      "id": "9.101.15",
      "mandala": 9,
      "sukta": 101,
      "rik": 15,
      "padas": [
        {
          "text": "स वी॒रो द᳚क्ष॒साध᳚नो॒ वि यस्त॒स्तम्भ॒ रोद᳚सी ।",
          "begin": 0.0,
          "end": 4.887
        },
        {
          "text": "हरि᳚ः प॒वित्रे᳚ अव्यत वे॒धा न योनि᳚मा॒सद᳚म् ॥",
          "begin": 4.887,
          "end": 10.4
        }
      ]
    },
    {
      "id": "9.101.16",
      "mandala": 9,
      "sukta": 101,
      "rik": 16,
      "padas": [
        {
          "text": "अव्यो॒ वारे᳚भिः पवते॒ सोमो॒ गव्ये॒ अधि᳚ त्व॒चि ।",
          "begin": 0.0,
          "end": 6.304
        },
        {
          "text": "कनि᳚क्रद॒द्वृषा॒ हरि॒रिन्द्र᳚स्या॒भ्ये᳚ति निष्कृ॒तम् ॥",
          "begin": 6.304,
          "end": 16.16
        }
      ]
    }
  ]
}