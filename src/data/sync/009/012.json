{
  "mandala": 9,
  "sukta": 12,
  "verses": [
    {
      "id": "9.12.1",
      "mandala": 9,
      "sukta": 12,
      "rik": 1,
      "padas": [
        {
          "text": "सोमा᳚ असृग्र॒मिन्द᳚वः सु॒ता ऋ॒तस्य॒ साद᳚ने ।",
          "begin": 0.0,
          "end": 7.061
        },
        {
          "text": "इन्द्रा᳚य॒ मधु᳚मत्तमाः ॥",
          "begin": 7.061,
          "end": 10.24
        }
      ]
    },
    {
      "id": "9.12.2",
      "mandala": 9,
      "sukta": 12,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒भि विप्रा᳚ अनूषत॒ गावो᳚ व॒त्सं न मा॒तरः᳚ ।",
          "begin": 0.0,
          "end": 6.069
        },
        {
          "text": "इन्द्रं॒ सोम᳚स्य पी॒तये᳚ ॥",
          "begin": 6.069,
          "end": 8.92
        }
      ]
    },
    {
      "id": "9.12.3",
      "mandala": 9,
      "sukta": 12,
      "rik": 3,
      "padas": [
        {
          "text": "म॒द॒च्युत्क्षे᳚ति॒ साद᳚ने॒ सिन्धो᳚रू॒र्मा वि᳚प॒श्चित् ।",
          "begin": 0.0,
          "end": 6.532
        },
        {
          "text": "सोमो᳚ गौ॒री अधि᳚ श्रि॒तः ॥",
          "begin": 6.532,
          "end": 9.84
        }
      ]
    },
    {
      "id": "9.12.4",
      "mandala": 9,
      "sukta": 12,
      "rik": 4,
      "padas": [
        {
          "text": "दि॒वो नाभा᳚ विचक्ष॒णो॑व्यो॒ वारे᳚ महीयते ।",
          "begin": 0.0,
          "end": 6.147
        },
        {
          "text": "सोमो॒ यः सु॒क्रतु᳚ः क॒विः ॥",
          "begin": 6.147,
          "end": 8.96
        }
      ]
    },
    {
      "id": "9.12.5",
      "mandala": 9,
      "sukta": 12,
      "rik": 5,
      "padas": [
        {
          "text": "यः सोमः᳚ क॒लशे॒ष्वाँ अ॒न्तः प॒वित्र॒ आहि᳚तः ।",
          "begin": 0.0,
          "end": 6.138
        },
        {
          "text": "तमिन्दु॒ः परि᳚ षस्वजे ॥",
          "begin": 6.138,
          "end": 8.56
        }
      ]
    },
    {
      "id": "9.12.6",
      "mandala": 9,
      "sukta": 12,
      "rik": 6,
      "padas": [
        {
          "text": "प्र वाच॒मिन्दु᳚रिष्यति समु॒द्रस्याधि᳚ वि॒ष्टपि᳚ ।",
          "begin": 0.0,
          "end": 4.98
        },
        {
          "text": "जिन्व॒न्कोशं᳚ मधु॒श्चुत᳚म् ॥",
          "begin": 4.98,
          "end": 8.44
        }
      ]
    },
    {
      "id": "9.12.7",
      "mandala": 9,
      "sukta": 12,
      "rik": 7,
      "padas": [
        {
          "text": "नित्य᳚स्तोत्रो॒ वन॒स्पति᳚र्धी॒नाम॒न्तः स᳚ब॒र्दुघः᳚ ।",
          "begin": 0.0,
          "end": 5.984
        },
        {
          "text": "हि॒न्वा॒नो मानु᳚षा यु॒गा ॥",
          "begin": 5.984,
          "end": 8.64
        }
      ]
    },
    {
      "id": "9.12.8",
      "mandala": 9,
      "sukta": 12,
      "rik": 8,
      "padas": [
        {
          "text": "अ॒भि प्रि॒या दि॒वस्प॒दा सोमो᳚ हिन्वा॒नो अ᳚र्षति ।",
          "begin": 0.0,
          "end": 6.253
        },
        {
          "text": "विप्र᳚स्य॒ धार᳚या क॒विः ॥",
          "begin": 6.253,
          "end": 8.76
        }
      ]
    },
    {
      "id": "9.12.9",
      "mandala": 9,
      "sukta": 12,
      "rik": 9,
      "padas": [
        {
          "text": "आ प᳚वमान धारय र॒यिं स॒हस्र᳚वर्चसम् ।",
          "begin": 0.0,
          "end": 5.284
        },
        {
          "text": "अ॒स्मे इ᳚न्दो स्वा॒भुव᳚म् ॥",
          "begin": 5.284,
          "end": 9.72
        }
      ]
    }
  ]
}