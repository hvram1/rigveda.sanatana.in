{
  "mandala": 9,
  "sukta": 44,
  "verses": [
    {
      "id": "9.44.1",
      "mandala": 9,
      "sukta": 44,
      "rik": 1,
      "padas": [
        {
          "text": "प्र ण᳚ इन्दो म॒हे तन᳚ ऊ॒र्मिं न बिभ्र᳚दर्षसि ।",
          "begin": 0.0,
          "end": 4.96
        },
        {
          "text": "अ॒भि दे॒वाँ अ॒यास्य᳚ः ॥",
          "begin": 4.96,
          "end": 7.88
        }
      ]
    },
    {
      "id": "9.44.2",
      "mandala": 9,
      "sukta": 44,
      "rik": 2,
      "padas": [
        {
          "text": "म॒ती जु॒ष्टो धि॒या हि॒तः सोमो᳚ हिन्वे परा॒वति᳚ ।",
          "begin": 0.0,
          "end": 5.42
        },
        {
          "text": "विप्र᳚स्य॒ धार᳚या क॒विः ॥",
          "begin": 5.42,
          "end": 7.84
        }
      ]
    },
    {
      "id": "9.44.3",
      "mandala": 9,
      "sukta": 44,
      "rik": 3,
      "padas": [
        {
          "text": "अ॒यं दे॒वेषु॒ जागृ᳚विः सु॒त ए᳚ति प॒वित्र॒ आ ।",
          "begin": 0.0,
          "end": 5.204
        },
        {
          "text": "सोमो᳚ याति॒ विच᳚र्षणिः ॥",
          "begin": 5.204,
          "end": 8.12
        }
      ]
    },
    {
      "id": "9.44.4",
      "mandala": 9,
      "sukta": 44,
      "rik": 4,
      "padas": [
        {
          "text": "स नः᳚ पवस्व वाज॒युश्च᳚क्रा॒णश्चारु᳚मध्व॒रम् ।",
          "begin": 0.0,
          "end": 4.957
        },
        {
          "text": "ब॒र्हिष्माँ॒ आ वि᳚वासति ॥",
          "begin": 4.957,
          "end": 7.64
        }
      ]
    },
    {
      "id": "9.44.5",
      "mandala": 9,
      "sukta": 44,
      "rik": 5,
      "padas": [
        {
          "text": "स नो॒ भगा᳚य वा॒यवे॒ विप्र᳚वीरः स॒दावृ᳚धः ।",
          "begin": 0.0,
          "end": 5.239
        },
        {
          "text": "सोमो᳚ दे॒वेष्वा य᳚मत् ॥",
          "begin": 5.239,
          "end": 8.36
        }
      ]
    },
    {
      "id": "9.44.6",
      "mandala": 9,
      "sukta": 44,
      "rik": 6,
      "padas": [
        {
          "text": "स नो᳚ अ॒द्य वसु᳚त्तये क्रतु॒विद्गा᳚तु॒वित्त᳚मः ।",
          "begin": 0.0,
          "end": 6.135
        },
        {
          "text": "वाजं᳚ जेषि॒ श्रवो᳚ बृ॒हत् ॥",
          "begin": 6.135,
          "end": 10.72
        }
      ]
    }
  ]
}