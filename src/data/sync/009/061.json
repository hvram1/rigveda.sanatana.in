{
  "mandala": 9,
  "sukta": 61,
  "verses": [
    {
      "id": "9.61.1",
      "mandala": 9,
      "sukta": 61,
      "rik": 1,
      "padas": [
        {
          "text": "अ॒या वी॒ती परि᳚ स्रव॒ यस्त᳚ इन्दो॒ मदे॒ष्वा ।",
          "begin": 0.0,
          "end": 6.407
        },
        {
          "text": "अ॒वाह᳚न्नव॒तीर्नव᳚ ॥",
          "begin": 6.407,
          "end": 8.96
        }
      ]
    },
    {
      "id": "9.61.2",
      "mandala": 9,
      "sukta": 61,
      "rik": 2,
      "padas": [
        {
          "text": "पुरः᳚ स॒द्य इ॒त्थाधि᳚ये॒ दिवो᳚दासाय॒ शम्ब᳚रम् ।",
          "begin": 0.0,
          "end": 4.677
        },
        {
          "text": "अध॒ त्यं तु॒र्वशं॒ यदु᳚म् ॥",
          "begin": 4.677,
          "end": 7.44
        }
      ]
    },
    {
      "id": "9.61.3",
      "mandala": 9,
      "sukta": 61,
      "rik": 3,
      "padas": [
        {
          "text": "परि᳚ णो॒ अश्व᳚मश्व॒विद्गोम᳚दिन्दो॒ हिर᳚ण्यवत् ।",
          "begin": 0.0,
          "end": 5.058
        },
        {
          "text": "क्षरा᳚ सह॒स्रिणी॒रिषः᳚ ॥",
          "begin": 5.058,
          "end": 7.68
        }
      ]
    },
    {
      "id": "9.61.4",
      "mandala": 9,
      "sukta": 61,
      "rik": 4,
      "padas": [
        {
          "text": "पव᳚मानस्य ते व॒यं प॒वित्र᳚मभ्युन्द॒तः ।",
          "begin": 0.0,
          "end": 4.136
        },
        {
          "text": "स॒खि॒त्वमा वृ᳚णीमहे ॥",
          "begin": 4.136,
          "end": 6.2
        }
      ]
    },
    {
      "id": "9.61.5",
      "mandala": 9,
      "sukta": 61,
      "rik": 5,
      "padas": [
        {
          "text": "ये ते᳚ प॒वित्र᳚मू॒र्मयो᳚॑भि॒क्षर᳚न्ति॒ धार᳚या ।",
          "begin": 0.0,
          "end": 5.847
        },
        {
          "text": "तेभि᳚र्नः सोम मृळय ॥",
          "begin": 5.847,
          "end": 7.8
        }
      ]
    },
    {
      "id": "9.61.6",
      "mandala": 9,
      "sukta": 61,
      "rik": 6,
      "padas": [
        {
          "text": "स नः᳚ पुना॒न आ भ᳚र र॒यिं वी॒रव᳚ती॒मिष᳚म् ।",
          "begin": 0.0,
          "end": 4.545
        },
        {
          "text": "ईशा᳚नः सोम वि॒श्वतः᳚ ॥",
          "begin": 4.545,
          "end": 7.2
        }
      ]
    },
    {
      "id": "9.61.7",
      "mandala": 9,
      "sukta": 61,
      "rik": 7,
      "padas": [
        {
          "text": "ए॒तमु॒ त्यं दश॒ क्षिपो᳚ मृ॒जन्ति॒ सिन्धु᳚मातरम् ।",
          "begin": 0.0,
          "end": 4.767
        },
        {
          "text": "समा᳚दि॒त्येभि᳚रख्यत ॥",
          "begin": 4.767,
          "end": 7.24
        }
      ]
    },
    {
      "id": "9.61.8",
      "mandala": 9,
      "sukta": 61,
      "rik": 8,
      "padas": [
        {
          "text": "समिन्द्रे᳚णो॒त वा॒युना᳚ सु॒त ए᳚ति प॒वित्र॒ आ ।",
          "begin": 0.0,
          "end": 5.499
        },
        {
          "text": "सं सूर्य᳚स्य र॒श्मिभि᳚ः ॥",
          "begin": 5.499,
          "end": 8.04
        }
      ]
    },
    {
      "id": "9.61.9",
      "mandala": 9,
      "sukta": 61,
      "rik": 9,
      "padas": [
        {
          "text": "स नो॒ भगा᳚य वा॒यवे᳚ पू॒ष्णे प᳚वस्व॒ मधु᳚मान् ।",
          "begin": 0.0,
          "end": 5.432
        },
        {
          "text": "चारु᳚र्मि॒त्रे वरु᳚णे च ॥",
          "begin": 5.432,
          "end": 8.16
        }
      ]
    },
    {
      "id": "9.61.10",
      "mandala": 9,
      "sukta": 61,
      "rik": 10,
      "padas": [
        {
          "text": "उ॒च्चा ते᳚ जा॒तमन्ध᳚सो दि॒वि षद्भूम्या द᳚दे ।",
          "begin": 0.0,
          "end": 4.989
        },
        {
          "text": "उ॒ग्रं शर्म॒ महि॒ श्रवः᳚ ॥",
          "begin": 4.989,
          "end": 7.2
        }
      ]
    },
    {
      "id": "9.61.11",
      "mandala": 9,
      "sukta": 61,
      "rik": 11,
      "padas": [
        {
          "text": "ए॒ना विश्वा᳚न्य॒र्य आ द्यु॒म्नानि॒ मानु᳚षाणाम् ।",
          "begin": 0.0,
          "end": 5.324
        },
        {
          "text": "सिषा᳚सन्तो वनामहे ॥",
          "begin": 5.324,
          "end": 7.8
        }
      ]
    },
    {
      "id": "9.61.12",
      "mandala": 9,
      "sukta": 61,
      "rik": 12,
      "padas": [
        {
          "text": "स न॒ इन्द्रा᳚य॒ यज्य᳚वे॒ वरु᳚णाय म॒रुद्भ्य᳚ः ।",
          "begin": 0.0,
          "end": 4.758
        },
        {
          "text": "व॒रि॒वो॒वित्परि᳚ स्रव ॥",
          "begin": 4.758,
          "end": 6.6
        }
      ]
    },
    {
      "id": "9.61.13",
      "mandala": 9,
      "sukta": 61,
      "rik": 13,
      "padas": [
        {
          "text": "उपो॒ षु जा॒तम॒प्तुरं॒ गोभि᳚र्भ॒ङ्गं परि᳚ष्कृतम् ।",
          "begin": 0.0,
          "end": 4.287
        },
        {
          "text": "इन्दुं᳚ दे॒वा अ᳚यासिषुः ॥",
          "begin": 4.287,
          "end": 7.28
        }
      ]
    },
    {
      "id": "9.61.14",
      "mandala": 9,
      "sukta": 61,
      "rik": 14,
      "padas": [
        {
          "text": "तमिद्व᳚र्धन्तु नो॒ गिरो᳚ व॒त्सं सं॒शिश्व᳚रीरिव ।",
          "begin": 0.0,
          "end": 4.439
        },
        {
          "text": "य इन्द्र᳚स्य हृदं॒सनि᳚ः ॥",
          "begin": 4.439,
          "end": 7.0
        }
      ]
    },
    {
      "id": "9.61.15",
      "mandala": 9,
      "sukta": 61,
      "rik": 15,
      "padas": [
        {
          "text": "अर्षा᳚ णः सोम॒ शं गवे᳚ धु॒क्षस्व᳚ पि॒प्युषी॒मिष᳚म् ।",
          "begin": 0.0,
          "end": 5.593
        },
        {
          "text": "वर्धा᳚ समु॒द्रमु॒क्थ्य᳚म् ॥",
          "begin": 5.593,
          "end": 8.28
        }
      ]
    },
    {
      "id": "9.61.16",
      "mandala": 9,
      "sukta": 61,
      "rik": 16,
      "padas": [
        {
          "text": "पव᳚मानो अजीजनद्दि॒वश्चि॒त्रं न त᳚न्य॒तुम् ।",
          "begin": 0.0,
          "end": 4.681
        },
        {
          "text": "ज्योति᳚र्वैश्वान॒रं बृ॒हत् ॥",
          "begin": 4.681,
          "end": 7.64
        }
      ]
    },
    {
      "id": "9.61.17",
      "mandala": 9,
      "sukta": 61,
      "rik": 17,
      "padas": [
        {
          "text": "पव᳚मानस्य ते॒ रसो॒ मदो᳚ राजन्नदुच्छु॒नः ।",
          "begin": 0.0,
          "end": 4.509
        },
        {
          "text": "वि वार॒मव्य᳚मर्षति ॥",
          "begin": 4.509,
          "end": 6.64
        }
      ]
    },
    {
      "id": "9.61.18",
      "mandala": 9,
      "sukta": 61,
      "rik": 18,
      "padas": [
        {
          "text": "पव᳚मान॒ रस॒स्तव॒ दक्षो॒ वि रा᳚जति द्यु॒मान् ।",
          "begin": 0.0,
          "end": 4.612
        },
        {
          "text": "ज्योति॒र्विश्वं॒ स्व᳚र्दृ॒शे ॥",
          "begin": 4.612,
          "end": 7.12
        }
      ]
    },
    {
      "id": "9.61.19",
      "mandala": 9,
      "sukta": 61,
      "rik": 19,
      "padas": [
        {
          "text": "यस्ते॒ मदो॒ वरे᳚ण्य॒स्तेना᳚ पव॒स्वान्ध᳚सा ।",
          "begin": 0.0,
          "end": 4.943
        },
        {
          "text": "दे॒वा॒वीर᳚घशंस॒हा ॥",
          "begin": 4.943,
          "end": 7.12
        }
      ]
    },
    {
      "id": "9.61.20",
      "mandala": 9,
      "sukta": 61,
      "rik": 20,
      "padas": [
        {
          "text": "जघ्नि᳚र्वृ॒त्रम᳚मि॒त्रियं॒ सस्नि॒र्वाजं᳚ दि॒वेदि᳚वे ।",
          "begin": 0.0,
          "end": 5.533
        },
        {
          "text": "गो॒षा उ᳚ अश्व॒सा अ᳚सि ॥",
          "begin": 5.533,
          "end": 8.36
        }
      ]
    },
    {
      "id": "9.61.21",
      "mandala": 9,
      "sukta": 61,
      "rik": 21,
      "padas": [
        {
          "text": "सम्मि᳚श्लो अरु॒षो भ᳚व सूप॒स्थाभि॒र्न धे॒नुभि᳚ः ।",
          "begin": 0.0,
          "end": 5.108
        },
        {
          "text": "सीद᳚ञ्छ्ये॒नो न योनि॒मा ॥",
          "begin": 5.108,
          "end": 7.88
        }
      ]
    },
    {
      "id": "9.61.22",
      "mandala": 9,
      "sukta": 61,
      "rik": 22,
      "padas": [
        {
          "text": "स प᳚वस्व॒ य आवि॒थेन्द्रं᳚ वृ॒त्राय॒ हन्त᳚वे ।",
          "begin": 0.0,
          "end": 4.733
        },
        {
          "text": "व॒व्रि॒वांसं᳚ म॒हीर॒पः ॥",
          "begin": 4.733,
          "end": 7.24
        }
      ]
    },
    {
      "id": "9.61.23",
      "mandala": 9,
      "sukta": 61,
      "rik": 23,
      "padas": [
        {
          "text": "सु॒वीरा᳚सो व॒यं धना॒ जये᳚म सोम मीढ्वः ।",
          "begin": 0.0,
          "end": 5.036
        },
        {
          "text": "पु॒ना॒नो व᳚र्ध नो॒ गिरः᳚ ॥",
          "begin": 5.036,
          "end": 7.52
        }
      ]
    },
    {
      "id": "9.61.24",
      "mandala": 9,
      "sukta": 61,
      "rik": 24,
      "padas": [
        {
          "text": "त्वोता᳚स॒स्तवाव᳚सा॒ स्याम᳚ व॒न्वन्त᳚ आ॒मुरः᳚ ।",
          "begin": 0.0,
          "end": 5.258
        },
        {
          "text": "सोम᳚ व्र॒तेषु᳚ जागृहि ॥",
          "begin": 5.258,
          "end": 7.8
        }
      ]
    },
    {
      "id": "9.61.25",
      "mandala": 9,
      "sukta": 61,
      "rik": 25,
      "padas": [
        {
          "text": "अ॒प॒घ्नन्प᳚वते॒ मृधो॑प॒ सोमो॒ अरा᳚व्णः ।",
          "begin": 0.0,
          "end": 4.643
        },
        {
          "text": "गच्छ॒न्निन्द्र᳚स्य निष्कृ॒तम् ॥",
          "begin": 4.643,
          "end": 7.0
        }
      ]
    },
    {
      "id": "9.61.26",
      "mandala": 9,
      "sukta": 61,
      "rik": 26,
      "padas": [
        {
          "text": "म॒हो नो᳚ रा॒य आ भ᳚र॒ पव᳚मान ज॒ही मृधः᳚ ।",
          "begin": 0.0,
          "end": 5.167
        },
        {
          "text": "रास्वे᳚न्दो वी॒रव॒द्यशः᳚ ॥",
          "begin": 5.167,
          "end": 7.88
        }
      ]
    },
    {
      "id": "9.61.27",
      "mandala": 9,
      "sukta": 61,
      "rik": 27,
      "padas": [
        {
          "text": "न त्वा᳚ श॒तं च॒न ह्रुतो॒ राधो॒ दित्स᳚न्त॒मा मि᳚नन् ।",
          "begin": 0.0,
          "end": 5.35
        },
        {
          "text": "यत्पु᳚ना॒नो म᳚ख॒स्यसे᳚ ॥",
          "begin": 5.35,
          "end": 7.96
        }
      ]
    },
    {
      "id": "9.61.28",
      "mandala": 9,
      "sukta": 61,
      "rik": 28,
      "padas": [
        {
          "text": "पव᳚स्वेन्दो॒ वृषा᳚ सु॒तः कृ॒धी नो᳚ य॒शसो॒ जने᳚ ।",
          "begin": 0.0,
          "end": 5.474
        },
        {
          "text": "विश्वा॒ अप॒ द्विषो᳚ जहि ॥",
          "begin": 5.474,
          "end": 8.16
        }
      ]
    },
    {
      "id": "9.61.29",
      "mandala": 9,
      "sukta": 61,
      "rik": 29,
      "padas": [
        {
          "text": "अस्य᳚ ते स॒ख्ये व॒यं तवे᳚न्दो द्यु॒म्न उ᳚त्त॒मे ।",
          "begin": 0.0,
          "end": 4.915
        },
        {
          "text": "सा॒स॒ह्याम᳚ पृतन्य॒तः ॥",
          "begin": 4.915,
          "end": 7.08
        }
      ]
    },
    {
      "id": "9.61.30",
      "mandala": 9,
      "sukta": 61,
      "rik": 30,
      "padas": [
        {
          "text": "या ते᳚ भी॒मान्यायु᳚धा ति॒ग्मानि॒ सन्ति॒ धूर्व᳚णे ।",
          "begin": 0.0,
          "end": 6.253
        },
        {
          "text": "रक्षा᳚ समस्य नो नि॒दः ॥",
          "begin": 6.253,
          "end": 12.28
        }
      ]
    }
  ]
}