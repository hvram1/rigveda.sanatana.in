{
  "mandala": 9,
  "sukta": 25,
  "verses": [
    {
      "id": "9.25.1",
      "mandala": 9,
      "sukta": 25,
      "rik": 1,
      "padas": [
        {
          "text": "पव᳚स्व दक्ष॒साध᳚नो दे॒वेभ्य᳚ः पी॒तये᳚ हरे ।",
          "begin": 0.0,
          "end": 6.451
        },
        {
          "text": "म॒रुद्भ्यो᳚ वा॒यवे॒ मदः᳚ ॥",
          "begin": 6.451,
          "end": 9.84
        }
      ]
    },
    {
      "id": "9.25.2",
      "mandala": 9,
      "sukta": 25,
      "rik": 2,
      "padas": [
        {
          "text": "पव᳚मान धि॒या हि॒तो॒३॒᳚॑भि योनिं॒ कनि᳚क्रदत् ।",
          "begin": 0.0,
          "end": 6.636
        },
        {
          "text": "धर्म᳚णा वा॒युमा वि᳚श ॥",
          "begin": 6.636,
          "end": 9.08
        }
      ]
    },
    {
      "id": "9.25.3",
      "mandala": 9,
      "sukta": 25,
      "rik": 3,
      "padas": [
        {
          "text": "सं दे॒वैः शो᳚भते॒ वृषा᳚ क॒विर्योना॒वधि᳚ प्रि॒यः ।",
          "begin": 0.0,
          "end": 6.253
        },
        {
          "text": "वृ॒त्र॒हा दे᳚व॒वीत᳚मः ॥",
          "begin": 6.253,
          "end": 8.84
        }
      ]
    },
    {
      "id": "9.25.4",
      "mandala": 9,
      "sukta": 25,
      "rik": 4,
      "padas": [
        {
          "text": "विश्वा᳚ रू॒पाण्या᳚वि॒शन्पु᳚ना॒नो या᳚ति हर्य॒तः ।",
          "begin": 0.0,
          "end": 6.273
        },
        {
          "text": "यत्रा॒मृता᳚स॒ आस᳚ते ॥",
          "begin": 6.273,
          "end": 8.88
        }
      ]
    },
    {
      "id": "9.25.5",
      "mandala": 9,
      "sukta": 25,
      "rik": 5,
      "padas": [
        {
          "text": "अ॒रु॒षो ज॒नय॒न्गिर॒ः सोमः᳚ पवत आयु॒षक् ।",
          "begin": 0.0,
          "end": 4.633
        },
        {
          "text": "इन्द्रं॒ गच्छ᳚न्क॒विक्र᳚तुः ॥",
          "begin": 4.633,
          "end": 7.44
        }
      ]
    },
    {
      "id": "9.25.6",
      "mandala": 9,
      "sukta": 25,
      "rik": 6,
      "padas": [
        {
          "text": "आ प᳚वस्व मदिन्तम प॒वित्रं॒ धार᳚या कवे ।",
          "begin": 0.0,
          "end": 5.037
        },
        {
          "text": "अ॒र्कस्य॒ योनि᳚मा॒सद᳚म् ॥",
          "begin": 5.037,
          "end": 9.16
        }
      ]
    }
  ]
}