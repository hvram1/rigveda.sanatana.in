{
  "mandala": 9,
  "sukta": 7,
  "verses": [
    {
      "id": "9.7.1",
      "mandala": 9,
      "sukta": 7,
      "rik": 1,
      "padas": [
        {
          "text": "असृ᳚ग्र॒मिन्द᳚वः प॒था धर्म᳚न्नृ॒तस्य᳚ सु॒श्रियः᳚ ।",
          "begin": 0.0,
          "end": 8.105
        },
        {
          "text": "वि॒दा॒ना अ᳚स्य॒ योज᳚नम् ॥",
          "begin": 8.105,
          "end": 11.04
        }
      ]
    },
    {
      "id": "9.7.2",
      "mandala": 9,
      "sukta": 7,
      "rik": 2,
      "padas": [
        {
          "text": "प्र धारा॒ मध्वो᳚ अग्रि॒यो म॒हीर॒पो वि गा᳚हते ।",
          "begin": 0.0,
          "end": 6.244
        },
        {
          "text": "ह॒विर्ह॒विष्षु॒ वन्द्य᳚ः ॥",
          "begin": 6.244,
          "end": 8.68
        }
      ]
    },
    {
      "id": "9.7.3",
      "mandala": 9,
      "sukta": 7,
      "rik": 3,
      "padas": [
        {
          "text": "प्र यु॒जो वा॒चो अ᳚ग्रि॒यो वृषाव᳚ चक्रद॒द्वने᳚ ।",
          "begin": 0.0,
          "end": 5.796
        },
        {
          "text": "सद्मा॒भि स॒त्यो अ᳚ध्व॒रः ॥",
          "begin": 5.796,
          "end": 8.68
        }
      ]
    },
    {
      "id": "9.7.4",
      "mandala": 9,
      "sukta": 7,
      "rik": 4,
      "padas": [
        {
          "text": "परि॒ यत्काव्या᳚ क॒विर्नृ॒म्णा वसा᳚नो॒ अर्ष᳚ति ।",
          "begin": 0.0,
          "end": 6.33
        },
        {
          "text": "स्व᳚र्वा॒जी सि᳚षासति ॥",
          "begin": 6.33,
          "end": 9.2
        }
      ]
    },
    {
      "id": "9.7.5",
      "mandala": 9,
      "sukta": 7,
      "rik": 5,
      "padas": [
        {
          "text": "पव᳚मानो अ॒भि स्पृधो॒ विशो॒ राजे᳚व सीदति ।",
          "begin": 0.0,
          "end": 5.363
        },
        {
          "text": "यदी᳚मृ॒ण्वन्ति᳚ वे॒धसः᳚ ॥",
          "begin": 5.363,
          "end": 9.16
        }
      ]
    },
    {
      "id": "9.7.6",
      "mandala": 9,
      "sukta": 7,
      "rik": 6,
      "padas": [
        {
          "text": "अव्यो॒ वारे॒ परि᳚ प्रि॒यो हरि॒र्वने᳚षु सीदति ।",
          "begin": 0.0,
          "end": 5.14
        },
        {
          "text": "रे॒भो व᳚नुष्यते म॒ती ॥",
          "begin": 5.14,
          "end": 8.28
        }
      ]
    },
    {
      "id": "9.7.7",
      "mandala": 9,
      "sukta": 7,
      "rik": 7,
      "padas": [
        {
          "text": "स वा॒युमिन्द्र᳚म॒श्विना᳚ सा॒कं मदे᳚न गच्छति ।",
          "begin": 0.0,
          "end": 5.727
        },
        {
          "text": "रणा॒ यो अ᳚स्य॒ धर्म᳚भिः ॥",
          "begin": 5.727,
          "end": 8.72
        }
      ]
    },
    {
      "id": "9.7.8",
      "mandala": 9,
      "sukta": 7,
      "rik": 8,
      "padas": [
        {
          "text": "आ मि॒त्रावरु᳚णा॒ भगं॒ मध्व᳚ः पवन्त ऊ॒र्मयः᳚ ।",
          "begin": 0.0,
          "end": 5.501
        },
        {
          "text": "वि॒दा॒ना अ᳚स्य॒ शक्म᳚भिः ॥",
          "begin": 5.501,
          "end": 8.64
        }
      ]
    },
    {
      "id": "9.7.9",
      "mandala": 9,
      "sukta": 7,
      "rik": 9,
      "padas": [
        {
          "text": "अ॒स्मभ्यं᳚ रोदसी र॒यिं मध्वो॒ वाज᳚स्य सा॒तये᳚ ।",
          "begin": 0.0,
          "end": 6.597
        },
        {
          "text": "श्रवो॒ वसू᳚नि॒ सं जि᳚तम् ॥",
          "begin": 6.597,
          "end": 11.6
        }
      ]
    }
  ]
}