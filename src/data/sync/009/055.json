{
  "mandala": 9,
  "sukta": 55,
  "verses": [
    {
      "id": "9.55.1",
      "mandala": 9,
      "sukta": 55,
      "rik": 1,
      "padas": [
        {
          "text": "यवं᳚यवं नो॒ अन्ध᳚सा पु॒ष्टम्पु᳚ष्टं॒ परि᳚ स्रव ।",
          "begin": 0.0,
          "end": 6.984
        },
        {
          "text": "सोम॒ विश्वा᳚ च॒ सौभ᳚गा ॥",
          "begin": 6.984,
          "end": 9.72
        }
      ]
    },
    {
      "id": "9.55.2",
      "mandala": 9,
      "sukta": 55,
      "rik": 2,
      "padas": [
        {
          "text": "इन्दो॒ यथा॒ तव॒ स्तवो॒ यथा᳚ ते जा॒तमन्ध᳚सः ।",
          "begin": 0.0,
          "end": 4.952
        },
        {
          "text": "नि ब॒र्हिषि᳚ प्रि॒ये स᳚दः ॥",
          "begin": 4.952,
          "end": 7.16
        }
      ]
    },
    {
      "id": "9.55.3",
      "mandala": 9,
      "sukta": 55,
      "rik": 3,
      "padas": [
        {
          "text": "उ॒त नो᳚ गो॒विद᳚श्व॒वित्पव᳚स्व सो॒मान्ध᳚सा ।",
          "begin": 0.0,
          "end": 4.962
        },
        {
          "text": "म॒क्षूत᳚मेभि॒रह᳚भिः ॥",
          "begin": 4.962,
          "end": 7.08
        }
      ]
    },
    {
      "id": "9.55.4",
      "mandala": 9,
      "sukta": 55,
      "rik": 4,
      "padas": [
        {
          "text": "यो जि॒नाति॒ न जीय᳚ते॒ हन्ति॒ शत्रु᳚म॒भीत्य᳚ ।",
          "begin": 0.0,
          "end": 4.14
        },
        {
          "text": "स प᳚वस्व सहस्रजित् ॥",
          "begin": 4.14,
          "end": 8.64
        }
      ]
    }
  ]
}