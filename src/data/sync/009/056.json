{
  "mandala": 9,
  "sukta": 56,
  "verses": [
    {
      "id": "9.56.1",
      "mandala": 9,
      "sukta": 56,
      "rik": 1,
      "padas": [
        {
          "text": "परि॒ सोम᳚ ऋ॒तं बृ॒हदा॒शुः प॒वित्रे᳚ अर्षति ।",
          "begin": 0.0,
          "end": 6.316
        },
        {
          "text": "वि॒घ्नन्रक्षां᳚सि देव॒युः ॥",
          "begin": 6.316,
          "end": 9.6
        }
      ]
    },
    {
      "id": "9.56.2",
      "mandala": 9,
      "sukta": 56,
      "rik": 2,
      "padas": [
        {
          "text": "यत्सोमो॒ वाज॒मर्ष᳚ति श॒तं धारा᳚ अप॒स्युवः᳚ ।",
          "begin": 0.0,
          "end": 4.969
        },
        {
          "text": "इन्द्र᳚स्य स॒ख्यमा᳚वि॒शन् ॥",
          "begin": 4.969,
          "end": 7.68
        }
      ]
    },
    {
      "id": "9.56.3",
      "mandala": 9,
      "sukta": 56,
      "rik": 3,
      "padas": [
        {
          "text": "अ॒भि त्वा॒ योष᳚णो॒ दश᳚ जा॒रं न क॒न्या᳚नूषत ।",
          "begin": 0.0,
          "end": 4.934
        },
        {
          "text": "मृ॒ज्यसे᳚ सोम सा॒तये᳚ ॥",
          "begin": 4.934,
          "end": 7.68
        }
      ]
    },
    {
      "id": "9.56.4",
      "mandala": 9,
      "sukta": 56,
      "rik": 4,
      "padas": [
        {
          "text": "त्वमिन्द्रा᳚य॒ विष्ण᳚वे स्वा॒दुरि᳚न्दो॒ परि᳚ स्रव ।",
          "begin": 0.0,
          "end": 7.1
        },
        {
          "text": "नॄन्त्स्तो॒तॄन्पा॒ह्यंह᳚सः ॥",
          "begin": 7.1,
          "end": 9.6
        }
      ]
    }
  ]
}