{
  "mandala": 9,
  "sukta": 54,
  "verses": [
    {
      "id": "9.54.1",
      "mandala": 9,
      "sukta": 54,
      "rik": 1,
      "padas": [
        {
          "text": "अ॒स्य प्र॒त्नामनु॒ द्युतं᳚ शु॒क्रं दु᳚दुह्रे॒ अह्र᳚यः ।",
          "begin": 0.0,
          "end": 6.925
        },
        {
          "text": "पयः᳚ सहस्र॒सामृषि᳚म् ॥",
          "begin": 6.925,
          "end": 10.24
        }
      ]
    },
    {
      "id": "9.54.2",
      "mandala": 9,
      "sukta": 54,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒यं सूर्य᳚ इवोप॒दृग॒यं सरां᳚सि धावति ।",
          "begin": 0.0,
          "end": 4.545
        },
        {
          "text": "स॒प्त प्र॒वत॒ आ दिव᳚म् ॥",
          "begin": 4.545,
          "end": 6.92
        }
      ]
    },
    {
      "id": "9.54.3",
      "mandala": 9,
      "sukta": 54,
      "rik": 3,
      "padas": [
        {
          "text": "अ॒यं विश्वा᳚नि तिष्ठति पुना॒नो भुव᳚नो॒परि᳚ ।",
          "begin": 0.0,
          "end": 4.63
        },
        {
          "text": "सोमो᳚ दे॒वो न सूर्य᳚ः ॥",
          "begin": 4.63,
          "end": 7.52
        }
      ]
    },
    {
      "id": "9.54.4",
      "mandala": 9,
      "sukta": 54,
      "rik": 4,
      "padas": [
        {
          "text": "परि᳚ णो दे॒ववी᳚तये॒ वाजाँ᳚ अर्षसि॒ गोम᳚तः ।",
          "begin": 0.0,
          "end": 6.9
        },
        {
          "text": "पु॒ना॒न इ᳚न्दविन्द्र॒युः ॥",
          "begin": 6.9,
          "end": 11.12
        }
      ]
    }
  ]
}