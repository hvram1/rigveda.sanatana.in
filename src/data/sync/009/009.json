{
  "mandala": 9,
  "sukta": 9,
  "verses": [
    {
      "id": "9.9.1",
      "mandala": 9,
      "sukta": 9,
      "rik": 1,
      "padas": [
        {
          "text": "परि᳚ प्रि॒या दि॒वः क॒विर्वयां᳚सि न॒प्त्यो᳚र्हि॒तः ।",
          "begin": 0.0,
          "end": 7.45
        },
        {
          "text": "सु॒वा॒नो या᳚ति क॒विक्र᳚तुः ॥",
          "begin": 7.45,
          "end": 10.76
        }
      ]
    },
    {
      "id": "9.9.2",
      "mandala": 9,
      "sukta": 9,
      "rik": 2,
      "padas": [
        {
          "text": "प्रप्र॒ क्षया᳚य॒ पन्य᳚से॒ जना᳚य॒ जुष्टो᳚ अ॒द्रुहे᳚ ।",
          "begin": 0.0,
          "end": 6.446
        },
        {
          "text": "वी॒त्य᳚र्ष॒ चनि᳚ष्ठया ॥",
          "begin": 6.446,
          "end": 9.2
        }
      ]
    },
    {
      "id": "9.9.3",
      "mandala": 9,
      "sukta": 9,
      "rik": 3,
      "padas": [
        {
          "text": "स सू॒नुर्मा॒तरा॒ शुचि᳚र्जा॒तो जा॒ते अ᳚रोचयत् ।",
          "begin": 0.0,
          "end": 6.175
        },
        {
          "text": "म॒हान्म॒ही ऋ᳚ता॒वृधा᳚ ॥",
          "begin": 6.175,
          "end": 9.08
        }
      ]
    },
    {
      "id": "9.9.4",
      "mandala": 9,
      "sukta": 9,
      "rik": 4,
      "padas": [
        {
          "text": "स स॒प्त धी॒तिभि᳚र्हि॒तो न॒द्यो᳚ अजिन्वद॒द्रुहः᳚ ।",
          "begin": 0.0,
          "end": 5.868
        },
        {
          "text": "या एक॒मक्षि᳚ वावृ॒धुः ॥",
          "begin": 5.868,
          "end": 8.6
        }
      ]
    },
    {
      "id": "9.9.5",
      "mandala": 9,
      "sukta": 9,
      "rik": 5,
      "padas": [
        {
          "text": "ता अ॒भि सन्त॒मस्तृ᳚तं म॒हे युवा᳚न॒मा द᳚धुः ।",
          "begin": 0.0,
          "end": 5.672
        },
        {
          "text": "इन्दु᳚मिन्द्र॒ तव᳚ व्र॒ते ॥",
          "begin": 5.672,
          "end": 8.48
        }
      ]
    },
    {
      "id": "9.9.6",
      "mandala": 9,
      "sukta": 9,
      "rik": 6,
      "padas": [
        {
          "text": "अ॒भि वह्नि॒रम᳚र्त्यः स॒प्त प᳚श्यति॒ वाव᳚हिः ।",
          "begin": 0.0,
          "end": 3.757
        },
        {
          "text": "क्रिवि᳚र्दे॒वीर᳚तर्पयत् ॥",
          "begin": 3.757,
          "end": 7.2
        }
      ]
    },
    {
      "id": "9.9.7",
      "mandala": 9,
      "sukta": 9,
      "rik": 7,
      "padas": [
        {
          "text": "अवा॒ कल्पे᳚षु नः पुम॒स्तमां᳚सि सोम॒ योध्या᳚ ।",
          "begin": 0.0,
          "end": 5.038
        },
        {
          "text": "तानि᳚ पुनान जङ्घनः ॥",
          "begin": 5.038,
          "end": 8.76
        }
      ]
    },
    {
      "id": "9.9.8",
      "mandala": 9,
      "sukta": 9,
      "rik": 8,
      "padas": [
        {
          "text": "नू नव्य᳚से॒ नवी᳚यसे सू॒क्ताय᳚ साधया प॒थः ।",
          "begin": 0.0,
          "end": 5.253
        },
        {
          "text": "प्र॒त्न॒वद्रो᳚चया॒ रुचः᳚ ॥",
          "begin": 5.253,
          "end": 9.24
        }
      ]
    },
    {
      "id": "9.9.9",
      "mandala": 9,
      "sukta": 9,
      "rik": 9,
      "padas": [
        {
          "text": "पव᳚मान॒ महि॒ श्रवो॒ गामश्वं᳚ रासि वी॒रव᳚त् ।",
          "begin": 0.0,
          "end": 5.819
        },
        {
          "text": "सना᳚ मे॒धां सना॒ स्व᳚ः ॥",
          "begin": 5.819,
          "end": 10.04
        }
      ]
    }
  ]
}