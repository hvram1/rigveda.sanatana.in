{
  "mandala": 9,
  "sukta": 47,
  "verses": [
    {
      "id": "9.47.1",
      "mandala": 9,
      "sukta": 47,
      "rik": 1,
      "padas": [
        {
          "text": "अ॒या सोमः᳚ सुकृ॒त्यया᳚ म॒हश्चि᳚द॒भ्य᳚वर्धत ।",
          "begin": 0.0,
          "end": 4.531
        },
        {
          "text": "म॒न्दा॒न उद्वृ᳚षायते ॥",
          "begin": 4.531,
          "end": 8.08
        }
      ]
    },
    {
      "id": "9.47.2",
      "mandala": 9,
      "sukta": 47,
      "rik": 2,
      "padas": [
        {
          "text": "कृ॒तानीद᳚स्य॒ कर्त्वा॒ चेत᳚न्ते दस्यु॒तर्ह᳚णा ।",
          "begin": 0.0,
          "end": 4.742
        },
        {
          "text": "ऋ॒णा च᳚ धृ॒ष्णुश्च᳚यते ॥",
          "begin": 4.742,
          "end": 6.76
        }
      ]
    },
    {
      "id": "9.47.3",
      "mandala": 9,
      "sukta": 47,
      "rik": 3,
      "padas": [
        {
          "text": "आत्सोम᳚ इन्द्रि॒यो रसो॒ वज्र᳚ः सहस्र॒सा भु᳚वत् ।",
          "begin": 0.0,
          "end": 4.849
        },
        {
          "text": "उ॒क्थं यद᳚स्य॒ जाय᳚ते ॥",
          "begin": 4.849,
          "end": 6.84
        }
      ]
    },
    {
      "id": "9.47.4",
      "mandala": 9,
      "sukta": 47,
      "rik": 4,
      "padas": [
        {
          "text": "स्व॒यं क॒विर्वि᳚ध॒र्तरि॒ विप्रा᳚य॒ रत्न᳚मिच्छति ।",
          "begin": 0.0,
          "end": 4.756
        },
        {
          "text": "यदी᳚ मर्मृ॒ज्यते॒ धियः᳚ ॥",
          "begin": 4.756,
          "end": 7.44
        }
      ]
    },
    {
      "id": "9.47.5",
      "mandala": 9,
      "sukta": 47,
      "rik": 5,
      "padas": [
        {
          "text": "सि॒षा॒सतू᳚ रयी॒णां वाजे॒ष्वर्व᳚तामिव ।",
          "begin": 0.0,
          "end": 5.53
        },
        {
          "text": "भरे᳚षु जि॒ग्युषा᳚मसि ॥",
          "begin": 5.53,
          "end": 10.56
        }
      ]
    }
  ]
}