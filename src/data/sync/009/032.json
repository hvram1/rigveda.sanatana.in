{
  "mandala": 9,
  "sukta": 32,
  "verses": [
    {
      "id": "9.32.1",
      "mandala": 9,
      "sukta": 32,
      "rik": 1,
      "padas": [
        {
          "text": "प्र सोमा᳚सो मद॒च्युत॒ः श्रव᳚से नो म॒घोनः᳚ ।",
          "begin": 0.0,
          "end": 6.435
        },
        {
          "text": "सु॒ता वि॒दथे᳚ अक्रमुः ॥",
          "begin": 6.435,
          "end": 9.16
        }
      ]
    },
    {
      "id": "9.32.2",
      "mandala": 9,
      "sukta": 32,
      "rik": 2,
      "padas": [
        {
          "text": "आदीं᳚ त्रि॒तस्य॒ योष᳚णो॒ हरिं᳚ हिन्व॒न्त्यद्रि᳚भिः ।",
          "begin": 0.0,
          "end": 5.689
        },
        {
          "text": "इन्दु॒मिन्द्रा᳚य पी॒तये᳚ ॥",
          "begin": 5.689,
          "end": 8.48
        }
      ]
    },
    {
      "id": "9.32.3",
      "mandala": 9,
      "sukta": 32,
      "rik": 3,
      "padas": [
        {
          "text": "आदीं᳚ हं॒सो यथा᳚ ग॒णं विश्व᳚स्यावीवशन्म॒तिम् ।",
          "begin": 0.0,
          "end": 5.374
        },
        {
          "text": "अत्यो॒ न गोभि᳚रज्यते ॥",
          "begin": 5.374,
          "end": 8.2
        }
      ]
    },
    {
      "id": "9.32.4",
      "mandala": 9,
      "sukta": 32,
      "rik": 4,
      "padas": [
        {
          "text": "उ॒भे सो᳚माव॒चाक᳚शन्मृ॒गो न त॒क्तो अ᳚र्षसि ।",
          "begin": 0.0,
          "end": 5.519
        },
        {
          "text": "सीद᳚न्नृ॒तस्य॒ योनि॒मा ॥",
          "begin": 5.519,
          "end": 8.2
        }
      ]
    },
    {
      "id": "9.32.5",
      "mandala": 9,
      "sukta": 32,
      "rik": 5,
      "padas": [
        {
          "text": "अ॒भि गावो᳚ अनूषत॒ योषा᳚ जा॒रमि᳚व प्रि॒यम् ।",
          "begin": 0.0,
          "end": 6.072
        },
        {
          "text": "अग᳚न्ना॒जिं यथा᳚ हि॒तम् ॥",
          "begin": 6.072,
          "end": 8.92
        }
      ]
    },
    {
      "id": "9.32.6",
      "mandala": 9,
      "sukta": 32,
      "rik": 6,
      "padas": [
        {
          "text": "अ॒स्मे धे᳚हि द्यु॒मद्यशो᳚ म॒घव᳚द्भ्यश्च॒ मह्यं᳚ च ।",
          "begin": 0.0,
          "end": 5.704
        },
        {
          "text": "स॒निं मे॒धामु॒त श्रवः᳚ ॥",
          "begin": 5.704,
          "end": 10.84
        }
      ]
    }
  ]
}