{
  "mandala": 9,
  "sukta": 51,
  "verses": [
    {
      "id": "9.51.1",
      "mandala": 9,
      "sukta": 51,
      "rik": 1,
      "padas": [
        {
          "text": "अध्व᳚र्यो॒ अद्रि᳚भिः सु॒तं सोमं᳚ प॒वित्र॒ आ सृ᳚ज ।",
          "begin": 0.0,
          "end": 6.669
        },
        {
          "text": "पु॒नी॒हीन्द्रा᳚य॒ पात᳚वे ॥",
          "begin": 6.669,
          "end": 9.64
        }
      ]
    },
    {
      "id": "9.51.2",
      "mandala": 9,
      "sukta": 51,
      "rik": 2,
      "padas": [
        {
          "text": "दि॒वः पी॒यूष᳚मुत्त॒मं सोम॒मिन्द्रा᳚य व॒ज्रिणे᳚ ।",
          "begin": 0.0,
          "end": 5.024
        },
        {
          "text": "सु॒नोता॒ मधु᳚मत्तमम् ॥",
          "begin": 5.024,
          "end": 7.16
        }
      ]
    },
    {
      "id": "9.51.3",
      "mandala": 9,
      "sukta": 51,
      "rik": 3,
      "padas": [
        {
          "text": "तव॒ त्य इ᳚न्दो॒ अन्ध᳚सो दे॒वा मधो॒र्व्य᳚श्नते ।",
          "begin": 0.0,
          "end": 5.042
        },
        {
          "text": "पव᳚मानस्य म॒रुतः᳚ ॥",
          "begin": 5.042,
          "end": 7.32
        }
      ]
    },
    {
      "id": "9.51.4",
      "mandala": 9,
      "sukta": 51,
      "rik": 4,
      "padas": [
        {
          "text": "त्वं हि सो᳚म व॒र्धय᳚न्त्सु॒तो मदा᳚य॒ भूर्ण᳚ये ।",
          "begin": 0.0,
          "end": 5.454
        },
        {
          "text": "वृष᳚न्त्स्तो॒तार᳚मू॒तये᳚ ॥",
          "begin": 5.454,
          "end": 8.68
        }
      ]
    },
    {
      "id": "9.51.5",
      "mandala": 9,
      "sukta": 51,
      "rik": 5,
      "padas": [
        {
          "text": "अ॒भ्य᳚र्ष विचक्षण प॒वित्रं॒ धार᳚या सु॒तः ।",
          "begin": 0.0,
          "end": 4.463
        },
        {
          "text": "अ॒भि वाज᳚मु॒त श्रवः᳚ ॥",
          "begin": 4.463,
          "end": 7.72
        }
      ]
    }
  ]
}