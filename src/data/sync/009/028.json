{
  "mandala": 9,
  "sukta": 28,
  "verses": [
    {
      "id": "9.28.1",
      "mandala": 9,
      "sukta": 28,
      "rik": 1,
      "padas": [
        {
          "text": "ए॒ष वा॒जी हि॒तो नृभि᳚र्विश्व॒विन्मन᳚स॒स्पति᳚ः ।",
          "begin": 0.0,
          "end": 5.783
        },
        {
          "text": "अव्यो॒ वारं॒ वि धा᳚वति ॥",
          "begin": 5.783,
          "end": 9.4
        }
      ]
    },
    {
      "id": "9.28.2",
      "mandala": 9,
      "sukta": 28,
      "rik": 2,
      "padas": [
        {
          "text": "ए॒ष प॒वित्रे᳚ अक्षर॒त्सोमो᳚ दे॒वेभ्य᳚ः सु॒तः ।",
          "begin": 0.0,
          "end": 5.951
        },
        {
          "text": "विश्वा॒ धामा᳚न्यावि॒शन् ॥",
          "begin": 5.951,
          "end": 8.88
        }
      ]
    },
    {
      "id": "9.28.3",
      "mandala": 9,
      "sukta": 28,
      "rik": 3,
      "padas": [
        {
          "text": "ए॒ष दे॒वः शु᳚भाय॒ते॑धि॒ योना॒वम᳚र्त्यः ।",
          "begin": 0.0,
          "end": 4.677
        },
        {
          "text": "वृ॒त्र॒हा दे᳚व॒वीत᳚मः ॥",
          "begin": 4.677,
          "end": 7.2
        }
      ]
    },
    {
      "id": "9.28.4",
      "mandala": 9,
      "sukta": 28,
      "rik": 4,
      "padas": [
        {
          "text": "ए॒ष वृषा॒ कनि᳚क्रदद्द॒शभि᳚र्जा॒मिभि᳚र्य॒तः ।",
          "begin": 0.0,
          "end": 5.006
        },
        {
          "text": "अ॒भि द्रोणा᳚नि धावति ॥",
          "begin": 5.006,
          "end": 7.68
        }
      ]
    },
    {
      "id": "9.28.5",
      "mandala": 9,
      "sukta": 28,
      "rik": 5,
      "padas": [
        {
          "text": "ए॒ष सूर्य᳚मरोचय॒त्पव᳚मानो॒ विच᳚र्षणिः ।",
          "begin": 0.0,
          "end": 4.692
        },
        {
          "text": "विश्वा॒ धामा᳚नि विश्व॒वित् ॥",
          "begin": 4.692,
          "end": 7.48
        }
      ]
    },
    {
      "id": "9.28.6",
      "mandala": 9,
      "sukta": 28,
      "rik": 6,
      "padas": [
        {
          "text": "ए॒ष शु॒ष्म्यदा᳚भ्य॒ः सोमः᳚ पुना॒नो अ᳚र्षति ।",
          "begin": 0.0,
          "end": 5.989
        },
        {
          "text": "दे॒वा॒वीर᳚घशंस॒हा ॥",
          "begin": 5.989,
          "end": 10.36
        }
      ]
    }
  ]
}