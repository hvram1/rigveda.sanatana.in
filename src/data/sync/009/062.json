{
  "mandala": 9,
  "sukta": 62,
  "verses": [
    {
      "id": "9.62.1",
      "mandala": 9,
      "sukta": 62,
      "rik": 1,
      "padas": [
        {
          "text": "ए॒ते अ᳚सृग्र॒मिन्द᳚वस्ति॒रः प॒वित्र᳚मा॒शवः᳚ ।",
          "begin": 0.0,
          "end": 6.827
        },
        {
          "text": "विश्वा᳚न्य॒भि सौभ᳚गा ॥",
          "begin": 6.827,
          "end": 9.48
        }
      ]
    },
    {
      "id": "9.62.2",
      "mandala": 9,
      "sukta": 62,
      "rik": 2,
      "padas": [
        {
          "text": "वि॒घ्नन्तो᳚ दुरि॒ता पु॒रु सु॒गा तो॒काय᳚ वा॒जिनः᳚ ।",
          "begin": 0.0,
          "end": 4.955
        },
        {
          "text": "तना᳚ कृ॒ण्वन्तो॒ अर्व᳚ते ॥",
          "begin": 4.955,
          "end": 7.76
        }
      ]
    },
    {
      "id": "9.62.3",
      "mandala": 9,
      "sukta": 62,
      "rik": 3,
      "padas": [
        {
          "text": "कृ॒ण्वन्तो॒ वरि᳚वो॒ गवे॒॑भ्य᳚र्षन्ति सुष्टु॒तिम् ।",
          "begin": 0.0,
          "end": 4.538
        },
        {
          "text": "इळा᳚म॒स्मभ्यं᳚ सं॒यत᳚म् ॥",
          "begin": 4.538,
          "end": 7.6
        }
      ]
    },
    {
      "id": "9.62.4",
      "mandala": 9,
      "sukta": 62,
      "rik": 4,
      "padas": [
        {
          "text": "असा᳚व्यं॒शुर्मदा᳚या॒प्सु दक्षो᳚ गिरि॒ष्ठाः ।",
          "begin": 0.0,
          "end": 5.548
        },
        {
          "text": "श्ये॒नो न योनि॒मास᳚दत् ॥",
          "begin": 5.548,
          "end": 8.0
        }
      ]
    },
    {
      "id": "9.62.5",
      "mandala": 9,
      "sukta": 62,
      "rik": 5,
      "padas": [
        {
          "text": "शु॒भ्रमन्धो᳚ दे॒ववा᳚तम॒प्सु धू॒तो नृभि᳚ः सु॒तः ।",
          "begin": 0.0,
          "end": 5.242
        },
        {
          "text": "स्वद᳚न्ति॒ गाव॒ः पयो᳚भिः ॥",
          "begin": 5.242,
          "end": 8.12
        }
      ]
    },
    {
      "id": "9.62.6",
      "mandala": 9,
      "sukta": 62,
      "rik": 6,
      "padas": [
        {
          "text": "आदी॒मश्वं॒ न हेता॒रो॑शू᳚शुभन्न॒मृता᳚य ।",
          "begin": 0.0,
          "end": 5.168
        },
        {
          "text": "मध्वो॒ रसं᳚ सध॒मादे᳚ ॥",
          "begin": 5.168,
          "end": 8.12
        }
      ]
    },
    {
      "id": "9.62.7",
      "mandala": 9,
      "sukta": 62,
      "rik": 7,
      "padas": [
        {
          "text": "यास्ते॒ धारा᳚ मधु॒श्चुतो॑सृ᳚ग्रमिन्द ऊ॒तये᳚ ।",
          "begin": 0.0,
          "end": 4.506
        },
        {
          "text": "ताभि᳚ः प॒वित्र॒मास᳚दः ॥",
          "begin": 4.506,
          "end": 7.36
        }
      ]
    },
    {
      "id": "9.62.8",
      "mandala": 9,
      "sukta": 62,
      "rik": 8,
      "padas": [
        {
          "text": "सो अ॒र्षेन्द्रा᳚य पी॒तये᳚ ति॒रो रोमा᳚ण्य॒व्यया᳚ ।",
          "begin": 0.0,
          "end": 6.247
        },
        {
          "text": "सीद॒न्योना॒ वने॒ष्वा ॥",
          "begin": 6.247,
          "end": 8.76
        }
      ]
    },
    {
      "id": "9.62.9",
      "mandala": 9,
      "sukta": 62,
      "rik": 9,
      "padas": [
        {
          "text": "त्वमि᳚न्दो॒ परि᳚ स्रव॒ स्वादि᳚ष्ठो॒ अङ्गि᳚रोभ्यः ।",
          "begin": 0.0,
          "end": 4.995
        },
        {
          "text": "व॒रि॒वो॒विद्घृ॒तं पयः᳚ ॥",
          "begin": 4.995,
          "end": 7.08
        }
      ]
    },
    {
      "id": "9.62.10",
      "mandala": 9,
      "sukta": 62,
      "rik": 10,
      "padas": [
        {
          "text": "अ॒यं विच᳚र्षणिर्हि॒तः पव᳚मान॒ः स चे᳚तति ।",
          "begin": 0.0,
          "end": 4.525
        },
        {
          "text": "हि॒न्वा॒न आप्यं᳚ बृ॒हत् ॥",
          "begin": 4.525,
          "end": 7.08
        }
      ]
    },
    {
      "id": "9.62.11",
      "mandala": 9,
      "sukta": 62,
      "rik": 11,
      "padas": [
        {
          "text": "ए॒ष वृषा॒ वृष᳚व्रत॒ः पव᳚मानो अशस्ति॒हा ।",
          "begin": 0.0,
          "end": 5.692
        },
        {
          "text": "कर॒द्वसू᳚नि दा॒शुषे᳚ ॥",
          "begin": 5.692,
          "end": 7.44
        }
      ]
    },
    {
      "id": "9.62.12",
      "mandala": 9,
      "sukta": 62,
      "rik": 12,
      "padas": [
        {
          "text": "आ प᳚वस्व सह॒स्रिणं᳚ र॒यिं गोम᳚न्तम॒श्विन᳚म् ।",
          "begin": 0.0,
          "end": 4.705
        },
        {
          "text": "पु॒रु॒श्च॒न्द्रं पु᳚रु॒स्पृह᳚म् ॥",
          "begin": 4.705,
          "end": 7.52
        }
      ]
    },
    {
      "id": "9.62.13",
      "mandala": 9,
      "sukta": 62,
      "rik": 13,
      "padas": [
        {
          "text": "ए॒ष स्य परि᳚ षिच्यते मर्मृ॒ज्यमा᳚न आ॒युभि᳚ः ।",
          "begin": 0.0,
          "end": 4.49
        },
        {
          "text": "उ॒रु॒गा॒यः क॒विक्र᳚तुः ॥",
          "begin": 4.49,
          "end": 6.76
        }
      ]
    },
    {
      "id": "9.62.14",
      "mandala": 9,
      "sukta": 62,
      "rik": 14,
      "padas": [
        {
          "text": "स॒हस्रो᳚तिः श॒ताम᳚घो वि॒मानो॒ रज᳚सः क॒विः ।",
          "begin": 0.0,
          "end": 4.825
        },
        {
          "text": "इन्द्रा᳚य पवते॒ मदः᳚ ॥",
          "begin": 4.825,
          "end": 7.24
        }
      ]
    },
    {
      "id": "9.62.15",
      "mandala": 9,
      "sukta": 62,
      "rik": 15,
      "padas": [
        {
          "text": "गि॒रा जा॒त इ॒ह स्तु॒त इन्दु॒रिन्द्रा᳚य धीयते ।",
          "begin": 0.0,
          "end": 3.976
        },
        {
          "text": "विर्योना᳚ वस॒तावि᳚व ॥",
          "begin": 3.976,
          "end": 7.08
        }
      ]
    },
    {
      "id": "9.62.16",
      "mandala": 9,
      "sukta": 62,
      "rik": 16,
      "padas": [
        {
          "text": "पव᳚मानः सु॒तो नृभि॒ः सोमो॒ वाज᳚मिवासरत् ।",
          "begin": 0.0,
          "end": 4.145
        },
        {
          "text": "च॒मूषु॒ शक्म᳚ना॒सद᳚म् ॥",
          "begin": 4.145,
          "end": 6.76
        }
      ]
    },
    {
      "id": "9.62.17",
      "mandala": 9,
      "sukta": 62,
      "rik": 17,
      "padas": [
        {
          "text": "तं त्रि᳚पृ॒ष्ठे त्रि᳚वन्धु॒रे रथे᳚ युञ्जन्ति॒ यात᳚वे ।",
          "begin": 0.0,
          "end": 5.033
        },
        {
          "text": "ऋषी᳚णां स॒प्त धी॒तिभि᳚ः ॥",
          "begin": 5.033,
          "end": 7.72
        }
      ]
    },
    {
      "id": "9.62.18",
      "mandala": 9,
      "sukta": 62,
      "rik": 18,
      "padas": [
        {
          "text": "तं सो᳚तारो धन॒स्पृत᳚मा॒शुं वाजा᳚य॒ यात᳚वे ।",
          "begin": 0.0,
          "end": 5.284
        },
        {
          "text": "हरिं᳚ हिनोत वा॒जिन᳚म् ॥",
          "begin": 5.284,
          "end": 7.92
        }
      ]
    },
    {
      "id": "9.62.19",
      "mandala": 9,
      "sukta": 62,
      "rik": 19,
      "padas": [
        {
          "text": "आ॒वि॒शन्क॒लशं᳚ सु॒तो विश्वा॒ अर्ष᳚न्न॒भि श्रियः᳚ ।",
          "begin": 0.0,
          "end": 5.421
        },
        {
          "text": "शूरो॒ न गोषु᳚ तिष्ठति ॥",
          "begin": 5.421,
          "end": 7.6
        }
      ]
    },
    {
      "id": "9.62.20",
      "mandala": 9,
      "sukta": 62,
      "rik": 20,
      "padas": [
        {
          "text": "आ त᳚ इन्दो॒ मदा᳚य॒ कं पयो᳚ दुहन्त्या॒यवः᳚ ।",
          "begin": 0.0,
          "end": 4.963
        },
        {
          "text": "दे॒वा दे॒वेभ्यो॒ मधु᳚ ॥",
          "begin": 4.963,
          "end": 7.2
        }
      ]
    },
    {
      "id": "9.62.21",
      "mandala": 9,
      "sukta": 62,
      "rik": 21,
      "padas": [
        {
          "text": "आ न॒ः सोमं᳚ प॒वित्र॒ आ सृ॒जता॒ मधु᳚मत्तमम् ।",
          "begin": 0.0,
          "end": 4.452
        },
        {
          "text": "दे॒वेभ्यो᳚ देव॒श्रुत्त᳚मम् ॥",
          "begin": 4.452,
          "end": 7.68
        }
      ]
    },
    {
      "id": "9.62.22",
      "mandala": 9,
      "sukta": 62,
      "rik": 22,
      "padas": [
        {
          "text": "ए॒ते सोमा᳚ असृक्षत गृणा॒नाः श्रव᳚से म॒हे ।",
          "begin": 0.0,
          "end": 4.883
        },
        {
          "text": "म॒दिन्त᳚मस्य॒ धार᳚या ॥",
          "begin": 4.883,
          "end": 6.84
        }
      ]
    },
    {
      "id": "9.62.23",
      "mandala": 9,
      "sukta": 62,
      "rik": 23,
      "padas": [
        {
          "text": "अ॒भि गव्या᳚नि वी॒तये᳚ नृ॒म्णा पु᳚ना॒नो अ᳚र्षसि ।",
          "begin": 0.0,
          "end": 5.314
        },
        {
          "text": "स॒नद्वा᳚ज॒ः परि᳚ स्रव ॥",
          "begin": 5.314,
          "end": 7.72
        }
      ]
    },
    {
      "id": "9.62.24",
      "mandala": 9,
      "sukta": 62,
      "rik": 24,
      "padas": [
        {
          "text": "उ॒त नो॒ गोम᳚ती॒रिषो॒ विश्वा᳚ अर्ष परि॒ष्टुभः᳚ ।",
          "begin": 0.0,
          "end": 4.554
        },
        {
          "text": "गृ॒णा॒नो ज॒मद᳚ग्निना ॥",
          "begin": 4.554,
          "end": 6.64
        }
      ]
    },
    {
      "id": "9.62.25",
      "mandala": 9,
      "sukta": 62,
      "rik": 25,
      "padas": [
        {
          "text": "पव᳚स्व वा॒चो अ᳚ग्रि॒यः सोम᳚ चि॒त्राभि᳚रू॒तिभि᳚ः ।",
          "begin": 0.0,
          "end": 4.766
        },
        {
          "text": "अ॒भि विश्वा᳚नि॒ काव्या᳚ ॥",
          "begin": 4.766,
          "end": 7.4
        }
      ]
    },
    {
      "id": "9.62.26",
      "mandala": 9,
      "sukta": 62,
      "rik": 26,
      "padas": [
        {
          "text": "त्वं स᳚मु॒द्रिया᳚ अ॒पो᳚॑ग्रि॒यो वाच᳚ ई॒रय᳚न् ।",
          "begin": 0.0,
          "end": 5.107
        },
        {
          "text": "पव᳚स्व विश्वमेजय ॥",
          "begin": 5.107,
          "end": 7.12
        }
      ]
    },
    {
      "id": "9.62.27",
      "mandala": 9,
      "sukta": 62,
      "rik": 27,
      "padas": [
        {
          "text": "तुभ्ये॒मा भुव᳚ना कवे महि॒म्ने सो᳚म तस्थिरे ।",
          "begin": 0.0,
          "end": 4.608
        },
        {
          "text": "तुभ्य᳚मर्षन्ति॒ सिन्ध᳚वः ॥",
          "begin": 4.608,
          "end": 6.72
        }
      ]
    },
    {
      "id": "9.62.28",
      "mandala": 9,
      "sukta": 62,
      "rik": 28,
      "padas": [
        {
          "text": "प्र ते᳚ दि॒वो न वृ॒ष्टयो॒ धारा᳚ यन्त्यस॒श्चतः᳚ ।",
          "begin": 0.0,
          "end": 5.088
        },
        {
          "text": "अ॒भि शु॒क्रामु᳚प॒स्तिर᳚म् ॥",
          "begin": 5.088,
          "end": 7.36
        }
      ]
    },
    {
      "id": "9.62.29",
      "mandala": 9,
      "sukta": 62,
      "rik": 29,
      "padas": [
        {
          "text": "इन्द्रा॒येन्दुं᳚ पुनीतनो॒ग्रं दक्षा᳚य॒ साध᳚नम् ।",
          "begin": 0.0,
          "end": 5.535
        },
        {
          "text": "ई॒शा॒नं वी॒तिरा᳚धसम् ॥",
          "begin": 5.535,
          "end": 7.88
        }
      ]
    },
    {
      "id": "9.62.30",
      "mandala": 9,
      "sukta": 62,
      "rik": 30,
      "padas": [
        {
          "text": "पव᳚मान ऋ॒तः क॒विः सोमः᳚ प॒वित्र॒मास᳚दत् ।",
          "begin": 0.0,
          "end": 4.507
        },
        {
          "text": "दध᳚त्स्तो॒त्रे सु॒वीर्य᳚म् ॥",
          "begin": 4.507,
          "end": 8.8
        }
      ]
    }
  ]
}