{
  "mandala": 9,
  "sukta": 26,
  "verses": [
    {
      "id": "9.26.1",
      "mandala": 9,
      "sukta": 26,
      "rik": 1,
      "padas": [
        {
          "text": "तम᳚मृक्षन्त वा॒जिन᳚मु॒पस्थे॒ अदि᳚ते॒रधि᳚ ।",
          "begin": 0.0,
          "end": 7.206
        },
        {
          "text": "विप्रा᳚सो॒ अण्व्या᳚ धि॒या ॥",
          "begin": 7.206,
          "end": 10.92
        }
      ]
    },
    {
      "id": "9.26.2",
      "mandala": 9,
      "sukta": 26,
      "rik": 2,
      "padas": [
        {
          "text": "तं गावो᳚ अ॒भ्य᳚नूषत स॒हस्र᳚धार॒मक्षि᳚तम् ।",
          "begin": 0.0,
          "end": 5.348
        },
        {
          "text": "इन्दुं᳚ ध॒र्तार॒मा दि॒वः ॥",
          "begin": 5.348,
          "end": 8.32
        }
      ]
    },
    {
      "id": "9.26.3",
      "mandala": 9,
      "sukta": 26,
      "rik": 3,
      "padas": [
        {
          "text": "तं वे॒धां मे॒धया᳚ह्य॒न्पव᳚मान॒मधि॒ द्यवि᳚ ।",
          "begin": 0.0,
          "end": 5.433
        },
        {
          "text": "ध॒र्ण॒सिं भूरि᳚धायसम् ॥",
          "begin": 5.433,
          "end": 7.8
        }
      ]
    },
    {
      "id": "9.26.4",
      "mandala": 9,
      "sukta": 26,
      "rik": 4,
      "padas": [
        {
          "text": "तम᳚ह्यन्भु॒रिजो᳚र्धि॒या सं॒वसा᳚नं वि॒वस्व᳚तः ।",
          "begin": 0.0,
          "end": 5.489
        },
        {
          "text": "पतिं᳚ वा॒चो अदा᳚भ्यम् ॥",
          "begin": 5.489,
          "end": 8.92
        }
      ]
    },
    {
      "id": "9.26.5",
      "mandala": 9,
      "sukta": 26,
      "rik": 5,
      "padas": [
        {
          "text": "तं साना॒वधि᳚ जा॒मयो॒ हरिं᳚ हिन्व॒न्त्यद्रि᳚भिः ।",
          "begin": 0.0,
          "end": 4.091
        },
        {
          "text": "ह॒र्य॒तं भूरि᳚चक्षसम् ॥",
          "begin": 4.091,
          "end": 7.28
        }
      ]
    },
    {
      "id": "9.26.6",
      "mandala": 9,
      "sukta": 26,
      "rik": 6,
      "padas": [
        {
          "text": "तं त्वा᳚ हिन्वन्ति वे॒धस॒ः पव᳚मान गिरा॒वृध᳚म् ।",
          "begin": 0.0,
          "end": 5.649
        },
        {
          "text": "इन्द॒विन्द्रा᳚य मत्स॒रम् ॥",
          "begin": 5.649,
          "end": 10.28
        }
      ]
    }
  ]
}