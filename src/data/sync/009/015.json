{
  "mandala": 9,
  "sukta": 15,
  "verses": [
    {
      "id": "9.15.1",
      "mandala": 9,
      "sukta": 15,
      "rik": 1,
      "padas": [
        {
          "text": "ए॒ष धि॒या या॒त्यण्व्या॒ शूरो॒ रथे᳚भिरा॒शुभि᳚ः ।",
          "begin": 0.0,
          "end": 7.715
        },
        {
          "text": "गच्छ॒न्निन्द्र᳚स्य निष्कृ॒तम् ॥",
          "begin": 7.715,
          "end": 10.68
        }
      ]
    },
    {
      "id": "9.15.2",
      "mandala": 9,
      "sukta": 15,
      "rik": 2,
      "padas": [
        {
          "text": "ए॒ष पु॒रू धि᳚यायते बृह॒ते दे॒वता᳚तये ।",
          "begin": 0.0,
          "end": 5.027
        },
        {
          "text": "यत्रा॒मृता᳚स॒ आस᳚ते ॥",
          "begin": 5.027,
          "end": 7.88
        }
      ]
    },
    {
      "id": "9.15.3",
      "mandala": 9,
      "sukta": 15,
      "rik": 3,
      "padas": [
        {
          "text": "ए॒ष हि॒तो वि नी᳚यते॒॑न्तः शु॒भ्राव᳚ता प॒था ।",
          "begin": 0.0,
          "end": 4.702
        },
        {
          "text": "यदी᳚ तु॒ञ्जन्ति॒ भूर्ण᳚यः ॥",
          "begin": 4.702,
          "end": 8.0
        }
      ]
    },
    {
      "id": "9.15.4",
      "mandala": 9,
      "sukta": 15,
      "rik": 4,
      "padas": [
        {
          "text": "ए॒ष शृङ्गा᳚णि॒ दोधु᳚व॒च्छिशी᳚ते यू॒थ्यो॒३॒᳚ वृषा᳚ ।",
          "begin": 0.0,
          "end": 7.616
        },
        {
          "text": "नृ॒म्णा दधा᳚न॒ ओज᳚सा ॥",
          "begin": 7.616,
          "end": 10.96
        }
      ]
    },
    {
      "id": "9.15.5",
      "mandala": 9,
      "sukta": 15,
      "rik": 5,
      "padas": [
        {
          "text": "ए॒ष रु॒क्मिभि᳚रीयते वा॒जी शु॒भ्रेभि᳚रं॒शुभि᳚ः ।",
          "begin": 0.0,
          "end": 5.087
        },
        {
          "text": "पति॒ः सिन्धू᳚नां॒ भव᳚न् ॥",
          "begin": 5.087,
          "end": 7.96
        }
      ]
    },
    {
      "id": "9.15.6",
      "mandala": 9,
      "sukta": 15,
      "rik": 6,
      "padas": [
        {
          "text": "ए॒ष वसू᳚नि पिब्द॒ना परु᳚षा ययि॒वाँ अति᳚ ।",
          "begin": 0.0,
          "end": 5.407
        },
        {
          "text": "अव॒ शादे᳚षु गच्छति ॥",
          "begin": 5.407,
          "end": 7.76
        }
      ]
    },
    {
      "id": "9.15.7",
      "mandala": 9,
      "sukta": 15,
      "rik": 7,
      "padas": [
        {
          "text": "ए॒तं मृ᳚जन्ति॒ मर्ज्य॒मुप॒ द्रोणे᳚ष्वा॒यवः᳚ ।",
          "begin": 0.0,
          "end": 4.953
        },
        {
          "text": "प्र॒च॒क्रा॒णं म॒हीरिषः᳚ ॥",
          "begin": 4.953,
          "end": 7.68
        }
      ]
    },
    {
      "id": "9.15.8",
      "mandala": 9,
      "sukta": 15,
      "rik": 8,
      "padas": [
        {
          "text": "ए॒तमु॒ त्यं दश॒ क्षिपो᳚ मृ॒जन्ति᳚ स॒प्त धी॒तयः᳚ ।",
          "begin": 0.0,
          "end": 5.569
        },
        {
          "text": "स्वा॒यु॒धं म॒दिन्त᳚मम् ॥",
          "begin": 5.569,
          "end": 8.72
        }
      ]
    }
  ]
}