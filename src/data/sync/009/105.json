{
  "mandala": 9,
  "sukta": 105,
  "verses": [
    {
      "id": "9.105.1",
      "mandala": 9,
      "sukta": 105,
      "rik": 1,
      "padas": [
        {
          "text": "तं वः᳚ सखायो॒ मदा᳚य पुना॒नम॒भि गा᳚यत ।",
          "begin": 0.0,
          "end": 6.775
        },
        {
          "text": "शिशुं॒ न य॒ज्ञैः स्व᳚दयन्त गू॒र्तिभि᳚ः ॥",
          "begin": 6.775,
          "end": 10.92
        }
      ]
    },
    {
      "id": "9.105.2",
      "mandala": 9,
      "sukta": 105,
      "rik": 2,
      "padas": [
        {
          "text": "सं व॒त्स इ᳚व मा॒तृभि॒रिन्दु᳚र्हिन्वा॒नो अ᳚ज्यते ।",
          "begin": 0.0,
          "end": 5.493
        },
        {
          "text": "दे॒वा॒वीर्मदो᳚ म॒तिभि॒ः परि᳚ष्कृतः ॥",
          "begin": 5.493,
          "end": 9.36
        }
      ]
    },
    {
      "id": "9.105.3",
      "mandala": 9,
      "sukta": 105,
      "rik": 3,
      "padas": [
        {
          "text": "अ॒यं दक्षा᳚य॒ साध᳚नो॒॑यं शर्धा᳚य वी॒तये᳚ ।",
          "begin": 0.0,
          "end": 6.189
        },
        {
          "text": "अ॒यं दे॒वेभ्यो॒ मधु᳚मत्तमः सु॒तः ॥",
          "begin": 6.189,
          "end": 9.72
        }
      ]
    },
    {
      "id": "9.105.4",
      "mandala": 9,
      "sukta": 105,
      "rik": 4,
      "padas": [
        {
          "text": "गोम᳚न्न इन्दो॒ अश्व᳚वत्सु॒तः सु᳚दक्ष धन्व ।",
          "begin": 0.0,
          "end": 5.726
        },
        {
          "text": "शुचिं᳚ ते॒ वर्ण॒मधि॒ गोषु᳚ दीधरम् ॥",
          "begin": 5.726,
          "end": 9.32
        }
      ]
    },
    {
      "id": "9.105.5",
      "mandala": 9,
      "sukta": 105,
      "rik": 5,
      "padas": [
        {
          "text": "स नो᳚ हरीणां पत॒ इन्दो᳚ दे॒वप्स᳚रस्तमः ।",
          "begin": 0.0,
          "end": 6.84
        },
        {
          "text": "सखे᳚व॒ सख्ये॒ नर्यो᳚ रु॒चे भ᳚व ॥",
          "begin": 6.84,
          "end": 10.08
        }
      ]
    },
    {
      "id": "9.105.6",
      "mandala": 9,
      "sukta": 105,
      "rik": 6,
      "padas": [
        {
          "text": "सने᳚मि॒ त्वम॒स्मदाँ अदे᳚वं॒ कं चि᳚द॒त्रिण᳚म् ।",
          "begin": 0.0,
          "end": 6.795
        },
        {
          "text": "सा॒ह्वाँ इ᳚न्दो॒ परि॒ बाधो॒ अप᳚ द्व॒युम् ॥",
          "begin": 6.795,
          "end": 13.52
        }
      ]
    }
  ]
}