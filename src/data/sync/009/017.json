{
  "mandala": 9,
  "sukta": 17,
  "verses": [
    {
      "id": "9.17.1",
      "mandala": 9,
      "sukta": 17,
      "rik": 1,
      "padas": [
        {
          "text": "प्र नि॒म्नेने᳚व॒ सिन्ध᳚वो॒ घ्नन्तो᳚ वृ॒त्राणि॒ भूर्ण᳚यः ।",
          "begin": 0.0,
          "end": 7.529
        },
        {
          "text": "सोमा᳚ असृग्रमा॒शवः᳚ ॥",
          "begin": 7.529,
          "end": 10.36
        }
      ]
    },
    {
      "id": "9.17.2",
      "mandala": 9,
      "sukta": 17,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒भि सु᳚वा॒नास॒ इन्द᳚वो वृ॒ष्टयः᳚ पृथि॒वीमि᳚व ।",
          "begin": 0.0,
          "end": 4.945
        },
        {
          "text": "इन्द्रं॒ सोमा᳚सो अक्षरन् ॥",
          "begin": 4.945,
          "end": 8.2
        }
      ]
    },
    {
      "id": "9.17.3",
      "mandala": 9,
      "sukta": 17,
      "rik": 3,
      "padas": [
        {
          "text": "अत्यू᳚र्मिर्मत्स॒रो मद॒ः सोमः᳚ प॒वित्रे᳚ अर्षति ।",
          "begin": 0.0,
          "end": 5.622
        },
        {
          "text": "वि॒घ्नन्रक्षां᳚सि देव॒युः ॥",
          "begin": 5.622,
          "end": 8.76
        }
      ]
    },
    {
      "id": "9.17.4",
      "mandala": 9,
      "sukta": 17,
      "rik": 4,
      "padas": [
        {
          "text": "आ क॒लशे᳚षु धावति प॒वित्रे॒ परि᳚ षिच्यते ।",
          "begin": 0.0,
          "end": 4.983
        },
        {
          "text": "उ॒क्थैर्य॒ज्ञेषु᳚ वर्धते ॥",
          "begin": 4.983,
          "end": 7.76
        }
      ]
    },
    {
      "id": "9.17.5",
      "mandala": 9,
      "sukta": 17,
      "rik": 5,
      "padas": [
        {
          "text": "अति॒ त्री सो᳚म रोच॒ना रोह॒न्न भ्रा᳚जसे॒ दिव᳚म् ।",
          "begin": 0.0,
          "end": 6.231
        },
        {
          "text": "इ॒ष्णन्त्सूर्यं॒ न चो᳚दयः ॥",
          "begin": 6.231,
          "end": 9.48
        }
      ]
    },
    {
      "id": "9.17.6",
      "mandala": 9,
      "sukta": 17,
      "rik": 6,
      "padas": [
        {
          "text": "अ॒भि विप्रा᳚ अनूषत मू॒र्धन्य॒ज्ञस्य᳚ का॒रवः᳚ ।",
          "begin": 0.0,
          "end": 5.868
        },
        {
          "text": "दधा᳚ना॒श्चक्ष᳚सि प्रि॒यम् ॥",
          "begin": 5.868,
          "end": 8.84
        }
      ]
    },
    {
      "id": "9.17.7",
      "mandala": 9,
      "sukta": 17,
      "rik": 7,
      "padas": [
        {
          "text": "तमु᳚ त्वा वा॒जिनं॒ नरो᳚ धी॒भिर्विप्रा᳚ अव॒स्यवः᳚ ।",
          "begin": 0.0,
          "end": 5.846
        },
        {
          "text": "मृ॒जन्ति᳚ दे॒वता᳚तये ॥",
          "begin": 5.846,
          "end": 8.72
        }
      ]
    },
    {
      "id": "9.17.8",
      "mandala": 9,
      "sukta": 17,
      "rik": 8,
      "padas": [
        {
          "text": "मधो॒र्धारा॒मनु᳚ क्षर ती॒व्रः स॒धस्थ॒मास᳚दः ।",
          "begin": 0.0,
          "end": 5.038
        },
        {
          "text": "चारु᳚रृ॒ताय᳚ पी॒तये᳚ ॥",
          "begin": 5.038,
          "end": 9.64
        }
      ]
    }
  ]
}