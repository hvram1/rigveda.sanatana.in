{
  "mandala": 9,
  "sukta": 23,
  "verses": [
    {
      "id": "9.23.1",
      "mandala": 9,
      "sukta": 23,
      "rik": 1,
      "padas": [
        {
          "text": "सोमा᳚ असृग्रमा॒शवो॒ मधो॒र्मद᳚स्य॒ धार᳚या ।",
          "begin": 0.0,
          "end": 6.921
        },
        {
          "text": "अ॒भि विश्वा᳚नि॒ काव्या᳚ ॥",
          "begin": 6.921,
          "end": 10.56
        }
      ]
    },
    {
      "id": "9.23.2",
      "mandala": 9,
      "sukta": 23,
      "rik": 2,
      "padas": [
        {
          "text": "अनु᳚ प्र॒त्नास᳚ आ॒यवः᳚ प॒दं नवी᳚यो अक्रमुः ।",
          "begin": 0.0,
          "end": 5.48
        },
        {
          "text": "रु॒चे ज᳚नन्त॒ सूर्य᳚म् ॥",
          "begin": 5.48,
          "end": 7.84
        }
      ]
    },
    {
      "id": "9.23.3",
      "mandala": 9,
      "sukta": 23,
      "rik": 3,
      "padas": [
        {
          "text": "आ प᳚वमान नो भरा॒र्यो अदा᳚शुषो॒ गय᳚म् ।",
          "begin": 0.0,
          "end": 5.315
        },
        {
          "text": "कृ॒धि प्र॒जाव᳚ती॒रिषः᳚ ॥",
          "begin": 5.315,
          "end": 7.96
        }
      ]
    },
    {
      "id": "9.23.4",
      "mandala": 9,
      "sukta": 23,
      "rik": 4,
      "padas": [
        {
          "text": "अ॒भि सोमा᳚स आ॒यव॒ः पव᳚न्ते॒ मद्यं॒ मद᳚म् ।",
          "begin": 0.0,
          "end": 6.066
        },
        {
          "text": "अ॒भि कोशं᳚ मधु॒श्चुत᳚म् ॥",
          "begin": 6.066,
          "end": 8.32
        }
      ]
    },
    {
      "id": "9.23.5",
      "mandala": 9,
      "sukta": 23,
      "rik": 5,
      "padas": [
        {
          "text": "सोमो᳚ अर्षति धर्ण॒सिर्दधा᳚न इन्द्रि॒यं रस᳚म् ।",
          "begin": 0.0,
          "end": 6.052
        },
        {
          "text": "सु॒वीरो᳚ अभिशस्ति॒पाः ॥",
          "begin": 6.052,
          "end": 8.96
        }
      ]
    },
    {
      "id": "9.23.6",
      "mandala": 9,
      "sukta": 23,
      "rik": 6,
      "padas": [
        {
          "text": "इन्द्रा᳚य सोम पवसे दे॒वेभ्य᳚ः सध॒माद्य᳚ः ।",
          "begin": 0.0,
          "end": 5.56
        },
        {
          "text": "इन्दो॒ वाजं᳚ सिषाससि ॥",
          "begin": 5.56,
          "end": 8.2
        }
      ]
    },
    {
      "id": "9.23.7",
      "mandala": 9,
      "sukta": 23,
      "rik": 7,
      "padas": [
        {
          "text": "अ॒स्य पी॒त्वा मदा᳚ना॒मिन्द्रो᳚ वृ॒त्राण्य᳚प्र॒ति ।",
          "begin": 0.0,
          "end": 9.077
        },
        {
          "text": "ज॒घान᳚ ज॒घन᳚च्च॒ नु ॥",
          "begin": 9.077,
          "end": 11.76
        }
      ]
    }
  ]
}