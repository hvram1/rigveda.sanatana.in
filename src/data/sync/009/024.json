{
  "mandala": 9,
  "sukta": 24,
  "verses": [
    {
      "id": "9.24.1",
      "mandala": 9,
      "sukta": 24,
      "rik": 1,
      "padas": [
        {
          "text": "प्र सोमा᳚सो अधन्विषु॒ः पव᳚मानास॒ इन्द᳚वः ।",
          "begin": 0.0,
          "end": 6.309
        },
        {
          "text": "श्री॒णा॒ना अ॒प्सु मृ᳚ञ्जत ॥",
          "begin": 6.309,
          "end": 9.48
        }
      ]
    },
    {
      "id": "9.24.2",
      "mandala": 9,
      "sukta": 24,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒भि गावो᳚ अधन्विषु॒रापो॒ न प्र॒वता᳚ य॒तीः ।",
          "begin": 0.0,
          "end": 5.615
        },
        {
          "text": "पु॒ना॒ना इन्द्र᳚माशत ॥",
          "begin": 5.615,
          "end": 8.12
        }
      ]
    },
    {
      "id": "9.24.3",
      "mandala": 9,
      "sukta": 24,
      "rik": 3,
      "padas": [
        {
          "text": "प्र प᳚वमान धन्वसि॒ सोमेन्द्रा᳚य॒ पात᳚वे ।",
          "begin": 0.0,
          "end": 4.69
        },
        {
          "text": "नृभि᳚र्य॒तो वि नी᳚यसे ॥",
          "begin": 4.69,
          "end": 7.2
        }
      ]
    },
    {
      "id": "9.24.4",
      "mandala": 9,
      "sukta": 24,
      "rik": 4,
      "padas": [
        {
          "text": "त्वं सो᳚म नृ॒माद᳚न॒ः पव᳚स्व चर्षणी॒सहे᳚ ।",
          "begin": 0.0,
          "end": 5.209
        },
        {
          "text": "सस्नि॒र्यो अ᳚नु॒माद्य᳚ः ॥",
          "begin": 5.209,
          "end": 8.08
        }
      ]
    },
    {
      "id": "9.24.5",
      "mandala": 9,
      "sukta": 24,
      "rik": 5,
      "padas": [
        {
          "text": "इन्दो॒ यदद्रि᳚भिः सु॒तः प॒वित्रं᳚ परि॒धाव᳚सि ।",
          "begin": 0.0,
          "end": 5.089
        },
        {
          "text": "अर॒मिन्द्र᳚स्य॒ धाम्ने᳚ ॥",
          "begin": 5.089,
          "end": 7.52
        }
      ]
    },
    {
      "id": "9.24.6",
      "mandala": 9,
      "sukta": 24,
      "rik": 6,
      "padas": [
        {
          "text": "पव᳚स्व वृत्रहन्तमो॒क्थेभि᳚रनु॒माद्य᳚ः ।",
          "begin": 0.0,
          "end": 4.592
        },
        {
          "text": "शुचि᳚ः पाव॒को अद्भु᳚तः ॥",
          "begin": 4.592,
          "end": 7.12
        }
      ]
    },
    {
      "id": "9.24.7",
      "mandala": 9,
      "sukta": 24,
      "rik": 7,
      "padas": [
        {
          "text": "शुचि᳚ः पाव॒क उ᳚च्यते॒ सोमः᳚ सु॒तस्य॒ मध्व᳚ः ।",
          "begin": 0.0,
          "end": 5.252
        },
        {
          "text": "दे॒वा॒वीर᳚घशंस॒हा ॥",
          "begin": 5.252,
          "end": 9.0
        }
      ]
    }
  ]
}