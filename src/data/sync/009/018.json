  "verses": [
    {
      "id": "9.18.1",
      "mandala": 9,
      "sukta": 18,
      "rik": 1,
      "padas": [
        {
          "text": "परि᳚ सुवा॒नो गि᳚रि॒ष्ठाः प॒वित्रे॒ सोमो᳚ अक्षाः ।",
          "begin": 0.0,
          "end": 7.071
        },
        {
          "text": "मदे᳚षु सर्व॒धा अ᳚सि ॥",
          "begin": 7.071,
          "end": 10.08
        }
      ]
    },
    {
      "id": "9.18.2",
      "mandala": 9,
      "sukta": 18,
      "rik": 2,
      "padas": [
        {
          "text": "त्वं विप्र॒स्त्वं क॒विर्मधु॒ प्र जा॒तमन्ध᳚सः ।",
          "begin": 0.0,
          "end": 4.917
        },
        {
          "text": "मदे᳚षु सर्व॒धा अ᳚सि ॥",
          "begin": 4.917,
          "end": 7.44
        }
      ]
    },
    {
      "id": "9.18.3",
      "mandala": 9,
      "sukta": 18,
      "rik": 3,
      "padas": [
        {
          "text": "तव॒ विश्वे᳚ स॒जोष᳚सो दे॒वासः᳚ पी॒तिमा᳚शत ।",
          "begin": 0.0,
          "end": 5.277
        },
        {
          "text": "मदे᳚षु सर्व॒धा अ᳚सि ॥",
          "begin": 5.277,
          "end": 8.36
        }
      ]
    },
    {
      "id": "9.18.4",
      "mandala": 9,
      "sukta": 18,
      "rik": 4,
      "padas": [
        {
          "text": "आ यो विश्वा᳚नि॒ वार्या॒ वसू᳚नि॒ हस्त᳚योर्द॒धे ।",
          "begin": 0.0,
          "end": 6.143
        },
        {
          "text": "मदे᳚षु सर्व॒धा अ᳚सि ॥",
          "begin": 6.143,
          "end": 8.68
        }
      ]
    },
    {
      "id": "9.18.5",
      "mandala": 9,
      "sukta": 18,
      "rik": 5,
      "padas": [
        {
          "text": "य इ॒मे रोद᳚सी म॒ही सं मा॒तरे᳚व॒ दोह᳚ते ।",
          "begin": 0.0,
          "end": 3.26
        },
        {
          "text": "मदे᳚षु सर्व॒धा अ᳚सि ॥",
          "begin": 3.26,
          "end": 7.88
        }
      ]
    },
    {
      "id": "9.18.6",
      "mandala": 9,
      "sukta": 18,
      "rik": 6,
      "padas": [
        {
          "text": "परि॒ यो रोद᳚सी उ॒भे स॒द्यो वाजे᳚भि॒रर्ष᳚ति ।",
          "begin": 0.0,
          "end": 5.502
        },
        {
          "text": "मदे᳚षु सर्व॒धा अ᳚सि ॥",
          "begin": 5.502,
          "end": 8.36
        }
      ]
    },
    {
      "id": "9.18.7",
      "mandala": 9,
      "sukta": 18,
      "rik": 7,
      "padas": [
        {
          "text": "स शु॒ष्मी क॒लशे॒ष्वा पु᳚ना॒नो अ᳚चिक्रदत् ।",
          "begin": 0.0,
          "end": 5.697
        },
        {
          "text": "मदे᳚षु सर्व॒धा अ᳚सि ॥",
          "begin": 5.697,
          "end": 10.24
        }
      ]
    }
  ]
}