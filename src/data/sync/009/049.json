{
  "mandala": 9,
  "sukta": 49,
  "verses": [
    {
      "id": "9.49.1",
      "mandala": 9,
      "sukta": 49,
      "rik": 1,
      "padas": [
        {
          "text": "पव᳚स्व वृ॒ष्टिमा सु नो॒॑पामू॒र्मिं दि॒वस्परि᳚ ।",
          "begin": 0.0,
          "end": 5.476
        },
        {
          "text": "अ॒य॒क्ष्मा बृ᳚ह॒तीरिषः᳚ ॥",
          "begin": 5.476,
          "end": 7.72
        }
      ]
    },
    {
      "id": "9.49.2",
      "mandala": 9,
      "sukta": 49,
      "rik": 2,
      "padas": [
        {
          "text": "तया᳚ पवस्व॒ धार᳚या॒ यया॒ गाव᳚ इ॒हागम᳚न् ।",
          "begin": 0.0,
          "end": 4.651
        },
        {
          "text": "जन्या᳚स॒ उप᳚ नो गृ॒हम् ॥",
          "begin": 4.651,
          "end": 7.08
        }
      ]
    },
    {
      "id": "9.49.3",
      "mandala": 9,
      "sukta": 49,
      "rik": 3,
      "padas": [
        {
          "text": "घृ॒तं प᳚वस्व॒ धार᳚या य॒ज्ञेषु᳚ देव॒वीत᳚मः ।",
          "begin": 0.0,
          "end": 4.404
        },
        {
          "text": "अ॒स्मभ्यं᳚ वृ॒ष्टिमा प᳚व ॥",
          "begin": 4.404,
          "end": 6.88
        }
      ]
    },
    {
      "id": "9.49.4",
      "mandala": 9,
      "sukta": 49,
      "rik": 4,
      "padas": [
        {
          "text": "स न᳚ ऊ॒र्जे व्य१॒᳚व्ययं᳚ प॒वित्रं᳚ धाव॒ धार᳚या ।",
          "begin": 0.0,
          "end": 5.995
        },
        {
          "text": "दे॒वासः᳚ शृ॒णव॒न्हि क᳚म् ॥",
          "begin": 5.995,
          "end": 8.52
        }
      ]
    },
    {
      "id": "9.49.5",
      "mandala": 9,
      "sukta": 49,
      "rik": 5,
      "padas": [
        {
          "text": "पव᳚मानो असिष्यद॒द्रक्षां᳚स्यप॒जङ्घ᳚नत् ।",
          "begin": 0.0,
          "end": 4.715
        },
        {
          "text": "प्र॒त्न॒वद्रो॒चय॒न्रुचः᳚ ॥",
          "begin": 4.715,
          "end": 9.16
        }
      ]
    }
  ]
}