{
  "mandala": 9,
  "sukta": 45,
  "verses": [
    {
      "id": "9.45.1",
      "mandala": 9,
      "sukta": 45,
      "rik": 1,
      "padas": [
        {
          "text": "स प᳚वस्व॒ मदा᳚य॒ कं नृ॒चक्षा᳚ दे॒ववी᳚तये ।",
          "begin": 0.0,
          "end": 6.897
        },
        {
          "text": "इन्द॒विन्द्रा᳚य पी॒तये᳚ ॥",
          "begin": 6.897,
          "end": 10.4
        }
      ]
    },
    {
      "id": "9.45.2",
      "mandala": 9,
      "sukta": 45,
      "rik": 2,
      "padas": [
        {
          "text": "स नो᳚ अर्षा॒भि दू॒त्यं१॒᳚ त्वमिन्द्रा᳚य तोशसे ।",
          "begin": 0.0,
          "end": 6.116
        },
        {
          "text": "दे॒वान्त्सखि᳚भ्य॒ आ वर᳚म् ॥",
          "begin": 6.116,
          "end": 8.96
        }
      ]
    },
    {
      "id": "9.45.3",
      "mandala": 9,
      "sukta": 45,
      "rik": 3,
      "padas": [
        {
          "text": "उ॒त त्वाम᳚रु॒णं व॒यं गोभि᳚रञ्ज्मो॒ मदा᳚य॒ कम् ।",
          "begin": 0.0,
          "end": 4.885
        },
        {
          "text": "वि नो᳚ रा॒ये दुरो᳚ वृधि ॥",
          "begin": 4.885,
          "end": 8.08
        }
      ]
    },
    {
      "id": "9.45.4",
      "mandala": 9,
      "sukta": 45,
      "rik": 4,
      "padas": [
        {
          "text": "अत्यू᳚ प॒वित्र᳚मक्रमीद्वा॒जी धुरं॒ न याम᳚नि ।",
          "begin": 0.0,
          "end": 5.131
        },
        {
          "text": "इन्दु᳚र्दे॒वेषु᳚ पत्यते ॥",
          "begin": 5.131,
          "end": 7.52
        }
      ]
    },
    {
      "id": "9.45.5",
      "mandala": 9,
      "sukta": 45,
      "rik": 5,
      "padas": [
        {
          "text": "समी॒ सखा᳚यो अस्वर॒न्वने॒ क्रीळ᳚न्त॒मत्य᳚विम् ।",
          "begin": 0.0,
          "end": 5.912
        },
        {
          "text": "इन्दुं᳚ ना॒वा अ᳚नूषत ॥",
          "begin": 5.912,
          "end": 8.72
        }
      ]
    },
    {
      "id": "9.45.6",
      "mandala": 9,
      "sukta": 45,
      "rik": 6,
      "padas": [
        {
          "text": "तया᳚ पवस्व॒ धार᳚या॒ यया᳚ पी॒तो वि॒चक्ष᳚से ।",
          "begin": 0.0,
          "end": 7.572
        },
        {
          "text": "इन्दो᳚ स्तो॒त्रे सु॒वीर्य᳚म् ॥",
          "begin": 7.572,
          "end": 10.44
        }
      ]
    }
  ]
}