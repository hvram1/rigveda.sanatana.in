{
  "mandala": 9,
  "sukta": 13,
  "verses": [
    {
      "id": "9.13.1",
      "mandala": 9,
      "sukta": 13,
      "rik": 1,
      "padas": [
        {
          "text": "सोमः᳚ पुना॒नो अ᳚र्षति स॒हस्र᳚धारो॒ अत्य᳚विः ।",
          "begin": 0.0,
          "end": 6.173
        },
        {
          "text": "वा॒योरिन्द्र᳚स्य निष्कृ॒तम् ॥",
          "begin": 6.173,
          "end": 9.08
        }
      ]
    },
    {
      "id": "9.13.2",
      "mandala": 9,
      "sukta": 13,
      "rik": 2,
      "padas": [
        {
          "text": "पव᳚मानमवस्यवो॒ विप्र᳚म॒भि प्र गा᳚यत ।",
          "begin": 0.0,
          "end": 5.128
        },
        {
          "text": "सु॒ष्वा॒णं दे॒ववी᳚तये ॥",
          "begin": 5.128,
          "end": 8.12
        }
      ]
    },
    {
      "id": "9.13.3",
      "mandala": 9,
      "sukta": 13,
      "rik": 3,
      "padas": [
        {
          "text": "पव᳚न्ते॒ वाज᳚सातये॒ सोमा᳚ः स॒हस्र᳚पाजसः ।",
          "begin": 0.0,
          "end": 6.085
        },
        {
          "text": "गृ॒णा॒ना दे॒ववी᳚तये ॥",
          "begin": 6.085,
          "end": 9.0
        }
      ]
    },
    {
      "id": "9.13.4",
      "mandala": 9,
      "sukta": 13,
      "rik": 4,
      "padas": [
        {
          "text": "उ॒त नो॒ वाज᳚सातये॒ पव᳚स्व बृह॒तीरिषः᳚ ।",
          "begin": 0.0,
          "end": 5.918
        },
        {
          "text": "द्यु॒मदि᳚न्दो सु॒वीर्य᳚म् ॥",
          "begin": 5.918,
          "end": 8.12
        }
      ]
    },
    {
      "id": "9.13.5",
      "mandala": 9,
      "sukta": 13,
      "rik": 5,
      "padas": [
        {
          "text": "ते नः᳚ सह॒स्रिणं᳚ र॒यिं पव᳚न्ता॒मा सु॒वीर्य᳚म् ।",
          "begin": 0.0,
          "end": 6.196
        },
        {
          "text": "सु॒वा॒ना दे॒वास॒ इन्द᳚वः ॥",
          "begin": 6.196,
          "end": 9.16
        }
      ]
    },
    {
      "id": "9.13.6",
      "mandala": 9,
      "sukta": 13,
      "rik": 6,
      "padas": [
        {
          "text": "अत्या᳚ हिया॒ना न हे॒तृभि॒रसृ᳚ग्रं॒ वाज᳚सातये ।",
          "begin": 0.0,
          "end": 6.072
        },
        {
          "text": "वि वार॒मव्य᳚मा॒शवः᳚ ॥",
          "begin": 6.072,
          "end": 8.56
        }
      ]
    },
    {
      "id": "9.13.7",
      "mandala": 9,
      "sukta": 13,
      "rik": 7,
      "padas": [
        {
          "text": "वा॒श्रा अ᳚र्ष॒न्तीन्द᳚वो॒॑भि व॒त्सं न धे॒नवः᳚ ।",
          "begin": 0.0,
          "end": 6.133
        },
        {
          "text": "द॒ध॒न्वि॒रे गभ᳚स्त्योः ॥",
          "begin": 6.133,
          "end": 8.72
        }
      ]
    },
    {
      "id": "9.13.8",
      "mandala": 9,
      "sukta": 13,
      "rik": 8,
      "padas": [
        {
          "text": "जुष्ट॒ इन्द्रा᳚य मत्स॒रः पव᳚मान॒ कनि᳚क्रदत् ।",
          "begin": 0.0,
          "end": 6.341
        },
        {
          "text": "विश्वा॒ अप॒ द्विषो᳚ जहि ॥",
          "begin": 6.341,
          "end": 8.52
        }
      ]
    },
    {
      "id": "9.13.9",
      "mandala": 9,
      "sukta": 13,
      "rik": 9,
      "padas": [
        {
          "text": "अ॒प॒घ्नन्तो॒ अरा᳚व्ण॒ः पव᳚मानाः स्व॒र्दृशः᳚ ।",
          "begin": 0.0,
          "end": 8.64
        },
        {
          "text": "योना᳚वृ॒तस्य᳚ सीदत ॥",
          "begin": 8.64,
          "end": 10.48
        }
      ]
    }
  ]
}