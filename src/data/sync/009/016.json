{
  "mandala": 9,
  "sukta": 16,
  "verses": [
    {
      "id": "9.16.1",
      "mandala": 9,
      "sukta": 16,
      "rik": 1,
      "padas": [
        {
          "text": "प्र ते᳚ सो॒तार᳚ ओ॒ण्यो॒३॒᳚ रसं॒ मदा᳚य॒ घृष्व᳚ये ।",
          "begin": 0.0,
          "end": 8.78
        },
        {
          "text": "सर्गो॒ न त॒क्त्येत᳚शः ॥",
          "begin": 8.78,
          "end": 11.4
        }
      ]
    },
    {
      "id": "9.16.2",
      "mandala": 9,
      "sukta": 16,
      "rik": 2,
      "padas": [
        {
          "text": "क्रत्वा॒ दक्ष᳚स्य र॒थ्य᳚म॒पो वसा᳚न॒मन्ध᳚सा ।",
          "begin": 0.0,
          "end": 4.883
        },
        {
          "text": "गो॒षामण्वे᳚षु सश्चिम ॥",
          "begin": 4.883,
          "end": 7.68
        }
      ]
    },
    {
      "id": "9.16.3",
      "mandala": 9,
      "sukta": 16,
      "rik": 3,
      "padas": [
        {
          "text": "अन᳚प्तम॒प्सु दु॒ष्टरं॒ सोमं᳚ प॒वित्र॒ आ सृ᳚ज ।",
          "begin": 0.0,
          "end": 5.03
        },
        {
          "text": "पु॒नी॒हीन्द्रा᳚य॒ पात᳚वे ॥",
          "begin": 5.03,
          "end": 8.04
        }
      ]
    },
    {
      "id": "9.16.4",
      "mandala": 9,
      "sukta": 16,
      "rik": 4,
      "padas": [
        {
          "text": "प्र पु᳚ना॒नस्य॒ चेत᳚सा॒ सोमः᳚ प॒वित्रे᳚ अर्षति ।",
          "begin": 0.0,
          "end": 4.938
        },
        {
          "text": "क्रत्वा᳚ स॒धस्थ॒मास᳚दत् ॥",
          "begin": 4.938,
          "end": 8.12
        }
      ]
    },
    {
      "id": "9.16.5",
      "mandala": 9,
      "sukta": 16,
      "rik": 5,
      "padas": [
        {
          "text": "प्र त्वा॒ नमो᳚भि॒रिन्द᳚व॒ इन्द्र॒ सोमा᳚ असृक्षत ।",
          "begin": 0.0,
          "end": 4.92
        },
        {
          "text": "म॒हे भरा᳚य का॒रिणः᳚ ॥",
          "begin": 4.92,
          "end": 8.12
        }
      ]
    },
    {
      "id": "9.16.6",
      "mandala": 9,
      "sukta": 16,
      "rik": 6,
      "padas": [
        {
          "text": "पु॒ना॒नो रू॒पे अ॒व्यये॒ विश्वा॒ अर्ष᳚न्न॒भि श्रियः᳚ ।",
          "begin": 0.0,
          "end": 6.288
        },
        {
          "text": "शूरो॒ न गोषु᳚ तिष्ठति ॥",
          "begin": 6.288,
          "end": 8.64
        }
      ]
    },
    {
      "id": "9.16.7",
      "mandala": 9,
      "sukta": 16,
      "rik": 7,
      "padas": [
        {
          "text": "दि॒वो न सानु᳚ पि॒प्युषी॒ धारा᳚ सु॒तस्य᳚ वे॒धसः᳚ ।",
          "begin": 0.0,
          "end": 4.66
        },
        {
          "text": "वृथा᳚ प॒वित्रे᳚ अर्षति ॥",
          "begin": 4.66,
          "end": 8.24
        }
      ]
    },
    {
      "id": "9.16.8",
      "mandala": 9,
      "sukta": 16,
      "rik": 8,
      "padas": [
        {
          "text": "त्वं सो᳚म विप॒श्चितं॒ तना᳚ पुना॒न आ॒युषु᳚ ।",
          "begin": 0.0,
          "end": 5.974
        },
        {
          "text": "अव्यो॒ वारं॒ वि धा᳚वसि ॥",
          "begin": 5.974,
          "end": 8.88
        }
      ]
    }
  ]
}