{
  "mandala": 9,
  "sukta": 104,
  "verses": [
    {
      "id": "9.104.1",
      "mandala": 9,
      "sukta": 104,
      "rik": 1,
      "padas": [
        {
          "text": "सखा᳚य॒ आ नि षी᳚दत पुना॒नाय॒ प्र गा᳚यत ।",
          "begin": 0.0,
          "end": 7.896
        },
        {
          "text": "शिशुं॒ न य॒ज्ञैः परि᳚ भूषत श्रि॒ये ॥",
          "begin": 7.896,
          "end": 11.84
        }
      ]
    },
    {
      "id": "9.104.2",
      "mandala": 9,
      "sukta": 104,
      "rik": 2,
      "padas": [
        {
          "text": "समी᳚ व॒त्सं न मा॒तृभि᳚ः सृ॒जता᳚ गय॒साध᳚नम् ।",
          "begin": 0.0,
          "end": 5.556
        },
        {
          "text": "दे॒वा॒व्यं१॒᳚ मद᳚म॒भि द्विश᳚वसम् ॥",
          "begin": 5.556,
          "end": 9.64
        }
      ]
    },
    {
      "id": "9.104.3",
      "mandala": 9,
      "sukta": 104,
      "rik": 3,
      "padas": [
        {
          "text": "पु॒नाता᳚ दक्ष॒साध᳚नं॒ यथा॒ शर्धा᳚य वी॒तये᳚ ।",
          "begin": 0.0,
          "end": 5.275
        },
        {
          "text": "यथा᳚ मि॒त्राय॒ वरु᳚णाय॒ शंत᳚मः ॥",
          "begin": 5.275,
          "end": 10.16
        }
      ]
    },
    {
      "id": "9.104.4",
      "mandala": 9,
      "sukta": 104,
      "rik": 4,
      "padas": [
        {
          "text": "अ॒स्मभ्यं᳚ त्वा वसु॒विद᳚म॒भि वाणी᳚रनूषत ।",
          "begin": 0.0,
          "end": 3.663
        },
        {
          "text": "गोभि᳚ष्टे॒ वर्ण᳚म॒भि वा᳚सयामसि ॥",
          "begin": 3.663,
          "end": 9.36
        }
      ]
    },
    {
      "id": "9.104.5",
      "mandala": 9,
      "sukta": 104,
      "rik": 5,
      "padas": [
        {
          "text": "स नो᳚ मदानां पत॒ इन्दो᳚ दे॒वप्स᳚रा असि ।",
          "begin": 0.0,
          "end": 6.326
        },
        {
          "text": "सखे᳚व॒ सख्ये᳚ गातु॒वित्त᳚मो भव ॥",
          "begin": 6.326,
          "end": 10.44
        }
      ]
    },
    {
      "id": "9.104.6",
      "mandala": 9,
      "sukta": 104,
      "rik": 6,
      "padas": [
        {
          "text": "सने᳚मि कृ॒ध्य१॒᳚स्मदा र॒क्षसं॒ कं चि᳚द॒त्रिण᳚म् ।",
          "begin": 0.0,
          "end": 6.181
        },
        {
          "text": "अपादे᳚वं द्व॒युमंहो᳚ युयोधि नः ॥",
          "begin": 6.181,
          "end": 11.96
        }
      ]
    }
  ]
}