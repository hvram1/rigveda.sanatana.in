{
  "mandala": 9,
  "sukta": 1,
  "verses": [
    {
      "id": "9.1.1",
      "mandala": 9,
      "sukta": 1,
      "rik": 1,
      "padas": [
        {
          "text": "स्वादि᳚ष्ठया॒ मदि᳚ष्ठया॒ पव᳚स्व सोम॒ धार᳚या ।",
          "begin": 0.0,
          "end": 5.398
        },
        {
          "text": "इन्द्रा᳚य॒ पात᳚वे सु॒तः ॥",
          "begin": 5.398,
          "end": 8.48
        }
      ]
    },
    {
      "id": "9.1.2",
      "mandala": 9,
      "sukta": 1,
      "rik": 2,
      "padas": [
        {
          "text": "र॒क्षो॒हा वि॒श्वच᳚र्षणिर॒भि योनि॒मयो᳚हतम् ।",
          "begin": 0.0,
          "end": 5.415
        },
        {
          "text": "द्रुणा᳚ स॒धस्थ॒मास᳚दत् ॥",
          "begin": 5.415,
          "end": 8.12
        }
      ]
    },
    {
      "id": "9.1.3",
      "mandala": 9,
      "sukta": 1,
      "rik": 3,
      "padas": [
        {
          "text": "व॒रि॒वो॒धात᳚मो भव॒ मंहि᳚ष्ठो वृत्र॒हन्त᳚मः ।",
          "begin": 0.0,
          "end": 4.923
        },
        {
          "text": "पर्षि॒ राधो᳚ म॒घोना᳚म् ॥",
          "begin": 4.923,
          "end": 7.96
        }
      ]
    },
    {
      "id": "9.1.4",
      "mandala": 9,
      "sukta": 1,
      "rik": 4,
      "padas": [
        {
          "text": "अ॒भ्य᳚र्ष म॒हानां᳚ दे॒वानां᳚ वी॒तिमन्ध᳚सा ।",
          "begin": 0.0,
          "end": 5.455
        },
        {
          "text": "अ॒भि वाज᳚मु॒त श्रवः᳚ ॥",
          "begin": 5.455,
          "end": 7.72
        }
      ]
    },
    {
      "id": "9.1.5",
      "mandala": 9,
      "sukta": 1,
      "rik": 5,
      "padas": [
        {
          "text": "त्वामच्छा᳚ चरामसि॒ तदिदर्थं᳚ दि॒वेदि᳚वे ।",
          "begin": 0.0,
          "end": 5.405
        },
        {
          "text": "इन्दो॒ त्वे न᳚ आ॒शसः᳚ ॥",
          "begin": 5.405,
          "end": 8.2
        }
      ]
    },
    {
      "id": "9.1.6",
      "mandala": 9,
      "sukta": 1,
      "rik": 6,
      "padas": [
        {
          "text": "पु॒नाति᳚ ते परि॒स्रुतं॒ सोमं॒ सूर्य᳚स्य दुहि॒ता ।",
          "begin": 0.0,
          "end": 5.424
        },
        {
          "text": "वारे᳚ण॒ शश्व᳚ता॒ तना᳚ ॥",
          "begin": 5.424,
          "end": 8.28
        }
      ]
    },
    {
      "id": "9.1.7",
      "mandala": 9,
      "sukta": 1,
      "rik": 7,
      "padas": [
        {
          "text": "तमी॒मण्वी᳚ः सम॒र्य आ गृ॒भ्णन्ति॒ योष᳚णो॒ दश᳚ ।",
          "begin": 0.0,
          "end": 6.842
        },
        {
          "text": "स्वसा᳚र॒ः पार्ये᳚ दि॒वि ॥",
          "begin": 6.842,
          "end": 8.8
        }
      ]
    },
    {
      "id": "9.1.8",
      "mandala": 9,
      "sukta": 1,
      "rik": 8,
      "padas": [
        {
          "text": "तमीं᳚ हिन्वन्त्य॒ग्रुवो॒ धम᳚न्ति बाकु॒रं दृति᳚म् ।",
          "begin": 0.0,
          "end": 5.799
        },
        {
          "text": "त्रि॒धातु᳚ वार॒णं मधु᳚ ॥",
          "begin": 5.799,
          "end": 8.12
        }
      ]
    },
    {
      "id": "9.1.9",
      "mandala": 9,
      "sukta": 1,
      "rik": 9,
      "padas": [
        {
          "text": "अ॒भी॒३॒᳚ममघ्न्या᳚ उ॒त श्री॒णन्ति᳚ धे॒नव॒ः शिशु᳚म् ।",
          "begin": 0.0,
          "end": 7.621
        },
        {
          "text": "सोम॒मिन्द्रा᳚य॒ पात᳚वे ॥",
          "begin": 7.621,
          "end": 10.68
        }
      ]
    },
    {
      "id": "9.1.10",
      "mandala": 9,
      "sukta": 1,
      "rik": 10,
      "padas": [
        {
          "text": "अ॒स्येदिन्द्रो॒ मदे॒ष्वा विश्वा᳚ वृ॒त्राणि᳚ जिघ्नते ।",
          "begin": 0.0,
          "end": 6.628
        },
        {
          "text": "शूरो᳚ म॒घा च᳚ मंहते ॥",
          "begin": 6.628,
          "end": 10.92
        }
      ]
    }
  ]
}