{
  "mandala": 9,
  "sukta": 41,
  "verses": [
    {
      "id": "9.41.1",
      "mandala": 9,
      "sukta": 41,
      "rik": 1,
      "padas": [
        {
          "text": "प्र ये गावो॒ न भूर्ण᳚यस्त्वे॒षा अ॒यासो॒ अक्र᳚मुः ।",
          "begin": 0.0,
          "end": 7.593
        },
        {
          "text": "घ्नन्त᳚ः कृ॒ष्णामप॒ त्वच᳚म् ॥",
          "begin": 7.593,
          "end": 10.56
        }
      ]
    },
    {
      "id": "9.41.2",
      "mandala": 9,
      "sukta": 41,
      "rik": 2,
      "padas": [
        {
          "text": "सु॒वि॒तस्य᳚ मनाम॒हे॑ति॒ सेतुं᳚ दुरा॒व्य᳚म् ।",
          "begin": 0.0,
          "end": 5.026
        },
        {
          "text": "सा॒ह्वांसो॒ दस्यु᳚मव्र॒तम् ॥",
          "begin": 5.026,
          "end": 8.0
        }
      ]
    },
    {
      "id": "9.41.3",
      "mandala": 9,
      "sukta": 41,
      "rik": 3,
      "padas": [
        {
          "text": "शृ॒ण्वे वृ॒ष्टेरि᳚व स्व॒नः पव᳚मानस्य शु॒ष्मिणः᳚ ।",
          "begin": 0.0,
          "end": 5.118
        },
        {
          "text": "चर᳚न्ति वि॒द्युतो᳚ दि॒वि ॥",
          "begin": 5.118,
          "end": 7.8
        }
      ]
    },
    {
      "id": "9.41.4",
      "mandala": 9,
      "sukta": 41,
      "rik": 4,
      "padas": [
        {
          "text": "आ प᳚वस्व म॒हीमिषं॒ गोम᳚दिन्दो॒ हिर᳚ण्यवत् ।",
          "begin": 0.0,
          "end": 5.197
        },
        {
          "text": "अश्वा᳚व॒द्वाज᳚वत्सु॒तः ॥",
          "begin": 5.197,
          "end": 7.88
        }
      ]
    },
    {
      "id": "9.41.5",
      "mandala": 9,
      "sukta": 41,
      "rik": 5,
      "padas": [
        {
          "text": "स प᳚वस्व विचर्षण॒ आ म॒ही रोद᳚सी पृण ।",
          "begin": 0.0,
          "end": 4.491
        },
        {
          "text": "उ॒षाः सूर्यो॒ न र॒श्मिभि᳚ः ॥",
          "begin": 4.491,
          "end": 7.28
        }
      ]
    },
    {
      "id": "9.41.6",
      "mandala": 9,
      "sukta": 41,
      "rik": 6,
      "padas": [
        {
          "text": "परि᳚ णः शर्म॒यन्त्या॒ धार᳚या सोम वि॒श्वतः᳚ ।",
          "begin": 0.0,
          "end": 5.578
        },
        {
          "text": "सरा᳚ र॒सेव᳚ वि॒ष्टप᳚म् ॥",
          "begin": 5.578,
          "end": 10.16
        }
      ]
    }
  ]
}