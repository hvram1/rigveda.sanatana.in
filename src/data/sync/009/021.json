{
  "mandala": 9,
  "sukta": 21,
  "verses": [
    {
      "id": "9.21.1",
      "mandala": 9,
      "sukta": 21,
      "rik": 1,
      "padas": [
        {
          "text": "ए॒ते धा᳚व॒न्तीन्द᳚व॒ः सोमा॒ इन्द्रा᳚य॒ घृष्व᳚यः ।",
          "begin": 0.0,
          "end": 7.978
        },
        {
          "text": "म॒त्स॒रासः᳚ स्व॒र्विदः᳚ ॥",
          "begin": 7.978,
          "end": 10.44
        }
      ]
    },
    {
      "id": "9.21.2",
      "mandala": 9,
      "sukta": 21,
      "rik": 2,
      "padas": [
        {
          "text": "प्र॒वृ॒ण्वन्तो᳚ अभि॒युज॒ः सुष्व᳚ये वरिवो॒विदः᳚ ।",
          "begin": 0.0,
          "end": 4.981
        },
        {
          "text": "स्व॒यं स्तो॒त्रे व᳚य॒स्कृतः᳚ ॥",
          "begin": 4.981,
          "end": 8.04
        }
      ]
    },
    {
      "id": "9.21.3",
      "mandala": 9,
      "sukta": 21,
      "rik": 3,
      "padas": [
        {
          "text": "वृथा॒ क्रीळ᳚न्त॒ इन्द᳚वः स॒धस्थ᳚म॒भ्येक॒मित् ।",
          "begin": 0.0,
          "end": 5.333
        },
        {
          "text": "सिन्धो᳚रू॒र्मा व्य᳚क्षरन् ॥",
          "begin": 5.333,
          "end": 8.68
        }
      ]
    },
    {
      "id": "9.21.4",
      "mandala": 9,
      "sukta": 21,
      "rik": 4,
      "padas": [
        {
          "text": "ए॒ते विश्वा᳚नि॒ वार्या॒ पव᳚मानास आशत ।",
          "begin": 0.0,
          "end": 5.411
        },
        {
          "text": "हि॒ता न सप्त᳚यो॒ रथे᳚ ॥",
          "begin": 5.411,
          "end": 7.96
        }
      ]
    },
    {
      "id": "9.21.5",
      "mandala": 9,
      "sukta": 21,
      "rik": 5,
      "padas": [
        {
          "text": "आस्मि᳚न्पि॒शङ्ग᳚मिन्दवो॒ दधा᳚ता वे॒नमा॒दिशे᳚ ।",
          "begin": 0.0,
          "end": 6.383
        },
        {
          "text": "यो अ॒स्मभ्य॒मरा᳚वा ॥",
          "begin": 6.383,
          "end": 9.08
        }
      ]
    },
    {
      "id": "9.21.6",
      "mandala": 9,
      "sukta": 21,
      "rik": 6,
      "padas": [
        {
          "text": "ऋ॒भुर्न रथ्यं॒ नवं॒ दधा᳚ता॒ केत᳚मा॒दिशे᳚ ।",
          "begin": 0.0,
          "end": 5.279
        },
        {
          "text": "शु॒क्राः प᳚वध्व॒मर्ण᳚सा ॥",
          "begin": 5.279,
          "end": 7.88
        }
      ]
    },
    {
      "id": "9.21.7",
      "mandala": 9,
      "sukta": 21,
      "rik": 7,
      "padas": [
        {
          "text": "ए॒त उ॒ त्ये अ᳚वीवश॒न्काष्ठां᳚ वा॒जिनो᳚ अक्रत ।",
          "begin": 0.0,
          "end": 6.683
        },
        {
          "text": "स॒तः प्रासा᳚विषुर्म॒तिम् ॥",
          "begin": 6.683,
          "end": 11.4
        }
      ]
    }
  ]
}