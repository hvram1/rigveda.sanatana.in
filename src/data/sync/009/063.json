{
  "mandala": 9,
  "sukta": 63,
  "verses": [
    {
      "id": "9.63.1",
      "mandala": 9,
      "sukta": 63,
      "rik": 1,
      "padas": [
        {
          "text": "आ प᳚वस्व सह॒स्रिणं᳚ र॒यिं सो᳚म सु॒वीर्य᳚म् ।",
          "begin": 0.0,
          "end": 7.681
        },
        {
          "text": "अ॒स्मे श्रवां᳚सि धारय ॥",
          "begin": 7.681,
          "end": 10.52
        }
      ]
    },
    {
      "id": "9.63.2",
      "mandala": 9,
      "sukta": 63,
      "rik": 2,
      "padas": [
        {
          "text": "इष॒मूर्जं᳚ च पिन्वस॒ इन्द्रा᳚य मत्स॒रिन्त᳚मः ।",
          "begin": 0.0,
          "end": 4.996
        },
        {
          "text": "च॒मूष्वा नि षी᳚दसि ॥",
          "begin": 4.996,
          "end": 7.32
        }
      ]
    },
    {
      "id": "9.63.3",
      "mandala": 9,
      "sukta": 63,
      "rik": 3,
      "padas": [
        {
          "text": "सु॒त इन्द्रा᳚य॒ विष्ण᳚वे॒ सोमः᳚ क॒लशे᳚ अक्षरत् ।",
          "begin": 0.0,
          "end": 5.236
        },
        {
          "text": "मधु᳚माँ अस्तु वा॒यवे᳚ ॥",
          "begin": 5.236,
          "end": 7.92
        }
      ]
    },
    {
      "id": "9.63.4",
      "mandala": 9,
      "sukta": 63,
      "rik": 4,
      "padas": [
        {
          "text": "ए॒ते अ᳚सृग्रमा॒शवो॑ति॒ ह्वरां᳚सि ब॒भ्रवः᳚ ।",
          "begin": 0.0,
          "end": 5.022
        },
        {
          "text": "सोमा᳚ ऋ॒तस्य॒ धार᳚या ॥",
          "begin": 5.022,
          "end": 7.52
        }
      ]
    },
    {
      "id": "9.63.5",
      "mandala": 9,
      "sukta": 63,
      "rik": 5,
      "padas": [
        {
          "text": "इन्द्रं॒ वर्ध᳚न्तो अ॒प्तुरः᳚ कृ॒ण्वन्तो॒ विश्व॒मार्य᳚म् ।",
          "begin": 0.0,
          "end": 6.043
        },
        {
          "text": "अ॒प॒घ्नन्तो॒ अरा᳚व्णः ॥",
          "begin": 6.043,
          "end": 8.56
        }
      ]
    },
    {
      "id": "9.63.6",
      "mandala": 9,
      "sukta": 63,
      "rik": 6,
      "padas": [
        {
          "text": "सु॒ता अनु॒ स्वमा रजो॒॑भ्य᳚र्षन्ति ब॒भ्रवः᳚ ।",
          "begin": 0.0,
          "end": 4.696
        },
        {
          "text": "इन्द्रं॒ गच्छ᳚न्त॒ इन्द᳚वः ॥",
          "begin": 4.696,
          "end": 7.28
        }
      ]
    },
    {
      "id": "9.63.7",
      "mandala": 9,
      "sukta": 63,
      "rik": 7,
      "padas": [
        {
          "text": "अ॒या प᳚वस्व॒ धार᳚या॒ यया॒ सूर्य॒मरो᳚चयः ।",
          "begin": 0.0,
          "end": 4.82
        },
        {
          "text": "हि॒न्वा॒नो मानु᳚षीर॒पः ॥",
          "begin": 4.82,
          "end": 7.16
        }
      ]
    },
    {
      "id": "9.63.8",
      "mandala": 9,
      "sukta": 63,
      "rik": 8,
      "padas": [
        {
          "text": "अयु᳚क्त॒ सूर॒ एत᳚शं॒ पव᳚मानो म॒नावधि᳚ ।",
          "begin": 0.0,
          "end": 4.314
        },
        {
          "text": "अ॒न्तरि᳚क्षेण॒ यात᳚वे ॥",
          "begin": 4.314,
          "end": 6.44
        }
      ]
    },
    {
      "id": "9.63.9",
      "mandala": 9,
      "sukta": 63,
      "rik": 9,
      "padas": [
        {
          "text": "उ॒त त्या ह॒रितो॒ दश॒ सूरो᳚ अयुक्त॒ यात᳚वे ।",
          "begin": 0.0,
          "end": 5.087
        },
        {
          "text": "इन्दु॒रिन्द्र॒ इति᳚ ब्रु॒वन् ॥",
          "begin": 5.087,
          "end": 6.92
        }
      ]
    },
    {
      "id": "9.63.10",
      "mandala": 9,
      "sukta": 63,
      "rik": 10,
      "padas": [
        {
          "text": "परी॒तो वा॒यवे᳚ सु॒तं गिर॒ इन्द्रा᳚य मत्स॒रम् ।",
          "begin": 0.0,
          "end": 4.744
        },
        {
          "text": "अव्यो॒ वारे᳚षु सिञ्चत ॥",
          "begin": 4.744,
          "end": 7.68
        }
      ]
    },
    {
      "id": "9.63.11",
      "mandala": 9,
      "sukta": 63,
      "rik": 11,
      "padas": [
        {
          "text": "पव᳚मान वि॒दा र॒यिम॒स्मभ्यं᳚ सोम दु॒ष्टर᳚म् ।",
          "begin": 0.0,
          "end": 4.494
        },
        {
          "text": "यो दू॒णाशो᳚ वनुष्य॒ता ॥",
          "begin": 4.494,
          "end": 7.84
        }
      ]
    },
    {
      "id": "9.63.12",
      "mandala": 9,
      "sukta": 63,
      "rik": 12,
      "padas": [
        {
          "text": "अ॒भ्य᳚र्ष सह॒स्रिणं᳚ र॒यिं गोम᳚न्तम॒श्विन᳚म् ।",
          "begin": 0.0,
          "end": 4.273
        },
        {
          "text": "अ॒भि वाज᳚मु॒त श्रवः᳚ ॥",
          "begin": 4.273,
          "end": 7.0
        }
      ]
    },
    {
      "id": "9.63.13",
      "mandala": 9,
      "sukta": 63,
      "rik": 13,
      "padas": [
        {
          "text": "सोमो᳚ दे॒वो न सूर्यो॑द्रि᳚भिः पवते सु॒तः ।",
          "begin": 0.0,
          "end": 5.211
        },
        {
          "text": "दधा᳚नः क॒लशे॒ रस᳚म् ॥",
          "begin": 5.211,
          "end": 7.8
        }
      ]
    },
    {
      "id": "9.63.14",
      "mandala": 9,
      "sukta": 63,
      "rik": 14,
      "padas": [
        {
          "text": "ए॒ते धामा॒न्यार्या᳚ शु॒क्रा ऋ॒तस्य॒ धार᳚या ।",
          "begin": 0.0,
          "end": 5.734
        },
        {
          "text": "वाजं॒ गोम᳚न्तमक्षरन् ॥",
          "begin": 5.734,
          "end": 8.12
        }
      ]
    },
    {
      "id": "9.63.15",
      "mandala": 9,
      "sukta": 63,
      "rik": 15,
      "padas": [
        {
          "text": "सु॒ता इन्द्रा᳚य व॒ज्रिणे॒ सोमा᳚सो॒ दध्या᳚शिरः ।",
          "begin": 0.0,
          "end": 5.687
        },
        {
          "text": "प॒वित्र॒मत्य᳚क्षरन् ॥",
          "begin": 5.687,
          "end": 8.04
        }
      ]
    },
    {
      "id": "9.63.16",
      "mandala": 9,
      "sukta": 63,
      "rik": 16,
      "padas": [
        {
          "text": "प्र सो᳚म॒ मधु᳚मत्तमो रा॒ये अ᳚र्ष प॒वित्र॒ आ ।",
          "begin": 0.0,
          "end": 5.561
        },
        {
          "text": "मदो॒ यो दे᳚व॒वीत᳚मः ॥",
          "begin": 5.561,
          "end": 7.68
        }
      ]
    },
    {
      "id": "9.63.17",
      "mandala": 9,
      "sukta": 63,
      "rik": 17,
      "padas": [
        {
          "text": "तमी᳚ मृजन्त्या॒यवो॒ हरिं᳚ न॒दीषु᳚ वा॒जिन᳚म् ।",
          "begin": 0.0,
          "end": 4.342
        },
        {
          "text": "इन्दु॒मिन्द्रा᳚य मत्स॒रम् ॥",
          "begin": 4.342,
          "end": 7.44
        }
      ]
    },
    {
      "id": "9.63.18",
      "mandala": 9,
      "sukta": 63,
      "rik": 18,
      "padas": [
        {
          "text": "आ प᳚वस्व॒ हिर᳚ण्यव॒दश्वा᳚वत्सोम वी॒रव᳚त् ।",
          "begin": 0.0,
          "end": 4.488
        },
        {
          "text": "वाजं॒ गोम᳚न्त॒मा भ᳚र ॥",
          "begin": 4.488,
          "end": 7.44
        }
      ]
    },
    {
      "id": "9.63.19",
      "mandala": 9,
      "sukta": 63,
      "rik": 19,
      "padas": [
        {
          "text": "परि॒ वाजे॒ न वा᳚ज॒युमव्यो॒ वारे᳚षु सिञ्चत ।",
          "begin": 0.0,
          "end": 4.777
        },
        {
          "text": "इन्द्रा᳚य॒ मधु᳚मत्तमम् ॥",
          "begin": 4.777,
          "end": 7.2
        }
      ]
    },
    {
      "id": "9.63.20",
      "mandala": 9,
      "sukta": 63,
      "rik": 20,
      "padas": [
        {
          "text": "क॒विं मृ᳚जन्ति॒ मर्ज्यं᳚ धी॒भिर्विप्रा᳚ अव॒स्यवः᳚ ।",
          "begin": 0.0,
          "end": 4.711
        },
        {
          "text": "वृषा॒ कनि᳚क्रदर्षति ॥",
          "begin": 4.711,
          "end": 7.24
        }
      ]
    },
    {
      "id": "9.63.21",
      "mandala": 9,
      "sukta": 63,
      "rik": 21,
      "padas": [
        {
          "text": "वृष᳚णं धी॒भिर॒प्तुरं॒ सोम᳚मृ॒तस्य॒ धार᳚या ।",
          "begin": 0.0,
          "end": 4.01
        },
        {
          "text": "म॒ती विप्रा॒ः सम᳚स्वरन् ॥",
          "begin": 4.01,
          "end": 6.92
        }
      ]
    },
    {
      "id": "9.63.22",
      "mandala": 9,
      "sukta": 63,
      "rik": 22,
      "padas": [
        {
          "text": "पव᳚स्व देवायु॒षगिन्द्रं᳚ गच्छतु ते॒ मदः᳚ ।",
          "begin": 0.0,
          "end": 4.601
        },
        {
          "text": "वा॒युमा रो᳚ह॒ धर्म᳚णा ॥",
          "begin": 4.601,
          "end": 7.0
        }
      ]
    },
    {
      "id": "9.63.23",
      "mandala": 9,
      "sukta": 63,
      "rik": 23,
      "padas": [
        {
          "text": "पव᳚मान॒ नि तो᳚शसे र॒यिं सो᳚म श्र॒वाय्य᳚म् ।",
          "begin": 0.0,
          "end": 4.526
        },
        {
          "text": "प्रि॒यः स᳚मु॒द्रमा वि᳚श ॥",
          "begin": 4.526,
          "end": 7.2
        }
      ]
    },
    {
      "id": "9.63.24",
      "mandala": 9,
      "sukta": 63,
      "rik": 24,
      "padas": [
        {
          "text": "अ॒प॒घ्नन्प᳚वसे॒ मृधः᳚ क्रतु॒वित्सो᳚म मत्स॒रः ।",
          "begin": 0.0,
          "end": 4.748
        },
        {
          "text": "नु॒दस्वादे᳚वयुं॒ जन᳚म् ॥",
          "begin": 4.748,
          "end": 7.44
        }
      ]
    },
    {
      "id": "9.63.25",
      "mandala": 9,
      "sukta": 63,
      "rik": 25,
      "padas": [
        {
          "text": "पव᳚माना असृक्षत॒ सोमा᳚ः शु॒क्रास॒ इन्द᳚वः ।",
          "begin": 0.0,
          "end": 5.07
        },
        {
          "text": "अ॒भि विश्वा᳚नि॒ काव्या᳚ ॥",
          "begin": 5.07,
          "end": 7.8
        }
      ]
    },
    {
      "id": "9.63.26",
      "mandala": 9,
      "sukta": 63,
      "rik": 26,
      "padas": [
        {
          "text": "पव᳚मानास आ॒शवः᳚ शु॒भ्रा अ᳚सृग्र॒मिन्द᳚वः ।",
          "begin": 0.0,
          "end": 4.624
        },
        {
          "text": "घ्नन्तो॒ विश्वा॒ अप॒ द्विषः᳚ ॥",
          "begin": 4.624,
          "end": 7.44
        }
      ]
    },
    {
      "id": "9.63.27",
      "mandala": 9,
      "sukta": 63,
      "rik": 27,
      "padas": [
        {
          "text": "पव᳚माना दि॒वस्पर्य॒न्तरि᳚क्षादसृक्षत ।",
          "begin": 0.0,
          "end": 3.849
        },
        {
          "text": "पृ॒थि॒व्या अधि॒ सान᳚वि ॥",
          "begin": 3.849,
          "end": 6.44
        }
      ]
    },
    {
      "id": "9.63.28",
      "mandala": 9,
      "sukta": 63,
      "rik": 28,
      "padas": [
        {
          "text": "पु॒ना॒नः सो᳚म॒ धार॒येन्दो॒ विश्वा॒ अप॒ स्रिधः᳚ ।",
          "begin": 0.0,
          "end": 5.263
        },
        {
          "text": "ज॒हि रक्षां᳚सि सुक्रतो ॥",
          "begin": 5.263,
          "end": 7.64
        }
      ]
    },
    {
      "id": "9.63.29",
      "mandala": 9,
      "sukta": 63,
      "rik": 29,
      "padas": [
        {
          "text": "अ॒प॒घ्नन्त्सो᳚म र॒क्षसो॒॑भ्य᳚र्ष॒ कनि᳚क्रदत् ।",
          "begin": 0.0,
          "end": 4.821
        },
        {
          "text": "द्यु॒मन्तं॒ शुष्म᳚मुत्त॒मम् ॥",
          "begin": 4.821,
          "end": 7.2
        }
      ]
    },
    {
      "id": "9.63.30",
      "mandala": 9,
      "sukta": 63,
      "rik": 30,
      "padas": [
        {
          "text": "अ॒स्मे वसू᳚नि धारय॒ सोम᳚ दि॒व्यानि॒ पार्थि᳚वा ।",
          "begin": 0.0,
          "end": 4.914
        },
        {
          "text": "इन्दो॒ विश्वा᳚नि॒ वार्या᳚ ॥",
          "begin": 4.914,
          "end": 8.92
        }
      ]
    }
  ]
}