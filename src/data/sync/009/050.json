{
  "mandala": 9,
  "sukta": 50,
  "verses": [
    {
      "id": "9.50.1",
      "mandala": 9,
      "sukta": 50,
      "rik": 1,
      "padas": [
        {
          "text": "उत्ते॒ शुष्मा᳚स ईरते॒ सिन्धो᳚रू॒र्मेरि᳚व स्व॒नः ।",
          "begin": 0.0,
          "end": 7.872
        },
        {
          "text": "वा॒णस्य᳚ चोदया प॒विम् ॥",
          "begin": 7.872,
          "end": 10.28
        }
      ]
    },
    {
      "id": "9.50.2",
      "mandala": 9,
      "sukta": 50,
      "rik": 2,
      "padas": [
        {
          "text": "प्र॒स॒वे त॒ उदी᳚रते ति॒स्रो वाचो᳚ मख॒स्युवः᳚ ।",
          "begin": 0.0,
          "end": 5.265
        },
        {
          "text": "यदव्य॒ एषि॒ सान᳚वि ॥",
          "begin": 5.265,
          "end": 7.36
        }
      ]
    },
    {
      "id": "9.50.3",
      "mandala": 9,
      "sukta": 50,
      "rik": 3,
      "padas": [
        {
          "text": "अव्यो॒ वारे॒ परि᳚ प्रि॒यं हरिं᳚ हिन्व॒न्त्यद्रि᳚भिः ।",
          "begin": 0.0,
          "end": 4.833
        },
        {
          "text": "पव᳚मानं मधु॒श्चुत᳚म् ॥",
          "begin": 4.833,
          "end": 7.12
        }
      ]
    },
    {
      "id": "9.50.4",
      "mandala": 9,
      "sukta": 50,
      "rik": 4,
      "padas": [
        {
          "text": "आ प᳚वस्व मदिन्तम प॒वित्रं॒ धार᳚या कवे ।",
          "begin": 0.0,
          "end": 4.194
        },
        {
          "text": "अ॒र्कस्य॒ योनि᳚मा॒सद᳚म् ॥",
          "begin": 4.194,
          "end": 6.48
        }
      ]
    },
    {
      "id": "9.50.5",
      "mandala": 9,
      "sukta": 50,
      "rik": 5,
      "padas": [
        {
          "text": "स प᳚वस्व मदिन्तम॒ गोभि᳚रञ्जा॒नो अ॒क्तुभि᳚ः ।",
          "begin": 0.0,
          "end": 4.941
        },
        {
          "text": "इन्द॒विन्द्रा᳚य पी॒तये᳚ ॥",
          "begin": 4.941,
          "end": 10.36
        }
      ]
    }
  ]
}