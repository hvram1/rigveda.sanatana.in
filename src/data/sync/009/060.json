{
  "mandala": 9,
  "sukta": 60,
  "verses": [
    {
      "id": "9.60.1",
      "mandala": 9,
      "sukta": 60,
      "rik": 1,
      "padas": [
        {
          "text": "प्र गा᳚य॒त्रेण᳚ गायत॒ पव᳚मानं॒ विच᳚र्षणिम् ।",
          "begin": 0.0,
          "end": 6.893
        },
        {
          "text": "इन्दुं᳚ स॒हस्र᳚चक्षसम् ॥",
          "begin": 6.893,
          "end": 9.2
        }
      ]
    },
    {
      "id": "9.60.2",
      "mandala": 9,
      "sukta": 60,
      "rik": 2,
      "padas": [
        {
          "text": "तं त्वा᳚ स॒हस्र᳚चक्षस॒मथो᳚ स॒हस्र᳚भर्णसम् ।",
          "begin": 0.0,
          "end": 4.783
        },
        {
          "text": "अति॒ वार᳚मपाविषुः ॥",
          "begin": 4.783,
          "end": 6.96
        }
      ]
    },
    {
      "id": "9.60.3",
      "mandala": 9,
      "sukta": 60,
      "rik": 3,
      "padas": [
        {
          "text": "अति॒ वारा॒न्पव᳚मानो असिष्यदत्क॒लशाँ᳚ अ॒भि धा᳚वति ।",
          "begin": 0.0,
          "end": 5.952
        },
        {
          "text": "इन्द्र᳚स्य॒ हार्द्या᳚वि॒शन् ॥",
          "begin": 5.952,
          "end": 8.8
        }
      ]
    },
    {
      "id": "9.60.4",
      "mandala": 9,
      "sukta": 60,
      "rik": 4,
      "padas": [
        {
          "text": "इन्द्र᳚स्य सोम॒ राध᳚से॒ शं प᳚वस्व विचर्षणे ।",
          "begin": 0.0,
          "end": 4.676
        },
        {
          "text": "प्र॒जाव॒द्रेत॒ आ भ᳚र ॥",
          "begin": 4.676,
          "end": 8.08
        }
      ]
    }
  ]
}