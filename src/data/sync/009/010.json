{
  "mandala": 9,
  "sukta": 10,
  "verses": [
    {
      "id": "9.10.1",
      "mandala": 9,
      "sukta": 10,
      "rik": 1,
      "padas": [
        {
          "text": "प्र स्वा॒नासो॒ रथा᳚ इ॒वार्व᳚न्तो॒ न श्र᳚व॒स्यवः᳚ ।",
          "begin": 0.0,
          "end": 8.74
        },
        {
          "text": "सोमा᳚सो रा॒ये अ᳚क्रमुः ॥",
          "begin": 8.74,
          "end": 12.6
        }
      ]
    },
    {
      "id": "9.10.2",
      "mandala": 9,
      "sukta": 10,
      "rik": 2,
      "padas": [
        {
          "text": "हि॒न्वा॒नासो॒ रथा᳚ इव दधन्वि॒रे गभ᳚स्त्योः ।",
          "begin": 0.0,
          "end": 5.008
        },
        {
          "text": "भरा᳚सः का॒रिणा᳚मिव ॥",
          "begin": 5.008,
          "end": 9.32
        }
      ]
    },
    {
      "id": "9.10.3",
      "mandala": 9,
      "sukta": 10,
      "rik": 3,
      "padas": [
        {
          "text": "राजा᳚नो॒ न प्रश᳚स्तिभि॒ः सोमा᳚सो॒ गोभि᳚रञ्जते ।",
          "begin": 0.0,
          "end": 6.879
        },
        {
          "text": "य॒ज्ञो न स॒प्त धा॒तृभि᳚ः ॥",
          "begin": 6.879,
          "end": 9.36
        }
      ]
    },
    {
      "id": "9.10.4",
      "mandala": 9,
      "sukta": 10,
      "rik": 4,
      "padas": [
        {
          "text": "परि᳚ सुवा॒नास॒ इन्द᳚वो॒ मदा᳚य ब॒र्हणा᳚ गि॒रा ।",
          "begin": 0.0,
          "end": 6.078
        },
        {
          "text": "सु॒ता अ᳚र्षन्ति॒ धार᳚या ॥",
          "begin": 6.078,
          "end": 8.88
        }
      ]
    },
    {
      "id": "9.10.5",
      "mandala": 9,
      "sukta": 10,
      "rik": 5,
      "padas": [
        {
          "text": "आ॒पा॒नासो᳚ वि॒वस्व᳚तो॒ जन᳚न्त उ॒षसो॒ भग᳚म् ।",
          "begin": 0.0,
          "end": 6.211
        },
        {
          "text": "सूरा॒ अण्वं॒ वि त᳚न्वते ॥",
          "begin": 6.211,
          "end": 9.56
        }
      ]
    },
    {
      "id": "9.10.6",
      "mandala": 9,
      "sukta": 10,
      "rik": 6,
      "padas": [
        {
          "text": "अप॒ द्वारा᳚ मती॒नां प्र॒त्ना ऋ᳚ण्वन्ति का॒रवः᳚ ।",
          "begin": 0.0,
          "end": 6.655
        },
        {
          "text": "वृष्णो॒ हर᳚स आ॒यवः᳚ ॥",
          "begin": 6.655,
          "end": 9.12
        }
      ]
    },
    {
      "id": "9.10.7",
      "mandala": 9,
      "sukta": 10,
      "rik": 7,
      "padas": [
        {
          "text": "स॒मी॒ची॒नास᳚ आसते॒ होता᳚रः स॒प्तजा᳚मयः ।",
          "begin": 0.0,
          "end": 6.061
        },
        {
          "text": "प॒दमेक᳚स्य॒ पिप्र᳚तः ॥",
          "begin": 6.061,
          "end": 8.88
        }
      ]
    },
    {
      "id": "9.10.8",
      "mandala": 9,
      "sukta": 10,
      "rik": 8,
      "padas": [
        {
          "text": "नाभा॒ नाभिं᳚ न॒ आ द᳚दे॒ चक्षु᳚श्चि॒त्सूर्ये॒ सचा᳚ ।",
          "begin": 0.0,
          "end": 6.304
        },
        {
          "text": "क॒वेरप᳚त्य॒मा दु᳚हे ॥",
          "begin": 6.304,
          "end": 8.96
        }
      ]
    },
    {
      "id": "9.10.9",
      "mandala": 9,
      "sukta": 10,
      "rik": 9,
      "padas": [
        {
          "text": "अ॒भि प्रि॒या दि॒वस्प॒दम᳚ध्व॒र्युभि॒र्गुहा᳚ हि॒तम् ।",
          "begin": 0.0,
          "end": 5.803
        },
        {
          "text": "सूरः᳚ पश्यति॒ चक्ष᳚सा ॥",
          "begin": 5.803,
          "end": 9.6
        }
      ]
    }
  ]
}