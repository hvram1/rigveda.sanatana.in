{
  "mandala": 9,
  "sukta": 53,
  "verses": [
    {
      "id": "9.53.1",
      "mandala": 9,
      "sukta": 53,
      "rik": 1,
      "padas": [
        {
          "text": "उत्ते॒ शुष्मा᳚सो अस्थू॒ रक्षो᳚ भि॒न्दन्तो᳚ अद्रिवः ।",
          "begin": 0.0,
          "end": 8.15
        },
        {
          "text": "नु॒दस्व॒ याः प᳚रि॒स्पृधः᳚ ॥",
          "begin": 8.15,
          "end": 10.84
        }
      ]
    },
    {
      "id": "9.53.2",
      "mandala": 9,
      "sukta": 53,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒या नि᳚ज॒घ्निरोज᳚सा रथसं॒गे धने᳚ हि॒ते ।",
          "begin": 0.0,
          "end": 4.377
        },
        {
          "text": "स्तवा॒ अबि᳚भ्युषा हृ॒दा ॥",
          "begin": 4.377,
          "end": 6.84
        }
      ]
    },
    {
      "id": "9.53.3",
      "mandala": 9,
      "sukta": 53,
      "rik": 3,
      "padas": [
        {
          "text": "अस्य᳚ व्र॒तानि॒ नाधृषे॒ पव᳚मानस्य दू॒ढ्या᳚ ।",
          "begin": 0.0,
          "end": 5.264
        },
        {
          "text": "रु॒ज यस्त्वा᳚ पृत॒न्यति᳚ ॥",
          "begin": 5.264,
          "end": 7.76
        }
      ]
    },
    {
      "id": "9.53.4",
      "mandala": 9,
      "sukta": 53,
      "rik": 4,
      "padas": [
        {
          "text": "तं हि᳚न्वन्ति मद॒च्युतं॒ हरिं᳚ न॒दीषु᳚ वा॒जिन᳚म् ।",
          "begin": 0.0,
          "end": 5.706
        },
        {
          "text": "इन्दु॒मिन्द्रा᳚य मत्स॒रम् ॥",
          "begin": 5.706,
          "end": 10.08
        }
      ]
    }
  ]
}