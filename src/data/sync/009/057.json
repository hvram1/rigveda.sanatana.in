{
  "mandala": 9,
  "sukta": 57,
  "verses": [
    {
      "id": "9.57.1",
      "mandala": 9,
      "sukta": 57,
      "rik": 1,
      "padas": [
        {
          "text": "प्र ते॒ धारा᳚ अस॒श्चतो᳚ दि॒वो न य᳚न्ति वृ॒ष्टयः᳚ ।",
          "begin": 0.0,
          "end": 7.853
        },
        {
          "text": "अच्छा॒ वाजं᳚ सह॒स्रिण᳚म् ॥",
          "begin": 7.853,
          "end": 10.96
        }
      ]
    },
    {
      "id": "9.57.2",
      "mandala": 9,
      "sukta": 57,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒भि प्रि॒याणि॒ काव्या॒ विश्वा॒ चक्षा᳚णो अर्षति ।",
          "begin": 0.0,
          "end": 4.906
        },
        {
          "text": "हरि᳚स्तुञ्जा॒न आयु᳚धा ॥",
          "begin": 4.906,
          "end": 7.0
        }
      ]
    },
    {
      "id": "9.57.3",
      "mandala": 9,
      "sukta": 57,
      "rik": 3,
      "padas": [
        {
          "text": "स म᳚र्मृजा॒न आ॒युभि॒रिभो॒ राजे᳚व सुव्र॒तः ।",
          "begin": 0.0,
          "end": 4.673
        },
        {
          "text": "श्ये॒नो न वंसु᳚ षीदति ॥",
          "begin": 4.673,
          "end": 6.92
        }
      ]
    },
    {
      "id": "9.57.4",
      "mandala": 9,
      "sukta": 57,
      "rik": 4,
      "padas": [
        {
          "text": "स नो॒ विश्वा᳚ दि॒वो वसू॒तो पृ᳚थि॒व्या अधि᳚ ।",
          "begin": 0.0,
          "end": 4.509
        },
        {
          "text": "पु॒ना॒न इ᳚न्द॒वा भ᳚र ॥",
          "begin": 4.509,
          "end": 7.24
        }
      ]
    }
  ]
}