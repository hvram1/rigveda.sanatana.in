{
  "mandala": 9,
  "sukta": 6,
  "verses": [
    {
      "id": "9.6.1",
      "mandala": 9,
      "sukta": 6,
      "rik": 1,
      "padas": [
        {
          "text": "म॒न्द्रया᳚ सोम॒ धार᳚या॒ वृषा᳚ पवस्व देव॒युः ।",
          "begin": 0.0,
          "end": 7.039
        },
        {
          "text": "अव्यो॒ वारे᳚ष्वस्म॒युः ॥",
          "begin": 7.039,
          "end": 10.96
        }
      ]
    },
    {
      "id": "9.6.2",
      "mandala": 9,
      "sukta": 6,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒भि त्यं मद्यं॒ मद॒मिन्द॒विन्द्र॒ इति᳚ क्षर ।",
          "begin": 0.0,
          "end": 4.986
        },
        {
          "text": "अ॒भि वा॒जिनो॒ अर्व᳚तः ॥",
          "begin": 4.986,
          "end": 7.48
        }
      ]
    },
    {
      "id": "9.6.3",
      "mandala": 9,
      "sukta": 6,
      "rik": 3,
      "padas": [
        {
          "text": "अ॒भि त्यं पू॒र्व्यं मदं᳚ सुवा॒नो अ᳚र्ष प॒वित्र॒ आ ।",
          "begin": 0.0,
          "end": 5.636
        },
        {
          "text": "अ॒भि वाज᳚मु॒त श्रवः᳚ ॥",
          "begin": 5.636,
          "end": 8.4
        }
      ]
    },
    {
      "id": "9.6.4",
      "mandala": 9,
      "sukta": 6,
      "rik": 4,
      "padas": [
        {
          "text": "अनु᳚ द्र॒प्सास॒ इन्द᳚व॒ आपो॒ न प्र॒वता᳚सरन् ।",
          "begin": 0.0,
          "end": 5.4
        },
        {
          "text": "पु॒ना॒ना इन्द्र᳚माशत ॥",
          "begin": 5.4,
          "end": 8.32
        }
      ]
    },
    {
      "id": "9.6.5",
      "mandala": 9,
      "sukta": 6,
      "rik": 5,
      "padas": [
        {
          "text": "यमत्य᳚मिव वा॒जिनं᳚ मृ॒जन्ति॒ योष᳚णो॒ दश᳚ ।",
          "begin": 0.0,
          "end": 5.641
        },
        {
          "text": "वने॒ क्रीळ᳚न्त॒मत्य᳚विम् ॥",
          "begin": 5.641,
          "end": 8.16
        }
      ]
    },
    {
      "id": "9.6.6",
      "mandala": 9,
      "sukta": 6,
      "rik": 6,
      "padas": [
        {
          "text": "तं गोभि॒र्वृष᳚णं॒ रसं॒ मदा᳚य दे॒ववी᳚तये ।",
          "begin": 0.0,
          "end": 6.037
        },
        {
          "text": "सु॒तं भरा᳚य॒ सं सृ᳚ज ॥",
          "begin": 6.037,
          "end": 8.72
        }
      ]
    },
    {
      "id": "9.6.7",
      "mandala": 9,
      "sukta": 6,
      "rik": 7,
      "padas": [
        {
          "text": "दे॒वो दे॒वाय॒ धार॒येन्द्रा᳚य पवते सु॒तः ।",
          "begin": 0.0,
          "end": 5.357
        },
        {
          "text": "पयो॒ यद᳚स्य पी॒पय᳚त् ॥",
          "begin": 5.357,
          "end": 8.12
        }
      ]
    },
    {
      "id": "9.6.8",
      "mandala": 9,
      "sukta": 6,
      "rik": 8,
      "padas": [
        {
          "text": "आ॒त्मा य॒ज्ञस्य॒ रंह्या᳚ सुष्वा॒णः प᳚वते सु॒तः ।",
          "begin": 0.0,
          "end": 6.218
        },
        {
          "text": "प्र॒त्नं नि पा᳚ति॒ काव्य᳚म् ॥",
          "begin": 6.218,
          "end": 9.64
        }
      ]
    },
    {
      "id": "9.6.9",
      "mandala": 9,
      "sukta": 6,
      "rik": 9,
      "padas": [
        {
          "text": "ए॒वा पु᳚ना॒न इ᳚न्द्र॒युर्मदं᳚ मदिष्ठ वी॒तये᳚ ।",
          "begin": 0.0,
          "end": 6.562
        },
        {
          "text": "गुहा᳚ चिद्दधिषे॒ गिरः᳚ ॥",
          "begin": 6.562,
          "end": 11.12
        }
      ]
    }
  ]
}