{
  "mandala": 9,
  "sukta": 39,
  "verses": [
    {
      "id": "9.39.1",
      "mandala": 9,
      "sukta": 39,
      "rik": 1,
      "padas": [
        {
          "text": "आ॒शुर᳚र्ष बृहन्मते॒ परि᳚ प्रि॒येण॒ धाम्ना᳚ ।",
          "begin": 0.0,
          "end": 7.446
        },
        {
          "text": "यत्र᳚ दे॒वा इति॒ ब्रव᳚न् ॥",
          "begin": 7.446,
          "end": 10.36
        }
      ]
    },
    {
      "id": "9.39.2",
      "mandala": 9,
      "sukta": 39,
      "rik": 2,
      "padas": [
        {
          "text": "प॒रि॒ष्कृ॒ण्वन्ननि᳚ष्कृतं॒ जना᳚य या॒तय॒न्निषः᳚ ।",
          "begin": 0.0,
          "end": 5.396
        },
        {
          "text": "वृ॒ष्टिं दि॒वः परि᳚ स्रव ॥",
          "begin": 5.396,
          "end": 7.68
        }
      ]
    },
    {
      "id": "9.39.3",
      "mandala": 9,
      "sukta": 39,
      "rik": 3,
      "padas": [
        {
          "text": "सु॒त ए᳚ति प॒वित्र॒ आ त्विषिं॒ दधा᳚न॒ ओज᳚सा ।",
          "begin": 0.0,
          "end": 5.897
        },
        {
          "text": "वि॒चक्षा᳚णो विरो॒चय᳚न् ॥",
          "begin": 5.897,
          "end": 8.56
        }
      ]
    },
    {
      "id": "9.39.4",
      "mandala": 9,
      "sukta": 39,
      "rik": 4,
      "padas": [
        {
          "text": "अ॒यं स यो दि॒वस्परि᳚ रघु॒यामा᳚ प॒वित्र॒ आ ।",
          "begin": 0.0,
          "end": 4.876
        },
        {
          "text": "सिन्धो᳚रू॒र्मा व्यक्ष᳚रत् ॥",
          "begin": 4.876,
          "end": 8.04
        }
      ]
    },
    {
      "id": "9.39.5",
      "mandala": 9,
      "sukta": 39,
      "rik": 5,
      "padas": [
        {
          "text": "आ॒विवा᳚सन्परा॒वतो॒ अथो᳚ अर्वा॒वतः᳚ सु॒तः ।",
          "begin": 0.0,
          "end": 6.021
        },
        {
          "text": "इन्द्रा᳚य सिच्यते॒ मधु᳚ ॥",
          "begin": 6.021,
          "end": 8.8
        }
      ]
    },
    {
      "id": "9.39.6",
      "mandala": 9,
      "sukta": 39,
      "rik": 6,
      "padas": [
        {
          "text": "स॒मी॒ची॒ना अ᳚नूषत॒ हरिं᳚ हिन्व॒न्त्यद्रि᳚भिः ।",
          "begin": 0.0,
          "end": 4.98
        },
        {
          "text": "योना᳚वृ॒तस्य᳚ सीदत ॥",
          "begin": 4.98,
          "end": 10.16
        }
      ]
    }
  ]
}