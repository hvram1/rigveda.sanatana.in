{
  "mandala": 9,
  "sukta": 34,
  "verses": [
    {
      "id": "9.34.1",
      "mandala": 9,
      "sukta": 34,
      "rik": 1,
      "padas": [
        {
          "text": "प्र सु᳚वा॒नो धार᳚या॒ तनेन्दु᳚र्हिन्वा॒नो अ᳚र्षति ।",
          "begin": 0.0,
          "end": 6.238
        },
        {
          "text": "रु॒जद्दृ॒ळ्हा व्योज᳚सा ॥",
          "begin": 6.238,
          "end": 8.88
        }
      ]
    },
    {
      "id": "9.34.2",
      "mandala": 9,
      "sukta": 34,
      "rik": 2,
      "padas": [
        {
          "text": "सु॒त इन्द्रा᳚य वा॒यवे॒ वरु᳚णाय म॒रुद्भ्य᳚ः ।",
          "begin": 0.0,
          "end": 4.969
        },
        {
          "text": "सोमो᳚ अर्षति॒ विष्ण᳚वे ॥",
          "begin": 4.969,
          "end": 7.6
        }
      ]
    },
    {
      "id": "9.34.3",
      "mandala": 9,
      "sukta": 34,
      "rik": 3,
      "padas": [
        {
          "text": "वृषा᳚णं॒ वृष᳚भिर्य॒तं सु॒न्वन्ति॒ सोम॒मद्रि᳚भिः ।",
          "begin": 0.0,
          "end": 5.142
        },
        {
          "text": "दु॒हन्ति॒ शक्म᳚ना॒ पयः᳚ ॥",
          "begin": 5.142,
          "end": 7.64
        }
      ]
    },
    {
      "id": "9.34.4",
      "mandala": 9,
      "sukta": 34,
      "rik": 4,
      "padas": [
        {
          "text": "भुव᳚त्त्रि॒तस्य॒ मर्ज्यो॒ भुव॒दिन्द्रा᳚य मत्स॒रः ।",
          "begin": 0.0,
          "end": 4.846
        },
        {
          "text": "सं रू॒पैर᳚ज्यते॒ हरि᳚ः ॥",
          "begin": 4.846,
          "end": 7.32
        }
      ]
    },
    {
      "id": "9.34.5",
      "mandala": 9,
      "sukta": 34,
      "rik": 5,
      "padas": [
        {
          "text": "अ॒भीमृ॒तस्य᳚ वि॒ष्टपं᳚ दुह॒ते पृश्नि᳚मातरः ।",
          "begin": 0.0,
          "end": 4.915
        },
        {
          "text": "चारु᳚ प्रि॒यत᳚मं ह॒विः ॥",
          "begin": 4.915,
          "end": 7.12
        }
      ]
    },
    {
      "id": "9.34.6",
      "mandala": 9,
      "sukta": 34,
      "rik": 6,
      "padas": [
        {
          "text": "समे᳚न॒मह्रु᳚ता इ॒मा गिरो᳚ अर्षन्ति स॒स्रुतः᳚ ।",
          "begin": 0.0,
          "end": 6.123
        },
        {
          "text": "धे॒नूर्वा॒श्रो अ᳚वीवशत् ॥",
          "begin": 6.123,
          "end": 10.96
        }
      ]
    }
  ]
}