{
  "mandala": 9,
  "sukta": 42,
  "verses": [
    {
      "id": "9.42.1",
      "mandala": 9,
      "sukta": 42,
      "rik": 1,
      "padas": [
        {
          "text": "ज॒नय᳚न्रोच॒ना दि॒वो ज॒नय᳚न्न॒प्सु सूर्य᳚म् ।",
          "begin": 0.0,
          "end": 7.724
        },
        {
          "text": "वसा᳚नो॒ गा अ॒पो हरि᳚ः ॥",
          "begin": 7.724,
          "end": 11.04
        }
      ]
    },
    {
      "id": "9.42.2",
      "mandala": 9,
      "sukta": 42,
      "rik": 2,
      "padas": [
        {
          "text": "ए॒ष प्र॒त्नेन॒ मन्म᳚ना दे॒वो दे॒वेभ्य॒स्परि᳚ ।",
          "begin": 0.0,
          "end": 5.394
        },
        {
          "text": "धार᳚या पवते सु॒तः ॥",
          "begin": 5.394,
          "end": 7.52
        }
      ]
    },
    {
      "id": "9.42.3",
      "mandala": 9,
      "sukta": 42,
      "rik": 3,
      "padas": [
        {
          "text": "वा॒वृ॒धा॒नाय॒ तूर्व᳚ये॒ पव᳚न्ते॒ वाज᳚सातये ।",
          "begin": 0.0,
          "end": 5.46
        },
        {
          "text": "सोमा᳚ः स॒हस्र᳚पाजसः ॥",
          "begin": 5.46,
          "end": 8.28
        }
      ]
    },
    {
      "id": "9.42.4",
      "mandala": 9,
      "sukta": 42,
      "rik": 4,
      "padas": [
        {
          "text": "दु॒हा॒नः प्र॒त्नमित्पयः᳚ प॒वित्रे॒ परि᳚ षिच्यते ।",
          "begin": 0.0,
          "end": 4.851
        },
        {
          "text": "क्रन्द᳚न्दे॒वाँ अ᳚जीजनत् ॥",
          "begin": 4.851,
          "end": 7.88
        }
      ]
    },
    {
      "id": "9.42.5",
      "mandala": 9,
      "sukta": 42,
      "rik": 5,
      "padas": [
        {
          "text": "अ॒भि विश्वा᳚नि॒ वार्या॒भि दे॒वाँ ऋ᳚ता॒वृधः᳚ ।",
          "begin": 0.0,
          "end": 5.448
        },
        {
          "text": "सोमः᳚ पुना॒नो अ᳚र्षति ॥",
          "begin": 5.448,
          "end": 8.08
        }
      ]
    },
    {
      "id": "9.42.6",
      "mandala": 9,
      "sukta": 42,
      "rik": 6,
      "padas": [
        {
          "text": "गोम᳚न्नः सोम वी॒रव॒दश्वा᳚व॒द्वाज᳚वत्सु॒तः ।",
          "begin": 0.0,
          "end": 5.585
        },
        {
          "text": "पव᳚स्व बृह॒तीरिषः᳚ ॥",
          "begin": 5.585,
          "end": 9.92
        }
      ]
    }
  ]
}