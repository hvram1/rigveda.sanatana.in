{
  "mandala": 9,
  "sukta": 52,
  "verses": [
    {
      "id": "9.52.1",
      "mandala": 9,
      "sukta": 52,
      "rik": 1,
      "padas": [
        {
          "text": "परि᳚ द्यु॒क्षः स॒नद्र᳚यि॒र्भर॒द्वाजं᳚ नो॒ अन्ध᳚सा ।",
          "begin": 0.0,
          "end": 7.081
        },
        {
          "text": "सु॒वा॒नो अ᳚र्ष प॒वित्र॒ आ ॥",
          "begin": 7.081,
          "end": 9.92
        }
      ]
    },
    {
      "id": "9.52.2",
      "mandala": 9,
      "sukta": 52,
      "rik": 2,
      "padas": [
        {
          "text": "तव᳚ प्र॒त्नेभि॒रध्व᳚भि॒रव्यो॒ वारे॒ परि᳚ प्रि॒यः ।",
          "begin": 0.0,
          "end": 4.534
        },
        {
          "text": "स॒हस्र᳚धारो या॒त्तना᳚ ॥",
          "begin": 4.534,
          "end": 7.12
        }
      ]
    },
    {
      "id": "9.52.3",
      "mandala": 9,
      "sukta": 52,
      "rik": 3,
      "padas": [
        {
          "text": "च॒रुर्न यस्तमी᳚ङ्ख॒येन्दो॒ न दान᳚मीङ्खय ।",
          "begin": 0.0,
          "end": 4.926
        },
        {
          "text": "व॒धैर्व᳚धस्नवीङ्खय ॥",
          "begin": 4.926,
          "end": 7.24
        }
      ]
    },
    {
      "id": "9.52.4",
      "mandala": 9,
      "sukta": 52,
      "rik": 4,
      "padas": [
        {
          "text": "नि शुष्म᳚मिन्दवेषां॒ पुरु᳚हूत॒ जना᳚नाम् ।",
          "begin": 0.0,
          "end": 4.498
        },
        {
          "text": "यो अ॒स्माँ आ॒दिदे᳚शति ॥",
          "begin": 4.498,
          "end": 7.96
        }
      ]
    },
    {
      "id": "9.52.5",
      "mandala": 9,
      "sukta": 52,
      "rik": 5,
      "padas": [
        {
          "text": "श॒तं न᳚ इन्द ऊ॒तिभि᳚ः स॒हस्रं᳚ वा॒ शुची᳚नाम् ।",
          "begin": 0.0,
          "end": 5.188
        },
        {
          "text": "पव᳚स्व मंह॒यद्र᳚यिः ॥",
          "begin": 5.188,
          "end": 8.72
        }
      ]
    }
  ]
}