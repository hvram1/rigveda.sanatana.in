{
  "mandala": 9,
  "sukta": 5,
  "verses": [
    {
      "id": "9.5.1",
      "mandala": 9,
      "sukta": 5,
      "rik": 1,
      "padas": [
        {
          "text": "समि᳚द्धो वि॒श्वत॒स्पति॒ः पव᳚मानो॒ वि रा᳚जति ।",
          "begin": 0.0,
          "end": 6.389
        },
        {
          "text": "प्री॒णन्वृषा॒ कनि᳚क्रदत् ॥",
          "begin": 6.389,
          "end": 9.64
        }
      ]
    },
    {
      "id": "9.5.2",
      "mandala": 9,
      "sukta": 5,
      "rik": 2,
      "padas": [
        {
          "text": "तनू॒नपा॒त्पव᳚मान॒ः शृङ्गे॒ शिशा᳚नो अर्षति ।",
          "begin": 0.0,
          "end": 5.81
        },
        {
          "text": "अ॒न्तरि᳚क्षेण॒ रार᳚जत् ॥",
          "begin": 5.81,
          "end": 8.28
        }
      ]
    },
    {
      "id": "9.5.3",
      "mandala": 9,
      "sukta": 5,
      "rik": 3,
      "padas": [
        {
          "text": "ई॒ळेन्य॒ः पव᳚मानो र॒यिर्वि रा᳚जति द्यु॒मान् ।",
          "begin": 0.0,
          "end": 6.352
        },
        {
          "text": "मधो॒र्धारा᳚भि॒रोज᳚सा ॥",
          "begin": 6.352,
          "end": 8.84
        }
      ]
    },
    {
      "id": "9.5.4",
      "mandala": 9,
      "sukta": 5,
      "rik": 4,
      "padas": [
        {
          "text": "ब॒र्हिः प्रा॒चीन॒मोज᳚सा॒ पव᳚मानः स्तृ॒णन्हरि᳚ः ।",
          "begin": 0.0,
          "end": 5.679
        },
        {
          "text": "दे॒वेषु᳚ दे॒व ई᳚यते ॥",
          "begin": 5.679,
          "end": 8.48
        }
      ]
    },
    {
      "id": "9.5.5",
      "mandala": 9,
      "sukta": 5,
      "rik": 5,
      "padas": [
        {
          "text": "उदातै᳚र्जिहते बृ॒हद्द्वारो᳚ दे॒वीर्हि᳚र॒ण्ययी᳚ः ।",
          "begin": 0.0,
          "end": 7.06
        },
        {
          "text": "पव᳚मानेन॒ सुष्टु᳚ताः ॥",
          "begin": 7.06,
          "end": 9.76
        }
      ]
    },
    {
      "id": "9.5.6",
      "mandala": 9,
      "sukta": 5,
      "rik": 6,
      "padas": [
        {
          "text": "सु॒शि॒ल्पे बृ᳚ह॒ती म॒ही पव᳚मानो वृषण्यति ।",
          "begin": 0.0,
          "end": 5.03
        },
        {
          "text": "नक्तो॒षासा॒ न द᳚र्श॒ते ॥",
          "begin": 5.03,
          "end": 7.56
        }
      ]
    },
    {
      "id": "9.5.7",
      "mandala": 9,
      "sukta": 5,
      "rik": 7,
      "padas": [
        {
          "text": "उ॒भा दे॒वा नृ॒चक्ष᳚सा॒ होता᳚रा॒ दैव्या᳚ हुवे ।",
          "begin": 0.0,
          "end": 6.226
        },
        {
          "text": "पव᳚मान॒ इन्द्रो॒ वृषा᳚ ॥",
          "begin": 6.226,
          "end": 8.92
        }
      ]
    },
    {
      "id": "9.5.8",
      "mandala": 9,
      "sukta": 5,
      "rik": 8,
      "padas": [
        {
          "text": "भार᳚ती॒ पव᳚मानस्य॒ सर᳚स्व॒तीळा᳚ म॒ही ।",
          "begin": 0.0,
          "end": 4.595
        },
        {
          "text": "इ॒मं नो᳚ य॒ज्ञमा ग᳚मन्ति॒स्रो दे॒वीः सु॒पेश᳚सः ॥",
          "begin": 4.595,
          "end": 10.12
        }
      ]
    },
    {
      "id": "9.5.9",
      "mandala": 9,
      "sukta": 5,
      "rik": 9,
      "padas": [
        {
          "text": "त्वष्टा᳚रमग्र॒जां गो॒पां पु᳚रो॒यावा᳚न॒मा हु᳚वे ।",
          "begin": 0.0,
          "end": 6.358
        },
        {
          "text": "इन्दु॒रिन्द्रो॒ वृषा॒ हरि॒ः पव᳚मानः प्र॒जाप᳚तिः ॥",
          "begin": 6.358,
          "end": 10.6
        }
      ]
    },
    {
      "id": "9.5.10",
      "mandala": 9,
      "sukta": 5,
      "rik": 10,
      "padas": [
        {
          "text": "वन॒स्पतिं᳚ पवमान॒ मध्वा॒ सम᳚ङ्ग्धि॒ धार᳚या ।",
          "begin": 0.0,
          "end": 5.406
        },
        {
          "text": "स॒हस्र᳚वल्शं॒ हरि᳚तं॒ भ्राज᳚मानं हिर॒ण्यय᳚म् ॥",
          "begin": 5.406,
          "end": 10.0
        }
      ]
    },
    {
      "id": "9.5.11",
      "mandala": 9,
      "sukta": 5,
      "rik": 11,
      "padas": [
        {
          "text": "विश्वे᳚ देवा॒ः स्वाहा᳚कृतिं॒ पव᳚मान॒स्या ग᳚त ।",
          "begin": 0.0,
          "end": 6.137
        },
        {
          "text": "वा॒युर्बृह॒स्पति॒ः सूर्यो॒॑ग्निरिन्द्र᳚ः स॒जोष᳚सः ॥",
          "begin": 6.137,
          "end": 12.88
        }
      ]
    }
  ]
}