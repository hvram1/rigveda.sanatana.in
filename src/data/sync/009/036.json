{
  "mandala": 9,
  "sukta": 36,
  "verses": [
    {
      "id": "9.36.1",
      "mandala": 9,
      "sukta": 36,
      "rik": 1,
      "padas": [
        {
          "text": "अस᳚र्जि॒ रथ्यो᳚ यथा प॒वित्रे᳚ च॒म्वो᳚ः सु॒तः ।",
          "begin": 0.0,
          "end": 3.814
        },
        {
          "text": "कार्ष्म᳚न्वा॒जी न्य᳚क्रमीत् ॥",
          "begin": 3.814,
          "end": 11.2
        }
      ]
    },
    {
      "id": "9.36.2",
      "mandala": 9,
      "sukta": 36,
      "rik": 2,
      "padas": [
        {
          "text": "स वह्नि᳚ः सोम॒ जागृ᳚वि॒ः पव᳚स्व देव॒वीरति᳚ ।",
          "begin": 0.0,
          "end": 4.861
        },
        {
          "text": "अ॒भि कोशं᳚ मधु॒श्चुत᳚म् ॥",
          "begin": 4.861,
          "end": 7.6
        }
      ]
    },
    {
      "id": "9.36.3",
      "mandala": 9,
      "sukta": 36,
      "rik": 3,
      "padas": [
        {
          "text": "स नो॒ ज्योतीं᳚षि पूर्व्य॒ पव᳚मान॒ वि रो᳚चय ।",
          "begin": 0.0,
          "end": 3.036
        },
        {
          "text": "क्रत्वे॒ दक्षा᳚य नो हिनु ॥",
          "begin": 3.036,
          "end": 8.16
        }
      ]
    },
    {
      "id": "9.36.4",
      "mandala": 9,
      "sukta": 36,
      "rik": 4,
      "padas": [
        {
          "text": "शु॒म्भमा᳚न ऋता॒युभि᳚र्मृ॒ज्यमा᳚नो॒ गभ᳚स्त्योः ।",
          "begin": 0.0,
          "end": 5.015
        },
        {
          "text": "पव᳚ते॒ वारे᳚ अ॒व्यये᳚ ॥",
          "begin": 5.015,
          "end": 9.12
        }
      ]
    },
    {
      "id": "9.36.5",
      "mandala": 9,
      "sukta": 36,
      "rik": 5,
      "padas": [
        {
          "text": "स विश्वा᳚ दा॒शुषे॒ वसु॒ सोमो᳚ दि॒व्यानि॒ पार्थि᳚वा ।",
          "begin": 0.0,
          "end": 5.602
        },
        {
          "text": "पव᳚ता॒मान्तरि᳚क्ष्या ॥",
          "begin": 5.602,
          "end": 8.2
        }
      ]
    },
    {
      "id": "9.36.6",
      "mandala": 9,
      "sukta": 36,
      "rik": 6,
      "padas": [
        {
          "text": "आ दि॒वस्पृ॒ष्ठम᳚श्व॒युर्ग᳚व्य॒युः सो᳚म रोहसि ।",
          "begin": 0.0,
          "end": 5.948
        },
        {
          "text": "वी॒र॒युः श᳚वसस्पते ॥",
          "begin": 5.948,
          "end": 9.32
        }
      ]
    }
  ]
}