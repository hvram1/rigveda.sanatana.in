    {
      "id": "9.107.1",
      "mandala": 9,
      "sukta": 107,
      "rik": 1,
      "padas": [
        {
          "text": "परी॒तो षि᳚ञ्चता सु॒तं सोमो॒ य उ᳚त्त॒मं ह॒विः ।",
          "begin": 0.0,
          "end": 4.911
        },
        {
          "text": "द॒ध॒न्वाँ यो नर्यो᳚ अ॒प्स्व१॒᳚न्तरा सु॒षाव॒ सोम॒मद्रि᳚भिः ॥",
          "begin": 4.911,
          "end": 14.08
        }
      ]
    },
    {
      "id": "9.107.2",
      "mandala": 9,
      "sukta": 107,
      "rik": 2,
      "padas": [
        {
          "text": "नू॒नं पु᳚ना॒नो॑वि᳚भि॒ः परि᳚ स्र॒वाद᳚ब्धः सुर॒भिन्त᳚रः ।",
          "begin": 0.0,
          "end": 5.806
        },
        {
          "text": "सु॒ते चि᳚त्त्वा॒प्सु म᳚दामो॒ अन्ध᳚सा श्री॒णन्तो॒ गोभि॒रुत्त᳚रम् ॥",
          "begin": 5.806,
          "end": 11.36
        }
      ]
    },
