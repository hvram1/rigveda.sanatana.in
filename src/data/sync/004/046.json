{
  "mandala": 4,
  "sukta": 46,
  "verses": [
    {
      "id": "4.46.1",
      "mandala": 4,
      "sukta": 46,
      "rik": 1,
      "padas": [
        {
          "text": "अग्रं᳚ पिबा॒ मधू᳚नां सु॒तं वा᳚यो॒ दिवि᳚ष्टिषु ।",
          "begin": 0.0,
          "end": 7.413
        },
        {
          "text": "त्वं हि पू᳚र्व॒पा असि᳚ ॥",
          "begin": 7.413,
          "end": 12.28
        }
      ]
    },
    {
      "id": "4.46.2",
      "mandala": 4,
      "sukta": 46,
      "rik": 2,
      "padas": [
        {
          "text": "श॒तेना᳚ नो अ॒भिष्टि᳚भिर्नि॒युत्वाँ॒ इन्द्र᳚सारथिः ।",
          "begin": 0.0,
          "end": 7.532
        },
        {
          "text": "वायो᳚ सु॒तस्य᳚ तृम्पतम् ॥",
          "begin": 7.532,
          "end": 10.88
        }
      ]
    },
    {
      "id": "4.46.3",
      "mandala": 4,
      "sukta": 46,
      "rik": 3,
      "padas": [
        {
          "text": "आ वां᳚ स॒हस्रं॒ हर᳚य॒ इन्द्र᳚वायू अ॒भि प्रयः᳚ ।",
          "begin": 0.0,
          "end": 7.506
        },
        {
          "text": "वह᳚न्तु॒ सोम᳚पीतये ॥",
          "begin": 7.506,
          "end": 10.6
        }
      ]
    },
    {
      "id": "4.46.4",
      "mandala": 4,
      "sukta": 46,
      "rik": 4,
      "padas": [
        {
          "text": "रथं॒ हिर᳚ण्यवन्धुर॒मिन्द्र᳚वायू स्वध्व॒रम् ।",
          "begin": 0.0,
          "end": 6.496
        },
        {
          "text": "आ हि स्थाथो᳚ दिवि॒स्पृश᳚म् ॥",
          "begin": 6.496,
          "end": 10.32
        }
      ]
    },
    {
      "id": "4.46.5",
      "mandala": 4,
      "sukta": 46,
      "rik": 5,
      "padas": [
        {
          "text": "रथे᳚न पृथु॒पाज᳚सा दा॒श्वांस॒मुप᳚ गच्छतम् ।",
          "begin": 0.0,
          "end": 5.887
        },
        {
          "text": "इन्द्र᳚वायू इ॒हा ग᳚तम् ॥",
          "begin": 5.887,
          "end": 9.68
        }
      ]
    },
    {
      "id": "4.46.6",
      "mandala": 4,
      "sukta": 46,
      "rik": 6,
      "padas": [
        {
          "text": "इन्द्र᳚वायू अ॒यं सु॒तस्तं दे॒वेभि᳚ः स॒जोष᳚सा ।",
          "begin": 0.0,
          "end": 5.935
        },
        {
          "text": "पिब᳚तं दा॒शुषो᳚ गृ॒हे ॥",
          "begin": 5.935,
          "end": 9.64
        }
      ]
    },
    {
      "id": "4.46.7",
      "mandala": 4,
      "sukta": 46,
      "rik": 7,
      "padas": [
        {
          "text": "इ॒ह प्र॒याण᳚मस्तु वा॒मिन्द्र᳚वायू वि॒मोच᳚नम् ।",
          "begin": 0.0,
          "end": 6.547
        },
        {
          "text": "इ॒ह वां॒ सोम᳚पीतये ॥",
          "begin": 6.547,
          "end": 11.76
        }
      ]
    }
  ]
}