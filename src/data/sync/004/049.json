{
  "mandala": 4,
  "sukta": 49,
  "verses": [
    {
      "id": "4.49.1",
      "mandala": 4,
      "sukta": 49,
      "rik": 1,
      "padas": [
        {
          "text": "इ॒दं वा᳚मा॒स्ये᳚ ह॒विः प्रि॒यमि᳚न्द्राबृहस्पती ।",
          "begin": 0.0,
          "end": 7.864
        },
        {
          "text": "उ॒क्थं मद᳚श्च शस्यते ॥",
          "begin": 7.864,
          "end": 10.64
        }
      ]
    },
    {
      "id": "4.49.2",
      "mandala": 4,
      "sukta": 49,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒यं वां॒ परि᳚ षिच्यते॒ सोम᳚ इन्द्राबृहस्पती ।",
          "begin": 0.0,
          "end": 5.904
        },
        {
          "text": "चारु॒र्मदा᳚य पी॒तये᳚ ॥",
          "begin": 5.904,
          "end": 9.92
        }
      ]
    },
    {
      "id": "4.49.3",
      "mandala": 4,
      "sukta": 49,
      "rik": 3,
      "padas": [
        {
          "text": "आ न᳚ इन्द्राबृहस्पती गृ॒हमिन्द्र᳚श्च गच्छतम् ।",
          "begin": 0.0,
          "end": 5.949
        },
        {
          "text": "सो॒म॒पा सोम᳚पीतये ॥",
          "begin": 5.949,
          "end": 8.8
        }
      ]
    },
    {
      "id": "4.49.4",
      "mandala": 4,
      "sukta": 49,
      "rik": 4,
      "padas": [
        {
          "text": "अ॒स्मे इ᳚न्द्राबृहस्पती र॒यिं ध᳚त्तं शत॒ग्विन᳚म् ।",
          "begin": 0.0,
          "end": 7.627
        },
        {
          "text": "अश्वा᳚वन्तं सह॒स्रिण᳚म् ॥",
          "begin": 7.627,
          "end": 11.28
        }
      ]
    },
    {
      "id": "4.49.5",
      "mandala": 4,
      "sukta": 49,
      "rik": 5,
      "padas": [
        {
          "text": "इन्द्रा॒बृह॒स्पती᳚ व॒यं सु॒ते गी॒र्भिर्ह᳚वामहे ।",
          "begin": 0.0,
          "end": 7.121
        },
        {
          "text": "अ॒स्य सोम᳚स्य पी॒तये᳚ ॥",
          "begin": 7.121,
          "end": 10.16
        }
      ]
    },
    {
      "id": "4.49.6",
      "mandala": 4,
      "sukta": 49,
      "rik": 6,
      "padas": [
        {
          "text": "सोम᳚मिन्द्राबृहस्पती॒ पिब᳚तं दा॒शुषो᳚ गृ॒हे ।",
          "begin": 0.0,
          "end": 7.158
        },
        {
          "text": "मा॒दये᳚थां॒ तदो᳚कसा ॥",
          "begin": 7.158,
          "end": 12.48
        }
      ]
    }
  ]
}