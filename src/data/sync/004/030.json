{
  "mandala": 4,
  "sukta": 30,
  "verses": [
    {
      "id": "4.30.1",
      "mandala": 4,
      "sukta": 30,
      "rik": 1,
      "padas": [
        {
          "text": "नकि᳚रिन्द्र॒ त्वदुत्त᳚रो॒ न ज्यायाँ᳚ अस्ति वृत्रहन् ।",
          "begin": 0.0,
          "end": 7.773
        },
        {
          "text": "नकि᳚रे॒वा यथा॒ त्वम् ॥",
          "begin": 7.773,
          "end": 11.2
        }
      ]
    },
    {
      "id": "4.30.2",
      "mandala": 4,
      "sukta": 30,
      "rik": 2,
      "padas": [
        {
          "text": "स॒त्रा ते॒ अनु᳚ कृ॒ष्टयो॒ विश्वा᳚ च॒क्रेव᳚ वावृतुः ।",
          "begin": 0.0,
          "end": 6.732
        },
        {
          "text": "स॒त्रा म॒हाँ अ᳚सि श्रु॒तः ॥",
          "begin": 6.732,
          "end": 9.88
        }
      ]
    },
    {
      "id": "4.30.3",
      "mandala": 4,
      "sukta": 30,
      "rik": 3,
      "padas": [
        {
          "text": "विश्वे᳚ च॒नेद॒ना त्वा᳚ दे॒वास᳚ इन्द्र युयुधुः ।",
          "begin": 0.0,
          "end": 5.401
        },
        {
          "text": "यदहा॒ नक्त॒माति᳚रः ॥",
          "begin": 5.401,
          "end": 9.56
        }
      ]
    },
    {
      "id": "4.30.4",
      "mandala": 4,
      "sukta": 30,
      "rik": 4,
      "padas": [
        {
          "text": "यत्रो॒त बा᳚धि॒तेभ्य᳚श्च॒क्रं कुत्सा᳚य॒ युध्य᳚ते ।",
          "begin": 0.0,
          "end": 6.713
        },
        {
          "text": "मु॒षा॒य इ᳚न्द्र॒ सूर्य᳚म् ॥",
          "begin": 6.713,
          "end": 10.08
        }
      ]
    },
    {
      "id": "4.30.5",
      "mandala": 4,
      "sukta": 30,
      "rik": 5,
      "padas": [
        {
          "text": "यत्र᳚ दे॒वाँ ऋ᳚घाय॒तो विश्वाँ॒ अयु᳚ध्य॒ एक॒ इत् ।",
          "begin": 0.0,
          "end": 6.311
        },
        {
          "text": "त्वमि᳚न्द्र व॒नूँरह᳚न् ॥",
          "begin": 6.311,
          "end": 9.56
        }
      ]
    },
    {
      "id": "4.30.6",
      "mandala": 4,
      "sukta": 30,
      "rik": 6,
      "padas": [
        {
          "text": "यत्रो॒त मर्त्या᳚य॒ कमरि᳚णा इन्द्र॒ सूर्य᳚म् ।",
          "begin": 0.0,
          "end": 5.429
        },
        {
          "text": "प्राव॒ः शची᳚भि॒रेत᳚शम् ॥",
          "begin": 5.429,
          "end": 9.28
        }
      ]
    },
    {
      "id": "4.30.7",
      "mandala": 4,
      "sukta": 30,
      "rik": 7,
      "padas": [
        {
          "text": "किमादु॒तासि᳚ वृत्रह॒न्मघ᳚वन्मन्यु॒मत्त᳚मः ।",
          "begin": 0.0,
          "end": 5.85
        },
        {
          "text": "अत्राह॒ दानु॒माति᳚रः ॥",
          "begin": 5.85,
          "end": 8.4
        }
      ]
    },
    {
      "id": "4.30.8",
      "mandala": 4,
      "sukta": 30,
      "rik": 8,
      "padas": [
        {
          "text": "ए॒तद्घेदु॒त वी॒र्य१॒᳚मिन्द्र᳚ च॒कर्थ॒ पौंस्य᳚म् ।",
          "begin": 0.0,
          "end": 5.811
        },
        {
          "text": "स्त्रियं॒ यद्दु᳚र्हणा॒युवं॒ वधी᳚र्दुहि॒तरं᳚ दि॒वः ॥",
          "begin": 5.811,
          "end": 12.32
        }
      ]
    },
    {
      "id": "4.30.9",
      "mandala": 4,
      "sukta": 30,
      "rik": 9,
      "padas": [
        {
          "text": "दि॒वश्चि᳚द्घा दुहि॒तरं᳚ म॒हान्म᳚ही॒यमा᳚नाम् ।",
          "begin": 0.0,
          "end": 6.603
        },
        {
          "text": "उ॒षास᳚मिन्द्र॒ सं पि᳚णक् ॥",
          "begin": 6.603,
          "end": 9.32
        }
      ]
    },
    {
      "id": "4.30.10",
      "mandala": 4,
      "sukta": 30,
      "rik": 10,
      "padas": [
        {
          "text": "अपो॒षा अन᳚सः सर॒त्सम्पि᳚ष्टा॒दह᳚ बि॒भ्युषी᳚ ।",
          "begin": 0.0,
          "end": 6.454
        },
        {
          "text": "नि यत्सीं᳚ शि॒श्नथ॒द्वृषा᳚ ॥",
          "begin": 6.454,
          "end": 10.12
        }
      ]
    },
    {
      "id": "4.30.11",
      "mandala": 4,
      "sukta": 30,
      "rik": 11,
      "padas": [
        {
          "text": "ए॒तद᳚स्या॒ अनः᳚ शये॒ सुस᳚म्पिष्टं॒ विपा॒श्या ।",
          "begin": 0.0,
          "end": 6.843
        },
        {
          "text": "स॒सार᳚ सीं परा॒वतः᳚ ॥",
          "begin": 6.843,
          "end": 9.92
        }
      ]
    },
    {
      "id": "4.30.12",
      "mandala": 4,
      "sukta": 30,
      "rik": 12,
      "padas": [
        {
          "text": "उ॒त सिन्धुं᳚ विबा॒ल्यं᳚ वितस्था॒नामधि॒ क्षमि᳚ ।",
          "begin": 0.0,
          "end": 6.644
        },
        {
          "text": "परि᳚ ष्ठा इन्द्र मा॒यया᳚ ॥",
          "begin": 6.644,
          "end": 10.16
        }
      ]
    },
    {
      "id": "4.30.13",
      "mandala": 4,
      "sukta": 30,
      "rik": 13,
      "padas": [
        {
          "text": "उ॒त शुष्ण᳚स्य धृष्णु॒या प्र मृ᳚क्षो अ॒भि वेद᳚नम् ।",
          "begin": 0.0,
          "end": 5.581
        },
        {
          "text": "पुरो॒ यद᳚स्य सम्पि॒णक् ॥",
          "begin": 5.581,
          "end": 8.8
        }
      ]
    },
    {
      "id": "4.30.14",
      "mandala": 4,
      "sukta": 30,
      "rik": 14,
      "padas": [
        {
          "text": "उ॒त दा॒सं कौ᳚लित॒रं बृ᳚ह॒तः पर्व᳚ता॒दधि᳚ ।",
          "begin": 0.0,
          "end": 6.606
        },
        {
          "text": "अवा᳚हन्निन्द्र॒ शम्ब᳚रम् ॥",
          "begin": 6.606,
          "end": 9.32
        }
      ]
    },
    {
      "id": "4.30.15",
      "mandala": 4,
      "sukta": 30,
      "rik": 15,
      "padas": [
        {
          "text": "उ॒त दा॒सस्य᳚ व॒र्चिनः᳚ स॒हस्रा᳚णि श॒ताव᳚धीः ।",
          "begin": 0.0,
          "end": 5.685
        },
        {
          "text": "अधि॒ पञ्च᳚ प्र॒धीँरि᳚व ॥",
          "begin": 5.685,
          "end": 9.04
        }
      ]
    },
    {
      "id": "4.30.16",
      "mandala": 4,
      "sukta": 30,
      "rik": 16,
      "padas": [
        {
          "text": "उ॒त त्यं पु॒त्रम॒ग्रुव॒ः परा᳚वृक्तं श॒तक्र᳚तुः ।",
          "begin": 0.0,
          "end": 6.056
        },
        {
          "text": "उ॒क्थेष्विन्द्र॒ आभ᳚जत् ॥",
          "begin": 6.056,
          "end": 8.88
        }
      ]
    },
    {
      "id": "4.30.17",
      "mandala": 4,
      "sukta": 30,
      "rik": 17,
      "padas": [
        {
          "text": "उ॒त त्या तु॒र्वशा॒यदू᳚ अस्ना॒तारा॒ शची॒पति᳚ः ।",
          "begin": 0.0,
          "end": 6.443
        },
        {
          "text": "इन्द्रो᳚ वि॒द्वाँ अ᳚पारयत् ॥",
          "begin": 6.443,
          "end": 10.2
        }
      ]
    },
    {
      "id": "4.30.18",
      "mandala": 4,
      "sukta": 30,
      "rik": 18,
      "padas": [
        {
          "text": "उ॒त त्या स॒द्य आर्या᳚ स॒रयो᳚रिन्द्र पा॒रतः᳚ ।",
          "begin": 0.0,
          "end": 7.151
        },
        {
          "text": "अर्णा᳚चि॒त्रर᳚थावधीः ॥",
          "begin": 7.151,
          "end": 10.52
        }
      ]
    },
    {
      "id": "4.30.19",
      "mandala": 4,
      "sukta": 30,
      "rik": 19,
      "padas": [
        {
          "text": "अनु॒ द्वा ज᳚हि॒ता न᳚यो॒॑न्धं श्रो॒णं च᳚ वृत्रहन् ।",
          "begin": 0.0,
          "end": 6.876
        },
        {
          "text": "न तत्ते᳚ सु॒म्नमष्ट᳚वे ॥",
          "begin": 6.876,
          "end": 9.64
        }
      ]
    },
    {
      "id": "4.30.20",
      "mandala": 4,
      "sukta": 30,
      "rik": 20,
      "padas": [
        {
          "text": "श॒तम᳚श्म॒न्मयी᳚नां पु॒रामिन्द्रो॒ व्या᳚स्यत् ।",
          "begin": 0.0,
          "end": 6.695
        },
        {
          "text": "दिवो᳚दासाय दा॒शुषे᳚ ॥",
          "begin": 6.695,
          "end": 10.68
        }
      ]
    },
    {
      "id": "4.30.21",
      "mandala": 4,
      "sukta": 30,
      "rik": 21,
      "padas": [
        {
          "text": "अस्वा᳚पयद्द॒भीत᳚ये स॒हस्रा᳚ त्रिं॒शतं॒ हथै᳚ः ।",
          "begin": 0.0,
          "end": 7.603
        },
        {
          "text": "दा॒साना॒मिन्द्रो᳚ मा॒यया᳚ ॥",
          "begin": 7.603,
          "end": 11.64
        }
      ]
    },
    {
      "id": "4.30.22",
      "mandala": 4,
      "sukta": 30,
      "rik": 22,
      "padas": [
        {
          "text": "स घेदु॒तासि᳚ वृत्रहन्त्समा॒न इ᳚न्द्र॒ गोप᳚तिः ।",
          "begin": 0.0,
          "end": 6.392
        },
        {
          "text": "यस्ता विश्वा᳚नि चिच्यु॒षे ॥",
          "begin": 6.392,
          "end": 9.76
        }
      ]
    },
    {
      "id": "4.30.23",
      "mandala": 4,
      "sukta": 30,
      "rik": 23,
      "padas": [
        {
          "text": "उ॒त नू॒नं यदि᳚न्द्रि॒यं क᳚रि॒ष्या इ᳚न्द्र॒ पौंस्य᳚म् ।",
          "begin": 0.0,
          "end": 6.877
        },
        {
          "text": "अ॒द्या नकि॒ष्टदा मि᳚नत् ॥",
          "begin": 6.877,
          "end": 10.16
        }
      ]
    },
    {
      "id": "4.30.24",
      "mandala": 4,
      "sukta": 30,
      "rik": 24,
      "padas": [
        {
          "text": "वा॒मंवा᳚मं त आदुरे दे॒वो द᳚दात्वर्य॒मा ।",
          "begin": 0.0,
          "end": 7.027
        },
        {
          "text": "वा॒मं पू॒षा वा॒मं भगो᳚ वा॒मं दे॒वः करू᳚ळती ॥",
          "begin": 7.027,
          "end": 15.0
        }
      ]
    }
  ]
}