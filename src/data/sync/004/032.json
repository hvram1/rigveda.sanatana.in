{
  "mandala": 4,
  "sukta": 32,
  "verses": [
    {
      "id": "4.32.1",
      "mandala": 4,
      "sukta": 32,
      "rik": 1,
      "padas": [
        {
          "text": "आ तू न᳚ इन्द्र वृत्रहन्न॒स्माक᳚म॒र्धमा ग᳚हि ।",
          "begin": 0.0,
          "end": 6.655
        },
        {
          "text": "म॒हान्म॒हीभि᳚रू॒तिभि᳚ः ॥",
          "begin": 6.655,
          "end": 9.6
        }
      ]
    },
    {
      "id": "4.32.2",
      "mandala": 4,
      "sukta": 32,
      "rik": 2,
      "padas": [
        {
          "text": "भृमि᳚श्चिद्घासि॒ तूतु᳚जि॒रा चि᳚त्र चि॒त्रिणी॒ष्वा ।",
          "begin": 0.0,
          "end": 6.091
        },
        {
          "text": "चि॒त्रं कृ᳚णोष्यू॒तये᳚ ॥",
          "begin": 6.091,
          "end": 9.28
        }
      ]
    },
    {
      "id": "4.32.3",
      "mandala": 4,
      "sukta": 32,
      "rik": 3,
      "padas": [
        {
          "text": "द॒भ्रेभि᳚श्चि॒च्छशी᳚यांसं॒ हंसि॒ व्राध᳚न्त॒मोज᳚सा ।",
          "begin": 0.0,
          "end": 7.197
        },
        {
          "text": "सखि᳚भि॒र्ये त्वे सचा᳚ ॥",
          "begin": 7.197,
          "end": 10.36
        }
      ]
    },
    {
      "id": "4.32.4",
      "mandala": 4,
      "sukta": 32,
      "rik": 4,
      "padas": [
        {
          "text": "व॒यमि᳚न्द्र॒ त्वे सचा᳚ व॒यं त्वा॒भि नो᳚नुमः ।",
          "begin": 0.0,
          "end": 6.759
        },
        {
          "text": "अ॒स्माँअ᳚स्माँ॒ इदुद᳚व ॥",
          "begin": 6.759,
          "end": 10.04
        }
      ]
    },
    {
      "id": "4.32.5",
      "mandala": 4,
      "sukta": 32,
      "rik": 5,
      "padas": [
        {
          "text": "स न᳚श्चि॒त्राभि᳚रद्रिवो॑नव॒द्याभि᳚रू॒तिभि᳚ः ।",
          "begin": 0.0,
          "end": 5.951
        },
        {
          "text": "अना᳚धृष्टाभि॒रा ग᳚हि ॥",
          "begin": 5.951,
          "end": 8.88
        }
      ]
    },
    {
      "id": "4.32.6",
      "mandala": 4,
      "sukta": 32,
      "rik": 6,
      "padas": [
        {
          "text": "भू॒यामो॒ षु त्वाव᳚त॒ः सखा᳚य इन्द्र॒ गोम᳚तः ।",
          "begin": 0.0,
          "end": 6.04
        },
        {
          "text": "युजो॒ वाजा᳚य॒ घृष्व᳚ये ॥",
          "begin": 6.04,
          "end": 9.4
        }
      ]
    },
    {
      "id": "4.32.7",
      "mandala": 4,
      "sukta": 32,
      "rik": 7,
      "padas": [
        {
          "text": "त्वं ह्येक॒ ईशि᳚ष॒ इन्द्र॒ वाज᳚स्य॒ गोम᳚तः ।",
          "begin": 0.0,
          "end": 5.683
        },
        {
          "text": "स नो᳚ यन्धि म॒हीमिष᳚म् ॥",
          "begin": 5.683,
          "end": 9.08
        }
      ]
    },
    {
      "id": "4.32.8",
      "mandala": 4,
      "sukta": 32,
      "rik": 8,
      "padas": [
        {
          "text": "न त्वा᳚ वरन्ते अ॒न्यथा॒ यद्दित्स᳚सि स्तु॒तो म॒घम् ।",
          "begin": 0.0,
          "end": 6.398
        },
        {
          "text": "स्तो॒तृभ्य᳚ इन्द्र गिर्वणः ॥",
          "begin": 6.398,
          "end": 9.4
        }
      ]
    },
    {
      "id": "4.32.9",
      "mandala": 4,
      "sukta": 32,
      "rik": 9,
      "padas": [
        {
          "text": "अ॒भि त्वा॒ गोत᳚मा गि॒रानू᳚षत॒ प्र दा॒वने᳚ ।",
          "begin": 0.0,
          "end": 6.142
        },
        {
          "text": "इन्द्र॒ वाजा᳚य॒ घृष्व᳚ये ॥",
          "begin": 6.142,
          "end": 9.24
        }
      ]
    },
    {
      "id": "4.32.10",
      "mandala": 4,
      "sukta": 32,
      "rik": 10,
      "padas": [
        {
          "text": "प्र ते᳚ वोचाम वी॒र्या॒३॒᳚ या म᳚न्दसा॒न आरु᳚जः ।",
          "begin": 0.0,
          "end": 8.911
        },
        {
          "text": "पुरो॒ दासी᳚र॒भीत्य᳚ ॥",
          "begin": 8.911,
          "end": 12.72
        }
      ]
    },
    {
      "id": "4.32.11",
      "mandala": 4,
      "sukta": 32,
      "rik": 11,
      "padas": [
        {
          "text": "ता ते᳚ गृणन्ति वे॒धसो॒ यानि᳚ च॒कर्थ॒ पौंस्या᳚ ।",
          "begin": 0.0,
          "end": 6.611
        },
        {
          "text": "सु॒तेष्वि᳚न्द्र गिर्वणः ॥",
          "begin": 6.611,
          "end": 9.72
        }
      ]
    },
    {
      "id": "4.32.12",
      "mandala": 4,
      "sukta": 32,
      "rik": 12,
      "padas": [
        {
          "text": "अवी᳚वृधन्त॒ गोत᳚मा॒ इन्द्र॒ त्वे स्तोम᳚वाहसः ।",
          "begin": 0.0,
          "end": 6.604
        },
        {
          "text": "ऐषु᳚ धा वी॒रव॒द्यशः᳚ ॥",
          "begin": 6.604,
          "end": 9.96
        }
      ]
    },
    {
      "id": "4.32.13",
      "mandala": 4,
      "sukta": 32,
      "rik": 13,
      "padas": [
        {
          "text": "यच्चि॒द्धि शश्व᳚ता॒मसीन्द्र॒ साधा᳚रण॒स्त्वम् ।",
          "begin": 0.0,
          "end": 5.302
        },
        {
          "text": "तं त्वा᳚ व॒यं ह᳚वामहे ॥",
          "begin": 5.302,
          "end": 8.76
        }
      ]
    },
    {
      "id": "4.32.14",
      "mandala": 4,
      "sukta": 32,
      "rik": 14,
      "padas": [
        {
          "text": "अ॒र्वा॒ची॒नो व᳚सो भवा॒स्मे सु म॒त्स्वान्ध᳚सः ।",
          "begin": 0.0,
          "end": 6.025
        },
        {
          "text": "सोमा᳚नामिन्द्र सोमपाः ॥",
          "begin": 6.025,
          "end": 9.72
        }
      ]
    },
    {
      "id": "4.32.15",
      "mandala": 4,
      "sukta": 32,
      "rik": 15,
      "padas": [
        {
          "text": "अ॒स्माकं᳚ त्वा मती॒नामा स्तोम᳚ इन्द्र यच्छतु ।",
          "begin": 0.0,
          "end": 5.796
        },
        {
          "text": "अ॒र्वागा व᳚र्तया॒ हरी᳚ ॥",
          "begin": 5.796,
          "end": 9.56
        }
      ]
    },
    {
      "id": "4.32.16",
      "mandala": 4,
      "sukta": 32,
      "rik": 16,
      "padas": [
        {
          "text": "पु॒रो॒ळाशं᳚ च नो॒ घसो᳚ जो॒षया᳚से॒ गिर᳚श्च नः ।",
          "begin": 0.0,
          "end": 6.466
        },
        {
          "text": "व॒धू॒युरि᳚व॒ योष᳚णाम् ॥",
          "begin": 6.466,
          "end": 9.68
        }
      ]
    },
    {
      "id": "4.32.17",
      "mandala": 4,
      "sukta": 32,
      "rik": 17,
      "padas": [
        {
          "text": "स॒हस्रं॒ व्यती᳚नां यु॒क्ताना॒मिन्द्र᳚मीमहे ।",
          "begin": 0.0,
          "end": 6.187
        },
        {
          "text": "श॒तं सोम᳚स्य खा॒र्य᳚ः ॥",
          "begin": 6.187,
          "end": 9.28
        }
      ]
    },
    {
      "id": "4.32.18",
      "mandala": 4,
      "sukta": 32,
      "rik": 18,
      "padas": [
        {
          "text": "स॒हस्रा᳚ ते श॒ता व॒यं गवा॒मा च्या᳚वयामसि ।",
          "begin": 0.0,
          "end": 6.747
        },
        {
          "text": "अ॒स्म॒त्रा राध᳚ एतु ते ॥",
          "begin": 6.747,
          "end": 9.72
        }
      ]
    },
    {
      "id": "4.32.19",
      "mandala": 4,
      "sukta": 32,
      "rik": 19,
      "padas": [
        {
          "text": "दश᳚ ते क॒लशा᳚नां॒ हिर᳚ण्यानामधीमहि ।",
          "begin": 0.0,
          "end": 6.347
        },
        {
          "text": "भू॒रि॒दा अ᳚सि वृत्रहन् ॥",
          "begin": 6.347,
          "end": 9.36
        }
      ]
    },
    {
      "id": "4.32.20",
      "mandala": 4,
      "sukta": 32,
      "rik": 20,
      "padas": [
        {
          "text": "भूरि᳚दा॒ भूरि᳚ देहि नो॒ मा द॒भ्रं भूर्या भ᳚र ।",
          "begin": 0.0,
          "end": 5.999
        },
        {
          "text": "भूरि॒ घेदि᳚न्द्र दित्ससि ॥",
          "begin": 5.999,
          "end": 9.04
        }
      ]
    },
    {
      "id": "4.32.21",
      "mandala": 4,
      "sukta": 32,
      "rik": 21,
      "padas": [
        {
          "text": "भू॒रि॒दा ह्यसि᳚ श्रु॒तः पु᳚रु॒त्रा शू᳚र वृत्रहन् ।",
          "begin": 0.0,
          "end": 6.865
        },
        {
          "text": "आ नो᳚ भजस्व॒ राध᳚सि ॥",
          "begin": 6.865,
          "end": 9.6
        }
      ]
    },
    {
      "id": "4.32.22",
      "mandala": 4,
      "sukta": 32,
      "rik": 22,
      "padas": [
        {
          "text": "प्र ते᳚ ब॒भ्रू वि᳚चक्षण॒ शंसा᳚मि गोषणो नपात् ।",
          "begin": 0.0,
          "end": 7.128
        },
        {
          "text": "माभ्यां॒ गा अनु᳚ शिश्रथः ॥",
          "begin": 7.128,
          "end": 10.76
        }
      ]
    },
    {
      "id": "4.32.23",
      "mandala": 4,
      "sukta": 32,
      "rik": 23,
      "padas": [
        {
          "text": "क॒नी॒न॒केव᳚ विद्र॒धे नवे᳚ द्रुप॒दे अ᳚र्भ॒के ।",
          "begin": 0.0,
          "end": 6.681
        },
        {
          "text": "ब॒भ्रू यामे᳚षु शोभेते ॥",
          "begin": 6.681,
          "end": 10.24
        }
      ]
    },
    {
      "id": "4.32.24",
      "mandala": 4,
      "sukta": 32,
      "rik": 24,
      "padas": [
        {
          "text": "अरं᳚ म उ॒स्रया॒म्णे॑र॒मनु᳚स्रयाम्णे ।",
          "begin": 0.0,
          "end": 6.231
        },
        {
          "text": "ब॒भ्रू यामे᳚ष्व॒स्रिधा᳚ ॥",
          "begin": 6.231,
          "end": 10.12
        }
      ]
    }
  ]
}