{
  "mandala": 8,
  "sukta": 43,
  "verses": [
    {
      "id": "8.43.1",
      "mandala": 8,
      "sukta": 43,
      "rik": 1,
      "padas": [
        {
          "text": "इ॒मे विप्र᳚स्य वे॒धसो॒॑ग्नेरस्तृ᳚तयज्वनः ।",
          "begin": 0.0,
          "end": 7.107
        },
        {
          "text": "गिर॒ः स्तोमा᳚स ईरते ॥",
          "begin": 7.107,
          "end": 10.52
        }
      ]
    },
    {
      "id": "8.43.2",
      "mandala": 8,
      "sukta": 43,
      "rik": 2,
      "padas": [
        {
          "text": "अस्मै᳚ ते प्रति॒हर्य᳚ते॒ जात᳚वेदो॒ विच᳚र्षणे ।",
          "begin": 0.0,
          "end": 5.362
        },
        {
          "text": "अग्ने॒ जना᳚मि सुष्टु॒तिम् ॥",
          "begin": 5.362,
          "end": 7.96
        }
      ]
    },
    {
      "id": "8.43.3",
      "mandala": 8,
      "sukta": 43,
      "rik": 3,
      "padas": [
        {
          "text": "आ॒रो॒का इ᳚व॒ घेदह᳚ ति॒ग्मा अ᳚ग्ने॒ तव॒ त्विषः᳚ ।",
          "begin": 0.0,
          "end": 5.199
        },
        {
          "text": "द॒द्भिर्वना᳚नि बप्सति ॥",
          "begin": 5.199,
          "end": 7.96
        }
      ]
    },
    {
      "id": "8.43.4",
      "mandala": 8,
      "sukta": 43,
      "rik": 4,
      "padas": [
        {
          "text": "हर᳚यो धू॒मके᳚तवो॒ वात᳚जूता॒ उप॒ द्यवि᳚ ।",
          "begin": 0.0,
          "end": 5.211
        },
        {
          "text": "यत᳚न्ते॒ वृथ᳚ग॒ग्नयः᳚ ॥",
          "begin": 5.211,
          "end": 7.92
        }
      ]
    },
    {
      "id": "8.43.5",
      "mandala": 8,
      "sukta": 43,
      "rik": 5,
      "padas": [
        {
          "text": "ए॒ते त्ये वृथ᳚ग॒ग्नय᳚ इ॒द्धास॒ः सम᳚दृक्षत ।",
          "begin": 0.0,
          "end": 5.035
        },
        {
          "text": "उ॒षसा᳚मिव के॒तवः᳚ ॥",
          "begin": 5.035,
          "end": 7.48
        }
      ]
    },
    {
      "id": "8.43.6",
      "mandala": 8,
      "sukta": 43,
      "rik": 6,
      "padas": [
        {
          "text": "कृ॒ष्णा रजां᳚सि पत्सु॒तः प्र॒याणे᳚ जा॒तवे᳚दसः ।",
          "begin": 0.0,
          "end": 5.856
        },
        {
          "text": "अ॒ग्निर्यद्रोध᳚ति॒ क्षमि᳚ ॥",
          "begin": 5.856,
          "end": 8.28
        }
      ]
    },
    {
      "id": "8.43.7",
      "mandala": 8,
      "sukta": 43,
      "rik": 7,
      "padas": [
        {
          "text": "धा॒सिं कृ᳚ण्वा॒न ओष᳚धी॒र्बप्स᳚द॒ग्निर्न वा᳚यति ।",
          "begin": 0.0,
          "end": 5.517
        },
        {
          "text": "पुन॒र्यन्तरु᳚णी॒रपि᳚ ॥",
          "begin": 5.517,
          "end": 7.8
        }
      ]
    },
    {
      "id": "8.43.8",
      "mandala": 8,
      "sukta": 43,
      "rik": 8,
      "padas": [
        {
          "text": "जि॒ह्वाभि॒रह॒ नन्न᳚मद॒र्चिषा᳚ जञ्जणा॒भव᳚न् ।",
          "begin": 0.0,
          "end": 4.504
        },
        {
          "text": "अ॒ग्निर्वने᳚षु रोचते ॥",
          "begin": 4.504,
          "end": 7.4
        }
      ]
    },
    {
      "id": "8.43.9",
      "mandala": 8,
      "sukta": 43,
      "rik": 9,
      "padas": [
        {
          "text": "अ॒प्स्व᳚ग्ने॒ सधि॒ष्टव॒ सौष᳚धी॒रनु᳚ रुध्यसे ।",
          "begin": 0.0,
          "end": 4.071
        },
        {
          "text": "गर्भे॒ सञ्जा᳚यसे॒ पुनः᳚ ॥",
          "begin": 4.071,
          "end": 7.44
        }
      ]
    },
    {
      "id": "8.43.10",
      "mandala": 8,
      "sukta": 43,
      "rik": 10,
      "padas": [
        {
          "text": "उद᳚ग्ने॒ तव॒ तद्घृ॒ताद॒र्ची रो᳚चत॒ आहु᳚तम् ।",
          "begin": 0.0,
          "end": 5.543
        },
        {
          "text": "निंसा᳚नं जु॒ह्वो॒३॒᳚ मुखे᳚ ॥",
          "begin": 5.543,
          "end": 10.36
        }
      ]
    },
    {
      "id": "8.43.11",
      "mandala": 8,
      "sukta": 43,
      "rik": 11,
      "padas": [
        {
          "text": "उ॒क्षान्ना᳚य व॒शान्ना᳚य॒ सोम᳚पृष्ठाय वे॒धसे᳚ ।",
          "begin": 0.0,
          "end": 7.057
        },
        {
          "text": "स्तोमै᳚र्विधेमा॒ग्नये᳚ ॥",
          "begin": 7.057,
          "end": 9.44
        }
      ]
    },
    {
      "id": "8.43.12",
      "mandala": 8,
      "sukta": 43,
      "rik": 12,
      "padas": [
        {
          "text": "उ॒त त्वा॒ नम᳚सा व॒यं होत॒र्वरे᳚ण्यक्रतो ।",
          "begin": 0.0,
          "end": 5.221
        },
        {
          "text": "अग्ने᳚ स॒मिद्भि᳚रीमहे ॥",
          "begin": 5.221,
          "end": 7.64
        }
      ]
    },
    {
      "id": "8.43.13",
      "mandala": 8,
      "sukta": 43,
      "rik": 13,
      "padas": [
        {
          "text": "उ॒त त्वा᳚ भृगु॒वच्छु᳚चे मनु॒ष्वद᳚ग्न आहुत ।",
          "begin": 0.0,
          "end": 4.836
        },
        {
          "text": "अ॒ङ्गि॒र॒स्वद्ध᳚वामहे ॥",
          "begin": 4.836,
          "end": 7.0
        }
      ]
    },
    {
      "id": "8.43.14",
      "mandala": 8,
      "sukta": 43,
      "rik": 14,
      "padas": [
        {
          "text": "त्वं ह्य᳚ग्ने अ॒ग्निना॒ विप्रो॒ विप्रे᳚ण॒ सन्त्स॒ता ।",
          "begin": 0.0,
          "end": 6.226
        },
        {
          "text": "सखा॒ सख्या᳚ समि॒ध्यसे᳚ ॥",
          "begin": 6.226,
          "end": 9.32
        }
      ]
    },
    {
      "id": "8.43.15",
      "mandala": 8,
      "sukta": 43,
      "rik": 15,
      "padas": [
        {
          "text": "स त्वं विप्रा᳚य दा॒शुषे᳚ र॒यिं दे᳚हि सह॒स्रिण᳚म् ।",
          "begin": 0.0,
          "end": 6.279
        },
        {
          "text": "अग्ने᳚ वी॒रव᳚ती॒मिष᳚म् ॥",
          "begin": 6.279,
          "end": 9.12
        }
      ]
    },
    {
      "id": "8.43.16",
      "mandala": 8,
      "sukta": 43,
      "rik": 16,
      "padas": [
        {
          "text": "अग्ने॒ भ्रात॒ः सह᳚स्कृत॒ रोहि᳚दश्व॒ शुचि᳚व्रत ।",
          "begin": 0.0,
          "end": 5.419
        },
        {
          "text": "इ॒मं स्तोमं᳚ जुषस्व मे ॥",
          "begin": 5.419,
          "end": 8.28
        }
      ]
    },
    {
      "id": "8.43.17",
      "mandala": 8,
      "sukta": 43,
      "rik": 17,
      "padas": [
        {
          "text": "उ॒त त्वा᳚ग्ने॒ मम॒ स्तुतो᳚ वा॒श्राय᳚ प्रति॒हर्य᳚ते ।",
          "begin": 0.0,
          "end": 5.872
        },
        {
          "text": "गो॒ष्ठं गाव᳚ इवाशत ॥",
          "begin": 5.872,
          "end": 8.4
        }
      ]
    },
    {
      "id": "8.43.18",
      "mandala": 8,
      "sukta": 43,
      "rik": 18,
      "padas": [
        {
          "text": "तुभ्यं॒ ता अ᳚ङ्गिरस्तम॒ विश्वा᳚ः सुक्षि॒तय॒ः पृथ᳚क् ।",
          "begin": 0.0,
          "end": 6.19
        },
        {
          "text": "अग्ने॒ कामा᳚य येमिरे ॥",
          "begin": 6.19,
          "end": 8.8
        }
      ]
    },
    {
      "id": "8.43.19",
      "mandala": 8,
      "sukta": 43,
      "rik": 19,
      "padas": [
        {
          "text": "अ॒ग्निं धी॒भिर्म᳚नी॒षिणो॒ मेधि᳚रासो विप॒श्चितः᳚ ।",
          "begin": 0.0,
          "end": 5.581
        },
        {
          "text": "अ॒द्म॒सद्या᳚य हिन्विरे ॥",
          "begin": 5.581,
          "end": 7.84
        }
      ]
    },
    {
      "id": "8.43.20",
      "mandala": 8,
      "sukta": 43,
      "rik": 20,
      "padas": [
        {
          "text": "तं त्वामज्मे᳚षु वा॒जिनं᳚ तन्वा॒ना अ᳚ग्ने अध्व॒रम् ।",
          "begin": 0.0,
          "end": 6.454
        },
        {
          "text": "वह्निं॒ होता᳚रमीळते ॥",
          "begin": 6.454,
          "end": 9.16
        }
      ]
    },
    {
      "id": "8.43.21",
      "mandala": 8,
      "sukta": 43,
      "rik": 21,
      "padas": [
        {
          "text": "पु॒रु॒त्रा हि स॒दृङ्ङसि॒ विशो॒ विश्वा॒ अनु᳚ प्र॒भुः ।",
          "begin": 0.0,
          "end": 4.952
        },
        {
          "text": "स॒मत्सु᳚ त्वा हवामहे ॥",
          "begin": 4.952,
          "end": 7.12
        }
      ]
    },
    {
      "id": "8.43.22",
      "mandala": 8,
      "sukta": 43,
      "rik": 22,
      "padas": [
        {
          "text": "तमी᳚ळिष्व॒ य आहु᳚तो॒॑ग्निर्वि॒भ्राज᳚ते घृ॒तैः ।",
          "begin": 0.0,
          "end": 5.358
        },
        {
          "text": "इ॒मं नः᳚ शृणव॒द्धव᳚म् ॥",
          "begin": 5.358,
          "end": 7.96
        }
      ]
    },
    {
      "id": "8.43.23",
      "mandala": 8,
      "sukta": 43,
      "rik": 23,
      "padas": [
        {
          "text": "तं त्वा᳚ व॒यं ह᳚वामहे शृ॒ण्वन्तं᳚ जा॒तवे᳚दसम् ।",
          "begin": 0.0,
          "end": 5.444
        },
        {
          "text": "अग्ने॒ घ्नन्त॒मप॒ द्विषः᳚ ॥",
          "begin": 5.444,
          "end": 8.64
        }
      ]
    },
    {
      "id": "8.43.24",
      "mandala": 8,
      "sukta": 43,
      "rik": 24,
      "padas": [
        {
          "text": "वि॒शां राजा᳚न॒मद्भु᳚त॒मध्य᳚क्षं॒ धर्म᳚णामि॒मम् ।",
          "begin": 0.0,
          "end": 4.999
        },
        {
          "text": "अ॒ग्निमी᳚ळे॒ स उ᳚ श्रवत् ॥",
          "begin": 4.999,
          "end": 8.36
        }
      ]
    },
    {
      "id": "8.43.25",
      "mandala": 8,
      "sukta": 43,
      "rik": 25,
      "padas": [
        {
          "text": "अ॒ग्निं वि॒श्वायु᳚वेपसं॒ मर्यं॒ न वा॒जिनं᳚ हि॒तम् ।",
          "begin": 0.0,
          "end": 5.478
        },
        {
          "text": "सप्तिं॒ न वा᳚जयामसि ॥",
          "begin": 5.478,
          "end": 8.08
        }
      ]
    },
    {
      "id": "8.43.26",
      "mandala": 8,
      "sukta": 43,
      "rik": 26,
      "padas": [
        {
          "text": "घ्नन्मृ॒ध्राण्यप॒ द्विषो॒ दह॒न्रक्षां᳚सि वि॒श्वहा᳚ ।",
          "begin": 0.0,
          "end": 5.29
        },
        {
          "text": "अग्ने᳚ ति॒ग्मेन᳚ दीदिहि ॥",
          "begin": 5.29,
          "end": 8.48
        }
      ]
    },
    {
      "id": "8.43.27",
      "mandala": 8,
      "sukta": 43,
      "rik": 27,
      "padas": [
        {
          "text": "यं त्वा॒ जना᳚स इन्ध॒ते म᳚नु॒ष्वद᳚ङ्गिरस्तम ।",
          "begin": 0.0,
          "end": 5.008
        },
        {
          "text": "अग्ने॒ स बो᳚धि मे॒ वचः᳚ ॥",
          "begin": 5.008,
          "end": 8.36
        }
      ]
    },
    {
      "id": "8.43.28",
      "mandala": 8,
      "sukta": 43,
      "rik": 28,
      "padas": [
        {
          "text": "यद᳚ग्ने दिवि॒जा अस्य᳚प्सु॒जा वा᳚ सहस्कृत ।",
          "begin": 0.0,
          "end": 4.606
        },
        {
          "text": "तं त्वा᳚ गी॒र्भिर्ह᳚वामहे ॥",
          "begin": 4.606,
          "end": 8.32
        }
      ]
    },
    {
      "id": "8.43.29",
      "mandala": 8,
      "sukta": 43,
      "rik": 29,
      "padas": [
        {
          "text": "तुभ्यं॒ घेत्ते जना᳚ इ॒मे विश्वा᳚ः सुक्षि॒तय॒ः पृथ᳚क् ।",
          "begin": 0.0,
          "end": 6.012
        },
        {
          "text": "धा॒सिं हि᳚न्व॒न्त्यत्त᳚वे ॥",
          "begin": 6.012,
          "end": 8.6
        }
      ]
    },
    {
      "id": "8.43.30",
      "mandala": 8,
      "sukta": 43,
      "rik": 30,
      "padas": [
        {
          "text": "ते घेद᳚ग्ने स्वा॒ध्यो॑हा॒ विश्वा᳚ नृ॒चक्ष᳚सः ।",
          "begin": 0.0,
          "end": 5.317
        },
        {
          "text": "तर᳚न्तः स्याम दु॒र्गहा᳚ ॥",
          "begin": 5.317,
          "end": 8.64
        }
      ]
    },
    {
      "id": "8.43.31",
      "mandala": 8,
      "sukta": 43,
      "rik": 31,
      "padas": [
        {
          "text": "अ॒ग्निं म॒न्द्रं पु᳚रुप्रि॒यं शी॒रं पा᳚व॒कशो᳚चिषम् ।",
          "begin": 0.0,
          "end": 4.961
        },
        {
          "text": "हृ॒द्भिर्म॒न्द्रेभि᳚रीमहे ॥",
          "begin": 4.961,
          "end": 8.12
        }
      ]
    },
    {
      "id": "8.43.32",
      "mandala": 8,
      "sukta": 43,
      "rik": 32,
      "padas": [
        {
          "text": "स त्वम᳚ग्ने वि॒भाव᳚सुः सृ॒जन्त्सूर्यो॒ न र॒श्मिभि᳚ः ।",
          "begin": 0.0,
          "end": 5.479
        },
        {
          "text": "शर्ध॒न्तमां᳚सि जिघ्नसे ॥",
          "begin": 5.479,
          "end": 8.2
        }
      ]
    },
    {
      "id": "8.43.33",
      "mandala": 8,
      "sukta": 43,
      "rik": 33,
      "padas": [
        {
          "text": "तत्ते᳚ सहस्व ईमहे दा॒त्रं यन्नोप॒दस्य᳚ति ।",
          "begin": 0.0,
          "end": 5.89
        },
        {
          "text": "त्वद᳚ग्ने॒ वार्यं॒ वसु᳚ ॥",
          "begin": 5.89,
          "end": 11.12
        }
      ]
    }
  ]
}