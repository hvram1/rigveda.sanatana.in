{
  "mandala": 8,
  "sukta": 7,
  "verses": [
    {
      "id": "8.7.1",
      "mandala": 8,
      "sukta": 7,
      "rik": 1,
      "padas": [
        {
          "text": "प्र यद्व᳚स्त्रि॒ष्टुभ॒मिषं॒ मरु᳚तो॒ विप्रो॒ अक्ष᳚रत् ।",
          "begin": 0.0,
          "end": 9.677
        },
        {
          "text": "वि पर्व᳚तेषु राजथ ॥",
          "begin": 9.677,
          "end": 12.24
        }
      ]
    },
    {
      "id": "8.7.2",
      "mandala": 8,
      "sukta": 7,
      "rik": 2,
      "padas": [
        {
          "text": "यद॒ङ्ग त᳚विषीयवो॒ यामं᳚ शुभ्रा॒ अचि᳚ध्वम् ।",
          "begin": 0.0,
          "end": 5.287
        },
        {
          "text": "नि पर्व᳚ता अहासत ॥",
          "begin": 5.287,
          "end": 8.12
        }
      ]
    },
    {
      "id": "8.7.3",
      "mandala": 8,
      "sukta": 7,
      "rik": 3,
      "padas": [
        {
          "text": "उदी᳚रयन्त वा॒युभि᳚र्वा॒श्रास॒ः पृश्नि᳚मातरः ।",
          "begin": 0.0,
          "end": 6.409
        },
        {
          "text": "धु॒क्षन्त᳚ पि॒प्युषी॒मिष᳚म् ॥",
          "begin": 6.409,
          "end": 9.36
        }
      ]
    },
    {
      "id": "8.7.4",
      "mandala": 8,
      "sukta": 7,
      "rik": 4,
      "padas": [
        {
          "text": "वप᳚न्ति म॒रुतो॒ मिहं॒ प्र वे᳚पयन्ति॒ पर्व᳚तान् ।",
          "begin": 0.0,
          "end": 6.713
        },
        {
          "text": "यद्यामं॒ यान्ति᳚ वा॒युभि᳚ः ॥",
          "begin": 6.713,
          "end": 9.24
        }
      ]
    },
    {
      "id": "8.7.5",
      "mandala": 8,
      "sukta": 7,
      "rik": 5,
      "padas": [
        {
          "text": "नि यद्यामा᳚य वो गि॒रिर्नि सिन्ध᳚वो॒ विध᳚र्मणे ।",
          "begin": 0.0,
          "end": 6.498
        },
        {
          "text": "म॒हे शुष्मा᳚य येमि॒रे ॥",
          "begin": 6.498,
          "end": 9.44
        }
      ]
    },
    {
      "id": "8.7.6",
      "mandala": 8,
      "sukta": 7,
      "rik": 6,
      "padas": [
        {
          "text": "यु॒ष्माँ उ॒ नक्त᳚मू॒तये᳚ यु॒ष्मान्दिवा᳚ हवामहे ।",
          "begin": 0.0,
          "end": 6.819
        },
        {
          "text": "यु॒ष्मान्प्र᳚य॒त्य᳚ध्व॒रे ॥",
          "begin": 6.819,
          "end": 9.68
        }
      ]
    },
    {
      "id": "8.7.7",
      "mandala": 8,
      "sukta": 7,
      "rik": 7,
      "padas": [
        {
          "text": "उदु॒ त्ये अ᳚रु॒णप्स᳚वश्चि॒त्रा यामे᳚भिरीरते ।",
          "begin": 0.0,
          "end": 6.235
        },
        {
          "text": "वा॒श्रा अधि॒ ष्णुना᳚ दि॒वः ॥",
          "begin": 6.235,
          "end": 9.76
        }
      ]
    },
    {
      "id": "8.7.8",
      "mandala": 8,
      "sukta": 7,
      "rik": 8,
      "padas": [
        {
          "text": "सृ॒जन्ति᳚ र॒श्मिमोज᳚सा॒ पन्थां॒ सूर्या᳚य॒ यात᳚वे ।",
          "begin": 0.0,
          "end": 5.785
        },
        {
          "text": "ते भा॒नुभि॒र्वि त᳚स्थिरे ॥",
          "begin": 5.785,
          "end": 8.8
        }
      ]
    },
    {
      "id": "8.7.9",
      "mandala": 8,
      "sukta": 7,
      "rik": 9,
      "padas": [
        {
          "text": "इ॒मां मे᳚ मरुतो॒ गिर᳚मि॒मं स्तोम᳚मृभुक्षणः ।",
          "begin": 0.0,
          "end": 6.109
        },
        {
          "text": "इ॒मं मे᳚ वनता॒ हव᳚म् ॥",
          "begin": 6.109,
          "end": 9.04
        }
      ]
    },
    {
      "id": "8.7.10",
      "mandala": 8,
      "sukta": 7,
      "rik": 10,
      "padas": [
        {
          "text": "त्रीणि॒ सरां᳚सि॒ पृश्न᳚यो दुदु॒ह्रे व॒ज्रिणे॒ मधु᳚ ।",
          "begin": 0.0,
          "end": 6.205
        },
        {
          "text": "उत्सं॒ कव᳚न्धमु॒द्रिण᳚म् ॥",
          "begin": 6.205,
          "end": 8.8
        }
      ]
    },
    {
      "id": "8.7.11",
      "mandala": 8,
      "sukta": 7,
      "rik": 11,
      "padas": [
        {
          "text": "मरु᳚तो॒ यद्ध᳚ वो दि॒वः सु᳚म्ना॒यन्तो॒ हवा᳚महे ।",
          "begin": 0.0,
          "end": 6.347
        },
        {
          "text": "आ तू न॒ उप᳚ गन्तन ॥",
          "begin": 6.347,
          "end": 8.68
        }
      ]
    },
    {
      "id": "8.7.12",
      "mandala": 8,
      "sukta": 7,
      "rik": 12,
      "padas": [
        {
          "text": "यू॒यं हि ष्ठा सु᳚दानवो॒ रुद्रा᳚ ऋभुक्षणो॒ दमे᳚ ।",
          "begin": 0.0,
          "end": 6.41
        },
        {
          "text": "उ॒त प्रचे᳚तसो॒ मदे᳚ ॥",
          "begin": 6.41,
          "end": 9.4
        }
      ]
    },
    {
      "id": "8.7.13",
      "mandala": 8,
      "sukta": 7,
      "rik": 13,
      "padas": [
        {
          "text": "आ नो᳚ र॒यिं म᳚द॒च्युतं᳚ पुरु॒क्षुं वि॒श्वधा᳚यसम् ।",
          "begin": 0.0,
          "end": 6.726
        },
        {
          "text": "इय᳚र्ता मरुतो दि॒वः ॥",
          "begin": 6.726,
          "end": 9.2
        }
      ]
    },
    {
      "id": "8.7.14",
      "mandala": 8,
      "sukta": 7,
      "rik": 14,
      "padas": [
        {
          "text": "अधी᳚व॒ यद्गि᳚री॒णां यामं᳚ शुभ्रा॒ अचि᳚ध्वम् ।",
          "begin": 0.0,
          "end": 6.35
        },
        {
          "text": "सु॒वा॒नैर्म᳚न्दध्व॒ इन्दु᳚भिः ॥",
          "begin": 6.35,
          "end": 10.16
        }
      ]
    },
    {
      "id": "8.7.15",
      "mandala": 8,
      "sukta": 7,
      "rik": 15,
      "padas": [
        {
          "text": "ए॒ताव᳚तश्चिदेषां सु॒म्नं भि᳚क्षेत॒ मर्त्य᳚ः ।",
          "begin": 0.0,
          "end": 6.231
        },
        {
          "text": "अदा᳚भ्यस्य॒ मन्म᳚भिः ॥",
          "begin": 6.231,
          "end": 8.92
        }
      ]
    },
    {
      "id": "8.7.16",
      "mandala": 8,
      "sukta": 7,
      "rik": 16,
      "padas": [
        {
          "text": "ये द्र॒प्सा इ᳚व॒ रोद᳚सी॒ धम॒न्त्यनु᳚ वृ॒ष्टिभि᳚ः ।",
          "begin": 0.0,
          "end": 6.491
        },
        {
          "text": "उत्सं᳚ दु॒हन्तो॒ अक्षि᳚तम् ॥",
          "begin": 6.491,
          "end": 9.32
        }
      ]
    },
    {
      "id": "8.7.17",
      "mandala": 8,
      "sukta": 7,
      "rik": 17,
      "padas": [
        {
          "text": "उदु᳚ स्वा॒नेभि᳚रीरत॒ उद्रथै॒रुदु᳚ वा॒युभि᳚ः ।",
          "begin": 0.0,
          "end": 5.377
        },
        {
          "text": "उत्स्तोमै॒ः पृश्नि᳚मातरः ॥",
          "begin": 5.377,
          "end": 8.6
        }
      ]
    },
    {
      "id": "8.7.18",
      "mandala": 8,
      "sukta": 7,
      "rik": 18,
      "padas": [
        {
          "text": "येना॒व तु॒र्वशं॒ यदुं॒ येन॒ कण्वं᳚ धन॒स्पृत᳚म् ।",
          "begin": 0.0,
          "end": 6.148
        },
        {
          "text": "रा॒ये सु तस्य᳚ धीमहि ॥",
          "begin": 6.148,
          "end": 8.72
        }
      ]
    },
    {
      "id": "8.7.19",
      "mandala": 8,
      "sukta": 7,
      "rik": 19,
      "padas": [
        {
          "text": "इ॒मा उ᳚ वः सुदानवो घृ॒तं न पि॒प्युषी॒रिषः᳚ ।",
          "begin": 0.0,
          "end": 5.917
        },
        {
          "text": "वर्धा᳚न्का॒ण्वस्य॒ मन्म᳚भिः ॥",
          "begin": 5.917,
          "end": 9.52
        }
      ]
    },
    {
      "id": "8.7.20",
      "mandala": 8,
      "sukta": 7,
      "rik": 20,
      "padas": [
        {
          "text": "क्व᳚ नू॒नं सु᳚दानवो॒ मद᳚था वृक्तबर्हिषः ।",
          "begin": 0.0,
          "end": 5.26
        },
        {
          "text": "ब्र॒ह्मा को वः᳚ सपर्यति ॥",
          "begin": 5.26,
          "end": 7.96
        }
      ]
    },
    {
      "id": "8.7.21",
      "mandala": 8,
      "sukta": 7,
      "rik": 21,
      "padas": [
        {
          "text": "न॒हि ष्म॒ यद्ध᳚ वः पु॒रा स्तोमे᳚भिर्वृक्तबर्हिषः ।",
          "begin": 0.0,
          "end": 6.429
        },
        {
          "text": "शर्धाँ᳚ ऋ॒तस्य॒ जिन्व᳚थ ॥",
          "begin": 6.429,
          "end": 9.48
        }
      ]
    },
    {
      "id": "8.7.22",
      "mandala": 8,
      "sukta": 7,
      "rik": 22,
      "padas": [
        {
          "text": "समु॒ त्ये म᳚ह॒तीर॒पः सं क्षो॒णी समु॒ सूर्य᳚म् ।",
          "begin": 0.0,
          "end": 6.215
        },
        {
          "text": "सं वज्रं᳚ पर्व॒शो द᳚धुः ॥",
          "begin": 6.215,
          "end": 9.52
        }
      ]
    },
    {
      "id": "8.7.23",
      "mandala": 8,
      "sukta": 7,
      "rik": 23,
      "padas": [
        {
          "text": "वि वृ॒त्रं प᳚र्व॒शो य᳚यु॒र्वि पर्व᳚ताँ अरा॒जिनः᳚ ।",
          "begin": 0.0,
          "end": 5.267
        },
        {
          "text": "च॒क्रा॒णा वृष्णि॒ पौंस्य᳚म् ॥",
          "begin": 5.267,
          "end": 9.56
        }
      ]
    },
    {
      "id": "8.7.24",
      "mandala": 8,
      "sukta": 7,
      "rik": 24,
      "padas": [
        {
          "text": "अनु᳚ त्रि॒तस्य॒ युध्य᳚त॒ः शुष्म᳚मावन्नु॒त क्रतु᳚म् ।",
          "begin": 0.0,
          "end": 5.006
        },
        {
          "text": "अन्विन्द्रं᳚ वृत्र॒तूर्ये᳚ ॥",
          "begin": 5.006,
          "end": 9.4
        }
      ]
    },
    {
      "id": "8.7.25",
      "mandala": 8,
      "sukta": 7,
      "rik": 25,
      "padas": [
        {
          "text": "वि॒द्युद्ध᳚स्ता अ॒भिद्य᳚व॒ः शिप्रा᳚ः शी॒र्षन्हि᳚र॒ण्ययी᳚ः ।",
          "begin": 0.0,
          "end": 7.199
        },
        {
          "text": "शु॒भ्रा व्य᳚ञ्जत श्रि॒ये ॥",
          "begin": 7.199,
          "end": 10.44
        }
      ]
    },
    {
      "id": "8.7.26",
      "mandala": 8,
      "sukta": 7,
      "rik": 26,
      "padas": [
        {
          "text": "उ॒शना॒ यत्प᳚रा॒वत᳚ उ॒क्ष्णो रन्ध्र॒मया᳚तन ।",
          "begin": 0.0,
          "end": 5.325
        },
        {
          "text": "द्यौर्न च᳚क्रदद्भि॒या ॥",
          "begin": 5.325,
          "end": 8.28
        }
      ]
    },
    {
      "id": "8.7.27",
      "mandala": 8,
      "sukta": 7,
      "rik": 27,
      "padas": [
        {
          "text": "आ नो᳚ म॒खस्य᳚ दा॒वने॑श्वै॒र्हिर᳚ण्यपाणिभिः ।",
          "begin": 0.0,
          "end": 6.868
        },
        {
          "text": "देवा᳚स॒ उप᳚ गन्तन ॥",
          "begin": 6.868,
          "end": 9.56
        }
      ]
    },
    {
      "id": "8.7.28",
      "mandala": 8,
      "sukta": 7,
      "rik": 28,
      "padas": [
        {
          "text": "यदे᳚षां॒ पृष᳚ती॒ रथे॒ प्रष्टि॒र्वह᳚ति॒ रोहि᳚तः ।",
          "begin": 0.0,
          "end": 6.286
        },
        {
          "text": "यान्ति᳚ शु॒भ्रा रि॒णन्न॒पः ॥",
          "begin": 6.286,
          "end": 9.2
        }
      ]
    },
    {
      "id": "8.7.29",
      "mandala": 8,
      "sukta": 7,
      "rik": 29,
      "padas": [
        {
          "text": "सु॒षोमे᳚ शर्य॒णाव᳚त्यार्जी॒के प॒स्त्या᳚वति ।",
          "begin": 0.0,
          "end": 6.19
        },
        {
          "text": "य॒युर्निच᳚क्रया॒ नरः᳚ ॥",
          "begin": 6.19,
          "end": 9.48
        }
      ]
    },
    {
      "id": "8.7.30",
      "mandala": 8,
      "sukta": 7,
      "rik": 30,
      "padas": [
        {
          "text": "क॒दा ग᳚च्छाथ मरुत इ॒त्था विप्रं॒ हव᳚मानम् ।",
          "begin": 0.0,
          "end": 5.349
        },
        {
          "text": "मा॒र्डी॒केभि॒र्नाध᳚मानम् ॥",
          "begin": 5.349,
          "end": 8.56
        }
      ]
    },
    {
      "id": "8.7.31",
      "mandala": 8,
      "sukta": 7,
      "rik": 31,
      "padas": [
        {
          "text": "कद्ध᳚ नू॒नं क᳚धप्रियो॒ यदिन्द्र॒मज᳚हातन ।",
          "begin": 0.0,
          "end": 5.046
        },
        {
          "text": "को वः᳚ सखि॒त्व ओ᳚हते ॥",
          "begin": 5.046,
          "end": 8.08
        }
      ]
    },
    {
      "id": "8.7.32",
      "mandala": 8,
      "sukta": 7,
      "rik": 32,
      "padas": [
        {
          "text": "स॒हो षु णो॒ वज्र᳚हस्तै॒ः कण्वा᳚सो अ॒ग्निं म॒रुद्भि᳚ः ।",
          "begin": 0.0,
          "end": 6.875
        },
        {
          "text": "स्तु॒षे हिर᳚ण्यवाशीभिः ॥",
          "begin": 6.875,
          "end": 10.0
        }
      ]
    },
    {
      "id": "8.7.33",
      "mandala": 8,
      "sukta": 7,
      "rik": 33,
      "padas": [
        {
          "text": "ओ षु वृष्ण॒ः प्रय᳚ज्यू॒ना नव्य᳚से सुवि॒ताय᳚ ।",
          "begin": 0.0,
          "end": 5.202
        },
        {
          "text": "व॒वृ॒त्यां चि॒त्रवा᳚जान् ॥",
          "begin": 5.202,
          "end": 8.96
        }
      ]
    },
    {
      "id": "8.7.34",
      "mandala": 8,
      "sukta": 7,
      "rik": 34,
      "padas": [
        {
          "text": "गि॒रय᳚श्चि॒न्नि जि᳚हते॒ पर्शा᳚नासो॒ मन्य᳚मानाः ।",
          "begin": 0.0,
          "end": 6.168
        },
        {
          "text": "पर्व᳚ताश्चि॒न्नि ये᳚मिरे ॥",
          "begin": 6.168,
          "end": 9.68
        }
      ]
    },
    {
      "id": "8.7.35",
      "mandala": 8,
      "sukta": 7,
      "rik": 35,
      "padas": [
        {
          "text": "आक्ष्ण॒यावा᳚नो वहन्त्य॒न्तरि᳚क्षेण॒ पत᳚तः ।",
          "begin": 0.0,
          "end": 6.332
        },
        {
          "text": "धाता᳚रः स्तुव॒ते वयः᳚ ॥",
          "begin": 6.332,
          "end": 9.72
        }
      ]
    },
    {
      "id": "8.7.36",
      "mandala": 8,
      "sukta": 7,
      "rik": 36,
      "padas": [
        {
          "text": "अ॒ग्निर्हि जानि᳚ पू॒र्व्यश्छन्दो॒ न सूरो᳚ अ॒र्चिषा᳚ ।",
          "begin": 0.0,
          "end": 6.894
        },
        {
          "text": "ते भा॒नुभि॒र्वि त᳚स्थिरे ॥",
          "begin": 6.894,
          "end": 13.12
        }
      ]
    }
  ]
}