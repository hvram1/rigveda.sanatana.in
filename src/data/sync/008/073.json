{
  "mandala": 8,
  "sukta": 73,
  "verses": [
    {
      "id": "8.73.1",
      "mandala": 8,
      "sukta": 73,
      "rik": 1,
      "padas": [
        {
          "text": "उदी᳚राथामृताय॒ते यु॒ञ्जाथा᳚मश्विना॒ रथ᳚म् ।",
          "begin": 0.0,
          "end": 10.072
        },
        {
          "text": "अन्ति॒ षद्भू᳚तु वा॒मवः᳚ ॥",
          "begin": 10.072,
          "end": 14.8
        }
      ]
    },
    {
      "id": "8.73.2",
      "mandala": 8,
      "sukta": 73,
      "rik": 2,
      "padas": [
        {
          "text": "नि॒मिष᳚श्चि॒ज्जवी᳚यसा॒ रथे॒ना या᳚तमश्विना ।",
          "begin": 0.0,
          "end": 5.909
        },
        {
          "text": "अन्ति॒ षद्भू᳚तु वा॒मवः᳚ ॥",
          "begin": 5.909,
          "end": 8.8
        }
      ]
    },
    {
      "id": "8.73.3",
      "mandala": 8,
      "sukta": 73,
      "rik": 3,
      "padas": [
        {
          "text": "उप᳚ स्तृणीत॒मत्र᳚ये हि॒मेन᳚ घ॒र्मम᳚श्विना ।",
          "begin": 0.0,
          "end": 4.55
        },
        {
          "text": "अन्ति॒ षद्भू᳚तु वा॒मवः᳚ ॥",
          "begin": 4.55,
          "end": 7.56
        }
      ]
    },
    {
      "id": "8.73.4",
      "mandala": 8,
      "sukta": 73,
      "rik": 4,
      "padas": [
        {
          "text": "कुह᳚ स्थ॒ः कुह᳚ जग्मथु॒ः कुह᳚ श्ये॒नेव᳚ पेतथुः ।",
          "begin": 0.0,
          "end": 4.243
        },
        {
          "text": "अन्ति॒ षद्भू᳚तु वा॒मवः᳚ ॥",
          "begin": 4.243,
          "end": 8.0
        }
      ]
    },
    {
      "id": "8.73.5",
      "mandala": 8,
      "sukta": 73,
      "rik": 5,
      "padas": [
        {
          "text": "यद॒द्य कर्हि॒ कर्हि᳚ चिच्छुश्रू॒यात᳚मि॒मं हव᳚म् ।",
          "begin": 0.0,
          "end": 4.934
        },
        {
          "text": "अन्ति॒ षद्भू᳚तु वा॒मवः᳚ ॥",
          "begin": 4.934,
          "end": 8.4
        }
      ]
    },
    {
      "id": "8.73.6",
      "mandala": 8,
      "sukta": 73,
      "rik": 6,
      "padas": [
        {
          "text": "अ॒श्विना᳚ याम॒हूत᳚मा॒ नेदि᳚ष्ठं या॒म्याप्य᳚म् ।",
          "begin": 0.0,
          "end": 5.416
        },
        {
          "text": "अन्ति॒ षद्भू᳚तु वा॒मवः᳚ ॥",
          "begin": 5.416,
          "end": 9.4
        }
      ]
    },
    {
      "id": "8.73.7",
      "mandala": 8,
      "sukta": 73,
      "rik": 7,
      "padas": [
        {
          "text": "अव᳚न्त॒मत्र᳚ये गृ॒हं कृ᳚णु॒तं यु॒वम᳚श्विना ।",
          "begin": 0.0,
          "end": 5.437
        },
        {
          "text": "अन्ति॒ षद्भू᳚तु वा॒मवः᳚ ॥",
          "begin": 5.437,
          "end": 8.32
        }
      ]
    },
    {
      "id": "8.73.8",
      "mandala": 8,
      "sukta": 73,
      "rik": 8,
      "padas": [
        {
          "text": "वरे᳚थे अ॒ग्निमा॒तपो॒ वद᳚ते व॒ल्ग्वत्र᳚ये ।",
          "begin": 0.0,
          "end": 5.767
        },
        {
          "text": "अन्ति॒ षद्भू᳚तु वा॒मवः᳚ ॥",
          "begin": 5.767,
          "end": 8.72
        }
      ]
    },
    {
      "id": "8.73.9",
      "mandala": 8,
      "sukta": 73,
      "rik": 9,
      "padas": [
        {
          "text": "प्र स॒प्तव᳚ध्रिरा॒शसा॒ धारा᳚म॒ग्नेर᳚शायत ।",
          "begin": 0.0,
          "end": 5.567
        },
        {
          "text": "अन्ति॒ षद्भू᳚तु वा॒मवः᳚ ॥",
          "begin": 5.567,
          "end": 8.4
        }
      ]
    },
    {
      "id": "8.73.10",
      "mandala": 8,
      "sukta": 73,
      "rik": 10,
      "padas": [
        {
          "text": "इ॒हा ग᳚तं वृषण्वसू शृणु॒तं म᳚ इ॒मं हव᳚म् ।",
          "begin": 0.0,
          "end": 5.204
        },
        {
          "text": "अन्ति॒ षद्भू᳚तु वा॒मवः᳚ ॥",
          "begin": 5.204,
          "end": 8.04
        }
      ]
    },
    {
      "id": "8.73.11",
      "mandala": 8,
      "sukta": 73,
      "rik": 11,
      "padas": [
        {
          "text": "किमि॒दं वां᳚ पुराण॒वज्जर᳚तोरिव शस्यते ।",
          "begin": 0.0,
          "end": 4.859
        },
        {
          "text": "अन्ति॒ षद्भू᳚तु वा॒मवः᳚ ॥",
          "begin": 4.859,
          "end": 8.2
        }
      ]
    },
    {
      "id": "8.73.12",
      "mandala": 8,
      "sukta": 73,
      "rik": 12,
      "padas": [
        {
          "text": "स॒मा॒नं वां᳚ सजा॒त्यं᳚ समा॒नो बन्धु᳚रश्विना ।",
          "begin": 0.0,
          "end": 6.151
        },
        {
          "text": "अन्ति॒ षद्भू᳚तु वा॒मवः᳚ ॥",
          "begin": 6.151,
          "end": 9.32
        }
      ]
    },
    {
      "id": "8.73.13",
      "mandala": 8,
      "sukta": 73,
      "rik": 13,
      "padas": [
        {
          "text": "यो वां॒ रजां᳚स्यश्विना॒ रथो᳚ वि॒याति॒ रोद᳚सी ।",
          "begin": 0.0,
          "end": 5.644
        },
        {
          "text": "अन्ति॒ षद्भू᳚तु वा॒मवः᳚ ॥",
          "begin": 5.644,
          "end": 9.16
        }
      ]
    },
    {
      "id": "8.73.14",
      "mandala": 8,
      "sukta": 73,
      "rik": 14,
      "padas": [
        {
          "text": "आ नो॒ गव्ये᳚भि॒रश्व्यै᳚ः स॒हस्रै॒रुप᳚ गच्छतम् ।",
          "begin": 0.0,
          "end": 5.482
        },
        {
          "text": "अन्ति॒ षद्भू᳚तु वा॒मवः᳚ ॥",
          "begin": 5.482,
          "end": 9.08
        }
      ]
    },
    {
      "id": "8.73.15",
      "mandala": 8,
      "sukta": 73,
      "rik": 15,
      "padas": [
        {
          "text": "मा नो॒ गव्ये᳚भि॒रश्व्यै᳚ः स॒हस्रे᳚भि॒रति᳚ ख्यतम् ।",
          "begin": 0.0,
          "end": 6.543
        },
        {
          "text": "अन्ति॒ षद्भू᳚तु वा॒मवः᳚ ॥",
          "begin": 6.543,
          "end": 9.44
        }
      ]
    },
    {
      "id": "8.73.16",
      "mandala": 8,
      "sukta": 73,
      "rik": 16,
      "padas": [
        {
          "text": "अ॒रु॒णप्सु᳚रु॒षा अ᳚भू॒दक॒र्ज्योति᳚रृ॒ताव᳚री ।",
          "begin": 0.0,
          "end": 5.395
        },
        {
          "text": "अन्ति॒ षद्भू᳚तु वा॒मवः᳚ ॥",
          "begin": 5.395,
          "end": 8.36
        }
      ]
    },
    {
      "id": "8.73.17",
      "mandala": 8,
      "sukta": 73,
      "rik": 17,
      "padas": [
        {
          "text": "अ॒श्विना॒ सु वि॒चाक᳚शद्वृ॒क्षं प᳚रशु॒माँ इ᳚व ।",
          "begin": 0.0,
          "end": 5.21
        },
        {
          "text": "अन्ति॒ षद्भू᳚तु वा॒मवः᳚ ॥",
          "begin": 5.21,
          "end": 8.4
        }
      ]
    },
    {
      "id": "8.73.18",
      "mandala": 8,
      "sukta": 73,
      "rik": 18,
      "padas": [
        {
          "text": "पुरं॒ न धृ᳚ष्ण॒वा रु᳚ज कृ॒ष्णया᳚ बाधि॒तो वि॒शा ।",
          "begin": 0.0,
          "end": 6.032
        },
        {
          "text": "अन्ति॒ षद्भू᳚तु वा॒मवः᳚ ॥",
          "begin": 6.032,
          "end": 10.6
        }
      ]
    }
  ]
}