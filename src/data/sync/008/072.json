{
  "mandala": 8,
  "sukta": 72,
  "verses": [
    {
      "id": "8.72.1",
      "mandala": 8,
      "sukta": 72,
      "rik": 1,
      "padas": [
        {
          "text": "ह॒विष्कृ᳚णुध्व॒मा ग᳚मदध्व॒र्युर्व᳚नते॒ पुनः᳚ ।",
          "begin": 0.0,
          "end": 8.744
        },
        {
          "text": "वि॒द्वाँ अ᳚स्य प्र॒शास᳚नम् ॥",
          "begin": 8.744,
          "end": 12.04
        }
      ]
    },
    {
      "id": "8.72.2",
      "mandala": 8,
      "sukta": 72,
      "rik": 2,
      "padas": [
        {
          "text": "नि ति॒ग्मम॒भ्यं१॒᳚शुं सीद॒द्धोता᳚ म॒नावधि᳚ ।",
          "begin": 0.0,
          "end": 5.996
        },
        {
          "text": "जु॒षा॒णो अ᳚स्य स॒ख्यम् ॥",
          "begin": 5.996,
          "end": 8.64
        }
      ]
    },
    {
      "id": "8.72.3",
      "mandala": 8,
      "sukta": 72,
      "rik": 3,
      "padas": [
        {
          "text": "अ॒न्तरि᳚च्छन्ति॒ तं जने᳚ रु॒द्रं प॒रो म᳚नी॒षया᳚ ।",
          "begin": 0.0,
          "end": 5.736
        },
        {
          "text": "गृ॒भ्णन्ति᳚ जि॒ह्वया᳚ स॒सम् ॥",
          "begin": 5.736,
          "end": 8.96
        }
      ]
    },
    {
      "id": "8.72.4",
      "mandala": 8,
      "sukta": 72,
      "rik": 4,
      "padas": [
        {
          "text": "जा॒म्य᳚तीतपे॒ धनु᳚र्वयो॒धा अ᳚रुह॒द्वन᳚म् ।",
          "begin": 0.0,
          "end": 6.132
        },
        {
          "text": "दृ॒षदं᳚ जि॒ह्वयाव᳚धीत् ॥",
          "begin": 6.132,
          "end": 9.08
        }
      ]
    },
    {
      "id": "8.72.5",
      "mandala": 8,
      "sukta": 72,
      "rik": 5,
      "padas": [
        {
          "text": "चर᳚न्व॒त्सो रुश᳚न्नि॒ह नि᳚दा॒तारं॒ न वि᳚न्दते ।",
          "begin": 0.0,
          "end": 5.352
        },
        {
          "text": "वेति॒ स्तोत᳚व अ॒म्ब्य᳚म् ॥",
          "begin": 5.352,
          "end": 9.32
        }
      ]
    },
    {
      "id": "8.72.6",
      "mandala": 8,
      "sukta": 72,
      "rik": 6,
      "padas": [
        {
          "text": "उ॒तो न्व᳚स्य॒ यन्म॒हदश्वा᳚व॒द्योज᳚नं बृ॒हद् ।",
          "begin": 0.0,
          "end": 5.959
        },
        {
          "text": "दा॒मा रथ᳚स्य॒ ददृ᳚शे ॥",
          "begin": 5.959,
          "end": 8.32
        }
      ]
    },
    {
      "id": "8.72.7",
      "mandala": 8,
      "sukta": 72,
      "rik": 7,
      "padas": [
        {
          "text": "दु॒हन्ति᳚ स॒प्तैका॒मुप॒ द्वा पञ्च᳚ सृजतः ।",
          "begin": 0.0,
          "end": 5.518
        },
        {
          "text": "ती॒र्थे सिन्धो॒रधि᳚ स्व॒रे ॥",
          "begin": 5.518,
          "end": 7.72
        }
      ]
    },
    {
      "id": "8.72.8",
      "mandala": 8,
      "sukta": 72,
      "rik": 8,
      "padas": [
        {
          "text": "आ द॒शभि᳚र्वि॒वस्व᳚त॒ इन्द्र॒ः कोश᳚मचुच्यवीत् ।",
          "begin": 0.0,
          "end": 5.083
        },
        {
          "text": "खेद᳚या त्रि॒वृता᳚ दि॒वः ॥",
          "begin": 5.083,
          "end": 8.68
        }
      ]
    },
    {
      "id": "8.72.9",
      "mandala": 8,
      "sukta": 72,
      "rik": 9,
      "padas": [
        {
          "text": "परि᳚ त्रि॒धातु᳚रध्व॒रं जू॒र्णिरे᳚ति॒ नवी᳚यसी ।",
          "begin": 0.0,
          "end": 5.391
        },
        {
          "text": "मध्वा॒ होता᳚रो अञ्जते ॥",
          "begin": 5.391,
          "end": 9.08
        }
      ]
    },
    {
      "id": "8.72.10",
      "mandala": 8,
      "sukta": 72,
      "rik": 10,
      "padas": [
        {
          "text": "सि॒ञ्चन्ति॒ नम᳚साव॒तमु॒च्चाच᳚क्रं॒ परि᳚ज्मानम् ।",
          "begin": 0.0,
          "end": 5.461
        },
        {
          "text": "नी॒चीन᳚बार॒मक्षि᳚तम् ॥",
          "begin": 5.461,
          "end": 7.88
        }
      ]
    },
    {
      "id": "8.72.11",
      "mandala": 8,
      "sukta": 72,
      "rik": 11,
      "padas": [
        {
          "text": "अ॒भ्यार॒मिदद्र᳚यो॒ निषि᳚क्तं॒ पुष्क᳚रे॒ मधु᳚ ।",
          "begin": 0.0,
          "end": 4.94
        },
        {
          "text": "अ॒व॒तस्य᳚ वि॒सर्ज᳚ने ॥",
          "begin": 4.94,
          "end": 7.0
        }
      ]
    },
    {
      "id": "8.72.12",
      "mandala": 8,
      "sukta": 72,
      "rik": 12,
      "padas": [
        {
          "text": "गाव॒ उपा᳚वताव॒तं म॒ही य॒ज्ञस्य᳚ र॒प्सुदा᳚ ।",
          "begin": 0.0,
          "end": 5.212
        },
        {
          "text": "उ॒भा कर्णा᳚ हिर॒ण्यया᳚ ॥",
          "begin": 5.212,
          "end": 9.16
        }
      ]
    },
    {
      "id": "8.72.13",
      "mandala": 8,
      "sukta": 72,
      "rik": 13,
      "padas": [
        {
          "text": "आ सु॒ते सि᳚ञ्चत॒ श्रियं॒ रोद᳚स्योरभि॒श्रिय᳚म् ।",
          "begin": 0.0,
          "end": 5.405
        },
        {
          "text": "र॒सा द᳚धीत वृष॒भम् ॥",
          "begin": 5.405,
          "end": 8.24
        }
      ]
    },
    {
      "id": "8.72.14",
      "mandala": 8,
      "sukta": 72,
      "rik": 14,
      "padas": [
        {
          "text": "ते जा᳚नत॒ स्वमो॒क्यं१॒᳚ सं व॒त्सासो॒ न मा॒तृभि᳚ः ।",
          "begin": 0.0,
          "end": 6.244
        },
        {
          "text": "मि॒थो न᳚सन्त जा॒मिभि᳚ः ॥",
          "begin": 6.244,
          "end": 9.44
        }
      ]
    },
    {
      "id": "8.72.15",
      "mandala": 8,
      "sukta": 72,
      "rik": 15,
      "padas": [
        {
          "text": "उप॒ स्रक्वे᳚षु॒ बप्स᳚तः कृण्व॒ते ध॒रुणं᳚ दि॒वि ।",
          "begin": 0.0,
          "end": 5.699
        },
        {
          "text": "इन्द्रे᳚ अ॒ग्ना नम॒ः स्व᳚ः ॥",
          "begin": 5.699,
          "end": 8.96
        }
      ]
    },
    {
      "id": "8.72.16",
      "mandala": 8,
      "sukta": 72,
      "rik": 16,
      "padas": [
        {
          "text": "अधु᳚क्षत्पि॒प्युषी॒मिष॒मूर्जं᳚ स॒प्तप᳚दीम॒रिः ।",
          "begin": 0.0,
          "end": 5.692
        },
        {
          "text": "सूर्य᳚स्य स॒प्त र॒श्मिभि᳚ः ॥",
          "begin": 5.692,
          "end": 8.6
        }
      ]
    },
    {
      "id": "8.72.17",
      "mandala": 8,
      "sukta": 72,
      "rik": 17,
      "padas": [
        {
          "text": "सोम᳚स्य मित्रावरु॒णोदि᳚ता॒ सूर॒ आ द᳚दे ।",
          "begin": 0.0,
          "end": 4.54
        },
        {
          "text": "तदातु᳚रस्य भेष॒जम् ॥",
          "begin": 4.54,
          "end": 7.28
        }
      ]
    },
    {
      "id": "8.72.18",
      "mandala": 8,
      "sukta": 72,
      "rik": 18,
      "padas": [
        {
          "text": "उ॒तो न्व᳚स्य॒ यत्प॒दं ह᳚र्य॒तस्य᳚ निधा॒न्य᳚म् ।",
          "begin": 0.0,
          "end": 6.407
        },
        {
          "text": "परि॒ द्यां जि॒ह्वया᳚तनत् ॥",
          "begin": 6.407,
          "end": 11.68
        }
      ]
    }
  ]
}