{
  "mandala": 8,
  "sukta": 23,
  "verses": [
    {
      "id": "8.23.1",
      "mandala": 8,
      "sukta": 23,
      "rik": 1,
      "padas": [
        {
          "text": "ईळि᳚ष्वा॒ हि प्र᳚ती॒व्यं१॒᳚ यज᳚स्व जा॒तवे᳚दसम् ।",
          "begin": 0.0,
          "end": 9.869
        },
        {
          "text": "च॒रि॒ष्णुधू᳚म॒मगृ᳚भीतशोचिषम् ॥",
          "begin": 9.869,
          "end": 13.96
        }
      ]
    },
    {
      "id": "8.23.2",
      "mandala": 8,
      "sukta": 23,
      "rik": 2,
      "padas": [
        {
          "text": "दा॒मानं᳚ विश्वचर्षणे॒॑ग्निं वि᳚श्वमनो गि॒रा ।",
          "begin": 0.0,
          "end": 5.693
        },
        {
          "text": "उ॒त स्तु᳚षे॒ विष्प᳚र्धसो॒ रथा᳚नाम् ॥",
          "begin": 5.693,
          "end": 9.6
        }
      ]
    },
    {
      "id": "8.23.3",
      "mandala": 8,
      "sukta": 23,
      "rik": 3,
      "padas": [
        {
          "text": "येषा᳚माबा॒ध ऋ॒ग्मिय᳚ इ॒षः पृ॒क्षश्च᳚ नि॒ग्रभे᳚ ।",
          "begin": 0.0,
          "end": 6.192
        },
        {
          "text": "उ॒प॒विदा॒ वह्नि᳚र्विन्दते॒ वसु᳚ ॥",
          "begin": 6.192,
          "end": 9.64
        }
      ]
    },
    {
      "id": "8.23.4",
      "mandala": 8,
      "sukta": 23,
      "rik": 4,
      "padas": [
        {
          "text": "उद᳚स्य शो॒चिर᳚स्थाद्दीदि॒युषो॒ व्य१॒᳚जर᳚म् ।",
          "begin": 0.0,
          "end": 5.105
        },
        {
          "text": "तपु᳚र्जम्भस्य सु॒द्युतो᳚ गण॒श्रियः᳚ ॥",
          "begin": 5.105,
          "end": 9.88
        }
      ]
    },
    {
      "id": "8.23.5",
      "mandala": 8,
      "sukta": 23,
      "rik": 5,
      "padas": [
        {
          "text": "उदु᳚ तिष्ठ स्वध्वर॒ स्तवा᳚नो दे॒व्या कृ॒पा ।",
          "begin": 0.0,
          "end": 5.312
        },
        {
          "text": "अ॒भि॒ख्या भा॒सा बृ᳚ह॒ता शु᳚शु॒क्वनि᳚ः ॥",
          "begin": 5.312,
          "end": 8.96
        }
      ]
    },
    {
      "id": "8.23.6",
      "mandala": 8,
      "sukta": 23,
      "rik": 6,
      "padas": [
        {
          "text": "अग्ने᳚ या॒हि सु᳚श॒स्तिभि᳚र्ह॒व्या जुह्वा᳚न आनु॒षक् ।",
          "begin": 0.0,
          "end": 6.232
        },
        {
          "text": "यथा᳚ दू॒तो ब॒भूथ᳚ हव्य॒वाह᳚नः ॥",
          "begin": 6.232,
          "end": 10.4
        }
      ]
    },
    {
      "id": "8.23.7",
      "mandala": 8,
      "sukta": 23,
      "rik": 7,
      "padas": [
        {
          "text": "अ॒ग्निं वः᳚ पू॒र्व्यं हु᳚वे॒ होता᳚रं चर्षणी॒नाम् ।",
          "begin": 0.0,
          "end": 5.532
        },
        {
          "text": "तम॒या वा॒चा गृ᳚णे॒ तमु᳚ वः स्तुषे ॥",
          "begin": 5.532,
          "end": 9.32
        }
      ]
    },
    {
      "id": "8.23.8",
      "mandala": 8,
      "sukta": 23,
      "rik": 8,
      "padas": [
        {
          "text": "य॒ज्ञेभि॒रद्भु᳚तक्रतुं॒ यं कृ॒पा सू॒दय᳚न्त॒ इत् ।",
          "begin": 0.0,
          "end": 5.631
        },
        {
          "text": "मि॒त्रं न जने॒ सुधि᳚तमृ॒ताव᳚नि ॥",
          "begin": 5.631,
          "end": 8.44
        }
      ]
    },
    {
      "id": "8.23.9",
      "mandala": 8,
      "sukta": 23,
      "rik": 9,
      "padas": [
        {
          "text": "ऋ॒तावा᳚नमृतायवो य॒ज्ञस्य॒ साध᳚नं गि॒रा ।",
          "begin": 0.0,
          "end": 6.354
        },
        {
          "text": "उपो᳚ एनं जुजुषु॒र्नम᳚सस्प॒दे ॥",
          "begin": 6.354,
          "end": 9.48
        }
      ]
    },
    {
      "id": "8.23.10",
      "mandala": 8,
      "sukta": 23,
      "rik": 10,
      "padas": [
        {
          "text": "अच्छा᳚ नो॒ अङ्गि᳚रस्तमं य॒ज्ञासो᳚ यन्तु सं॒यतः᳚ ।",
          "begin": 0.0,
          "end": 6.684
        },
        {
          "text": "होता॒ यो अस्ति᳚ वि॒क्ष्वा य॒शस्त᳚मः ॥",
          "begin": 6.684,
          "end": 10.76
        }
      ]
    },
    {
      "id": "8.23.11",
      "mandala": 8,
      "sukta": 23,
      "rik": 11,
      "padas": [
        {
          "text": "अग्ने॒ तव॒ त्ये अ᳚ज॒रेन्धा᳚नासो बृ॒हद्भाः ।",
          "begin": 0.0,
          "end": 6.23
        },
        {
          "text": "अश्वा᳚ इव॒ वृष᳚णस्तविषी॒यवः᳚ ॥",
          "begin": 6.23,
          "end": 10.08
        }
      ]
    },
    {
      "id": "8.23.12",
      "mandala": 8,
      "sukta": 23,
      "rik": 12,
      "padas": [
        {
          "text": "स त्वं न᳚ ऊर्जां पते र॒यिं रा᳚स्व सु॒वीर्य᳚म् ।",
          "begin": 0.0,
          "end": 6.449
        },
        {
          "text": "प्राव᳚ नस्तो॒के तन᳚ये स॒मत्स्वा ॥",
          "begin": 6.449,
          "end": 10.08
        }
      ]
    },
    {
      "id": "8.23.13",
      "mandala": 8,
      "sukta": 23,
      "rik": 13,
      "padas": [
        {
          "text": "यद्वा उ᳚ वि॒श्पति᳚ः शि॒तः सुप्री᳚तो॒ मनु᳚षो वि॒शि ।",
          "begin": 0.0,
          "end": 4.581
        },
        {
          "text": "विश्वेद॒ग्निः प्रति॒ रक्षां᳚सि सेधति ॥",
          "begin": 4.581,
          "end": 10.2
        }
      ]
    },
    {
      "id": "8.23.14",
      "mandala": 8,
      "sukta": 23,
      "rik": 14,
      "padas": [
        {
          "text": "श्रु॒ष्ट्य᳚ग्ने॒ नव᳚स्य मे॒ स्तोम᳚स्य वीर विश्पते ।",
          "begin": 0.0,
          "end": 4.889
        },
        {
          "text": "नि मा॒यिन॒स्तपु᳚षा र॒क्षसो᳚ दह ॥",
          "begin": 4.889,
          "end": 8.92
        }
      ]
    },
    {
      "id": "8.23.15",
      "mandala": 8,
      "sukta": 23,
      "rik": 15,
      "padas": [
        {
          "text": "न तस्य᳚ मा॒यया᳚ च॒न रि॒पुरी᳚शीत॒ मर्त्य᳚ः ।",
          "begin": 0.0,
          "end": 5.878
        },
        {
          "text": "यो अ॒ग्नये᳚ द॒दाश᳚ ह॒व्यदा᳚तिभिः ॥",
          "begin": 5.878,
          "end": 10.44
        }
      ]
    },
    {
      "id": "8.23.16",
      "mandala": 8,
      "sukta": 23,
      "rik": 16,
      "padas": [
        {
          "text": "व्य᳚श्वस्त्वा वसु॒विद᳚मुक्ष॒ण्युर᳚प्रीणा॒दृषि᳚ः ।",
          "begin": 0.0,
          "end": 4.966
        },
        {
          "text": "म॒हो रा॒ये तमु᳚ त्वा॒ समि᳚धीमहि ॥",
          "begin": 4.966,
          "end": 8.72
        }
      ]
    },
    {
      "id": "8.23.17",
      "mandala": 8,
      "sukta": 23,
      "rik": 17,
      "padas": [
        {
          "text": "उ॒शना᳚ का॒व्यस्त्वा॒ नि होता᳚रमसादयत् ।",
          "begin": 0.0,
          "end": 6.032
        },
        {
          "text": "आ॒य॒जिं त्वा॒ मन᳚वे जा॒तवे᳚दसम् ॥",
          "begin": 6.032,
          "end": 9.68
        }
      ]
    },
    {
      "id": "8.23.18",
      "mandala": 8,
      "sukta": 23,
      "rik": 18,
      "padas": [
        {
          "text": "विश्वे॒ हि त्वा᳚ स॒जोष᳚सो दे॒वासो᳚ दू॒तमक्र᳚त ।",
          "begin": 0.0,
          "end": 6.619
        },
        {
          "text": "श्रु॒ष्टी दे᳚व प्रथ॒मो य॒ज्ञियो᳚ भुवः ॥",
          "begin": 6.619,
          "end": 11.24
        }
      ]
    },
    {
      "id": "8.23.19",
      "mandala": 8,
      "sukta": 23,
      "rik": 19,
      "padas": [
        {
          "text": "इ॒मं घा᳚ वी॒रो अ॒मृतं᳚ दू॒तं कृ᳚ण्वीत॒ मर्त्य᳚ः ।",
          "begin": 0.0,
          "end": 7.152
        },
        {
          "text": "पा॒व॒कं कृ॒ष्णव᳚र्तनिं॒ विहा᳚यसम् ॥",
          "begin": 7.152,
          "end": 11.12
        }
      ]
    },
    {
      "id": "8.23.20",
      "mandala": 8,
      "sukta": 23,
      "rik": 20,
      "padas": [
        {
          "text": "तं हु᳚वेम य॒तस्रु᳚चः सु॒भासं᳚ शु॒क्रशो᳚चिषम् ।",
          "begin": 0.0,
          "end": 6.074
        },
        {
          "text": "वि॒शाम॒ग्निम॒जरं᳚ प्र॒त्नमीड्य᳚म् ॥",
          "begin": 6.074,
          "end": 10.44
        }
      ]
    },
    {
      "id": "8.23.21",
      "mandala": 8,
      "sukta": 23,
      "rik": 21,
      "padas": [
        {
          "text": "यो अ᳚स्मै ह॒व्यदा᳚तिभि॒राहु᳚तिं॒ मर्तो॑वि᳚धत् ।",
          "begin": 0.0,
          "end": 6.099
        },
        {
          "text": "भूरि॒ पोषं॒ स ध᳚त्ते वी॒रव॒द्यशः᳚ ॥",
          "begin": 6.099,
          "end": 9.8
        }
      ]
    },
    {
      "id": "8.23.22",
      "mandala": 8,
      "sukta": 23,
      "rik": 22,
      "padas": [
        {
          "text": "प्र॒थ॒मं जा॒तवे᳚दसम॒ग्निं य॒ज्ञेषु᳚ पू॒र्व्यम् ।",
          "begin": 0.0,
          "end": 5.298
        },
        {
          "text": "प्रति॒ स्रुगे᳚ति॒ नम᳚सा ह॒विष्म᳚ती ॥",
          "begin": 5.298,
          "end": 9.16
        }
      ]
    },
    {
      "id": "8.23.23",
      "mandala": 8,
      "sukta": 23,
      "rik": 23,
      "padas": [
        {
          "text": "आभि᳚र्विधेमा॒ग्नये॒ ज्येष्ठा᳚भिर्व्यश्व॒वत् ।",
          "begin": 0.0,
          "end": 6.335
        },
        {
          "text": "मंहि᳚ष्ठाभिर्म॒तिभि᳚ः शु॒क्रशो᳚चिषे ॥",
          "begin": 6.335,
          "end": 10.32
        }
      ]
    },
    {
      "id": "8.23.24",
      "mandala": 8,
      "sukta": 23,
      "rik": 24,
      "padas": [
        {
          "text": "नू॒नम᳚र्च॒ विहा᳚यसे॒ स्तोमे᳚भिः स्थूरयूप॒वत् ।",
          "begin": 0.0,
          "end": 6.426
        },
        {
          "text": "ऋषे᳚ वैयश्व॒ दम्या᳚या॒ग्नये᳚ ॥",
          "begin": 6.426,
          "end": 11.12
        }
      ]
    },
    {
      "id": "8.23.25",
      "mandala": 8,
      "sukta": 23,
      "rik": 25,
      "padas": [
        {
          "text": "अति᳚थिं॒ मानु᳚षाणां सू॒नुं वन॒स्पती᳚नाम् ।",
          "begin": 0.0,
          "end": 5.463
        },
        {
          "text": "विप्रा᳚ अ॒ग्निमव᳚से प्र॒त्नमी᳚ळते ॥",
          "begin": 5.463,
          "end": 10.44
        }
      ]
    },
    {
      "id": "8.23.26",
      "mandala": 8,
      "sukta": 23,
      "rik": 26,
      "padas": [
        {
          "text": "म॒हो विश्वाँ᳚ अ॒भि ष॒तो॒३॒᳚॑भि ह॒व्यानि॒ मानु᳚षा ।",
          "begin": 0.0,
          "end": 7.456
        },
        {
          "text": "अग्ने॒ नि ष᳚त्सि॒ नम॒साधि᳚ ब॒र्हिषि᳚ ॥",
          "begin": 7.456,
          "end": 11.4
        }
      ]
    },
    {
      "id": "8.23.27",
      "mandala": 8,
      "sukta": 23,
      "rik": 27,
      "padas": [
        {
          "text": "वंस्वा᳚ नो॒ वार्या᳚ पु॒रु वंस्व᳚ रा॒यः पु᳚रु॒स्पृहः᳚ ।",
          "begin": 0.0,
          "end": 6.861
        },
        {
          "text": "सु॒वीर्य᳚स्य प्र॒जाव᳚तो॒ यश᳚स्वतः ॥",
          "begin": 6.861,
          "end": 10.56
        }
      ]
    },
    {
      "id": "8.23.28",
      "mandala": 8,
      "sukta": 23,
      "rik": 28,
      "padas": [
        {
          "text": "त्वं व᳚रो सु॒षाम्णे॑ग्ने॒ जना᳚य चोदय ।",
          "begin": 0.0,
          "end": 5.261
        },
        {
          "text": "सदा᳚ वसो रा॒तिं य᳚विष्ठ॒ शश्व᳚ते ॥",
          "begin": 5.261,
          "end": 9.12
        }
      ]
    },
    {
      "id": "8.23.29",
      "mandala": 8,
      "sukta": 23,
      "rik": 29,
      "padas": [
        {
          "text": "त्वं हि सु᳚प्र॒तूरसि॒ त्वं नो॒ गोम᳚ती॒रिषः᳚ ।",
          "begin": 0.0,
          "end": 4.578
        },
        {
          "text": "म॒हो रा॒यः सा॒तिम᳚ग्ने॒ अपा᳚ वृधि ॥",
          "begin": 4.578,
          "end": 9.72
        }
      ]
    },
    {
      "id": "8.23.30",
      "mandala": 8,
      "sukta": 23,
      "rik": 30,
      "padas": [
        {
          "text": "अग्ने॒ त्वं य॒शा अ॒स्या मि॒त्रावरु᳚णा वह ।",
          "begin": 0.0,
          "end": 5.846
        },
        {
          "text": "ऋ॒तावा᳚ना स॒म्राजा᳚ पू॒तद᳚क्षसा ॥",
          "begin": 5.846,
          "end": 12.0
        }
      ]
    }
  ]
}