{
  "mandala": 8,
  "sukta": 94,
  "verses": [
    {
      "id": "8.94.1",
      "mandala": 8,
      "sukta": 94,
      "rik": 1,
      "padas": [
        {
          "text": "गौर्ध᳚यति म॒रुतां᳚ श्रव॒स्युर्मा॒ता म॒घोना᳚म् ।",
          "begin": 0.0,
          "end": 7.499
        },
        {
          "text": "यु॒क्ता वह्नी॒ रथा᳚नाम् ॥",
          "begin": 7.499,
          "end": 10.8
        }
      ]
    },
    {
      "id": "8.94.2",
      "mandala": 8,
      "sukta": 94,
      "rik": 2,
      "padas": [
        {
          "text": "यस्या᳚ दे॒वा उ॒पस्थे᳚ व्र॒ता विश्वे᳚ धा॒रय᳚न्ते ।",
          "begin": 0.0,
          "end": 5.955
        },
        {
          "text": "सूर्या॒मासा᳚ दृ॒शे कम् ॥",
          "begin": 5.955,
          "end": 9.92
        }
      ]
    },
    {
      "id": "8.94.3",
      "mandala": 8,
      "sukta": 94,
      "rik": 3,
      "padas": [
        {
          "text": "तत्सु नो॒ विश्वे᳚ अ॒र्य आ सदा᳚ गृणन्ति का॒रवः᳚ ।",
          "begin": 0.0,
          "end": 5.862
        },
        {
          "text": "म॒रुत॒ः सोम᳚पीतये ॥",
          "begin": 5.862,
          "end": 7.96
        }
      ]
    },
    {
      "id": "8.94.4",
      "mandala": 8,
      "sukta": 94,
      "rik": 4,
      "padas": [
        {
          "text": "अस्ति॒ सोमो᳚ अ॒यं सु॒तः पिब᳚न्त्यस्य म॒रुतः᳚ ।",
          "begin": 0.0,
          "end": 5.313
        },
        {
          "text": "उ॒त स्व॒राजो᳚ अ॒श्विना᳚ ॥",
          "begin": 5.313,
          "end": 8.04
        }
      ]
    },
    {
      "id": "8.94.5",
      "mandala": 8,
      "sukta": 94,
      "rik": 5,
      "padas": [
        {
          "text": "पिब᳚न्ति मि॒त्रो अ᳚र्य॒मा तना᳚ पू॒तस्य॒ वरु᳚णः ।",
          "begin": 0.0,
          "end": 5.884
        },
        {
          "text": "त्रि॒ष॒ध॒स्थस्य॒ जाव᳚तः ॥",
          "begin": 5.884,
          "end": 8.12
        }
      ]
    },
    {
      "id": "8.94.6",
      "mandala": 8,
      "sukta": 94,
      "rik": 6,
      "padas": [
        {
          "text": "उ॒तो न्व᳚स्य॒ जोष॒माँ इन्द्र᳚ः सु॒तस्य॒ गोम᳚तः ।",
          "begin": 0.0,
          "end": 5.111
        },
        {
          "text": "प्रा॒तर्होते᳚व मत्सति ॥",
          "begin": 5.111,
          "end": 7.68
        }
      ]
    },
    {
      "id": "8.94.7",
      "mandala": 8,
      "sukta": 94,
      "rik": 7,
      "padas": [
        {
          "text": "कद᳚त्विषन्त सू॒रय᳚स्ति॒र आप᳚ इव॒ स्रिधः᳚ ।",
          "begin": 0.0,
          "end": 4.721
        },
        {
          "text": "अर्ष᳚न्ति पू॒तद᳚क्षसः ॥",
          "begin": 4.721,
          "end": 7.24
        }
      ]
    },
    {
      "id": "8.94.8",
      "mandala": 8,
      "sukta": 94,
      "rik": 8,
      "padas": [
        {
          "text": "कद्वो᳚ अ॒द्य म॒हानां᳚ दे॒वाना॒मवो᳚ वृणे ।",
          "begin": 0.0,
          "end": 5.399
        },
        {
          "text": "त्मना᳚ च द॒स्मव᳚र्चसाम् ॥",
          "begin": 5.399,
          "end": 8.08
        }
      ]
    },
    {
      "id": "8.94.9",
      "mandala": 8,
      "sukta": 94,
      "rik": 9,
      "padas": [
        {
          "text": "आ ये विश्वा॒ पार्थि᳚वानि प॒प्रथ᳚न्रोच॒ना दि॒वः ।",
          "begin": 0.0,
          "end": 5.517
        },
        {
          "text": "म॒रुत॒ः सोम᳚पीतये ॥",
          "begin": 5.517,
          "end": 7.52
        }
      ]
    },
    {
      "id": "8.94.10",
      "mandala": 8,
      "sukta": 94,
      "rik": 10,
      "padas": [
        {
          "text": "त्यान्नु पू॒तद᳚क्षसो दि॒वो वो᳚ मरुतो हुवे ।",
          "begin": 0.0,
          "end": 4.971
        },
        {
          "text": "अ॒स्य सोम᳚स्य पी॒तये᳚ ॥",
          "begin": 4.971,
          "end": 7.36
        }
      ]
    },
    {
      "id": "8.94.11",
      "mandala": 8,
      "sukta": 94,
      "rik": 11,
      "padas": [
        {
          "text": "त्यान्नु ये वि रोद᳚सी तस्त॒भुर्म॒रुतो᳚ हुवे ।",
          "begin": 0.0,
          "end": 5.14
        },
        {
          "text": "अ॒स्य सोम᳚स्य पी॒तये᳚ ॥",
          "begin": 5.14,
          "end": 7.56
        }
      ]
    },
    {
      "id": "8.94.12",
      "mandala": 8,
      "sukta": 94,
      "rik": 12,
      "padas": [
        {
          "text": "त्यं नु मारु᳚तं ग॒णं गि᳚रि॒ष्ठां वृष᳚णं हुवे ।",
          "begin": 0.0,
          "end": 5.594
        },
        {
          "text": "अ॒स्य सोम᳚स्य पी॒तये᳚ ॥",
          "begin": 5.594,
          "end": 9.72
        }
      ]
    }
  ]
}