{
  "mandala": 8,
  "sukta": 6,
  "verses": [
    {
      "id": "8.6.1",
      "mandala": 8,
      "sukta": 6,
      "rik": 1,
      "padas": [
        {
          "text": "म॒हाँ इन्द्रो॒ य ओज᳚सा प॒र्जन्यो᳚ वृष्टि॒माँ इ᳚व ।",
          "begin": 0.0,
          "end": 10.19
        },
        {
          "text": "स्तोमै᳚र्व॒त्सस्य᳚ वावृधे ॥",
          "begin": 10.19,
          "end": 14.28
        }
      ]
    },
    {
      "id": "8.6.2",
      "mandala": 8,
      "sukta": 6,
      "rik": 2,
      "padas": [
        {
          "text": "प्र॒जामृ॒तस्य॒ पिप्र᳚त॒ः प्र यद्भर᳚न्त॒ वह्न᳚यः ।",
          "begin": 0.0,
          "end": 6.101
        },
        {
          "text": "विप्रा᳚ ऋ॒तस्य॒ वाह᳚सा ॥",
          "begin": 6.101,
          "end": 8.96
        }
      ]
    },
    {
      "id": "8.6.3",
      "mandala": 8,
      "sukta": 6,
      "rik": 3,
      "padas": [
        {
          "text": "कण्वा॒ इन्द्रं॒ यदक्र᳚त॒ स्तोमै᳚र्य॒ज्ञस्य॒ साध᳚नम् ।",
          "begin": 0.0,
          "end": 6.262
        },
        {
          "text": "जा॒मि ब्रु᳚वत॒ आयु᳚धम् ॥",
          "begin": 6.262,
          "end": 9.2
        }
      ]
    },
    {
      "id": "8.6.4",
      "mandala": 8,
      "sukta": 6,
      "rik": 4,
      "padas": [
        {
          "text": "सम᳚स्य म॒न्यवे॒ विशो॒ विश्वा᳚ नमन्त कृ॒ष्टयः᳚ ।",
          "begin": 0.0,
          "end": 6.114
        },
        {
          "text": "स॒मु॒द्राये᳚व॒ सिन्ध᳚वः ॥",
          "begin": 6.114,
          "end": 9.24
        }
      ]
    },
    {
      "id": "8.6.5",
      "mandala": 8,
      "sukta": 6,
      "rik": 5,
      "padas": [
        {
          "text": "ओज॒स्तद᳚स्य तित्विष उ॒भे यत्स॒मव᳚र्तयत् ।",
          "begin": 0.0,
          "end": 5.323
        },
        {
          "text": "इन्द्र॒श्चर्मे᳚व॒ रोद᳚सी ॥",
          "begin": 5.323,
          "end": 8.64
        }
      ]
    },
    {
      "id": "8.6.6",
      "mandala": 8,
      "sukta": 6,
      "rik": 6,
      "padas": [
        {
          "text": "वि चि᳚द्वृ॒त्रस्य॒ दोध᳚तो॒ वज्रे᳚ण श॒तप᳚र्वणा ।",
          "begin": 0.0,
          "end": 5.994
        },
        {
          "text": "शिरो᳚ बिभेद वृ॒ष्णिना᳚ ॥",
          "begin": 5.994,
          "end": 9.24
        }
      ]
    },
    {
      "id": "8.6.7",
      "mandala": 8,
      "sukta": 6,
      "rik": 7,
      "padas": [
        {
          "text": "इ॒मा अ॒भि प्र णो᳚नुमो वि॒पामग्रे᳚षु धी॒तयः᳚ ।",
          "begin": 0.0,
          "end": 6.699
        },
        {
          "text": "अ॒ग्नेः शो॒चिर्न दि॒द्युतः᳚ ॥",
          "begin": 6.699,
          "end": 10.0
        }
      ]
    },
    {
      "id": "8.6.8",
      "mandala": 8,
      "sukta": 6,
      "rik": 8,
      "padas": [
        {
          "text": "गुहा᳚ स॒तीरुप॒ त्मना॒ प्र यच्छोच᳚न्त धी॒तयः᳚ ।",
          "begin": 0.0,
          "end": 5.456
        },
        {
          "text": "कण्वा᳚ ऋ॒तस्य॒ धार᳚या ॥",
          "begin": 5.456,
          "end": 9.8
        }
      ]
    },
    {
      "id": "8.6.9",
      "mandala": 8,
      "sukta": 6,
      "rik": 9,
      "padas": [
        {
          "text": "प्र तमि᳚न्द्र नशीमहि र॒यिं गोम᳚न्तम॒श्विन᳚म् ।",
          "begin": 0.0,
          "end": 5.172
        },
        {
          "text": "प्र ब्रह्म᳚ पू॒र्वचि᳚त्तये ॥",
          "begin": 5.172,
          "end": 9.04
        }
      ]
    },
    {
      "id": "8.6.10",
      "mandala": 8,
      "sukta": 6,
      "rik": 10,
      "padas": [
        {
          "text": "अ॒हमिद्धि पि॒तुष्परि᳚ मे॒धामृ॒तस्य᳚ ज॒ग्रभ᳚ ।",
          "begin": 0.0,
          "end": 5.134
        },
        {
          "text": "अ॒हं सूर्य᳚ इवाजनि ॥",
          "begin": 5.134,
          "end": 7.56
        }
      ]
    },
    {
      "id": "8.6.11",
      "mandala": 8,
      "sukta": 6,
      "rik": 11,
      "padas": [
        {
          "text": "अ॒हं प्र॒त्नेन॒ मन्म᳚ना॒ गिरः᳚ शुम्भामि कण्व॒वत् ।",
          "begin": 0.0,
          "end": 5.871
        },
        {
          "text": "येनेन्द्र॒ः शुष्म॒मिद्द॒धे ॥",
          "begin": 5.871,
          "end": 8.48
        }
      ]
    },
    {
      "id": "8.6.12",
      "mandala": 8,
      "sukta": 6,
      "rik": 12,
      "padas": [
        {
          "text": "ये त्वामि᳚न्द्र॒ न तु᳚ष्टु॒वुरृष᳚यो॒ ये च᳚ तुष्टु॒वुः ।",
          "begin": 0.0,
          "end": 5.171
        },
        {
          "text": "ममेद्व᳚र्धस्व॒ सुष्टु᳚तः ॥",
          "begin": 5.171,
          "end": 9.08
        }
      ]
    },
    {
      "id": "8.6.13",
      "mandala": 8,
      "sukta": 6,
      "rik": 13,
      "padas": [
        {
          "text": "यद᳚स्य म॒न्युरध्व᳚नी॒द्वि वृ॒त्रं प᳚र्व॒शो रु॒जन् ।",
          "begin": 0.0,
          "end": 6.188
        },
        {
          "text": "अ॒पः स᳚मु॒द्रमैर᳚यत् ॥",
          "begin": 6.188,
          "end": 8.56
        }
      ]
    },
    {
      "id": "8.6.14",
      "mandala": 8,
      "sukta": 6,
      "rik": 14,
      "padas": [
        {
          "text": "नि शुष्ण᳚ इन्द्र धर्ण॒सिं वज्रं᳚ जघन्थ॒ दस्य᳚वि ।",
          "begin": 0.0,
          "end": 5.623
        },
        {
          "text": "वृषा॒ ह्यु᳚ग्र शृण्वि॒षे ॥",
          "begin": 5.623,
          "end": 8.28
        }
      ]
    },
    {
      "id": "8.6.15",
      "mandala": 8,
      "sukta": 6,
      "rik": 15,
      "padas": [
        {
          "text": "न द्याव॒ इन्द्र॒मोज᳚सा॒ नान्तरि᳚क्षाणि व॒ज्रिण᳚म् ।",
          "begin": 0.0,
          "end": 5.448
        },
        {
          "text": "न वि᳚व्यचन्त॒ भूम᳚यः ॥",
          "begin": 5.448,
          "end": 9.16
        }
      ]
    },
    {
      "id": "8.6.16",
      "mandala": 8,
      "sukta": 6,
      "rik": 16,
      "padas": [
        {
          "text": "यस्त᳚ इन्द्र म॒हीर॒पः स्त᳚भू॒यमा᳚न॒ आश᳚यत् ।",
          "begin": 0.0,
          "end": 5.749
        },
        {
          "text": "नि तं पद्या᳚सु शिश्नथः ॥",
          "begin": 5.749,
          "end": 8.8
        }
      ]
    },
    {
      "id": "8.6.17",
      "mandala": 8,
      "sukta": 6,
      "rik": 17,
      "padas": [
        {
          "text": "य इ॒मे रोद᳚सी म॒ही स᳚मी॒ची स॒मज᳚ग्रभीत् ।",
          "begin": 0.0,
          "end": 4.771
        },
        {
          "text": "तमो᳚भिरिन्द्र॒ तं गु᳚हः ॥",
          "begin": 4.771,
          "end": 8.44
        }
      ]
    },
    {
      "id": "8.6.18",
      "mandala": 8,
      "sukta": 6,
      "rik": 18,
      "padas": [
        {
          "text": "य इ᳚न्द्र॒ यत᳚यस्त्वा॒ भृग᳚वो॒ ये च᳚ तुष्टु॒वुः ।",
          "begin": 0.0,
          "end": 4.631
        },
        {
          "text": "ममेदु᳚ग्र श्रुधी॒ हव᳚म् ॥",
          "begin": 4.631,
          "end": 8.24
        }
      ]
    },
    {
      "id": "8.6.19",
      "mandala": 8,
      "sukta": 6,
      "rik": 19,
      "padas": [
        {
          "text": "इ॒मास्त᳚ इन्द्र॒ पृश्न᳚यो घृ॒तं दु᳚हत आ॒शिर᳚म् ।",
          "begin": 0.0,
          "end": 5.48
        },
        {
          "text": "ए॒नामृ॒तस्य᳚ पि॒प्युषी᳚ः ॥",
          "begin": 5.48,
          "end": 8.72
        }
      ]
    },
    {
      "id": "8.6.20",
      "mandala": 8,
      "sukta": 6,
      "rik": 20,
      "padas": [
        {
          "text": "या इ᳚न्द्र प्र॒स्व᳚स्त्वा॒सा गर्भ॒मच᳚क्रिरन् ।",
          "begin": 0.0,
          "end": 5.729
        },
        {
          "text": "परि॒ धर्मे᳚व॒ सूर्य᳚म् ॥",
          "begin": 5.729,
          "end": 9.56
        }
      ]
    },
    {
      "id": "8.6.21",
      "mandala": 8,
      "sukta": 6,
      "rik": 21,
      "padas": [
        {
          "text": "त्वामिच्छ᳚वसस्पते॒ कण्वा᳚ उ॒क्थेन᳚ वावृधुः ।",
          "begin": 0.0,
          "end": 6.48
        },
        {
          "text": "त्वां सु॒तास॒ इन्द᳚वः ॥",
          "begin": 6.48,
          "end": 8.72
        }
      ]
    },
    {
      "id": "8.6.22",
      "mandala": 8,
      "sukta": 6,
      "rik": 22,
      "padas": [
        {
          "text": "तवेदि᳚न्द्र॒ प्रणी᳚तिषू॒त प्रश᳚स्तिरद्रिवः ।",
          "begin": 0.0,
          "end": 6.232
        },
        {
          "text": "य॒ज्ञो वि᳚तन्त॒साय्य᳚ः ॥",
          "begin": 6.232,
          "end": 9.2
        }
      ]
    },
    {
      "id": "8.6.23",
      "mandala": 8,
      "sukta": 6,
      "rik": 23,
      "padas": [
        {
          "text": "आ न᳚ इन्द्र म॒हीमिषं॒ पुरं॒ न द᳚र्षि॒ गोम᳚तीम् ।",
          "begin": 0.0,
          "end": 5.545
        },
        {
          "text": "उ॒त प्र॒जां सु॒वीर्य᳚म् ॥",
          "begin": 5.545,
          "end": 8.84
        }
      ]
    },
    {
      "id": "8.6.24",
      "mandala": 8,
      "sukta": 6,
      "rik": 24,
      "padas": [
        {
          "text": "उ॒त त्यदा॒श्वश्व्यं॒ यदि᳚न्द्र॒ नाहु᳚षी॒ष्वा ।",
          "begin": 0.0,
          "end": 5.808
        },
        {
          "text": "अग्रे᳚ वि॒क्षु प्र॒दीद᳚यत् ॥",
          "begin": 5.808,
          "end": 9.16
        }
      ]
    },
    {
      "id": "8.6.25",
      "mandala": 8,
      "sukta": 6,
      "rik": 25,
      "padas": [
        {
          "text": "अ॒भि व्र॒जं न त᳚त्निषे॒ सूर᳚ उपा॒कच᳚क्षसम् ।",
          "begin": 0.0,
          "end": 4.917
        },
        {
          "text": "यदि᳚न्द्र मृ॒ळया᳚सि नः ॥",
          "begin": 4.917,
          "end": 8.56
        }
      ]
    },
    {
      "id": "8.6.26",
      "mandala": 8,
      "sukta": 6,
      "rik": 26,
      "padas": [
        {
          "text": "यद॒ङ्ग त᳚विषी॒यस॒ इन्द्र᳚ प्र॒राज᳚सि क्षि॒तीः ।",
          "begin": 0.0,
          "end": 5.048
        },
        {
          "text": "म॒हाँ अ᳚पा॒र ओज᳚सा ॥",
          "begin": 5.048,
          "end": 8.2
        }
      ]
    },
    {
      "id": "8.6.27",
      "mandala": 8,
      "sukta": 6,
      "rik": 27,
      "padas": [
        {
          "text": "तं त्वा᳚ ह॒विष्म᳚ती॒र्विश॒ उप᳚ ब्रुवत ऊ॒तये᳚ ।",
          "begin": 0.0,
          "end": 5.951
        },
        {
          "text": "उ॒रु॒ज्रय᳚स॒मिन्दु᳚भिः ॥",
          "begin": 5.951,
          "end": 8.48
        }
      ]
    },
    {
      "id": "8.6.28",
      "mandala": 8,
      "sukta": 6,
      "rik": 28,
      "padas": [
        {
          "text": "उ॒प॒ह्व॒रे गि᳚री॒णां सं᳚ग॒थे च᳚ न॒दीना᳚म् ।",
          "begin": 0.0,
          "end": 5.565
        },
        {
          "text": "धि॒या विप्रो᳚ अजायत ॥",
          "begin": 5.565,
          "end": 9.04
        }
      ]
    },
    {
      "id": "8.6.29",
      "mandala": 8,
      "sukta": 6,
      "rik": 29,
      "padas": [
        {
          "text": "अतः᳚ समु॒द्रमु॒द्वत᳚श्चिकि॒त्वाँ अव᳚ पश्यति ।",
          "begin": 0.0,
          "end": 5.218
        },
        {
          "text": "यतो᳚ विपा॒न एज᳚ति ॥",
          "begin": 5.218,
          "end": 8.4
        }
      ]
    },
    {
      "id": "8.6.30",
      "mandala": 8,
      "sukta": 6,
      "rik": 30,
      "padas": [
        {
          "text": "आदित्प्र॒त्नस्य॒ रेत᳚सो॒ ज्योति᳚ष्पश्यन्ति वास॒रम् ।",
          "begin": 0.0,
          "end": 5.918
        },
        {
          "text": "प॒रो यदि॒ध्यते᳚ दि॒वा ॥",
          "begin": 5.918,
          "end": 8.96
        }
      ]
    },
    {
      "id": "8.6.31",
      "mandala": 8,
      "sukta": 6,
      "rik": 31,
      "padas": [
        {
          "text": "कण्वा᳚स इन्द्र ते म॒तिं विश्वे᳚ वर्धन्ति॒ पौंस्य᳚म् ।",
          "begin": 0.0,
          "end": 7.102
        },
        {
          "text": "उ॒तो श᳚विष्ठ॒ वृष्ण्य᳚म् ॥",
          "begin": 7.102,
          "end": 9.76
        }
      ]
    },
    {
      "id": "8.6.32",
      "mandala": 8,
      "sukta": 6,
      "rik": 32,
      "padas": [
        {
          "text": "इ॒मां म᳚ इन्द्र सुष्टु॒तिं जु॒षस्व॒ प्र सु माम᳚व ।",
          "begin": 0.0,
          "end": 4.991
        },
        {
          "text": "उ॒त प्र व᳚र्धया म॒तिम् ॥",
          "begin": 4.991,
          "end": 7.76
        }
      ]
    },
    {
      "id": "8.6.33",
      "mandala": 8,
      "sukta": 6,
      "rik": 33,
      "padas": [
        {
          "text": "उ॒त ब्र᳚ह्म॒ण्या व॒यं तुभ्यं᳚ प्रवृद्ध वज्रिवः ।",
          "begin": 0.0,
          "end": 4.249
        },
        {
          "text": "विप्रा᳚ अतक्ष्म जी॒वसे᳚ ॥",
          "begin": 4.249,
          "end": 9.84
        }
      ]
    },
    {
      "id": "8.6.34",
      "mandala": 8,
      "sukta": 6,
      "rik": 34,
      "padas": [
        {
          "text": "अ॒भि कण्वा᳚ अनूष॒तापो॒ न प्र॒वता᳚ य॒तीः ।",
          "begin": 0.0,
          "end": 5.705
        },
        {
          "text": "इन्द्रं॒ वन᳚न्वती म॒तिः ॥",
          "begin": 5.705,
          "end": 9.24
        }
      ]
    },
    {
      "id": "8.6.35",
      "mandala": 8,
      "sukta": 6,
      "rik": 35,
      "padas": [
        {
          "text": "इन्द्र᳚मु॒क्थानि᳚ वावृधुः समु॒द्रमि᳚व॒ सिन्ध᳚वः ।",
          "begin": 0.0,
          "end": 4.174
        },
        {
          "text": "अनु᳚त्तमन्युम॒जर᳚म् ॥",
          "begin": 4.174,
          "end": 7.72
        }
      ]
    },
    {
      "id": "8.6.36",
      "mandala": 8,
      "sukta": 6,
      "rik": 36,
      "padas": [
        {
          "text": "आ नो᳚ याहि परा॒वतो॒ हरि᳚भ्यां हर्य॒ताभ्या᳚म् ।",
          "begin": 0.0,
          "end": 6.832
        },
        {
          "text": "इ॒ममि᳚न्द्र सु॒तं पि᳚ब ॥",
          "begin": 6.832,
          "end": 9.44
        }
      ]
    },
    {
      "id": "8.6.37",
      "mandala": 8,
      "sukta": 6,
      "rik": 37,
      "padas": [
        {
          "text": "त्वामिद्वृ᳚त्रहन्तम॒ जना᳚सो वृ॒क्तब᳚र्हिषः ।",
          "begin": 0.0,
          "end": 5.514
        },
        {
          "text": "हव᳚न्ते॒ वाज᳚सातये ॥",
          "begin": 5.514,
          "end": 8.28
        }
      ]
    },
    {
      "id": "8.6.38",
      "mandala": 8,
      "sukta": 6,
      "rik": 38,
      "padas": [
        {
          "text": "अनु᳚ त्वा॒ रोद᳚सी उ॒भे च॒क्रं न व॒र्त्येत᳚शम् ।",
          "begin": 0.0,
          "end": 5.132
        },
        {
          "text": "अनु᳚ सुवा॒नास॒ इन्द᳚वः ॥",
          "begin": 5.132,
          "end": 8.04
        }
      ]
    },
    {
      "id": "8.6.39",
      "mandala": 8,
      "sukta": 6,
      "rik": 39,
      "padas": [
        {
          "text": "मन्द᳚स्वा॒ सु स्व᳚र्णर उ॒तेन्द्र᳚ शर्य॒णाव᳚ति ।",
          "begin": 0.0,
          "end": 5.669
        },
        {
          "text": "मत्स्वा॒ विव᳚स्वतो म॒ती ॥",
          "begin": 5.669,
          "end": 8.28
        }
      ]
    },
    {
      "id": "8.6.40",
      "mandala": 8,
      "sukta": 6,
      "rik": 40,
      "padas": [
        {
          "text": "वा॒वृ॒धा॒न उप॒ द्यवि॒ वृषा᳚ व॒ज्र्य᳚रोरवीत् ।",
          "begin": 0.0,
          "end": 4.993
        },
        {
          "text": "वृ॒त्र॒हा सो᳚म॒पात᳚मः ॥",
          "begin": 4.993,
          "end": 8.52
        }
      ]
    },
    {
      "id": "8.6.41",
      "mandala": 8,
      "sukta": 6,
      "rik": 41,
      "padas": [
        {
          "text": "ऋषि॒र्हि पू᳚र्व॒जा अस्येक॒ ईशा᳚न॒ ओज᳚सा ।",
          "begin": 0.0,
          "end": 6.435
        },
        {
          "text": "इन्द्र᳚ चोष्कू॒यसे॒ वसु᳚ ॥",
          "begin": 6.435,
          "end": 9.4
        }
      ]
    },
    {
      "id": "8.6.42",
      "mandala": 8,
      "sukta": 6,
      "rik": 42,
      "padas": [
        {
          "text": "अ॒स्माकं᳚ त्वा सु॒ताँ उप᳚ वी॒तपृ᳚ष्ठा अ॒भि प्रयः᳚ ।",
          "begin": 0.0,
          "end": 6.418
        },
        {
          "text": "श॒तं व᳚हन्तु॒ हर᳚यः ॥",
          "begin": 6.418,
          "end": 8.72
        }
      ]
    },
    {
      "id": "8.6.43",
      "mandala": 8,
      "sukta": 6,
      "rik": 43,
      "padas": [
        {
          "text": "इ॒मां सु पू॒र्व्यां धियं॒ मधो᳚र्घृ॒तस्य᳚ पि॒प्युषी᳚म् ।",
          "begin": 0.0,
          "end": 6.718
        },
        {
          "text": "कण्वा᳚ उ॒क्थेन᳚ वावृधुः ॥",
          "begin": 6.718,
          "end": 9.96
        }
      ]
    },
    {
      "id": "8.6.44",
      "mandala": 8,
      "sukta": 6,
      "rik": 44,
      "padas": [
        {
          "text": "इन्द्र॒मिद्विम᳚हीनां॒ मेधे᳚ वृणीत॒ मर्त्य᳚ः ।",
          "begin": 0.0,
          "end": 6.035
        },
        {
          "text": "इन्द्रं᳚ सनि॒ष्युरू॒तये᳚ ॥",
          "begin": 6.035,
          "end": 9.4
        }
      ]
    },
    {
      "id": "8.6.45",
      "mandala": 8,
      "sukta": 6,
      "rik": 45,
      "padas": [
        {
          "text": "अ॒र्वाञ्चं᳚ त्वा पुरुष्टुत प्रि॒यमे᳚धस्तुता॒ हरी᳚ ।",
          "begin": 0.0,
          "end": 7.016
        },
        {
          "text": "सो॒म॒पेया᳚य वक्षतः ॥",
          "begin": 7.016,
          "end": 10.04
        }
      ]
    },
    {
      "id": "8.6.46",
      "mandala": 8,
      "sukta": 6,
      "rik": 46,
      "padas": [
        {
          "text": "श॒तम॒हं ति॒रिन्दि᳚रे स॒हस्रं॒ पर्शा॒वा द᳚दे ।",
          "begin": 0.0,
          "end": 5.006
        },
        {
          "text": "राधां᳚सि॒ याद्वा᳚नाम् ॥",
          "begin": 5.006,
          "end": 9.04
        }
      ]
    },
    {
      "id": "8.6.47",
      "mandala": 8,
      "sukta": 6,
      "rik": 47,
      "padas": [
        {
          "text": "त्रीणि᳚ श॒तान्यर्व᳚तां स॒हस्रा॒ दश॒ गोना᳚म् ।",
          "begin": 0.0,
          "end": 5.556
        },
        {
          "text": "द॒दुष्प॒ज्राय॒ साम्ने᳚ ॥",
          "begin": 5.556,
          "end": 8.96
        }
      ]
    },
    {
      "id": "8.6.48",
      "mandala": 8,
      "sukta": 6,
      "rik": 48,
      "padas": [
        {
          "text": "उदा᳚नट् ककु॒हो दिव॒मुष्ट्रा᳚ञ्चतु॒र्युजो॒ दद᳚त् ।",
          "begin": 0.0,
          "end": 7.007
        },
        {
          "text": "श्रव᳚सा॒ याद्वं॒ जन᳚म् ॥",
          "begin": 7.007,
          "end": 13.08
        }
      ]
    }
  ]
}