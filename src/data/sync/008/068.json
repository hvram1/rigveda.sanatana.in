{
  "mandala": 8,
  "sukta": 68,
  "verses": [
    {
      "id": "8.68.1",
      "mandala": 8,
      "sukta": 68,
      "rik": 1,
      "padas": [
        {
          "text": "आ त्वा॒ रथं॒ यथो॒तये᳚ सु॒म्नाय᳚ वर्तयामसि ।",
          "begin": 0.0,
          "end": 5.345
        },
        {
          "text": "तु॒वि॒कू॒र्मिमृ᳚ती॒षह॒मिन्द्र॒ शवि᳚ष्ठ॒ सत्प᳚ते ॥",
          "begin": 5.345,
          "end": 9.84
        }
      ]
    },
    {
      "id": "8.68.2",
      "mandala": 8,
      "sukta": 68,
      "rik": 2,
      "padas": [
        {
          "text": "तुवि᳚शुष्म॒ तुवि᳚क्रतो॒ शची᳚वो॒ विश्व᳚या मते ।",
          "begin": 0.0,
          "end": 4.649
        },
        {
          "text": "आ प᳚प्राथ महित्व॒ना ॥",
          "begin": 4.649,
          "end": 7.72
        }
      ]
    },
    {
      "id": "8.68.3",
      "mandala": 8,
      "sukta": 68,
      "rik": 3,
      "padas": [
        {
          "text": "यस्य᳚ ते महि॒ना म॒हः परि᳚ ज्मा॒यन्त᳚मी॒यतु᳚ः ।",
          "begin": 0.0,
          "end": 5.313
        },
        {
          "text": "हस्ता॒ वज्रं᳚ हिर॒ण्यय᳚म् ॥",
          "begin": 5.313,
          "end": 8.52
        }
      ]
    },
    {
      "id": "8.68.4",
      "mandala": 8,
      "sukta": 68,
      "rik": 4,
      "padas": [
        {
          "text": "वि॒श्वान᳚रस्य व॒स्पति॒मना᳚नतस्य॒ शव᳚सः ।",
          "begin": 0.0,
          "end": 4.863
        },
        {
          "text": "एवै᳚श्च चर्षणी॒नामू॒ती हु᳚वे॒ रथा᳚नाम् ॥",
          "begin": 4.863,
          "end": 10.24
        }
      ]
    },
    {
      "id": "8.68.5",
      "mandala": 8,
      "sukta": 68,
      "rik": 5,
      "padas": [
        {
          "text": "अ॒भिष्ट᳚ये स॒दावृ᳚धं॒ स्व᳚र्मीळ्हेषु॒ यं नरः᳚ ।",
          "begin": 0.0,
          "end": 7.328
        },
        {
          "text": "नाना॒ हव᳚न्त ऊ॒तये᳚ ॥",
          "begin": 7.328,
          "end": 9.4
        }
      ]
    },
    {
      "id": "8.68.6",
      "mandala": 8,
      "sukta": 68,
      "rik": 6,
      "padas": [
        {
          "text": "प॒रोमा᳚त्र॒मृची᳚षम॒मिन्द्र᳚मु॒ग्रं सु॒राध᳚सम् ।",
          "begin": 0.0,
          "end": 6.275
        },
        {
          "text": "ईशा᳚नं चि॒द्वसू᳚नाम् ॥",
          "begin": 6.275,
          "end": 9.4
        }
      ]
    },
    {
      "id": "8.68.7",
      "mandala": 8,
      "sukta": 68,
      "rik": 7,
      "padas": [
        {
          "text": "तंत॒मिद्राध᳚से म॒ह इन्द्रं᳚ चोदामि पी॒तये᳚ ।",
          "begin": 0.0,
          "end": 4.75
        },
        {
          "text": "यः पू॒र्व्यामनु᳚ष्टुति॒मीशे᳚ कृष्टी॒नां नृ॒तुः ॥",
          "begin": 4.75,
          "end": 10.96
        }
      ]
    },
    {
      "id": "8.68.8",
      "mandala": 8,
      "sukta": 68,
      "rik": 8,
      "padas": [
        {
          "text": "न यस्य᳚ ते शवसान स॒ख्यमा॒नंश॒ मर्त्य᳚ः ।",
          "begin": 0.0,
          "end": 5.218
        },
        {
          "text": "नकि॒ः शवां᳚सि ते नशत् ॥",
          "begin": 5.218,
          "end": 7.96
        }
      ]
    },
    {
      "id": "8.68.9",
      "mandala": 8,
      "sukta": 68,
      "rik": 9,
      "padas": [
        {
          "text": "त्वोता᳚स॒स्त्वा यु॒जाप्सु सूर्ये᳚ म॒हद्धन᳚म् ।",
          "begin": 0.0,
          "end": 6.833
        },
        {
          "text": "जये᳚म पृ॒त्सु व᳚ज्रिवः ॥",
          "begin": 6.833,
          "end": 9.16
        }
      ]
    },
    {
      "id": "8.68.10",
      "mandala": 8,
      "sukta": 68,
      "rik": 10,
      "padas": [
        {
          "text": "तं त्वा᳚ य॒ज्ञेभि᳚रीमहे॒ तं गी॒र्भिर्गि᳚र्वणस्तम ।",
          "begin": 0.0,
          "end": 5.884
        },
        {
          "text": "इन्द्र॒ यथा᳚ चि॒दावि᳚थ॒ वाजे᳚षु पुरु॒माय्य᳚म् ॥",
          "begin": 5.884,
          "end": 11.64
        }
      ]
    },
    {
      "id": "8.68.11",
      "mandala": 8,
      "sukta": 68,
      "rik": 11,
      "padas": [
        {
          "text": "यस्य᳚ ते स्वा॒दु स॒ख्यं स्वा॒द्वी प्रणी᳚तिरद्रिवः ।",
          "begin": 0.0,
          "end": 6.88
        },
        {
          "text": "य॒ज्ञो वि᳚तन्त॒साय्य᳚ः ॥",
          "begin": 6.88,
          "end": 9.44
        }
      ]
    },
    {
      "id": "8.68.12",
      "mandala": 8,
      "sukta": 68,
      "rik": 12,
      "padas": [
        {
          "text": "उ॒रु ण᳚स्त॒न्वे॒३॒᳚ तन᳚ उ॒रु क्षया᳚य नस्कृधि ।",
          "begin": 0.0,
          "end": 6.341
        },
        {
          "text": "उ॒रु णो᳚ यन्धि जी॒वसे᳚ ॥",
          "begin": 6.341,
          "end": 10.12
        }
      ]
    },
    {
      "id": "8.68.13",
      "mandala": 8,
      "sukta": 68,
      "rik": 13,
      "padas": [
        {
          "text": "उ॒रुं नृभ्य᳚ उ॒रुं गव᳚ उ॒रुं रथा᳚य॒ पन्था᳚म् ।",
          "begin": 0.0,
          "end": 5.727
        },
        {
          "text": "दे॒ववी᳚तिं मनामहे ॥",
          "begin": 5.727,
          "end": 8.44
        }
      ]
    },
    {
      "id": "8.68.14",
      "mandala": 8,
      "sukta": 68,
      "rik": 14,
      "padas": [
        {
          "text": "उप᳚ मा॒ षड्द्वाद्वा॒ नर॒ः सोम᳚स्य॒ हर्ष्या᳚ ।",
          "begin": 0.0,
          "end": 5.573
        },
        {
          "text": "तिष्ठ᳚न्ति स्वादुरा॒तयः᳚ ॥",
          "begin": 5.573,
          "end": 8.8
        }
      ]
    },
    {
      "id": "8.68.15",
      "mandala": 8,
      "sukta": 68,
      "rik": 15,
      "padas": [
        {
          "text": "ऋ॒ज्रावि᳚न्द्रो॒त आ द᳚दे॒ हरी॒ ऋक्ष᳚स्य सू॒नवि᳚ ।",
          "begin": 0.0,
          "end": 5.945
        },
        {
          "text": "आ॒श्व॒मे॒धस्य॒ रोहि᳚ता ॥",
          "begin": 5.945,
          "end": 8.56
        }
      ]
    },
    {
      "id": "8.68.16",
      "mandala": 8,
      "sukta": 68,
      "rik": 16,
      "padas": [
        {
          "text": "सु॒रथाँ᳚ आतिथि॒ग्वे स्व᳚भी॒शूँरा॒र्क्षे ।",
          "begin": 0.0,
          "end": 6.287
        },
        {
          "text": "आ॒श्व॒मे॒धे सु॒पेश᳚सः ॥",
          "begin": 6.287,
          "end": 8.88
        }
      ]
    },
    {
      "id": "8.68.17",
      "mandala": 8,
      "sukta": 68,
      "rik": 17,
      "padas": [
        {
          "text": "षळश्वाँ᳚ आतिथि॒ग्व इ᳚न्द्रो॒ते व॒धूम᳚तः ।",
          "begin": 0.0,
          "end": 4.631
        },
        {
          "text": "सचा᳚ पू॒तक्र᳚तौ सनम् ॥",
          "begin": 4.631,
          "end": 8.84
        }
      ]
    },
    {
      "id": "8.68.18",
      "mandala": 8,
      "sukta": 68,
      "rik": 18,
      "padas": [
        {
          "text": "ऐषु᳚ चेत॒द्वृष᳚ण्वत्य॒न्तरृ॒ज्रेष्वरु᳚षी ।",
          "begin": 0.0,
          "end": 5.665
        },
        {
          "text": "स्व॒भी॒शुः कशा᳚वती ॥",
          "begin": 5.665,
          "end": 8.32
        }
      ]
    },
    {
      "id": "8.68.19",
      "mandala": 8,
      "sukta": 68,
      "rik": 19,
      "padas": [
        {
          "text": "न यु॒ष्मे वा᳚जबन्धवो निनि॒त्सुश्च॒न मर्त्य᳚ः ।",
          "begin": 0.0,
          "end": 6.115
        },
        {
          "text": "अ॒व॒द्यमधि᳚ दीधरत् ॥",
          "begin": 6.115,
          "end": 9.56
        }
      ]
    }
  ]
}