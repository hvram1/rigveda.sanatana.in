{
  "mandala": 8,
  "sukta": 56,
  "verses": [
    {
      "id": "8.56.1",
      "mandala": 8,
      "sukta": 56,
      "rik": 1,
      "padas": [
        {
          "text": "प्रति᳚ ते दस्यवे वृक॒ राधो᳚ अद॒र्श्यह्र᳚यम् ।",
          "begin": 0.0,
          "end": 7.153
        },
        {
          "text": "द्यौर्न प्र᳚थि॒ना शवः᳚ ॥",
          "begin": 7.153,
          "end": 10.36
        }
      ]
    },
    {
      "id": "8.56.2",
      "mandala": 8,
      "sukta": 56,
      "rik": 2,
      "padas": [
        {
          "text": "दश॒ मह्यं᳚ पौतक्र॒तः स॒हस्रा॒ दस्य᳚वे॒ वृकः᳚ ।",
          "begin": 0.0,
          "end": 5.54
        },
        {
          "text": "नित्या᳚द्रा॒यो अ᳚मंहत ॥",
          "begin": 5.54,
          "end": 8.52
        }
      ]
    },
    {
      "id": "8.56.3",
      "mandala": 8,
      "sukta": 56,
      "rik": 3,
      "padas": [
        {
          "text": "श॒तं मे᳚ गर्द॒भानां᳚ श॒तमूर्णा᳚वतीनाम् ।",
          "begin": 0.0,
          "end": 5.843
        },
        {
          "text": "श॒तं दा॒साँ अति॒ स्रजः᳚ ॥",
          "begin": 5.843,
          "end": 8.76
        }
      ]
    },
    {
      "id": "8.56.4",
      "mandala": 8,
      "sukta": 56,
      "rik": 4,
      "padas": [
        {
          "text": "तत्रो॒ अपि॒ प्राणी᳚यत पू॒तक्र᳚तायै॒ व्य᳚क्ता ।",
          "begin": 0.0,
          "end": 5.72
        },
        {
          "text": "अश्वा᳚ना॒मिन्न यू॒थ्या᳚म् ॥",
          "begin": 5.72,
          "end": 8.96
        }
      ]
    },
    {
      "id": "8.56.5",
      "mandala": 8,
      "sukta": 56,
      "rik": 5,
      "padas": [
        {
          "text": "अचे᳚त्य॒ग्निश्चि᳚कि॒तुर्ह᳚व्य॒वाट् स सु॒मद्र᳚थः ।",
          "begin": 0.0,
          "end": 5.587
        },
        {
          "text": "अ॒ग्निः शु॒क्रेण᳚ शो॒चिषा᳚ बृ॒हत्सूरो᳚ अरोचत दि॒वि सूर्यो᳚ अरोचत ॥",
          "begin": 5.587,
          "end": 13.28
        }
      ]
    }
  ]
}