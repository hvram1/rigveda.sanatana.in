{
  "mandala": 8,
  "sukta": 93,
  "verses": [
    {
      "id": "8.93.1",
      "mandala": 8,
      "sukta": 93,
      "rik": 1,
      "padas": [
        {
          "text": "उद्घेद॒भि श्रु॒ताम᳚घं वृष॒भं नर्या᳚पसम् ।",
          "begin": 0.0,
          "end": 8.192
        },
        {
          "text": "अस्ता᳚रमेषि सूर्य ॥",
          "begin": 8.192,
          "end": 11.28
        }
      ]
    },
    {
      "id": "8.93.2",
      "mandala": 8,
      "sukta": 93,
      "rik": 2,
      "padas": [
        {
          "text": "नव॒ यो न᳚व॒तिं पुरो᳚ बि॒भेद᳚ बा॒ह्वो᳚जसा ।",
          "begin": 0.0,
          "end": 5.197
        },
        {
          "text": "अहिं᳚ च वृत्र॒हाव᳚धीत् ॥",
          "begin": 5.197,
          "end": 8.68
        }
      ]
    },
    {
      "id": "8.93.3",
      "mandala": 8,
      "sukta": 93,
      "rik": 3,
      "padas": [
        {
          "text": "स न॒ इन्द्र᳚ः शि॒वः सखाश्वा᳚व॒द्गोम॒द्यव᳚मत् ।",
          "begin": 0.0,
          "end": 5.569
        },
        {
          "text": "उ॒रुधा᳚रेव दोहते ॥",
          "begin": 5.569,
          "end": 8.04
        }
      ]
    },
    {
      "id": "8.93.4",
      "mandala": 8,
      "sukta": 93,
      "rik": 4,
      "padas": [
        {
          "text": "यद॒द्य कच्च᳚ वृत्रहन्नु॒दगा᳚ अ॒भि सू᳚र्य ।",
          "begin": 0.0,
          "end": 5.224
        },
        {
          "text": "सर्वं॒ तदि᳚न्द्र ते॒ वशे᳚ ॥",
          "begin": 5.224,
          "end": 8.4
        }
      ]
    },
    {
      "id": "8.93.5",
      "mandala": 8,
      "sukta": 93,
      "rik": 5,
      "padas": [
        {
          "text": "यद्वा᳚ प्रवृद्ध सत्पते॒ न म᳚रा॒ इति॒ मन्य᳚से ।",
          "begin": 0.0,
          "end": 5.328
        },
        {
          "text": "उ॒तो तत्स॒त्यमित्तव᳚ ॥",
          "begin": 5.328,
          "end": 7.52
        }
      ]
    },
    {
      "id": "8.93.6",
      "mandala": 8,
      "sukta": 93,
      "rik": 6,
      "padas": [
        {
          "text": "ये सोमा᳚सः परा॒वति॒ ये अ᳚र्वा॒वति᳚ सुन्वि॒रे ।",
          "begin": 0.0,
          "end": 6.176
        },
        {
          "text": "सर्वाँ॒स्ताँ इ᳚न्द्र गच्छसि ॥",
          "begin": 6.176,
          "end": 9.92
        }
      ]
    },
    {
      "id": "8.93.7",
      "mandala": 8,
      "sukta": 93,
      "rik": 7,
      "padas": [
        {
          "text": "तमिन्द्रं᳚ वाजयामसि म॒हे वृ॒त्राय॒ हन्त᳚वे ।",
          "begin": 0.0,
          "end": 5.291
        },
        {
          "text": "स वृषा᳚ वृष॒भो भु᳚वत् ॥",
          "begin": 5.291,
          "end": 7.96
        }
      ]
    },
    {
      "id": "8.93.8",
      "mandala": 8,
      "sukta": 93,
      "rik": 8,
      "padas": [
        {
          "text": "इन्द्र॒ः स दाम᳚ने कृ॒त ओजि᳚ष्ठ॒ः स मदे᳚ हि॒तः ।",
          "begin": 0.0,
          "end": 5.437
        },
        {
          "text": "द्यु॒म्नी श्लो॒की स सो॒म्यः ॥",
          "begin": 5.437,
          "end": 8.4
        }
      ]
    },
    {
      "id": "8.93.9",
      "mandala": 8,
      "sukta": 93,
      "rik": 9,
      "padas": [
        {
          "text": "गि॒रा वज्रो॒ न सम्भृ᳚त॒ः सब᳚लो॒ अन᳚पच्युतः ।",
          "begin": 0.0,
          "end": 5.408
        },
        {
          "text": "व॒व॒क्ष ऋ॒ष्वो अस्तृ᳚तः ॥",
          "begin": 5.408,
          "end": 8.24
        }
      ]
    },
    {
      "id": "8.93.10",
      "mandala": 8,
      "sukta": 93,
      "rik": 10,
      "padas": [
        {
          "text": "दु॒र्गे चि᳚न्नः सु॒गं कृ᳚धि गृणा॒न इ᳚न्द्र गिर्वणः ।",
          "begin": 0.0,
          "end": 5.66
        },
        {
          "text": "त्वं च᳚ मघव॒न्वशः᳚ ॥",
          "begin": 5.66,
          "end": 8.24
        }
      ]
    },
    {
      "id": "8.93.11",
      "mandala": 8,
      "sukta": 93,
      "rik": 11,
      "padas": [
        {
          "text": "यस्य᳚ ते॒ नू चि᳚दा॒दिशं॒ न मि॒नन्ति᳚ स्व॒राज्य᳚म् ।",
          "begin": 0.0,
          "end": 4.451
        },
        {
          "text": "न दे॒वो नाध्रि᳚गु॒र्जनः᳚ ॥",
          "begin": 4.451,
          "end": 8.0
        }
      ]
    },
    {
      "id": "8.93.12",
      "mandala": 8,
      "sukta": 93,
      "rik": 12,
      "padas": [
        {
          "text": "अधा᳚ ते॒ अप्र᳚तिष्कुतं दे॒वी शुष्मं᳚ सपर्यतः ।",
          "begin": 0.0,
          "end": 6.255
        },
        {
          "text": "उ॒भे सु᳚शिप्र॒ रोद᳚सी ॥",
          "begin": 6.255,
          "end": 8.52
        }
      ]
    },
    {
      "id": "8.93.13",
      "mandala": 8,
      "sukta": 93,
      "rik": 13,
      "padas": [
        {
          "text": "त्वमे॒तद᳚धारयः कृ॒ष्णासु॒ रोहि᳚णीषु च ।",
          "begin": 0.0,
          "end": 4.022
        },
        {
          "text": "परु᳚ष्णीषु॒ रुश॒त्पयः᳚ ॥",
          "begin": 4.022,
          "end": 6.88
        }
      ]
    },
    {
      "id": "8.93.14",
      "mandala": 8,
      "sukta": 93,
      "rik": 14,
      "padas": [
        {
          "text": "वि यदहे॒रध᳚ त्वि॒षो विश्वे᳚ दे॒वासो॒ अक्र᳚मुः ।",
          "begin": 0.0,
          "end": 5.888
        },
        {
          "text": "वि॒दन्मृ॒गस्य॒ ताँ अमः᳚ ॥",
          "begin": 5.888,
          "end": 8.68
        }
      ]
    },
    {
      "id": "8.93.15",
      "mandala": 8,
      "sukta": 93,
      "rik": 15,
      "padas": [
        {
          "text": "आदु᳚ मे निव॒रो भु᳚वद्वृत्र॒हादि᳚ष्ट॒ पौंस्य᳚म् ।",
          "begin": 0.0,
          "end": 4.889
        },
        {
          "text": "अजा᳚तशत्रु॒रस्तृ᳚तः ॥",
          "begin": 4.889,
          "end": 7.96
        }
      ]
    },
    {
      "id": "8.93.16",
      "mandala": 8,
      "sukta": 93,
      "rik": 16,
      "padas": [
        {
          "text": "श्रु॒तं वो᳚ वृत्र॒हन्त᳚मं॒ प्र शर्धं᳚ चर्षणी॒नाम् ।",
          "begin": 0.0,
          "end": 6.008
        },
        {
          "text": "आ शु᳚षे॒ राध᳚से म॒हे ॥",
          "begin": 6.008,
          "end": 8.4
        }
      ]
    },
    {
      "id": "8.93.17",
      "mandala": 8,
      "sukta": 93,
      "rik": 17,
      "padas": [
        {
          "text": "अ॒या धि॒या च᳚ गव्य॒या पुरु᳚णाम॒न्पुरु᳚ष्टुत ।",
          "begin": 0.0,
          "end": 4.275
        },
        {
          "text": "यत्सोमे᳚सोम॒ आभ᳚वः ॥",
          "begin": 4.275,
          "end": 7.72
        }
      ]
    },
    {
      "id": "8.93.18",
      "mandala": 8,
      "sukta": 93,
      "rik": 18,
      "padas": [
        {
          "text": "बो॒धिन्म᳚ना॒ इद᳚स्तु नो वृत्र॒हा भूर्या᳚सुतिः ।",
          "begin": 0.0,
          "end": 5.391
        },
        {
          "text": "शृ॒णोतु᳚ श॒क्र आ॒शिष᳚म् ॥",
          "begin": 5.391,
          "end": 8.4
        }
      ]
    },
    {
      "id": "8.93.19",
      "mandala": 8,
      "sukta": 93,
      "rik": 19,
      "padas": [
        {
          "text": "कया॒ त्वं न᳚ ऊ॒त्याभि प्र म᳚न्दसे वृषन् ।",
          "begin": 0.0,
          "end": 4.658
        },
        {
          "text": "कया᳚ स्तो॒तृभ्य॒ आ भ᳚र ॥",
          "begin": 4.658,
          "end": 7.96
        }
      ]
    },
    {
      "id": "8.93.20",
      "mandala": 8,
      "sukta": 93,
      "rik": 20,
      "padas": [
        {
          "text": "कस्य॒ वृषा᳚ सु॒ते सचा᳚ नि॒युत्वा᳚न्वृष॒भो र᳚णत् ।",
          "begin": 0.0,
          "end": 6.019
        },
        {
          "text": "वृ॒त्र॒हा सोम᳚पीतये ॥",
          "begin": 6.019,
          "end": 8.36
        }
      ]
    },
    {
      "id": "8.93.21",
      "mandala": 8,
      "sukta": 93,
      "rik": 21,
      "padas": [
        {
          "text": "अ॒भी षु ण॒स्त्वं र॒यिं म᳚न्दसा॒नः स᳚ह॒स्रिण᳚म् ।",
          "begin": 0.0,
          "end": 4.573
        },
        {
          "text": "प्र॒य॒न्ता बो᳚धि दा॒शुषे᳚ ॥",
          "begin": 4.573,
          "end": 8.36
        }
      ]
    },
    {
      "id": "8.93.22",
      "mandala": 8,
      "sukta": 93,
      "rik": 22,
      "padas": [
        {
          "text": "पत्नी᳚वन्तः सु॒ता इ॒म उ॒शन्तो᳚ यन्ति वी॒तये᳚ ।",
          "begin": 0.0,
          "end": 6.045
        },
        {
          "text": "अ॒पां जग्मि᳚र्निचुम्पु॒णः ॥",
          "begin": 6.045,
          "end": 9.4
        }
      ]
    },
    {
      "id": "8.93.23",
      "mandala": 8,
      "sukta": 93,
      "rik": 23,
      "padas": [
        {
          "text": "इ॒ष्टा होत्रा᳚ असृक्ष॒तेन्द्रं᳚ वृ॒धासो᳚ अध्व॒रे ।",
          "begin": 0.0,
          "end": 6.271
        },
        {
          "text": "अच्छा᳚वभृ॒थमोज᳚सा ॥",
          "begin": 6.271,
          "end": 9.08
        }
      ]
    },
    {
      "id": "8.93.24",
      "mandala": 8,
      "sukta": 93,
      "rik": 24,
      "padas": [
        {
          "text": "इ॒ह त्या स᳚ध॒माद्या॒ हरी॒ हिर᳚ण्यकेश्या ।",
          "begin": 0.0,
          "end": 4.857
        },
        {
          "text": "वो॒ळ्हाम॒भि प्रयो᳚ हि॒तम् ॥",
          "begin": 4.857,
          "end": 7.72
        }
      ]
    },
    {
      "id": "8.93.25",
      "mandala": 8,
      "sukta": 93,
      "rik": 25,
      "padas": [
        {
          "text": "तुभ्यं॒ सोमा᳚ः सु॒ता इ॒मे स्ती॒र्णं ब॒र्हिर्वि᳚भावसो ।",
          "begin": 0.0,
          "end": 5.983
        },
        {
          "text": "स्तो॒तृभ्य॒ इन्द्र॒मा व᳚ह ॥",
          "begin": 5.983,
          "end": 8.36
        }
      ]
    },
    {
      "id": "8.93.26",
      "mandala": 8,
      "sukta": 93,
      "rik": 26,
      "padas": [
        {
          "text": "आ ते॒ दक्षं॒ वि रो᳚च॒ना दध॒द्रत्ना॒ वि दा॒शुषे᳚ ।",
          "begin": 0.0,
          "end": 4.84
        },
        {
          "text": "स्तो॒तृभ्य॒ इन्द्र᳚मर्चत ॥",
          "begin": 4.84,
          "end": 7.96
        }
      ]
    },
    {
      "id": "8.93.27",
      "mandala": 8,
      "sukta": 93,
      "rik": 27,
      "padas": [
        {
          "text": "आ ते᳚ दधामीन्द्रि॒यमु॒क्था विश्वा᳚ शतक्रतो ।",
          "begin": 0.0,
          "end": 5.441
        },
        {
          "text": "स्तो॒तृभ्य᳚ इन्द्र मृळय ॥",
          "begin": 5.441,
          "end": 7.8
        }
      ]
    },
    {
      "id": "8.93.28",
      "mandala": 8,
      "sukta": 93,
      "rik": 28,
      "padas": [
        {
          "text": "भ॒द्रम्भ᳚द्रं न॒ आ भ॒रेष॒मूर्जं᳚ शतक्रतो ।",
          "begin": 0.0,
          "end": 5.286
        },
        {
          "text": "यदि᳚न्द्र मृ॒ळया᳚सि नः ॥",
          "begin": 5.286,
          "end": 7.8
        }
      ]
    },
    {
      "id": "8.93.29",
      "mandala": 8,
      "sukta": 93,
      "rik": 29,
      "padas": [
        {
          "text": "स नो॒ विश्वा॒न्या भ᳚र सुवि॒तानि᳚ शतक्रतो ।",
          "begin": 0.0,
          "end": 4.547
        },
        {
          "text": "यदि᳚न्द्र मृ॒ळया᳚सि नः ॥",
          "begin": 4.547,
          "end": 7.2
        }
      ]
    },
    {
      "id": "8.93.30",
      "mandala": 8,
      "sukta": 93,
      "rik": 30,
      "padas": [
        {
          "text": "त्वामिद्वृ᳚त्रहन्तम सु॒ताव᳚न्तो हवामहे ।",
          "begin": 0.0,
          "end": 4.466
        },
        {
          "text": "यदि᳚न्द्र मृ॒ळया᳚सि नः ॥",
          "begin": 4.466,
          "end": 7.36
        }
      ]
    },
    {
      "id": "8.93.31",
      "mandala": 8,
      "sukta": 93,
      "rik": 31,
      "padas": [
        {
          "text": "उप᳚ नो॒ हरि᳚भिः सु॒तं या॒हि म᳚दानां पते ।",
          "begin": 0.0,
          "end": 3.961
        },
        {
          "text": "उप᳚ नो॒ हरि᳚भिः सु॒तम् ॥",
          "begin": 3.961,
          "end": 6.16
        }
      ]
    },
    {
      "id": "8.93.32",
      "mandala": 8,
      "sukta": 93,
      "rik": 32,
      "padas": [
        {
          "text": "द्वि॒ता यो वृ᳚त्र॒हन्त᳚मो वि॒द इन्द्र᳚ः श॒तक्र᳚तुः ।",
          "begin": 0.0,
          "end": 4.359
        },
        {
          "text": "उप᳚ नो॒ हरि᳚भिः सु॒तम् ॥",
          "begin": 4.359,
          "end": 6.2
        }
      ]
    },
    {
      "id": "8.93.33",
      "mandala": 8,
      "sukta": 93,
      "rik": 33,
      "padas": [
        {
          "text": "त्वं हि वृ᳚त्रहन्नेषां पा॒ता सोमा᳚ना॒मसि᳚ ।",
          "begin": 0.0,
          "end": 4.733
        },
        {
          "text": "उप᳚ नो॒ हरि᳚भिः सु॒तम् ॥",
          "begin": 4.733,
          "end": 6.88
        }
      ]
    },
    {
      "id": "8.93.34",
      "mandala": 8,
      "sukta": 93,
      "rik": 34,
      "padas": [
        {
          "text": "इन्द्र᳚ इ॒षे द᳚दातु न ऋभु॒क्षण᳚मृ॒भुं र॒यिम् ।",
          "begin": 0.0,
          "end": 4.359
        },
        {
          "text": "वा॒जी द᳚दातु वा॒जिन᳚म् ॥",
          "begin": 4.359,
          "end": 7.8
        }
      ]
    }
  ]
}