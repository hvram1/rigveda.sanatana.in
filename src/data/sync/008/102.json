{
  "mandala": 8,
  "sukta": 102,
  "verses": [
    {
      "id": "8.102.1",
      "mandala": 8,
      "sukta": 102,
      "rik": 1,
      "padas": [
        {
          "text": "त्वम᳚ग्ने बृ॒हद्वयो॒ दधा᳚सि देव दा॒शुषे᳚ ।",
          "begin": 0.0,
          "end": 9.315
        },
        {
          "text": "क॒विर्गृ॒हप᳚ति॒र्युवा᳚ ॥",
          "begin": 9.315,
          "end": 12.92
        }
      ]
    },
    {
      "id": "8.102.2",
      "mandala": 8,
      "sukta": 102,
      "rik": 2,
      "padas": [
        {
          "text": "स न॒ ईळा᳚नया स॒ह दे॒वाँ अ᳚ग्ने दुव॒स्युवा᳚ ।",
          "begin": 0.0,
          "end": 5.261
        },
        {
          "text": "चि॒किद्वि᳚भान॒वा व᳚ह ॥",
          "begin": 5.261,
          "end": 7.92
        }
      ]
    },
    {
      "id": "8.102.3",
      "mandala": 8,
      "sukta": 102,
      "rik": 3,
      "padas": [
        {
          "text": "त्वया᳚ ह स्विद्यु॒जा व॒यं चोदि᳚ष्ठेन यविष्ठ्य ।",
          "begin": 0.0,
          "end": 4.545
        },
        {
          "text": "अ॒भि ष्मो॒ वाज᳚सातये ॥",
          "begin": 4.545,
          "end": 7.44
        }
      ]
    },
    {
      "id": "8.102.4",
      "mandala": 8,
      "sukta": 102,
      "rik": 4,
      "padas": [
        {
          "text": "औ॒र्व॒भृ॒गु॒वच्छुचि᳚मप्नवान॒वदा हु᳚वे ।",
          "begin": 0.0,
          "end": 4.946
        },
        {
          "text": "अ॒ग्निं स᳚मु॒द्रवा᳚ससम् ॥",
          "begin": 4.946,
          "end": 7.64
        }
      ]
    },
    {
      "id": "8.102.5",
      "mandala": 8,
      "sukta": 102,
      "rik": 5,
      "padas": [
        {
          "text": "हु॒वे वात᳚स्वनं क॒विं प॒र्जन्य᳚क्रन्द्यं॒ सहः᳚ ।",
          "begin": 0.0,
          "end": 5.287
        },
        {
          "text": "अ॒ग्निं स᳚मु॒द्रवा᳚ससम् ॥",
          "begin": 5.287,
          "end": 7.92
        }
      ]
    },
    {
      "id": "8.102.6",
      "mandala": 8,
      "sukta": 102,
      "rik": 6,
      "padas": [
        {
          "text": "आ स॒वं स᳚वि॒तुर्य᳚था॒ भग᳚स्येव भु॒जिं हु᳚वे ।",
          "begin": 0.0,
          "end": 4.777
        },
        {
          "text": "अ॒ग्निं स᳚मु॒द्रवा᳚ससम् ॥",
          "begin": 4.777,
          "end": 7.6
        }
      ]
    },
    {
      "id": "8.102.7",
      "mandala": 8,
      "sukta": 102,
      "rik": 7,
      "padas": [
        {
          "text": "अ॒ग्निं वो᳚ वृ॒धन्त᳚मध्व॒राणां᳚ पुरू॒तम᳚म् ।",
          "begin": 0.0,
          "end": 5.724
        },
        {
          "text": "अच्छा॒ नप्त्रे॒ सह᳚स्वते ॥",
          "begin": 5.724,
          "end": 8.2
        }
      ]
    },
    {
      "id": "8.102.8",
      "mandala": 8,
      "sukta": 102,
      "rik": 8,
      "padas": [
        {
          "text": "अ॒यं यथा᳚ न आ॒भुव॒त्त्वष्टा᳚ रू॒पेव॒ तक्ष्या᳚ ।",
          "begin": 0.0,
          "end": 5.224
        },
        {
          "text": "अ॒स्य क्रत्वा॒ यश᳚स्वतः ॥",
          "begin": 5.224,
          "end": 8.48
        }
      ]
    },
    {
      "id": "8.102.9",
      "mandala": 8,
      "sukta": 102,
      "rik": 9,
      "padas": [
        {
          "text": "अ॒यं विश्वा᳚ अ॒भि श्रियो॒॑ग्निर्दे॒वेषु᳚ पत्यते ।",
          "begin": 0.0,
          "end": 5.688
        },
        {
          "text": "आ वाजै॒रुप᳚ नो गमत् ॥",
          "begin": 5.688,
          "end": 8.12
        }
      ]
    },
    {
      "id": "8.102.10",
      "mandala": 8,
      "sukta": 102,
      "rik": 10,
      "padas": [
        {
          "text": "विश्वे᳚षामि॒ह स्तु᳚हि॒ होतॄ᳚णां य॒शस्त᳚मम् ।",
          "begin": 0.0,
          "end": 4.934
        },
        {
          "text": "अ॒ग्निं य॒ज्ञेषु᳚ पू॒र्व्यम् ॥",
          "begin": 4.934,
          "end": 8.24
        }
      ]
    },
    {
      "id": "8.102.11",
      "mandala": 8,
      "sukta": 102,
      "rik": 11,
      "padas": [
        {
          "text": "शी॒रं पा᳚व॒कशो᳚चिषं॒ ज्येष्ठो॒ यो दमे॒ष्वा ।",
          "begin": 0.0,
          "end": 6.179
        },
        {
          "text": "दी॒दाय᳚ दीर्घ॒श्रुत्त᳚मः ॥",
          "begin": 6.179,
          "end": 9.0
        }
      ]
    },
    {
      "id": "8.102.12",
      "mandala": 8,
      "sukta": 102,
      "rik": 12,
      "padas": [
        {
          "text": "तमर्व᳚न्तं॒ न सा᳚न॒सिं गृ᳚णी॒हि वि᳚प्र शु॒ष्मिण᳚म् ।",
          "begin": 0.0,
          "end": 5.402
        },
        {
          "text": "मि॒त्रं न या᳚त॒यज्ज᳚नम् ॥",
          "begin": 5.402,
          "end": 8.56
        }
      ]
    },
    {
      "id": "8.102.13",
      "mandala": 8,
      "sukta": 102,
      "rik": 13,
      "padas": [
        {
          "text": "उप᳚ त्वा जा॒मयो॒ गिरो॒ देदि᳚शतीर्हवि॒ष्कृतः᳚ ।",
          "begin": 0.0,
          "end": 5.066
        },
        {
          "text": "वा॒योरनी᳚के अस्थिरन् ॥",
          "begin": 5.066,
          "end": 8.04
        }
      ]
    },
    {
      "id": "8.102.14",
      "mandala": 8,
      "sukta": 102,
      "rik": 14,
      "padas": [
        {
          "text": "यस्य᳚ त्रि॒धात्ववृ᳚तं ब॒र्हिस्त॒स्थावसं᳚दिनम् ।",
          "begin": 0.0,
          "end": 5.421
        },
        {
          "text": "आप᳚श्चि॒न्नि द᳚धा प॒दम् ॥",
          "begin": 5.421,
          "end": 7.52
        }
      ]
    },
    {
      "id": "8.102.15",
      "mandala": 8,
      "sukta": 102,
      "rik": 15,
      "padas": [
        {
          "text": "प॒दं दे॒वस्य᳚ मी॒ळ्हुषो॑ना᳚धृष्टाभिरू॒तिभि᳚ः ।",
          "begin": 0.0,
          "end": 5.326
        },
        {
          "text": "भ॒द्रा सूर्य᳚ इवोप॒दृक् ॥",
          "begin": 5.326,
          "end": 7.88
        }
      ]
    },
    {
      "id": "8.102.16",
      "mandala": 8,
      "sukta": 102,
      "rik": 16,
      "padas": [
        {
          "text": "अग्ने᳚ घृ॒तस्य᳚ धी॒तिभि᳚स्तेपा॒नो दे᳚व शो॒चिषा᳚ ।",
          "begin": 0.0,
          "end": 5.983
        },
        {
          "text": "आ दे॒वान्व᳚क्षि॒ यक्षि᳚ च ॥",
          "begin": 5.983,
          "end": 8.6
        }
      ]
    },
    {
      "id": "8.102.17",
      "mandala": 8,
      "sukta": 102,
      "rik": 17,
      "padas": [
        {
          "text": "तं त्वा᳚जनन्त मा॒तरः᳚ क॒विं दे॒वासो᳚ अङ्गिरः ।",
          "begin": 0.0,
          "end": 5.436
        },
        {
          "text": "ह॒व्य॒वाह॒मम᳚र्त्यम् ॥",
          "begin": 5.436,
          "end": 8.16
        }
      ]
    },
    {
      "id": "8.102.18",
      "mandala": 8,
      "sukta": 102,
      "rik": 18,
      "padas": [
        {
          "text": "प्रचे᳚तसं त्वा क॒वे॑ग्ने᳚ दू॒तं वरे᳚ण्यम् ।",
          "begin": 0.0,
          "end": 5.742
        },
        {
          "text": "ह॒व्य॒वाहं॒ नि षे᳚दिरे ॥",
          "begin": 5.742,
          "end": 8.4
        }
      ]
    },
    {
      "id": "8.102.19",
      "mandala": 8,
      "sukta": 102,
      "rik": 19,
      "padas": [
        {
          "text": "न॒हि मे॒ अस्त्यघ्न्या॒ न स्वधि᳚ति॒र्वन᳚न्वति ।",
          "begin": 0.0,
          "end": 4.141
        },
        {
          "text": "अथै᳚ता॒दृग्भ᳚रामि ते ॥",
          "begin": 4.141,
          "end": 7.84
        }
      ]
    },
    {
      "id": "8.102.20",
      "mandala": 8,
      "sukta": 102,
      "rik": 20,
      "padas": [
        {
          "text": "यद᳚ग्ने॒ कानि॒ कानि᳚ चि॒दा ते॒ दारू᳚णि द॒ध्मसि᳚ ।",
          "begin": 0.0,
          "end": 4.865
        },
        {
          "text": "ता जु᳚षस्व यविष्ठ्य ॥",
          "begin": 4.865,
          "end": 7.44
        }
      ]
    },
    {
      "id": "8.102.21",
      "mandala": 8,
      "sukta": 102,
      "rik": 21,
      "padas": [
        {
          "text": "यदत्त्यु᳚प॒जिह्वि᳚का॒ यद्व॒म्रो अ᳚ति॒सर्प᳚ति ।",
          "begin": 0.0,
          "end": 4.723
        },
        {
          "text": "सर्वं॒ तद᳚स्तु ते घृ॒तम् ॥",
          "begin": 4.723,
          "end": 7.16
        }
      ]
    },
    {
      "id": "8.102.22",
      "mandala": 8,
      "sukta": 102,
      "rik": 22,
      "padas": [
        {
          "text": "अ॒ग्निमिन्धा᳚नो॒ मन᳚सा॒ धियं᳚ सचेत॒ मर्त्य᳚ः ।",
          "begin": 0.0,
          "end": 6.07
        },
        {
          "text": "अ॒ग्निमी᳚धे वि॒वस्व᳚भिः ॥",
          "begin": 6.07,
          "end": 10.36
        }
      ]
    }
  ]
}