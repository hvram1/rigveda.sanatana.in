{
  "mandala": 8,
  "sukta": 8,
  "verses": [
    {
      "id": "8.8.1",
      "mandala": 8,
      "sukta": 8,
      "rik": 1,
      "padas": [
        {
          "text": "आ नो॒ विश्वा᳚भिरू॒तिभि॒रश्वि᳚ना॒ गच्छ᳚तं यु॒वम् ।",
          "begin": 0.0,
          "end": 8.786
        },
        {
          "text": "दस्रा॒ हिर᳚ण्यवर्तनी॒ पिब᳚तं सो॒म्यं मधु᳚ ॥",
          "begin": 8.786,
          "end": 14.08
        }
      ]
    },
    {
      "id": "8.8.2",
      "mandala": 8,
      "sukta": 8,
      "rik": 2,
      "padas": [
        {
          "text": "आ नू॒नं या᳚तमश्विना॒ रथे᳚न॒ सूर्य᳚त्वचा ।",
          "begin": 0.0,
          "end": 6.622
        },
        {
          "text": "भुजी॒ हिर᳚ण्यपेशसा॒ कवी॒ गम्भी᳚रचेतसा ॥",
          "begin": 6.622,
          "end": 11.96
        }
      ]
    },
    {
      "id": "8.8.3",
      "mandala": 8,
      "sukta": 8,
      "rik": 3,
      "padas": [
        {
          "text": "आ या᳚तं॒ नहु᳚ष॒स्पर्यान्तरि᳚क्षात्सुवृ॒क्तिभि᳚ः ।",
          "begin": 0.0,
          "end": 6.373
        },
        {
          "text": "पिबा᳚थो अश्विना॒ मधु॒ कण्वा᳚नां॒ सव᳚ने सु॒तम् ॥",
          "begin": 6.373,
          "end": 12.56
        }
      ]
    },
    {
      "id": "8.8.4",
      "mandala": 8,
      "sukta": 8,
      "rik": 4,
      "padas": [
        {
          "text": "आ नो᳚ यातं दि॒वस्पर्यान्तरि᳚क्षादधप्रिया ।",
          "begin": 0.0,
          "end": 5.749
        },
        {
          "text": "पु॒त्रः कण्व᳚स्य वामि॒ह सु॒षाव᳚ सो॒म्यं मधु᳚ ॥",
          "begin": 5.749,
          "end": 10.8
        }
      ]
    },
    {
      "id": "8.8.5",
      "mandala": 8,
      "sukta": 8,
      "rik": 5,
      "padas": [
        {
          "text": "आ नो᳚ यात॒मुप᳚श्रु॒त्यश्वि᳚ना॒ सोम᳚पीतये ।",
          "begin": 0.0,
          "end": 5.806
        },
        {
          "text": "स्वाहा॒ स्तोम᳚स्य वर्धना॒ प्र क᳚वी धी॒तिभि᳚र्नरा ॥",
          "begin": 5.806,
          "end": 11.76
        }
      ]
    },
    {
      "id": "8.8.6",
      "mandala": 8,
      "sukta": 8,
      "rik": 6,
      "padas": [
        {
          "text": "यच्चि॒द्धि वां᳚ पु॒र ऋष᳚यो जुहू॒रे॑व᳚से नरा ।",
          "begin": 0.0,
          "end": 5.965
        },
        {
          "text": "आ या᳚तमश्वि॒ना ग᳚त॒मुपे॒मां सु᳚ष्टु॒तिं मम᳚ ॥",
          "begin": 5.965,
          "end": 10.72
        }
      ]
    },
    {
      "id": "8.8.7",
      "mandala": 8,
      "sukta": 8,
      "rik": 7,
      "padas": [
        {
          "text": "दि॒वश्चि᳚द्रोच॒नादध्या नो᳚ गन्तं स्वर्विदा ।",
          "begin": 0.0,
          "end": 6.05
        },
        {
          "text": "धी॒भिर्व᳚त्सप्रचेतसा॒ स्तोमे᳚भिर्हवनश्रुता ॥",
          "begin": 6.05,
          "end": 11.28
        }
      ]
    },
    {
      "id": "8.8.8",
      "mandala": 8,
      "sukta": 8,
      "rik": 8,
      "padas": [
        {
          "text": "किम॒न्ये पर्या᳚सते॒॑स्मत्स्तोमे᳚भिर॒श्विना᳚ ।",
          "begin": 0.0,
          "end": 5.992
        },
        {
          "text": "पु॒त्रः कण्व᳚स्य वा॒मृषि᳚र्गी॒र्भिर्व॒त्सो अ᳚वीवृधत् ॥",
          "begin": 5.992,
          "end": 12.2
        }
      ]
    },
    {
      "id": "8.8.9",
      "mandala": 8,
      "sukta": 8,
      "rik": 9,
      "padas": [
        {
          "text": "आ वां॒ विप्र᳚ इ॒हाव॒से॑ह्व॒त्स्तोमे᳚भिरश्विना ।",
          "begin": 0.0,
          "end": 6.104
        },
        {
          "text": "अरि᳚प्रा॒ वृत्र᳚हन्तमा॒ ता नो᳚ भूतं मयो॒भुवा᳚ ॥",
          "begin": 6.104,
          "end": 12.32
        }
      ]
    },
    {
      "id": "8.8.10",
      "mandala": 8,
      "sukta": 8,
      "rik": 10,
      "padas": [
        {
          "text": "आ यद्वां॒ योष᳚णा॒ रथ॒मति᳚ष्ठद्वाजिनीवसू ।",
          "begin": 0.0,
          "end": 5.788
        },
        {
          "text": "विश्वा᳚न्यश्विना यु॒वं प्र धी॒तान्य᳚गच्छतम् ॥",
          "begin": 5.788,
          "end": 10.84
        }
      ]
    },
    {
      "id": "8.8.11",
      "mandala": 8,
      "sukta": 8,
      "rik": 11,
      "padas": [
        {
          "text": "अतः᳚ स॒हस्र᳚निर्णिजा॒ रथे॒ना या᳚तमश्विना ।",
          "begin": 0.0,
          "end": 5.721
        },
        {
          "text": "व॒त्सो वां॒ मधु᳚म॒द्वचो॑शं᳚सीत्का॒व्यः क॒विः ॥",
          "begin": 5.721,
          "end": 11.64
        }
      ]
    },
    {
      "id": "8.8.12",
      "mandala": 8,
      "sukta": 8,
      "rik": 12,
      "padas": [
        {
          "text": "पु॒रु॒म॒न्द्रा पु᳚रू॒वसू᳚ मनो॒तरा᳚ रयी॒णाम् ।",
          "begin": 0.0,
          "end": 6.947
        },
        {
          "text": "स्तोमं᳚ मे अ॒श्विना᳚वि॒मम॒भि वह्नी᳚ अनूषाताम् ॥",
          "begin": 6.947,
          "end": 12.88
        }
      ]
    },
    {
      "id": "8.8.13",
      "mandala": 8,
      "sukta": 8,
      "rik": 13,
      "padas": [
        {
          "text": "आ नो॒ विश्वा᳚न्यश्विना ध॒त्तं राधां॒स्यह्र᳚या ।",
          "begin": 0.0,
          "end": 6.583
        },
        {
          "text": "कृ॒तं न᳚ ऋ॒त्विया᳚वतो॒ मा नो᳚ रीरधतं नि॒दे ॥",
          "begin": 6.583,
          "end": 12.08
        }
      ]
    },
    {
      "id": "8.8.14",
      "mandala": 8,
      "sukta": 8,
      "rik": 14,
      "padas": [
        {
          "text": "यन्ना᳚सत्या परा॒वति॒ यद्वा॒ स्थो अध्यम्ब᳚रे ।",
          "begin": 0.0,
          "end": 6.94
        },
        {
          "text": "अतः᳚ स॒हस्र᳚निर्णिजा॒ रथे॒ना या᳚तमश्विना ॥",
          "begin": 6.94,
          "end": 11.96
        }
      ]
    },
    {
      "id": "8.8.15",
      "mandala": 8,
      "sukta": 8,
      "rik": 15,
      "padas": [
        {
          "text": "यो वां᳚ नासत्या॒वृषि᳚र्गी॒र्भिर्व॒त्सो अवी᳚वृधत् ।",
          "begin": 0.0,
          "end": 6.623
        },
        {
          "text": "तस्मै᳚ स॒हस्र᳚निर्णिज॒मिषं᳚ धत्तं घृत॒श्चुत᳚म् ॥",
          "begin": 6.623,
          "end": 13.72
        }
      ]
    },
    {
      "id": "8.8.16",
      "mandala": 8,
      "sukta": 8,
      "rik": 16,
      "padas": [
        {
          "text": "प्रास्मा॒ ऊर्जं᳚ घृत॒श्चुत॒मश्वि᳚ना॒ यच्छ᳚तं यु॒वम् ।",
          "begin": 0.0,
          "end": 6.779
        },
        {
          "text": "यो वां᳚ सु॒म्नाय᳚ तु॒ष्टव᳚द्वसू॒याद्दा᳚नुनस्पती ॥",
          "begin": 6.779,
          "end": 13.92
        }
      ]
    },
    {
      "id": "8.8.17",
      "mandala": 8,
      "sukta": 8,
      "rik": 17,
      "padas": [
        {
          "text": "आ नो᳚ गन्तं रिशादसे॒मं स्तोमं᳚ पुरुभुजा ।",
          "begin": 0.0,
          "end": 6.452
        },
        {
          "text": "कृ॒तं नः᳚ सु॒श्रियो᳚ नरे॒मा दा᳚तम॒भिष्ट᳚ये ॥",
          "begin": 6.452,
          "end": 12.64
        }
      ]
    },
    {
      "id": "8.8.18",
      "mandala": 8,
      "sukta": 8,
      "rik": 18,
      "padas": [
        {
          "text": "आ वां॒ विश्वा᳚भिरू॒तिभि᳚ः प्रि॒यमे᳚धा अहूषत ।",
          "begin": 0.0,
          "end": 6.096
        },
        {
          "text": "राज᳚न्तावध्व॒राणा॒मश्वि᳚ना॒ याम᳚हूतिषु ॥",
          "begin": 6.096,
          "end": 12.64
        }
      ]
    },
    {
      "id": "8.8.19",
      "mandala": 8,
      "sukta": 8,
      "rik": 19,
      "padas": [
        {
          "text": "आ नो᳚ गन्तं मयो॒भुवाश्वि᳚ना श॒म्भुवा᳚ यु॒वम् ।",
          "begin": 0.0,
          "end": 6.403
        },
        {
          "text": "यो वां᳚ विपन्यू धी॒तिभि᳚र्गी॒र्भिर्व॒त्सो अवी᳚वृधत् ॥",
          "begin": 6.403,
          "end": 13.68
        }
      ]
    },
    {
      "id": "8.8.20",
      "mandala": 8,
      "sukta": 8,
      "rik": 20,
      "padas": [
        {
          "text": "याभि॒ः कण्वं॒ मेधा᳚तिथिं॒ याभि॒र्वशं॒ दश᳚व्रजम् ।",
          "begin": 0.0,
          "end": 7.13
        },
        {
          "text": "याभि॒र्गोश᳚र्य॒माव᳚तं॒ ताभि᳚र्नो॑वतं नरा ॥",
          "begin": 7.13,
          "end": 12.64
        }
      ]
    },
    {
      "id": "8.8.21",
      "mandala": 8,
      "sukta": 8,
      "rik": 21,
      "padas": [
        {
          "text": "याभि᳚र्नरा त्र॒सद᳚स्यु॒माव᳚तं॒ कृत्व्ये॒ धने᳚ ।",
          "begin": 0.0,
          "end": 5.635
        },
        {
          "text": "ताभि॒ः ष्व१॒᳚स्माँ अ᳚श्विना॒ प्राव᳚तं॒ वाज᳚सातये ॥",
          "begin": 5.635,
          "end": 12.2
        }
      ]
    },
    {
      "id": "8.8.22",
      "mandala": 8,
      "sukta": 8,
      "rik": 22,
      "padas": [
        {
          "text": "प्र वां॒ स्तोमा᳚ः सुवृ॒क्तयो॒ गिरो᳚ वर्धन्त्वश्विना ।",
          "begin": 0.0,
          "end": 6.172
        },
        {
          "text": "पुरु᳚त्रा॒ वृत्र᳚हन्तमा॒ ता नो᳚ भूतं पुरु॒स्पृहा᳚ ॥",
          "begin": 6.172,
          "end": 12.12
        }
      ]
    },
    {
      "id": "8.8.23",
      "mandala": 8,
      "sukta": 8,
      "rik": 23,
      "padas": [
        {
          "text": "त्रीणि᳚ प॒दान्य॒श्विनो᳚रा॒विः सान्ति॒ गुहा᳚ प॒रः ।",
          "begin": 0.0,
          "end": 7.304
        },
        {
          "text": "क॒वी ऋ॒तस्य॒ पत्म᳚भिर॒र्वाग्जी॒वेभ्य॒स्परि᳚ ॥",
          "begin": 7.304,
          "end": 15.4
        }
      ]
    }
  ]
}