{
  "mandala": 8,
  "sukta": 55,
  "verses": [
    {
      "id": "8.55.1",
      "mandala": 8,
      "sukta": 55,
      "rik": 1,
      "padas": [
        {
          "text": "भूरीदिन्द्र᳚स्य वी॒र्यं१॒᳚ व्यख्य᳚म॒भ्याय᳚ति ।",
          "begin": 0.0,
          "end": 6.01
        },
        {
          "text": "राध᳚स्ते दस्यवे वृक ॥",
          "begin": 6.01,
          "end": 9.92
        }
      ]
    },
    {
      "id": "8.55.2",
      "mandala": 8,
      "sukta": 55,
      "rik": 2,
      "padas": [
        {
          "text": "श॒तं श्वे॒तास᳚ उ॒क्षणो᳚ दि॒वि तारो॒ न रो᳚चन्ते ।",
          "begin": 0.0,
          "end": 5.868
        },
        {
          "text": "म॒ह्ना दिवं॒ न त᳚स्तभुः ॥",
          "begin": 5.868,
          "end": 8.24
        }
      ]
    },
    {
      "id": "8.55.3",
      "mandala": 8,
      "sukta": 55,
      "rik": 3,
      "padas": [
        {
          "text": "श॒तं वे॒णूञ्छ॒तं शुनः᳚ श॒तं चर्मा᳚णि म्ला॒तानि᳚ ।",
          "begin": 0.0,
          "end": 5.601
        },
        {
          "text": "श॒तं मे᳚ बल्बजस्तु॒का अरु᳚षीणां॒ चतु᳚ःशतम् ॥",
          "begin": 5.601,
          "end": 10.24
        }
      ]
    },
    {
      "id": "8.55.4",
      "mandala": 8,
      "sukta": 55,
      "rik": 4,
      "padas": [
        {
          "text": "सु॒दे॒वाः स्थ᳚ काण्वायना॒ वयो᳚वयो विच॒रन्त᳚ः ।",
          "begin": 0.0,
          "end": 5.952
        },
        {
          "text": "अश्वा᳚सो॒ न च᳚ङ्क्रमत ॥",
          "begin": 5.952,
          "end": 8.92
        }
      ]
    },
    {
      "id": "8.55.5",
      "mandala": 8,
      "sukta": 55,
      "rik": 5,
      "padas": [
        {
          "text": "आदित्सा॒प्तस्य᳚ चर्किर॒न्नानू᳚नस्य॒ महि॒ श्रवः᳚ ।",
          "begin": 0.0,
          "end": 6.275
        },
        {
          "text": "श्यावी᳚रतिध्व॒सन्प॒थश्चक्षु᳚षा च॒न सं॒नशे᳚ ॥",
          "begin": 6.275,
          "end": 12.56
        }
      ]
    }
  ]
}