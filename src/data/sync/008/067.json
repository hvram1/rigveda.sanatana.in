{
  "mandala": 8,
  "sukta": 67,
  "verses": [
    {
      "id": "8.67.1",
      "mandala": 8,
      "sukta": 67,
      "rik": 1,
      "padas": [
        {
          "text": "त्यान्नु क्ष॒त्रियाँ॒ अव᳚ आदि॒त्यान्या᳚चिषामहे ।",
          "begin": 0.0,
          "end": 8.716
        },
        {
          "text": "सु॒मृ॒ळी॒काँ अ॒भिष्ट᳚ये ॥",
          "begin": 8.716,
          "end": 11.56
        }
      ]
    },
    {
      "id": "8.67.2",
      "mandala": 8,
      "sukta": 67,
      "rik": 2,
      "padas": [
        {
          "text": "मि॒त्रो नो॒ अत्यं᳚ह॒तिं वरु᳚णः पर्षदर्य॒मा ।",
          "begin": 0.0,
          "end": 5.872
        },
        {
          "text": "आ॒दि॒त्यासो॒ यथा᳚ वि॒दुः ॥",
          "begin": 5.872,
          "end": 8.36
        }
      ]
    },
    {
      "id": "8.67.3",
      "mandala": 8,
      "sukta": 67,
      "rik": 3,
      "padas": [
        {
          "text": "तेषां॒ हि चि॒त्रमु॒क्थ्यं१॒᳚ वरू᳚थ॒मस्ति᳚ दा॒शुषे᳚ ।",
          "begin": 0.0,
          "end": 6.533
        },
        {
          "text": "आ॒दि॒त्याना᳚मरं॒कृते᳚ ॥",
          "begin": 6.533,
          "end": 9.4
        }
      ]
    },
    {
      "id": "8.67.4",
      "mandala": 8,
      "sukta": 67,
      "rik": 4,
      "padas": [
        {
          "text": "महि᳚ वो मह॒तामवो॒ वरु᳚ण॒ मित्रार्य᳚मन् ।",
          "begin": 0.0,
          "end": 4.814
        },
        {
          "text": "अवां॒स्या वृ᳚णीमहे ॥",
          "begin": 4.814,
          "end": 7.04
        }
      ]
    },
    {
      "id": "8.67.5",
      "mandala": 8,
      "sukta": 67,
      "rik": 5,
      "padas": [
        {
          "text": "जी॒वान्नो᳚ अ॒भि धे᳚त॒नादि᳚त्यासः पु॒रा हथा᳚त् ।",
          "begin": 0.0,
          "end": 6.157
        },
        {
          "text": "कद्ध᳚ स्थ हवनश्रुतः ॥",
          "begin": 6.157,
          "end": 8.92
        }
      ]
    },
    {
      "id": "8.67.6",
      "mandala": 8,
      "sukta": 67,
      "rik": 6,
      "padas": [
        {
          "text": "यद्व᳚ः श्रा॒न्ताय᳚ सुन्व॒ते वरू᳚थ॒मस्ति॒ यच्छ॒र्दिः ।",
          "begin": 0.0,
          "end": 5.936
        },
        {
          "text": "तेना᳚ नो॒ अधि᳚ वोचत ॥",
          "begin": 5.936,
          "end": 8.72
        }
      ]
    },
    {
      "id": "8.67.7",
      "mandala": 8,
      "sukta": 67,
      "rik": 7,
      "padas": [
        {
          "text": "अस्ति᳚ देवा अं॒होरु॒र्वस्ति॒ रत्न॒मना᳚गसः ।",
          "begin": 0.0,
          "end": 4.287
        },
        {
          "text": "आदि᳚त्या॒ अद्भु᳚तैनसः ॥",
          "begin": 4.287,
          "end": 8.36
        }
      ]
    },
    {
      "id": "8.67.8",
      "mandala": 8,
      "sukta": 67,
      "rik": 8,
      "padas": [
        {
          "text": "मा न॒ः सेतु᳚ः सिषेद॒यं म॒हे वृ᳚णक्तु न॒स्परि᳚ ।",
          "begin": 0.0,
          "end": 5.076
        },
        {
          "text": "इन्द्र॒ इद्धि श्रु॒तो व॒शी ॥",
          "begin": 5.076,
          "end": 7.44
        }
      ]
    },
    {
      "id": "8.67.9",
      "mandala": 8,
      "sukta": 67,
      "rik": 9,
      "padas": [
        {
          "text": "मा नो᳚ मृ॒चा रि᳚पू॒णां वृ᳚जि॒नाना᳚मविष्यवः ।",
          "begin": 0.0,
          "end": 6.011
        },
        {
          "text": "देवा᳚ अ॒भि प्र मृ᳚क्षत ॥",
          "begin": 6.011,
          "end": 8.72
        }
      ]
    },
    {
      "id": "8.67.10",
      "mandala": 8,
      "sukta": 67,
      "rik": 10,
      "padas": [
        {
          "text": "उ॒त त्वाम᳚दिते मह्य॒हं दे॒व्युप᳚ ब्रुवे ।",
          "begin": 0.0,
          "end": 4.301
        },
        {
          "text": "सु॒मृ॒ळी॒काम॒भिष्ट᳚ये ॥",
          "begin": 4.301,
          "end": 6.44
        }
      ]
    },
    {
      "id": "8.67.11",
      "mandala": 8,
      "sukta": 67,
      "rik": 11,
      "padas": [
        {
          "text": "पर्षि᳚ दी॒ने ग᳚भी॒र आँ उग्र᳚पुत्रे॒ जिघां᳚सतः ।",
          "begin": 0.0,
          "end": 5.92
        },
        {
          "text": "माकि᳚स्तो॒कस्य᳚ नो रिषत् ॥",
          "begin": 5.92,
          "end": 8.6
        }
      ]
    },
    {
      "id": "8.67.12",
      "mandala": 8,
      "sukta": 67,
      "rik": 12,
      "padas": [
        {
          "text": "अ॒ने॒हो न᳚ उरुव्रज॒ उरू᳚चि॒ वि प्रस᳚र्तवे ।",
          "begin": 0.0,
          "end": 5.462
        },
        {
          "text": "कृ॒धि तो॒काय᳚ जी॒वसे᳚ ॥",
          "begin": 5.462,
          "end": 7.6
        }
      ]
    },
    {
      "id": "8.67.13",
      "mandala": 8,
      "sukta": 67,
      "rik": 13,
      "padas": [
        {
          "text": "ये मू॒र्धानः᳚ क्षिती॒नामद᳚ब्धास॒ः स्वय᳚शसः ।",
          "begin": 0.0,
          "end": 5.581
        },
        {
          "text": "व्र॒ता रक्ष᳚न्ते अ॒द्रुहः᳚ ॥",
          "begin": 5.581,
          "end": 9.08
        }
      ]
    },
    {
      "id": "8.67.14",
      "mandala": 8,
      "sukta": 67,
      "rik": 14,
      "padas": [
        {
          "text": "ते न᳚ आ॒स्नो वृका᳚णा॒मादि᳚त्यासो मु॒मोच᳚त ।",
          "begin": 0.0,
          "end": 5.669
        },
        {
          "text": "स्ते॒नं ब॒द्धमि᳚वादिते ॥",
          "begin": 5.669,
          "end": 8.08
        }
      ]
    },
    {
      "id": "8.67.15",
      "mandala": 8,
      "sukta": 67,
      "rik": 15,
      "padas": [
        {
          "text": "अपो॒ षु ण᳚ इ॒यं शरु॒रादि᳚त्या॒ अप᳚ दुर्म॒तिः ।",
          "begin": 0.0,
          "end": 5.079
        },
        {
          "text": "अ॒स्मदे॒त्वज᳚घ्नुषी ॥",
          "begin": 5.079,
          "end": 7.2
        }
      ]
    },
    {
      "id": "8.67.16",
      "mandala": 8,
      "sukta": 67,
      "rik": 16,
      "padas": [
        {
          "text": "शश्व॒द्धि वः᳚ सुदानव॒ आदि᳚त्या ऊ॒तिभि᳚र्व॒यम् ।",
          "begin": 0.0,
          "end": 5.637
        },
        {
          "text": "पु॒रा नू॒नं बु᳚भु॒ज्महे᳚ ॥",
          "begin": 5.637,
          "end": 8.4
        }
      ]
    },
    {
      "id": "8.67.17",
      "mandala": 8,
      "sukta": 67,
      "rik": 17,
      "padas": [
        {
          "text": "शश्व᳚न्तं॒ हि प्र᳚चेतसः प्रति॒यन्तं᳚ चि॒देन᳚सः ।",
          "begin": 0.0,
          "end": 5.74
        },
        {
          "text": "देवा᳚ः कृणु॒थ जी॒वसे᳚ ॥",
          "begin": 5.74,
          "end": 8.96
        }
      ]
    },
    {
      "id": "8.67.18",
      "mandala": 8,
      "sukta": 67,
      "rik": 18,
      "padas": [
        {
          "text": "तत्सु नो॒ नव्यं॒ सन्य᳚स॒ आदि᳚त्या॒ यन्मुमो᳚चति ।",
          "begin": 0.0,
          "end": 5.47
        },
        {
          "text": "ब॒न्धाद्ब॒द्धमि᳚वादिते ॥",
          "begin": 5.47,
          "end": 7.96
        }
      ]
    },
    {
      "id": "8.67.19",
      "mandala": 8,
      "sukta": 67,
      "rik": 19,
      "padas": [
        {
          "text": "नास्माक᳚मस्ति॒ तत्तर॒ आदि᳚त्यासो अति॒ष्कदे᳚ ।",
          "begin": 0.0,
          "end": 5.144
        },
        {
          "text": "यू॒यम॒स्मभ्यं᳚ मृळत ॥",
          "begin": 5.144,
          "end": 8.8
        }
      ]
    },
    {
      "id": "8.67.20",
      "mandala": 8,
      "sukta": 67,
      "rik": 20,
      "padas": [
        {
          "text": "मा नो᳚ हे॒तिर्वि॒वस्व᳚त॒ आदि᳚त्याः कृ॒त्रिमा॒ शरु᳚ः ।",
          "begin": 0.0,
          "end": 6.31
        },
        {
          "text": "पु॒रा नु ज॒रसो᳚ वधीत् ॥",
          "begin": 6.31,
          "end": 9.08
        }
      ]
    },
    {
      "id": "8.67.21",
      "mandala": 8,
      "sukta": 67,
      "rik": 21,
      "padas": [
        {
          "text": "वि षु द्वेषो॒ व्यं᳚ह॒तिमादि᳚त्यासो॒ वि संहि᳚तम् ।",
          "begin": 0.0,
          "end": 5.547
        },
        {
          "text": "विष्व॒ग्वि वृ᳚हता॒ रपः᳚ ॥",
          "begin": 5.547,
          "end": 9.56
        }
      ]
    }
  ]
}