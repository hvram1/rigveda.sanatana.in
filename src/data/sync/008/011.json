{
  "mandala": 8,
  "sukta": 11,
  "verses": [
    {
      "id": "8.11.1",
      "mandala": 8,
      "sukta": 11,
      "rik": 1,
      "padas": [
        {
          "text": "त्वम᳚ग्ने व्रत॒पा अ᳚सि दे॒व आ मर्त्ये॒ष्वा ।",
          "begin": 0.0,
          "end": 9.92
        },
        {
          "text": "त्वं य॒ज्ञेष्वीड्य᳚ः ॥",
          "begin": 9.92,
          "end": 13.48
        }
      ]
    },
    {
      "id": "8.11.2",
      "mandala": 8,
      "sukta": 11,
      "rik": 2,
      "padas": [
        {
          "text": "त्वम᳚सि प्र॒शस्यो᳚ वि॒दथे᳚षु सहन्त्य ।",
          "begin": 0.0,
          "end": 4.937
        },
        {
          "text": "अग्ने᳚ र॒थीर᳚ध्व॒राणा᳚म् ॥",
          "begin": 4.937,
          "end": 9.4
        }
      ]
    },
    {
      "id": "8.11.3",
      "mandala": 8,
      "sukta": 11,
      "rik": 3,
      "padas": [
        {
          "text": "स त्वम॒स्मदप॒ द्विषो᳚ युयो॒धि जा᳚तवेदः ।",
          "begin": 0.0,
          "end": 4.509
        },
        {
          "text": "अदे᳚वीरग्ने॒ अरा᳚तीः ॥",
          "begin": 4.509,
          "end": 10.6
        }
      ]
    },
    {
      "id": "8.11.4",
      "mandala": 8,
      "sukta": 11,
      "rik": 4,
      "padas": [
        {
          "text": "अन्ति᳚ चि॒त्सन्त॒मह᳚ य॒ज्ञं मर्त᳚स्य रि॒पोः ।",
          "begin": 0.0,
          "end": 5.718
        },
        {
          "text": "नोप᳚ वेषि जातवेदः ॥",
          "begin": 5.718,
          "end": 8.48
        }
      ]
    },
    {
      "id": "8.11.5",
      "mandala": 8,
      "sukta": 11,
      "rik": 5,
      "padas": [
        {
          "text": "मर्ता॒ अम᳚र्त्यस्य ते॒ भूरि॒ नाम᳚ मनामहे ।",
          "begin": 0.0,
          "end": 6.074
        },
        {
          "text": "विप्रा᳚सो जा॒तवे᳚दसः ॥",
          "begin": 6.074,
          "end": 9.64
        }
      ]
    },
    {
      "id": "8.11.6",
      "mandala": 8,
      "sukta": 11,
      "rik": 6,
      "padas": [
        {
          "text": "विप्रं॒ विप्रा॒सो॑व᳚से दे॒वं मर्ता᳚स ऊ॒तये᳚ ।",
          "begin": 0.0,
          "end": 6.725
        },
        {
          "text": "अ॒ग्निं गी॒र्भिर्ह᳚वामहे ॥",
          "begin": 6.725,
          "end": 9.8
        }
      ]
    },
    {
      "id": "8.11.7",
      "mandala": 8,
      "sukta": 11,
      "rik": 7,
      "padas": [
        {
          "text": "आ ते᳚ व॒त्सो मनो᳚ यमत्पर॒माच्चि᳚त्स॒धस्था᳚त् ।",
          "begin": 0.0,
          "end": 6.573
        },
        {
          "text": "अग्ने॒ त्वांका᳚मया गि॒रा ॥",
          "begin": 6.573,
          "end": 11.48
        }
      ]
    },
    {
      "id": "8.11.8",
      "mandala": 8,
      "sukta": 11,
      "rik": 8,
      "padas": [
        {
          "text": "पु॒रु॒त्रा हि स॒दृङ्ङसि॒ विशो॒ विश्वा॒ अनु᳚ प्र॒भुः ।",
          "begin": 0.0,
          "end": 6.106
        },
        {
          "text": "स॒मत्सु᳚ त्वा हवामहे ॥",
          "begin": 6.106,
          "end": 8.8
        }
      ]
    },
    {
      "id": "8.11.9",
      "mandala": 8,
      "sukta": 11,
      "rik": 9,
      "padas": [
        {
          "text": "स॒मत्स्व॒ग्निमव᳚से वाज॒यन्तो᳚ हवामहे ।",
          "begin": 0.0,
          "end": 5.825
        },
        {
          "text": "वाजे᳚षु चि॒त्ररा᳚धसम् ॥",
          "begin": 5.825,
          "end": 10.0
        }
      ]
    },
    {
      "id": "8.11.10",
      "mandala": 8,
      "sukta": 11,
      "rik": 10,
      "padas": [
        {
          "text": "प्र॒त्नो हि क॒मीड्यो᳚ अध्व॒रेषु᳚ स॒नाच्च॒ होता॒ नव्य᳚श्च॒ सत्सि᳚ ।",
          "begin": 0.0,
          "end": 9.378
        },
        {
          "text": "स्वां चा᳚ग्ने त॒न्वं᳚ पि॒प्रय᳚स्वा॒स्मभ्यं᳚ च॒ सौभ᳚ग॒मा य᳚जस्व ॥",
          "begin": 9.378,
          "end": 20.0
        }
      ]
    }
  ]
}