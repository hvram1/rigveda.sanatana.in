{
  "mandala": 8,
  "sukta": 38,
  "verses": [
    {
      "id": "8.38.1",
      "mandala": 8,
      "sukta": 38,
      "rik": 1,
      "padas": [
        {
          "text": "य॒ज्ञस्य॒ हि स्थ ऋ॒त्विजा॒ सस्नी॒ वाजे᳚षु॒ कर्म᳚सु ।",
          "begin": 0.0,
          "end": 7.863
        },
        {
          "text": "इन्द्रा᳚ग्नी॒ तस्य᳚ बोधतम् ॥",
          "begin": 7.863,
          "end": 10.96
        }
      ]
    },
    {
      "id": "8.38.2",
      "mandala": 8,
      "sukta": 38,
      "rik": 2,
      "padas": [
        {
          "text": "तो॒शासा᳚ रथ॒यावा᳚ना वृत्र॒हणाप᳚राजिता ।",
          "begin": 0.0,
          "end": 5.16
        },
        {
          "text": "इन्द्रा᳚ग्नी॒ तस्य᳚ बोधतम् ॥",
          "begin": 5.16,
          "end": 8.0
        }
      ]
    },
    {
      "id": "8.38.3",
      "mandala": 8,
      "sukta": 38,
      "rik": 3,
      "padas": [
        {
          "text": "इ॒दं वां᳚ मदि॒रं मध्वधु᳚क्ष॒न्नद्रि᳚भि॒र्नरः᳚ ।",
          "begin": 0.0,
          "end": 4.874
        },
        {
          "text": "इन्द्रा᳚ग्नी॒ तस्य᳚ बोधतम् ॥",
          "begin": 4.874,
          "end": 7.44
        }
      ]
    },
    {
      "id": "8.38.4",
      "mandala": 8,
      "sukta": 38,
      "rik": 4,
      "padas": [
        {
          "text": "जु॒षेथां᳚ य॒ज्ञमि॒ष्टये᳚ सु॒तं सोमं᳚ सधस्तुती ।",
          "begin": 0.0,
          "end": 5.088
        },
        {
          "text": "इन्द्रा᳚ग्नी॒ आ ग᳚तं नरा ॥",
          "begin": 5.088,
          "end": 8.28
        }
      ]
    },
    {
      "id": "8.38.5",
      "mandala": 8,
      "sukta": 38,
      "rik": 5,
      "padas": [
        {
          "text": "इ॒मा जु᳚षेथां॒ सव᳚ना॒ येभि᳚र्ह॒व्यान्यू॒हथु᳚ः ।",
          "begin": 0.0,
          "end": 4.995
        },
        {
          "text": "इन्द्रा᳚ग्नी॒ आ ग᳚तं नरा ॥",
          "begin": 4.995,
          "end": 7.8
        }
      ]
    },
    {
      "id": "8.38.6",
      "mandala": 8,
      "sukta": 38,
      "rik": 6,
      "padas": [
        {
          "text": "इ॒मां गा᳚य॒त्रव᳚र्तनिं जु॒षेथां᳚ सुष्टु॒तिं मम᳚ ।",
          "begin": 0.0,
          "end": 5.44
        },
        {
          "text": "इन्द्रा᳚ग्नी॒ आ ग᳚तं नरा ॥",
          "begin": 5.44,
          "end": 8.28
        }
      ]
    },
    {
      "id": "8.38.7",
      "mandala": 8,
      "sukta": 38,
      "rik": 7,
      "padas": [
        {
          "text": "प्रा॒त॒र्याव᳚भि॒रा ग᳚तं दे॒वेभि᳚र्जेन्यावसू ।",
          "begin": 0.0,
          "end": 4.773
        },
        {
          "text": "इन्द्रा᳚ग्नी॒ सोम᳚पीतये ॥",
          "begin": 4.773,
          "end": 7.48
        }
      ]
    },
    {
      "id": "8.38.8",
      "mandala": 8,
      "sukta": 38,
      "rik": 8,
      "padas": [
        {
          "text": "श्या॒वाश्व᳚स्य सुन्व॒तो॑त्री᳚णां शृणुतं॒ हव᳚म् ।",
          "begin": 0.0,
          "end": 5.025
        },
        {
          "text": "इन्द्रा᳚ग्नी॒ सोम᳚पीतये ॥",
          "begin": 5.025,
          "end": 7.96
        }
      ]
    },
    {
      "id": "8.38.9",
      "mandala": 8,
      "sukta": 38,
      "rik": 9,
      "padas": [
        {
          "text": "ए॒वा वा᳚मह्व ऊ॒तये॒ यथाहु᳚वन्त॒ मेधि᳚राः ।",
          "begin": 0.0,
          "end": 5.217
        },
        {
          "text": "इन्द्रा᳚ग्नी॒ सोम᳚पीतये ॥",
          "begin": 5.217,
          "end": 7.96
        }
      ]
    },
    {
      "id": "8.38.10",
      "mandala": 8,
      "sukta": 38,
      "rik": 10,
      "padas": [
        {
          "text": "आहं सर᳚स्वतीवतोरिन्द्रा॒ग्न्योरवो᳚ वृणे ।",
          "begin": 0.0,
          "end": 5.55
        },
        {
          "text": "याभ्यां᳚ गाय॒त्रमृ॒च्यते᳚ ॥",
          "begin": 5.55,
          "end": 11.2
        }
      ]
    }
  ]
}