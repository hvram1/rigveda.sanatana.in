{
  "mandala": 8,
  "sukta": 44,
  "verses": [
    {
      "id": "8.44.1",
      "mandala": 8,
      "sukta": 44,
      "rik": 1,
      "padas": [
        {
          "text": "स॒मिधा॒ग्निं दु᳚वस्यत घृ॒तैर्बो᳚धय॒ताति᳚थिम् ।",
          "begin": 0.0,
          "end": 8.598
        },
        {
          "text": "आस्मि᳚न्ह॒व्या जु᳚होतन ॥",
          "begin": 8.598,
          "end": 11.48
        }
      ]
    },
    {
      "id": "8.44.2",
      "mandala": 8,
      "sukta": 44,
      "rik": 2,
      "padas": [
        {
          "text": "अग्ने॒ स्तोमं᳚ जुषस्व मे॒ वर्ध᳚स्वा॒नेन॒ मन्म᳚ना ।",
          "begin": 0.0,
          "end": 5.222
        },
        {
          "text": "प्रति᳚ सू॒क्तानि᳚ हर्य नः ॥",
          "begin": 5.222,
          "end": 7.88
        }
      ]
    },
    {
      "id": "8.44.3",
      "mandala": 8,
      "sukta": 44,
      "rik": 3,
      "padas": [
        {
          "text": "अ॒ग्निं दू॒तं पु॒रो द᳚धे हव्य॒वाह॒मुप᳚ ब्रुवे ।",
          "begin": 0.0,
          "end": 5.087
        },
        {
          "text": "दे॒वाँ आ सा᳚दयादि॒ह ॥",
          "begin": 5.087,
          "end": 8.0
        }
      ]
    },
    {
      "id": "8.44.4",
      "mandala": 8,
      "sukta": 44,
      "rik": 4,
      "padas": [
        {
          "text": "उत्ते᳚ बृ॒हन्तो᳚ अ॒र्चयः᳚ समिधा॒नस्य᳚ दीदिवः ।",
          "begin": 0.0,
          "end": 5.119
        },
        {
          "text": "अग्ने᳚ शु॒क्रास᳚ ईरते ॥",
          "begin": 5.119,
          "end": 8.28
        }
      ]
    },
    {
      "id": "8.44.5",
      "mandala": 8,
      "sukta": 44,
      "rik": 5,
      "padas": [
        {
          "text": "उप᳚ त्वा जु॒ह्वो॒३॒᳚ मम᳚ घृ॒ताची᳚र्यन्तु हर्यत ।",
          "begin": 0.0,
          "end": 6.846
        },
        {
          "text": "अग्ने᳚ ह॒व्या जु᳚षस्व नः ॥",
          "begin": 6.846,
          "end": 9.72
        }
      ]
    },
    {
      "id": "8.44.6",
      "mandala": 8,
      "sukta": 44,
      "rik": 6,
      "padas": [
        {
          "text": "म॒न्द्रं होता᳚रमृ॒त्विजं᳚ चि॒त्रभा᳚नुं वि॒भाव᳚सुम् ।",
          "begin": 0.0,
          "end": 5.644
        },
        {
          "text": "अ॒ग्निमी᳚ळे॒ स उ᳚ श्रवत् ॥",
          "begin": 5.644,
          "end": 8.48
        }
      ]
    },
    {
      "id": "8.44.7",
      "mandala": 8,
      "sukta": 44,
      "rik": 7,
      "padas": [
        {
          "text": "प्र॒त्नं होता᳚र॒मीड्यं॒ जुष्ट᳚म॒ग्निं क॒विक्र᳚तुम् ।",
          "begin": 0.0,
          "end": 5.262
        },
        {
          "text": "अ॒ध्व॒राणा᳚मभि॒श्रिय᳚म् ॥",
          "begin": 5.262,
          "end": 7.96
        }
      ]
    },
    {
      "id": "8.44.8",
      "mandala": 8,
      "sukta": 44,
      "rik": 8,
      "padas": [
        {
          "text": "जु॒षा॒णो अ᳚ङ्गिरस्तमे॒मा ह॒व्यान्या᳚नु॒षक् ।",
          "begin": 0.0,
          "end": 5.527
        },
        {
          "text": "अग्ने᳚ य॒ज्ञं न᳚य ऋतु॒था ॥",
          "begin": 5.527,
          "end": 9.12
        }
      ]
    },
    {
      "id": "8.44.9",
      "mandala": 8,
      "sukta": 44,
      "rik": 9,
      "padas": [
        {
          "text": "स॒मि॒धा॒न उ᳚ सन्त्य॒ शुक्र᳚शोच इ॒हा व᳚ह ।",
          "begin": 0.0,
          "end": 4.344
        },
        {
          "text": "चि॒कि॒त्वान्दैव्यं॒ जन᳚म् ॥",
          "begin": 4.344,
          "end": 7.2
        }
      ]
    },
    {
      "id": "8.44.10",
      "mandala": 8,
      "sukta": 44,
      "rik": 10,
      "padas": [
        {
          "text": "विप्रं॒ होता᳚रम॒द्रुहं᳚ धू॒मके᳚तुं वि॒भाव᳚सुम् ।",
          "begin": 0.0,
          "end": 5.728
        },
        {
          "text": "य॒ज्ञानां᳚ के॒तुमी᳚महे ॥",
          "begin": 5.728,
          "end": 8.92
        }
      ]
    },
    {
      "id": "8.44.11",
      "mandala": 8,
      "sukta": 44,
      "rik": 11,
      "padas": [
        {
          "text": "अग्ने॒ नि पा᳚हि न॒स्त्वं प्रति᳚ ष्म देव॒ रीष᳚तः ।",
          "begin": 0.0,
          "end": 5.032
        },
        {
          "text": "भि॒न्धि द्वेषः᳚ सहस्कृत ॥",
          "begin": 5.032,
          "end": 7.44
        }
      ]
    },
    {
      "id": "8.44.12",
      "mandala": 8,
      "sukta": 44,
      "rik": 12,
      "padas": [
        {
          "text": "अ॒ग्निः प्र॒त्नेन॒ मन्म᳚ना॒ शुम्भा᳚नस्त॒न्वं१॒᳚ स्वाम् ।",
          "begin": 0.0,
          "end": 5.862
        },
        {
          "text": "क॒विर्विप्रे᳚ण वावृधे ॥",
          "begin": 5.862,
          "end": 9.0
        }
      ]
    },
    {
      "id": "8.44.13",
      "mandala": 8,
      "sukta": 44,
      "rik": 13,
      "padas": [
        {
          "text": "ऊ॒र्जो नपा᳚त॒मा हु᳚वे॒॑ग्निं पा᳚व॒कशो᳚चिषम् ।",
          "begin": 0.0,
          "end": 5.876
        },
        {
          "text": "अ॒स्मिन्य॒ज्ञे स्व᳚ध्व॒रे ॥",
          "begin": 5.876,
          "end": 8.52
        }
      ]
    },
    {
      "id": "8.44.14",
      "mandala": 8,
      "sukta": 44,
      "rik": 14,
      "padas": [
        {
          "text": "स नो᳚ मित्रमह॒स्त्वमग्ने᳚ शु॒क्रेण᳚ शो॒चिषा᳚ ।",
          "begin": 0.0,
          "end": 5.684
        },
        {
          "text": "दे॒वैरा स᳚त्सि ब॒र्हिषि᳚ ॥",
          "begin": 5.684,
          "end": 8.16
        }
      ]
    },
    {
      "id": "8.44.15",
      "mandala": 8,
      "sukta": 44,
      "rik": 15,
      "padas": [
        {
          "text": "यो अ॒ग्निं त॒न्वो॒३॒᳚ दमे᳚ दे॒वं मर्त᳚ः सप॒र्यति᳚ ।",
          "begin": 0.0,
          "end": 7.427
        },
        {
          "text": "तस्मा॒ इद्दी᳚दय॒द्वसु᳚ ॥",
          "begin": 7.427,
          "end": 10.6
        }
      ]
    },
    {
      "id": "8.44.16",
      "mandala": 8,
      "sukta": 44,
      "rik": 16,
      "padas": [
        {
          "text": "अ॒ग्निर्मू॒र्धा दि॒वः क॒कुत्पति᳚ः पृथि॒व्या अ॒यम् ।",
          "begin": 0.0,
          "end": 4.786
        },
        {
          "text": "अ॒पां रेतां᳚सि जिन्वति ॥",
          "begin": 4.786,
          "end": 7.96
        }
      ]
    },
    {
      "id": "8.44.17",
      "mandala": 8,
      "sukta": 44,
      "rik": 17,
      "padas": [
        {
          "text": "उद᳚ग्ने॒ शुच᳚य॒स्तव᳚ शु॒क्रा भ्राज᳚न्त ईरते ।",
          "begin": 0.0,
          "end": 5.358
        },
        {
          "text": "तव॒ ज्योतीं᳚ष्य॒र्चयः᳚ ॥",
          "begin": 5.358,
          "end": 8.12
        }
      ]
    },
    {
      "id": "8.44.18",
      "mandala": 8,
      "sukta": 44,
      "rik": 18,
      "padas": [
        {
          "text": "ईशि᳚षे॒ वार्य᳚स्य॒ हि दा॒त्रस्या᳚ग्ने॒ स्व᳚र्पतिः ।",
          "begin": 0.0,
          "end": 5.69
        },
        {
          "text": "स्तो॒ता स्यां॒ तव॒ शर्म᳚णि ॥",
          "begin": 5.69,
          "end": 8.48
        }
      ]
    },
    {
      "id": "8.44.19",
      "mandala": 8,
      "sukta": 44,
      "rik": 19,
      "padas": [
        {
          "text": "त्वाम᳚ग्ने मनी॒षिण॒स्त्वां हि᳚न्वन्ति॒ चित्ति᳚भिः ।",
          "begin": 0.0,
          "end": 4.993
        },
        {
          "text": "त्वां व᳚र्धन्तु नो॒ गिरः᳚ ॥",
          "begin": 4.993,
          "end": 7.8
        }
      ]
    },
    {
      "id": "8.44.20",
      "mandala": 8,
      "sukta": 44,
      "rik": 20,
      "padas": [
        {
          "text": "अद᳚ब्धस्य स्व॒धाव᳚तो दू॒तस्य॒ रेभ᳚त॒ः सदा᳚ ।",
          "begin": 0.0,
          "end": 4.8
        },
        {
          "text": "अ॒ग्नेः स॒ख्यं वृ᳚णीमहे ॥",
          "begin": 4.8,
          "end": 8.04
        }
      ]
    },
    {
      "id": "8.44.21",
      "mandala": 8,
      "sukta": 44,
      "rik": 21,
      "padas": [
        {
          "text": "अ॒ग्निः शुचि᳚व्रततम॒ः शुचि॒र्विप्र॒ः शुचि᳚ः क॒विः ।",
          "begin": 0.0,
          "end": 5.079
        },
        {
          "text": "शुची᳚ रोचत॒ आहु᳚तः ॥",
          "begin": 5.079,
          "end": 7.72
        }
      ]
    },
    {
      "id": "8.44.22",
      "mandala": 8,
      "sukta": 44,
      "rik": 22,
      "padas": [
        {
          "text": "उ॒त त्वा᳚ धी॒तयो॒ मम॒ गिरो᳚ वर्धन्तु वि॒श्वहा᳚ ।",
          "begin": 0.0,
          "end": 5.535
        },
        {
          "text": "अग्ने᳚ स॒ख्यस्य᳚ बोधि नः ॥",
          "begin": 5.535,
          "end": 8.6
        }
      ]
    },
    {
      "id": "8.44.23",
      "mandala": 8,
      "sukta": 44,
      "rik": 23,
      "padas": [
        {
          "text": "यद᳚ग्ने॒ स्याम॒हं त्वं त्वं वा᳚ घा॒ स्या अ॒हम् ।",
          "begin": 0.0,
          "end": 5.783
        },
        {
          "text": "स्युष्टे᳚ स॒त्या इ॒हाशिषः᳚ ॥",
          "begin": 5.783,
          "end": 9.12
        }
      ]
    },
    {
      "id": "8.44.24",
      "mandala": 8,
      "sukta": 44,
      "rik": 24,
      "padas": [
        {
          "text": "वसु॒र्वसु᳚पति॒र्हि क॒मस्य᳚ग्ने वि॒भाव᳚सुः ।",
          "begin": 0.0,
          "end": 4.527
        },
        {
          "text": "स्याम᳚ ते सुम॒तावपि᳚ ॥",
          "begin": 4.527,
          "end": 6.8
        }
      ]
    },
    {
      "id": "8.44.25",
      "mandala": 8,
      "sukta": 44,
      "rik": 25,
      "padas": [
        {
          "text": "अग्ने᳚ धृ॒तव्र᳚ताय ते समु॒द्राये᳚व॒ सिन्ध᳚वः ।",
          "begin": 0.0,
          "end": 5.145
        },
        {
          "text": "गिरो᳚ वा॒श्रास᳚ ईरते ॥",
          "begin": 5.145,
          "end": 8.6
        }
      ]
    },
    {
      "id": "8.44.26",
      "mandala": 8,
      "sukta": 44,
      "rik": 26,
      "padas": [
        {
          "text": "युवा᳚नं वि॒श्पतिं᳚ क॒विं वि॒श्वादं᳚ पुरु॒वेप᳚सम् ।",
          "begin": 0.0,
          "end": 6.283
        },
        {
          "text": "अ॒ग्निं शु᳚म्भामि॒ मन्म᳚भिः ॥",
          "begin": 6.283,
          "end": 8.76
        }
      ]
    },
    {
      "id": "8.44.27",
      "mandala": 8,
      "sukta": 44,
      "rik": 27,
      "padas": [
        {
          "text": "य॒ज्ञानां᳚ र॒थ्ये᳚ व॒यं ति॒ग्मज᳚म्भाय वी॒ळवे᳚ ।",
          "begin": 0.0,
          "end": 6.332
        },
        {
          "text": "स्तोमै᳚रिषेमा॒ग्नये᳚ ॥",
          "begin": 6.332,
          "end": 9.4
        }
      ]
    },
    {
      "id": "8.44.28",
      "mandala": 8,
      "sukta": 44,
      "rik": 28,
      "padas": [
        {
          "text": "अ॒यम᳚ग्ने॒ त्वे अपि᳚ जरि॒ता भू᳚तु सन्त्य ।",
          "begin": 0.0,
          "end": 5.341
        },
        {
          "text": "तस्मै᳚ पावक मृळय ॥",
          "begin": 5.341,
          "end": 7.76
        }
      ]
    },
    {
      "id": "8.44.29",
      "mandala": 8,
      "sukta": 44,
      "rik": 29,
      "padas": [
        {
          "text": "धीरो॒ ह्यस्य᳚द्म॒सद्विप्रो॒ न जागृ᳚वि॒ः सदा᳚ ।",
          "begin": 0.0,
          "end": 4.787
        },
        {
          "text": "अग्ने᳚ दी॒दय᳚सि॒ द्यवि᳚ ॥",
          "begin": 4.787,
          "end": 7.72
        }
      ]
    },
    {
      "id": "8.44.30",
      "mandala": 8,
      "sukta": 44,
      "rik": 30,
      "padas": [
        {
          "text": "पु॒राग्ने᳚ दुरि॒तेभ्य᳚ः पु॒रा मृ॒ध्रेभ्य᳚ः कवे ।",
          "begin": 0.0,
          "end": 5.713
        },
        {
          "text": "प्र ण॒ आयु᳚र्वसो तिर ॥",
          "begin": 5.713,
          "end": 10.84
        }
      ]
    }
  ]
}