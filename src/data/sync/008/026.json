{
  "mandala": 8,
  "sukta": 26,
  "verses": [
    {
      "id": "8.26.1",
      "mandala": 8,
      "sukta": 26,
      "rik": 1,
      "padas": [
        {
          "text": "यु॒वोरु॒ षू रथं᳚ हुवे स॒धस्तु᳚त्याय सू॒रिषु᳚ ।",
          "begin": 0.0,
          "end": 6.484
        },
        {
          "text": "अतू᳚र्तदक्षा वृषणा वृषण्वसू ॥",
          "begin": 6.484,
          "end": 12.48
        }
      ]
    },
    {
      "id": "8.26.2",
      "mandala": 8,
      "sukta": 26,
      "rik": 2,
      "padas": [
        {
          "text": "यु॒वं व᳚रो सु॒षाम्णे᳚ म॒हे तने᳚ नासत्या ।",
          "begin": 0.0,
          "end": 5.517
        },
        {
          "text": "अवो᳚भिर्याथो वृषणा वृषण्वसू ॥",
          "begin": 5.517,
          "end": 9.56
        }
      ]
    },
    {
      "id": "8.26.3",
      "mandala": 8,
      "sukta": 26,
      "rik": 3,
      "padas": [
        {
          "text": "ता वा᳚म॒द्य ह᳚वामहे ह॒व्येभि᳚र्वाजिनीवसू ।",
          "begin": 0.0,
          "end": 7.208
        },
        {
          "text": "पू॒र्वीरि॒ष इ॒षय᳚न्ता॒वति᳚ क्ष॒पः ॥",
          "begin": 7.208,
          "end": 10.16
        }
      ]
    },
    {
      "id": "8.26.4",
      "mandala": 8,
      "sukta": 26,
      "rik": 4,
      "padas": [
        {
          "text": "आ वां॒ वाहि᳚ष्ठो अश्विना॒ रथो᳚ यातु श्रु॒तो न᳚रा ।",
          "begin": 0.0,
          "end": 6.431
        },
        {
          "text": "उप॒ स्तोमा᳚न्तु॒रस्य᳚ दर्शथः श्रि॒ये ॥",
          "begin": 6.431,
          "end": 10.32
        }
      ]
    },
    {
      "id": "8.26.5",
      "mandala": 8,
      "sukta": 26,
      "rik": 5,
      "padas": [
        {
          "text": "जु॒हु॒रा॒णा चि᳚दश्वि॒ना म᳚न्येथां वृषण्वसू ।",
          "begin": 0.0,
          "end": 4.918
        },
        {
          "text": "यु॒वं हि रु᳚द्रा॒ पर्ष᳚थो॒ अति॒ द्विषः᳚ ॥",
          "begin": 4.918,
          "end": 9.4
        }
      ]
    },
    {
      "id": "8.26.6",
      "mandala": 8,
      "sukta": 26,
      "rik": 6,
      "padas": [
        {
          "text": "द॒स्रा हि विश्व᳚मानु॒षङ्म॒क्षूभि᳚ः परि॒दीय᳚थः ।",
          "begin": 0.0,
          "end": 5.292
        },
        {
          "text": "धि॒यं॒जि॒न्वा मधु᳚वर्णा शु॒भस्पती᳚ ॥",
          "begin": 5.292,
          "end": 8.8
        }
      ]
    },
    {
      "id": "8.26.7",
      "mandala": 8,
      "sukta": 26,
      "rik": 7,
      "padas": [
        {
          "text": "उप᳚ नो यातमश्विना रा॒या वि᳚श्व॒पुषा᳚ स॒ह ।",
          "begin": 0.0,
          "end": 5.55
        },
        {
          "text": "म॒घवा᳚ना सु॒वीरा॒वन᳚पच्युता ॥",
          "begin": 5.55,
          "end": 9.12
        }
      ]
    },
    {
      "id": "8.26.8",
      "mandala": 8,
      "sukta": 26,
      "rik": 8,
      "padas": [
        {
          "text": "आ मे᳚ अ॒स्य प्र᳚ती॒व्य१॒᳚मिन्द्र᳚नासत्या गतम् ।",
          "begin": 0.0,
          "end": 6.589
        },
        {
          "text": "दे॒वा दे॒वेभि᳚र॒द्य स॒चन᳚स्तमा ॥",
          "begin": 6.589,
          "end": 9.96
        }
      ]
    },
    {
      "id": "8.26.9",
      "mandala": 8,
      "sukta": 26,
      "rik": 9,
      "padas": [
        {
          "text": "व॒यं हि वां॒ हवा᳚मह उक्ष॒ण्यन्तो᳚ व्यश्व॒वत् ।",
          "begin": 0.0,
          "end": 6.248
        },
        {
          "text": "सु॒म॒तिभि॒रुप᳚ विप्रावि॒हा ग᳚तम् ॥",
          "begin": 6.248,
          "end": 8.8
        }
      ]
    },
    {
      "id": "8.26.10",
      "mandala": 8,
      "sukta": 26,
      "rik": 10,
      "padas": [
        {
          "text": "अ॒श्विना॒ स्वृ᳚षे स्तुहि कु॒वित्ते॒ श्रव᳚तो॒ हव᳚म् ।",
          "begin": 0.0,
          "end": 5.695
        },
        {
          "text": "नेदी᳚यसः कूळयातः प॒णीँरु॒त ॥",
          "begin": 5.695,
          "end": 9.84
        }
      ]
    },
    {
      "id": "8.26.11",
      "mandala": 8,
      "sukta": 26,
      "rik": 11,
      "padas": [
        {
          "text": "वै॒य॒श्वस्य᳚ श्रुतं नरो॒तो मे᳚ अ॒स्य वे᳚दथः ।",
          "begin": 0.0,
          "end": 4.987
        },
        {
          "text": "स॒जोष᳚सा॒ वरु᳚णो मि॒त्रो अ᳚र्य॒मा ॥",
          "begin": 4.987,
          "end": 10.08
        }
      ]
    },
    {
      "id": "8.26.12",
      "mandala": 8,
      "sukta": 26,
      "rik": 12,
      "padas": [
        {
          "text": "यु॒वाद᳚त्तस्य धिष्ण्या यु॒वानी᳚तस्य सू॒रिभि᳚ः ।",
          "begin": 0.0,
          "end": 5.755
        },
        {
          "text": "अह᳚रहर्वृषण॒ मह्यं᳚ शिक्षतम् ॥",
          "begin": 5.755,
          "end": 9.32
        }
      ]
    },
    {
      "id": "8.26.13",
      "mandala": 8,
      "sukta": 26,
      "rik": 13,
      "padas": [
        {
          "text": "यो वां᳚ य॒ज्ञेभि॒रावृ॒तो॑धि᳚वस्त्रा व॒धूरि᳚व ।",
          "begin": 0.0,
          "end": 6.154
        },
        {
          "text": "स॒प॒र्यन्ता᳚ शु॒भे च᳚क्राते अ॒श्विना᳚ ॥",
          "begin": 6.154,
          "end": 10.08
        }
      ]
    },
    {
      "id": "8.26.14",
      "mandala": 8,
      "sukta": 26,
      "rik": 14,
      "padas": [
        {
          "text": "यो वा᳚मुरु॒व्यच᳚स्तमं॒ चिके᳚तति नृ॒पाय्य᳚म् ।",
          "begin": 0.0,
          "end": 5.826
        },
        {
          "text": "व॒र्तिर᳚श्विना॒ परि᳚ यातमस्म॒यू ॥",
          "begin": 5.826,
          "end": 9.16
        }
      ]
    },
    {
      "id": "8.26.15",
      "mandala": 8,
      "sukta": 26,
      "rik": 15,
      "padas": [
        {
          "text": "अ॒स्मभ्यं॒ सु वृ᳚षण्वसू या॒तं व॒र्तिर्नृ॒पाय्य᳚म् ।",
          "begin": 0.0,
          "end": 5.541
        },
        {
          "text": "वि॒षु॒द्रुहे᳚व य॒ज्ञमू᳚हथुर्गि॒रा ॥",
          "begin": 5.541,
          "end": 9.4
        }
      ]
    },
    {
      "id": "8.26.16",
      "mandala": 8,
      "sukta": 26,
      "rik": 16,
      "padas": [
        {
          "text": "वाहि᳚ष्ठो वां॒ हवा᳚नां॒ स्तोमो᳚ दू॒तो हु᳚वन्नरा ।",
          "begin": 0.0,
          "end": 6.698
        },
        {
          "text": "यु॒वाभ्यां᳚ भूत्वश्विना ॥",
          "begin": 6.698,
          "end": 9.92
        }
      ]
    },
    {
      "id": "8.26.17",
      "mandala": 8,
      "sukta": 26,
      "rik": 17,
      "padas": [
        {
          "text": "यद॒दो दि॒वो अ᳚र्ण॒व इ॒षो वा॒ मद᳚थो गृ॒हे ।",
          "begin": 0.0,
          "end": 5.343
        },
        {
          "text": "श्रु॒तमिन्मे᳚ अमर्त्या ॥",
          "begin": 5.343,
          "end": 8.12
        }
      ]
    },
    {
      "id": "8.26.18",
      "mandala": 8,
      "sukta": 26,
      "rik": 18,
      "padas": [
        {
          "text": "उ॒त स्या श्वे᳚त॒याव᳚री॒ वाहि᳚ष्ठा वां न॒दीना᳚म् ।",
          "begin": 0.0,
          "end": 6.398
        },
        {
          "text": "सिन्धु॒र्हिर᳚ण्यवर्तनिः ॥",
          "begin": 6.398,
          "end": 9.24
        }
      ]
    },
    {
      "id": "8.26.19",
      "mandala": 8,
      "sukta": 26,
      "rik": 19,
      "padas": [
        {
          "text": "स्मदे॒तया᳚ सुकी॒र्त्याश्वि᳚ना श्वे॒तया᳚ धि॒या ।",
          "begin": 0.0,
          "end": 6.401
        },
        {
          "text": "वहे᳚थे शुभ्रयावाना ॥",
          "begin": 6.401,
          "end": 9.48
        }
      ]
    },
    {
      "id": "8.26.20",
      "mandala": 8,
      "sukta": 26,
      "rik": 20,
      "padas": [
        {
          "text": "यु॒क्ष्वा हि त्वं र᳚था॒सहा᳚ यु॒वस्व॒ पोष्या᳚ वसो ।",
          "begin": 0.0,
          "end": 6.389
        },
        {
          "text": "आन्नो᳚ वायो॒ मधु᳚ पिबा॒स्माकं॒ सव॒ना ग᳚हि ॥",
          "begin": 6.389,
          "end": 11.0
        }
      ]
    },
    {
      "id": "8.26.21",
      "mandala": 8,
      "sukta": 26,
      "rik": 21,
      "padas": [
        {
          "text": "तव᳚ वायवृतस्पते॒ त्वष्टु᳚र्जामातरद्भुत ।",
          "begin": 0.0,
          "end": 4.832
        },
        {
          "text": "अवां॒स्या वृ᳚णीमहे ॥",
          "begin": 4.832,
          "end": 7.56
        }
      ]
    },
    {
      "id": "8.26.22",
      "mandala": 8,
      "sukta": 26,
      "rik": 22,
      "padas": [
        {
          "text": "त्वष्टु॒र्जामा᳚तरं व॒यमीशा᳚नं रा॒य ई᳚महे ।",
          "begin": 0.0,
          "end": 6.167
        },
        {
          "text": "सु॒ताव᳚न्तो वा॒युं द्यु॒म्ना जना᳚सः ॥",
          "begin": 6.167,
          "end": 10.64
        }
      ]
    },
    {
      "id": "8.26.23",
      "mandala": 8,
      "sukta": 26,
      "rik": 23,
      "padas": [
        {
          "text": "वायो᳚ या॒हि शि॒वा दि॒वो वह᳚स्वा॒ सु स्वश्व्य᳚म् ।",
          "begin": 0.0,
          "end": 5.887
        },
        {
          "text": "वह᳚स्व म॒हः पृ᳚थु॒पक्ष᳚सा॒ रथे᳚ ॥",
          "begin": 5.887,
          "end": 9.36
        }
      ]
    },
    {
      "id": "8.26.24",
      "mandala": 8,
      "sukta": 26,
      "rik": 24,
      "padas": [
        {
          "text": "त्वां हि सु॒प्सर᳚स्तमं नृ॒षद᳚नेषु हू॒महे᳚ ।",
          "begin": 0.0,
          "end": 4.815
        },
        {
          "text": "ग्रावा᳚णं॒ नाश्व᳚पृष्ठं मं॒हना᳚ ॥",
          "begin": 4.815,
          "end": 9.48
        }
      ]
    },
    {
      "id": "8.26.25",
      "mandala": 8,
      "sukta": 26,
      "rik": 25,
      "padas": [
        {
          "text": "स त्वं नो᳚ देव॒ मन᳚सा॒ वायो᳚ मन्दा॒नो अ᳚ग्रि॒यः ।",
          "begin": 0.0,
          "end": 8.004
        },
        {
          "text": "कृ॒धि वाजाँ᳚ अ॒पो धियः᳚ ॥",
          "begin": 8.004,
          "end": 15.08
        }
      ]
    }
  ]
}