{
  "mandala": 8,
  "sukta": 85,
  "verses": [
    {
      "id": "8.85.1",
      "mandala": 8,
      "sukta": 85,
      "rik": 1,
      "padas": [
        {
          "text": "आ मे॒ हवं᳚ नास॒त्याश्वि᳚ना॒ गच्छ᳚तं यु॒वम् ।",
          "begin": 0.0,
          "end": 7.188
        },
        {
          "text": "मध्व॒ः सोम᳚स्य पी॒तये᳚ ॥",
          "begin": 7.188,
          "end": 10.24
        }
      ]
    },
    {
      "id": "8.85.2",
      "mandala": 8,
      "sukta": 85,
      "rik": 2,
      "padas": [
        {
          "text": "इ॒मं मे॒ स्तोम᳚मश्विने॒मं मे᳚ शृणुतं॒ हव᳚म् ।",
          "begin": 0.0,
          "end": 5.865
        },
        {
          "text": "मध्व॒ः सोम᳚स्य पी॒तये᳚ ॥",
          "begin": 5.865,
          "end": 8.72
        }
      ]
    },
    {
      "id": "8.85.3",
      "mandala": 8,
      "sukta": 85,
      "rik": 3,
      "padas": [
        {
          "text": "अ॒यं वां॒ कृष्णो᳚ अश्विना॒ हव᳚ते वाजिनीवसू ।",
          "begin": 0.0,
          "end": 6.009
        },
        {
          "text": "मध्व॒ः सोम᳚स्य पी॒तये᳚ ॥",
          "begin": 6.009,
          "end": 8.76
        }
      ]
    },
    {
      "id": "8.85.4",
      "mandala": 8,
      "sukta": 85,
      "rik": 4,
      "padas": [
        {
          "text": "शृ॒णु॒तं ज᳚रि॒तुर्हवं॒ कृष्ण᳚स्य स्तुव॒तो न᳚रा ।",
          "begin": 0.0,
          "end": 5.039
        },
        {
          "text": "मध्व॒ः सोम᳚स्य पी॒तये᳚ ॥",
          "begin": 5.039,
          "end": 7.72
        }
      ]
    },
    {
      "id": "8.85.5",
      "mandala": 8,
      "sukta": 85,
      "rik": 5,
      "padas": [
        {
          "text": "छ॒र्दिर्य᳚न्त॒मदा᳚भ्यं॒ विप्रा᳚य स्तुव॒ते न᳚रा ।",
          "begin": 0.0,
          "end": 6.091
        },
        {
          "text": "मध्व॒ः सोम᳚स्य पी॒तये᳚ ॥",
          "begin": 6.091,
          "end": 8.96
        }
      ]
    },
    {
      "id": "8.85.6",
      "mandala": 8,
      "sukta": 85,
      "rik": 6,
      "padas": [
        {
          "text": "गच्छ᳚तं दा॒शुषो᳚ गृ॒हमि॒त्था स्तु᳚व॒तो अ᳚श्विना ।",
          "begin": 0.0,
          "end": 5.39
        },
        {
          "text": "मध्व॒ः सोम᳚स्य पी॒तये᳚ ॥",
          "begin": 5.39,
          "end": 8.72
        }
      ]
    },
    {
      "id": "8.85.7",
      "mandala": 8,
      "sukta": 85,
      "rik": 7,
      "padas": [
        {
          "text": "यु॒ञ्जाथां॒ रास᳚भं॒ रथे᳚ वी॒ड्व᳚ङ्गे वृषण्वसू ।",
          "begin": 0.0,
          "end": 5.864
        },
        {
          "text": "मध्व॒ः सोम᳚स्य पी॒तये᳚ ॥",
          "begin": 5.864,
          "end": 9.32
        }
      ]
    },
    {
      "id": "8.85.8",
      "mandala": 8,
      "sukta": 85,
      "rik": 8,
      "padas": [
        {
          "text": "त्रि॒व॒न्धु॒रेण᳚ त्रि॒वृता॒ रथे॒ना या᳚तमश्विना ।",
          "begin": 0.0,
          "end": 5.556
        },
        {
          "text": "मध्व॒ः सोम᳚स्य पी॒तये᳚ ॥",
          "begin": 5.556,
          "end": 8.28
        }
      ]
    },
    {
      "id": "8.85.9",
      "mandala": 8,
      "sukta": 85,
      "rik": 9,
      "padas": [
        {
          "text": "नू मे॒ गिरो᳚ नास॒त्याश्वि᳚ना॒ प्राव᳚तं यु॒वम् ।",
          "begin": 0.0,
          "end": 5.952
        },
        {
          "text": "मध्व॒ः सोम᳚स्य पी॒तये᳚ ॥",
          "begin": 5.952,
          "end": 9.48
        }
      ]
    }
  ]
}