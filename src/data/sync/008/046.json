    {
      "id": "8.46.31",
      "mandala": 8,
      "sukta": 46,
      "rik": 31,
      "padas": [
        {
          "text": "अध॒ यच्चार᳚थे ग॒णे श॒तमुष्ट्राँ॒ अचि᳚क्रदत् ।",
          "begin": 0.0,
          "end": 4.764
        },
        {
          "text": "अध॒ श्वित्ने᳚षु विंश॒तिं श॒ता ॥",
          "begin": 4.764,
          "end": 7.76
        }
      ]
    },
    {
      "id": "8.46.32",
      "mandala": 8,
      "sukta": 46,
      "rik": 32,
      "padas": [
        {
          "text": "श॒तं दा॒से ब᳚ल्बू॒थे विप्र॒स्तरु᳚क्ष॒ आ द᳚दे ।",
          "begin": 0.0,
          "end": 4.627
        },
        {
          "text": "ते ते᳚ वायवि॒मे जना॒ मद॒न्तीन्द्र᳚गोपा॒ मद᳚न्ति दे॒वगो᳚पाः ॥",
          "begin": 4.627,
          "end": 11.36
        }
      ]
    },
    {
      "id": "8.46.33",
      "mandala": 8,
      "sukta": 46,
      "rik": 33,
      "padas": [
        {
          "text": "अध॒ स्या योष᳚णा म॒ही प्र᳚ती॒ची वश᳚म॒श्व्यम् ।",
          "begin": 0.0,
          "end": 5.449
        },
        {
          "text": "अधि᳚रुक्मा॒ वि नी᳚यते ॥",
          "begin": 5.449,
          "end": 10.24
        }
      ]
    }
