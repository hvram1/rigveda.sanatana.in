{
  "mandala": 8,
  "sukta": 12,
  "verses": [
    {
      "id": "8.12.1",
      "mandala": 8,
      "sukta": 12,
      "rik": 1,
      "padas": [
        {
          "text": "य इ᳚न्द्र सोम॒पात᳚मो॒ मदः᳚ शविष्ठ॒ चेत᳚ति ।",
          "begin": 0.0,
          "end": 15.821
        },
        {
          "text": "येना॒ हंसि॒ न्य१॒᳚त्रिणं॒ तमी᳚महे ॥",
          "begin": 15.821,
          "end": 21.24
        }
      ]
    },
    {
      "id": "8.12.2",
      "mandala": 8,
      "sukta": 12,
      "rik": 2,
      "padas": [
        {
          "text": "येना॒ दश᳚ग्व॒मध्रि᳚गुं वे॒पय᳚न्तं॒ स्व᳚र्णरम् ।",
          "begin": 0.0,
          "end": 5.784
        },
        {
          "text": "येना᳚ समु॒द्रमावि᳚था॒ तमी᳚महे ॥",
          "begin": 5.784,
          "end": 10.84
        }
      ]
    },
    {
      "id": "8.12.3",
      "mandala": 8,
      "sukta": 12,
      "rik": 3,
      "padas": [
        {
          "text": "येन॒ सिन्धुं᳚ म॒हीर॒पो रथाँ᳚ इव प्रचो॒दयः᳚ ।",
          "begin": 0.0,
          "end": 5.288
        },
        {
          "text": "पन्था᳚मृ॒तस्य॒ यात᳚वे॒ तमी᳚महे ॥",
          "begin": 5.288,
          "end": 11.12
        }
      ]
    },
    {
      "id": "8.12.4",
      "mandala": 8,
      "sukta": 12,
      "rik": 4,
      "padas": [
        {
          "text": "इ॒मं स्तोम᳚म॒भिष्ट᳚ये घृ॒तं न पू॒तम᳚द्रिवः ।",
          "begin": 0.0,
          "end": 5.097
        },
        {
          "text": "येना॒ नु स॒द्य ओज᳚सा व॒वक्षि᳚थ ॥",
          "begin": 5.097,
          "end": 9.96
        }
      ]
    },
    {
      "id": "8.12.5",
      "mandala": 8,
      "sukta": 12,
      "rik": 5,
      "padas": [
        {
          "text": "इ॒मं जु᳚षस्व गिर्वणः समु॒द्र इ᳚व पिन्वते ।",
          "begin": 0.0,
          "end": 5.162
        },
        {
          "text": "इन्द्र॒ विश्वा᳚भिरू॒तिभि᳚र्व॒वक्षि᳚थ ॥",
          "begin": 5.162,
          "end": 8.32
        }
      ]
    },
    {
      "id": "8.12.6",
      "mandala": 8,
      "sukta": 12,
      "rik": 6,
      "padas": [
        {
          "text": "यो नो᳚ दे॒वः प᳚रा॒वतः᳚ सखित्व॒नाय᳚ माम॒हे ।",
          "begin": 0.0,
          "end": 6.599
        },
        {
          "text": "दि॒वो न वृ॒ष्टिं प्र॒थय᳚न्व॒वक्षि᳚थ ॥",
          "begin": 6.599,
          "end": 10.16
        }
      ]
    },
    {
      "id": "8.12.7",
      "mandala": 8,
      "sukta": 12,
      "rik": 7,
      "padas": [
        {
          "text": "व॒व॒क्षुर᳚स्य के॒तवो᳚ उ॒त वज्रो॒ गभ᳚स्त्योः ।",
          "begin": 0.0,
          "end": 4.773
        },
        {
          "text": "यत्सूर्यो॒ न रोद᳚सी॒ अव᳚र्धयत् ॥",
          "begin": 4.773,
          "end": 8.8
        }
      ]
    },
    {
      "id": "8.12.8",
      "mandala": 8,
      "sukta": 12,
      "rik": 8,
      "padas": [
        {
          "text": "यदि᳚ प्रवृद्ध सत्पते स॒हस्रं᳚ महि॒षाँ अघः᳚ ।",
          "begin": 0.0,
          "end": 6.776
        },
        {
          "text": "आदित्त᳚ इन्द्रि॒यं महि॒ प्र वा᳚वृधे ॥",
          "begin": 6.776,
          "end": 9.96
        }
      ]
    },
    {
      "id": "8.12.9",
      "mandala": 8,
      "sukta": 12,
      "rik": 9,
      "padas": [
        {
          "text": "इन्द्र॒ः सूर्य᳚स्य र॒श्मिभि॒र्न्य᳚र्शसा॒नमो᳚षति ।",
          "begin": 0.0,
          "end": 6.031
        },
        {
          "text": "अ॒ग्निर्वने᳚व सास॒हिः प्र वा᳚वृधे ॥",
          "begin": 6.031,
          "end": 9.56
        }
      ]
    },
    {
      "id": "8.12.10",
      "mandala": 8,
      "sukta": 12,
      "rik": 10,
      "padas": [
        {
          "text": "इ॒यं त᳚ ऋ॒त्विया᳚वती धी॒तिरे᳚ति॒ नवी᳚यसी ।",
          "begin": 0.0,
          "end": 5.976
        },
        {
          "text": "स॒प॒र्यन्ती᳚ पुरुप्रि॒या मिमी᳚त॒ इत् ॥",
          "begin": 5.976,
          "end": 10.04
        }
      ]
    },
    {
      "id": "8.12.11",
      "mandala": 8,
      "sukta": 12,
      "rik": 11,
      "padas": [
        {
          "text": "गर्भो᳚ य॒ज्ञस्य᳚ देव॒युः क्रतुं᳚ पुनीत आनु॒षक् ।",
          "begin": 0.0,
          "end": 6.33
        },
        {
          "text": "स्तोमै॒रिन्द्र᳚स्य वावृधे॒ मिमी᳚त॒ इत् ॥",
          "begin": 6.33,
          "end": 10.16
        }
      ]
    },
    {
      "id": "8.12.12",
      "mandala": 8,
      "sukta": 12,
      "rik": 12,
      "padas": [
        {
          "text": "स॒निर्मि॒त्रस्य᳚ पप्रथ॒ इन्द्र॒ः सोम᳚स्य पी॒तये᳚ ।",
          "begin": 0.0,
          "end": 4.089
        },
        {
          "text": "प्राची॒ वाशी᳚व सुन्व॒ते मिमी᳚त॒ इत् ॥",
          "begin": 4.089,
          "end": 9.4
        }
      ]
    },
    {
      "id": "8.12.13",
      "mandala": 8,
      "sukta": 12,
      "rik": 13,
      "padas": [
        {
          "text": "यं विप्रा᳚ उ॒क्थवा᳚हसो॑भिप्रम॒न्दुरा॒यवः᳚ ।",
          "begin": 0.0,
          "end": 9.882
        },
        {
          "text": "घृ॒तं न पि᳚प्य आ॒सन्यृ॒तस्य॒ यत् ॥",
          "begin": 9.882,
          "end": 13.84
        }
      ]
    },
    {
      "id": "8.12.14",
      "mandala": 8,
      "sukta": 12,
      "rik": 14,
      "padas": [
        {
          "text": "उ॒त स्व॒राजे॒ अदि᳚ति॒ः स्तोम॒मिन्द्रा᳚य जीजनत् ।",
          "begin": 0.0,
          "end": 5.099
        },
        {
          "text": "पु॒रु॒प्र॒श॒स्तमू॒तय᳚ ऋ॒तस्य॒ यत् ॥",
          "begin": 5.099,
          "end": 8.52
        }
      ]
    },
    {
      "id": "8.12.15",
      "mandala": 8,
      "sukta": 12,
      "rik": 15,
      "padas": [
        {
          "text": "अ॒भि वह्न᳚य ऊ॒तये॑नू᳚षत॒ प्रश᳚स्तये ।",
          "begin": 0.0,
          "end": 4.673
        },
        {
          "text": "न दे᳚व॒ विव्र᳚ता॒ हरी᳚ ऋ॒तस्य॒ यत् ॥",
          "begin": 4.673,
          "end": 8.72
        }
      ]
    },
    {
      "id": "8.12.16",
      "mandala": 8,
      "sukta": 12,
      "rik": 16,
      "padas": [
        {
          "text": "यत्सोम᳚मिन्द्र॒ विष्ण᳚वि॒ यद्वा᳚ घ त्रि॒त आ॒प्त्ये ।",
          "begin": 0.0,
          "end": 5.474
        },
        {
          "text": "यद्वा᳚ म॒रुत्सु॒ मन्द᳚से॒ समिन्दु᳚भिः ॥",
          "begin": 5.474,
          "end": 9.16
        }
      ]
    },
    {
      "id": "8.12.17",
      "mandala": 8,
      "sukta": 12,
      "rik": 17,
      "padas": [
        {
          "text": "यद्वा᳚ शक्र परा॒वति᳚ समु॒द्रे अधि॒ मन्द᳚से ।",
          "begin": 0.0,
          "end": 5.063
        },
        {
          "text": "अ॒स्माक॒मित्सु॒ते र᳚णा॒ समिन्दु᳚भिः ॥",
          "begin": 5.063,
          "end": 8.56
        }
      ]
    },
    {
      "id": "8.12.18",
      "mandala": 8,
      "sukta": 12,
      "rik": 18,
      "padas": [
        {
          "text": "यद्वासि᳚ सुन्व॒तो वृ॒धो यज᳚मानस्य सत्पते ।",
          "begin": 0.0,
          "end": 4.516
        },
        {
          "text": "उ॒क्थे वा॒ यस्य॒ रण्य᳚सि॒ समिन्दु᳚भिः ॥",
          "begin": 4.516,
          "end": 7.8
        }
      ]
    },
    {
      "id": "8.12.19",
      "mandala": 8,
      "sukta": 12,
      "rik": 19,
      "padas": [
        {
          "text": "दे॒वंदे᳚वं॒ वो॑व᳚स॒ इन्द्र᳚मिन्द्रं गृणी॒षणि᳚ ।",
          "begin": 0.0,
          "end": 5.226
        },
        {
          "text": "अधा᳚ य॒ज्ञाय᳚ तु॒र्वणे॒ व्या᳚नशुः ॥",
          "begin": 5.226,
          "end": 9.52
        }
      ]
    },
    {
      "id": "8.12.20",
      "mandala": 8,
      "sukta": 12,
      "rik": 20,
      "padas": [
        {
          "text": "य॒ज्ञेभि᳚र्य॒ज्ञवा᳚हसं॒ सोमे᳚भिः सोम॒पात᳚मम् ।",
          "begin": 0.0,
          "end": 5.596
        },
        {
          "text": "होत्रा᳚भि॒रिन्द्रं᳚ वावृधु॒र्व्या᳚नशुः ॥",
          "begin": 5.596,
          "end": 10.92
        }
      ]
    },
    {
      "id": "8.12.21",
      "mandala": 8,
      "sukta": 12,
      "rik": 21,
      "padas": [
        {
          "text": "म॒हीर᳚स्य॒ प्रणी᳚तयः पू॒र्वीरु॒त प्रश᳚स्तयः ।",
          "begin": 0.0,
          "end": 5.467
        },
        {
          "text": "विश्वा॒ वसू᳚नि दा॒शुषे॒ व्या᳚नशुः ॥",
          "begin": 5.467,
          "end": 9.64
        }
      ]
    },
    {
      "id": "8.12.22",
      "mandala": 8,
      "sukta": 12,
      "rik": 22,
      "padas": [
        {
          "text": "इन्द्रं᳚ वृ॒त्राय॒ हन्त᳚वे दे॒वासो᳚ दधिरे पु॒रः ।",
          "begin": 0.0,
          "end": 5.837
        },
        {
          "text": "इन्द्रं॒ वाणी᳚रनूषता॒ समोज᳚से ॥",
          "begin": 5.837,
          "end": 9.56
        }
      ]
    },
    {
      "id": "8.12.23",
      "mandala": 8,
      "sukta": 12,
      "rik": 23,
      "padas": [
        {
          "text": "म॒हान्तं᳚ महि॒ना व॒यं स्तोमे᳚भिर्हवन॒श्रुत᳚म् ।",
          "begin": 0.0,
          "end": 5.861
        },
        {
          "text": "अ॒र्कैर॒भि प्र णो᳚नुम॒ः समोज᳚से ॥",
          "begin": 5.861,
          "end": 9.48
        }
      ]
    },
    {
      "id": "8.12.24",
      "mandala": 8,
      "sukta": 12,
      "rik": 24,
      "padas": [
        {
          "text": "न यं वि᳚वि॒क्तो रोद᳚सी॒ नान्तरि᳚क्षाणि व॒ज्रिण᳚म् ।",
          "begin": 0.0,
          "end": 5.236
        },
        {
          "text": "अमा॒दिद᳚स्य तित्विषे॒ समोज᳚सः ॥",
          "begin": 5.236,
          "end": 8.24
        }
      ]
    },
    {
      "id": "8.12.25",
      "mandala": 8,
      "sukta": 12,
      "rik": 25,
      "padas": [
        {
          "text": "यदि᳚न्द्र पृत॒नाज्ये᳚ दे॒वास्त्वा᳚ दधि॒रे पु॒रः ।",
          "begin": 0.0,
          "end": 6.307
        },
        {
          "text": "आदित्ते᳚ हर्य॒ता हरी᳚ ववक्षतुः ॥",
          "begin": 6.307,
          "end": 10.28
        }
      ]
    },
    {
      "id": "8.12.26",
      "mandala": 8,
      "sukta": 12,
      "rik": 26,
      "padas": [
        {
          "text": "य॒दा वृ॒त्रं न᳚दी॒वृतं॒ शव᳚सा वज्रि॒न्नव᳚धीः ।",
          "begin": 0.0,
          "end": 4.998
        },
        {
          "text": "आदित्ते᳚ हर्य॒ता हरी᳚ ववक्षतुः ॥",
          "begin": 4.998,
          "end": 9.0
        }
      ]
    },
    {
      "id": "8.12.27",
      "mandala": 8,
      "sukta": 12,
      "rik": 27,
      "padas": [
        {
          "text": "य॒दा ते॒ विष्णु॒रोज᳚सा॒ त्रीणि᳚ प॒दा वि᳚चक्र॒मे ।",
          "begin": 0.0,
          "end": 5.212
        },
        {
          "text": "आदित्ते᳚ हर्य॒ता हरी᳚ ववक्षतुः ॥",
          "begin": 5.212,
          "end": 9.12
        }
      ]
    },
    {
      "id": "8.12.28",
      "mandala": 8,
      "sukta": 12,
      "rik": 28,
      "padas": [
        {
          "text": "य॒दा ते᳚ हर्य॒ता हरी᳚ वावृ॒धाते᳚ दि॒वेदि᳚वे ।",
          "begin": 0.0,
          "end": 5.878
        },
        {
          "text": "आदित्ते॒ विश्वा॒ भुव᳚नानि येमिरे ॥",
          "begin": 5.878,
          "end": 9.12
        }
      ]
    },
    {
      "id": "8.12.29",
      "mandala": 8,
      "sukta": 12,
      "rik": 29,
      "padas": [
        {
          "text": "य॒दा ते॒ मारु᳚ती॒र्विश॒स्तुभ्य᳚मिन्द्र नियेमि॒रे ।",
          "begin": 0.0,
          "end": 4.761
        },
        {
          "text": "आदित्ते॒ विश्वा॒ भुव᳚नानि येमिरे ॥",
          "begin": 4.761,
          "end": 7.92
        }
      ]
    },
    {
      "id": "8.12.30",
      "mandala": 8,
      "sukta": 12,
      "rik": 30,
      "padas": [
        {
          "text": "य॒दा सूर्य᳚म॒मुं दि॒वि शु॒क्रं ज्योति॒रधा᳚रयः ।",
          "begin": 0.0,
          "end": 5.155
        },
        {
          "text": "आदित्ते॒ विश्वा॒ भुव᳚नानि येमिरे ॥",
          "begin": 5.155,
          "end": 8.24
        }
      ]
    },
    {
      "id": "8.12.31",
      "mandala": 8,
      "sukta": 12,
      "rik": 31,
      "padas": [
        {
          "text": "इ॒मां त᳚ इन्द्र सुष्टु॒तिं विप्र᳚ इयर्ति धी॒तिभि᳚ः ।",
          "begin": 0.0,
          "end": 4.868
        },
        {
          "text": "जा॒मिं प॒देव॒ पिप्र᳚तीं॒ प्राध्व॒रे ॥",
          "begin": 4.868,
          "end": 8.2
        }
      ]
    },
    {
      "id": "8.12.32",
      "mandala": 8,
      "sukta": 12,
      "rik": 32,
      "padas": [
        {
          "text": "यद᳚स्य॒ धाम᳚नि प्रि॒ये स᳚मीची॒नासो॒ अस्व᳚रन् ।",
          "begin": 0.0,
          "end": 4.857
        },
        {
          "text": "नाभा᳚ य॒ज्ञस्य᳚ दो॒हना॒ प्राध्व॒रे ॥",
          "begin": 4.857,
          "end": 9.56
        }
      ]
    },
    {
      "id": "8.12.33",
      "mandala": 8,
      "sukta": 12,
      "rik": 33,
      "padas": [
        {
          "text": "सु॒वीर्यं॒ स्वश्व्यं᳚ सु॒गव्य᳚मिन्द्र दद्धि नः ।",
          "begin": 0.0,
          "end": 5.713
        },
        {
          "text": "होते᳚व पू॒र्वचि᳚त्तये॒ प्राध्व॒रे ॥",
          "begin": 5.713,
          "end": 11.88
        }
      ]
    }
  ]
}