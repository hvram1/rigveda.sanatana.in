{
  "mandala": 8,
  "sukta": 32,
  "verses": [
    {
      "id": "8.32.1",
      "mandala": 8,
      "sukta": 32,
      "rik": 1,
      "padas": [
        {
          "text": "प्र कृ॒तान्यृ᳚जी॒षिण॒ः कण्वा॒ इन्द्र᳚स्य॒ गाथ᳚या ।",
          "begin": 0.0,
          "end": 5.724
        },
        {
          "text": "मदे॒ सोम᳚स्य वोचत ॥",
          "begin": 5.724,
          "end": 7.96
        }
      ]
    },
    {
      "id": "8.32.2",
      "mandala": 8,
      "sukta": 32,
      "rik": 2,
      "padas": [
        {
          "text": "यः सृबि᳚न्द॒मन᳚र्शनिं॒ पिप्रुं᳚ दा॒सम᳚ही॒शुव᳚म् ।",
          "begin": 0.0,
          "end": 5.61
        },
        {
          "text": "वधी᳚दु॒ग्रो रि॒णन्न॒पः ॥",
          "begin": 5.61,
          "end": 8.64
        }
      ]
    },
    {
      "id": "8.32.3",
      "mandala": 8,
      "sukta": 32,
      "rik": 3,
      "padas": [
        {
          "text": "न्यर्बु᳚दस्य वि॒ष्टपं᳚ व॒र्ष्माणं᳚ बृह॒तस्ति᳚र ।",
          "begin": 0.0,
          "end": 5.632
        },
        {
          "text": "कृ॒षे तदि᳚न्द्र॒ पौंस्य᳚म् ॥",
          "begin": 5.632,
          "end": 8.36
        }
      ]
    },
    {
      "id": "8.32.4",
      "mandala": 8,
      "sukta": 32,
      "rik": 4,
      "padas": [
        {
          "text": "प्रति᳚ श्रु॒ताय᳚ वो धृ॒षत्तूर्णा᳚शं॒ न गि॒रेरधि᳚ ।",
          "begin": 0.0,
          "end": 5.368
        },
        {
          "text": "हु॒वे सु᳚शि॒प्रमू॒तये᳚ ॥",
          "begin": 5.368,
          "end": 7.88
        }
      ]
    },
    {
      "id": "8.32.5",
      "mandala": 8,
      "sukta": 32,
      "rik": 5,
      "padas": [
        {
          "text": "स गोरश्व᳚स्य॒ वि व्र॒जं म᳚न्दा॒नः सो॒म्येभ्य᳚ः ।",
          "begin": 0.0,
          "end": 5.323
        },
        {
          "text": "पुरं॒ न शू᳚र दर्षसि ॥",
          "begin": 5.323,
          "end": 8.12
        }
      ]
    },
    {
      "id": "8.32.6",
      "mandala": 8,
      "sukta": 32,
      "rik": 6,
      "padas": [
        {
          "text": "यदि᳚ मे रा॒रणः᳚ सु॒त उ॒क्थे वा॒ दध᳚से॒ चनः᳚ ।",
          "begin": 0.0,
          "end": 4.91
        },
        {
          "text": "आ॒रादुप᳚ स्व॒धा ग᳚हि ॥",
          "begin": 4.91,
          "end": 7.08
        }
      ]
    },
    {
      "id": "8.32.7",
      "mandala": 8,
      "sukta": 32,
      "rik": 7,
      "padas": [
        {
          "text": "व॒यं घा᳚ ते॒ अपि᳚ ष्मसि स्तो॒तार᳚ इन्द्र गिर्वणः ।",
          "begin": 0.0,
          "end": 5.65
        },
        {
          "text": "त्वं नो᳚ जिन्व सोमपाः ॥",
          "begin": 5.65,
          "end": 8.52
        }
      ]
    },
    {
      "id": "8.32.8",
      "mandala": 8,
      "sukta": 32,
      "rik": 8,
      "padas": [
        {
          "text": "उ॒त नः᳚ पि॒तुमा भ᳚र संररा॒णो अवि᳚क्षितम् ।",
          "begin": 0.0,
          "end": 4.716
        },
        {
          "text": "मघ᳚व॒न्भूरि᳚ ते॒ वसु᳚ ॥",
          "begin": 4.716,
          "end": 6.8
        }
      ]
    },
    {
      "id": "8.32.9",
      "mandala": 8,
      "sukta": 32,
      "rik": 9,
      "padas": [
        {
          "text": "उ॒त नो॒ गोम᳚तस्कृधि॒ हिर᳚ण्यवतो अ॒श्विनः᳚ ।",
          "begin": 0.0,
          "end": 5.311
        },
        {
          "text": "इळा᳚भि॒ः सं र᳚भेमहि ॥",
          "begin": 5.311,
          "end": 7.88
        }
      ]
    },
    {
      "id": "8.32.10",
      "mandala": 8,
      "sukta": 32,
      "rik": 10,
      "padas": [
        {
          "text": "बृ॒बदु᳚क्थं हवामहे सृ॒प्रक᳚रस्नमू॒तये᳚ ।",
          "begin": 0.0,
          "end": 4.882
        },
        {
          "text": "साधु᳚ कृ॒ण्वन्त॒मव᳚से ॥",
          "begin": 4.882,
          "end": 7.2
        }
      ]
    },
    {
      "id": "8.32.11",
      "mandala": 8,
      "sukta": 32,
      "rik": 11,
      "padas": [
        {
          "text": "यः सं॒स्थे चि᳚च्छ॒तक्र᳚तु॒रादीं᳚ कृ॒णोति᳚ वृत्र॒हा ।",
          "begin": 0.0,
          "end": 5.591
        },
        {
          "text": "ज॒रि॒तृभ्य᳚ः पुरू॒वसु᳚ः ॥",
          "begin": 5.591,
          "end": 7.96
        }
      ]
    },
    {
      "id": "8.32.12",
      "mandala": 8,
      "sukta": 32,
      "rik": 12,
      "padas": [
        {
          "text": "स नः᳚ श॒क्रश्चि॒दा श᳚क॒द्दान᳚वाँ अन्तराभ॒रः ।",
          "begin": 0.0,
          "end": 5.33
        },
        {
          "text": "इन्द्रो॒ विश्वा᳚भिरू॒तिभि᳚ः ॥",
          "begin": 5.33,
          "end": 8.36
        }
      ]
    },
    {
      "id": "8.32.13",
      "mandala": 8,
      "sukta": 32,
      "rik": 13,
      "padas": [
        {
          "text": "यो रा॒यो॒३॒᳚॑वनि᳚र्म॒हान्त्सु᳚पा॒रः सु᳚न्व॒तः सखा᳚ ।",
          "begin": 0.0,
          "end": 8.371
        },
        {
          "text": "तमिन्द्र᳚म॒भि गा᳚यत ॥",
          "begin": 8.371,
          "end": 11.04
        }
      ]
    },
    {
      "id": "8.32.14",
      "mandala": 8,
      "sukta": 32,
      "rik": 14,
      "padas": [
        {
          "text": "आ॒य॒न्तारं॒ महि᳚ स्थि॒रं पृत᳚नासु श्रवो॒जित᳚म् ।",
          "begin": 0.0,
          "end": 4.644
        },
        {
          "text": "भूरे॒रीशा᳚न॒मोज᳚सा ॥",
          "begin": 4.644,
          "end": 8.04
        }
      ]
    },
    {
      "id": "8.32.15",
      "mandala": 8,
      "sukta": 32,
      "rik": 15,
      "padas": [
        {
          "text": "नकि᳚रस्य॒ शची᳚नां निय॒न्ता सू॒नृता᳚नाम् ।",
          "begin": 0.0,
          "end": 5.29
        },
        {
          "text": "नकि᳚र्व॒क्ता न दा॒दिति᳚ ॥",
          "begin": 5.29,
          "end": 8.12
        }
      ]
    },
    {
      "id": "8.32.16",
      "mandala": 8,
      "sukta": 32,
      "rik": 16,
      "padas": [
        {
          "text": "न नू॒नं ब्र॒ह्मणा᳚मृ॒णं प्रा᳚शू॒नाम᳚स्ति सुन्व॒ताम् ।",
          "begin": 0.0,
          "end": 5.945
        },
        {
          "text": "न सोमो᳚ अप्र॒ता प᳚पे ॥",
          "begin": 5.945,
          "end": 8.56
        }
      ]
    },
    {
      "id": "8.32.17",
      "mandala": 8,
      "sukta": 32,
      "rik": 17,
      "padas": [
        {
          "text": "पन्य॒ इदुप᳚ गायत॒ पन्य᳚ उ॒क्थानि᳚ शंसत ।",
          "begin": 0.0,
          "end": 4.617
        },
        {
          "text": "ब्रह्मा᳚ कृणोत॒ पन्य॒ इत् ॥",
          "begin": 4.617,
          "end": 7.24
        }
      ]
    },
    {
      "id": "8.32.18",
      "mandala": 8,
      "sukta": 32,
      "rik": 18,
      "padas": [
        {
          "text": "पन्य॒ आ द᳚र्दिरच्छ॒ता स॒हस्रा᳚ वा॒ज्यवृ᳚तः ।",
          "begin": 0.0,
          "end": 5.239
        },
        {
          "text": "इन्द्रो॒ यो यज्व᳚नो वृ॒धः ॥",
          "begin": 5.239,
          "end": 7.8
        }
      ]
    },
    {
      "id": "8.32.19",
      "mandala": 8,
      "sukta": 32,
      "rik": 19,
      "padas": [
        {
          "text": "वि षू च᳚र स्व॒धा अनु᳚ कृष्टी॒नामन्वा॒हुवः᳚ ।",
          "begin": 0.0,
          "end": 5.064
        },
        {
          "text": "इन्द्र॒ पिब᳚ सु॒ताना᳚म् ॥",
          "begin": 5.064,
          "end": 7.36
        }
      ]
    },
    {
      "id": "8.32.20",
      "mandala": 8,
      "sukta": 32,
      "rik": 20,
      "padas": [
        {
          "text": "पिब॒ स्वधै᳚नवानामु॒त यस्तुग्र्ये॒ सचा᳚ ।",
          "begin": 0.0,
          "end": 5.105
        },
        {
          "text": "उ॒तायमि᳚न्द्र॒ यस्तव᳚ ॥",
          "begin": 5.105,
          "end": 7.52
        }
      ]
    },
    {
      "id": "8.32.21",
      "mandala": 8,
      "sukta": 32,
      "rik": 21,
      "padas": [
        {
          "text": "अती᳚हि मन्युषा॒विणं᳚ सुषु॒वांस᳚मु॒पार᳚णे ।",
          "begin": 0.0,
          "end": 5.333
        },
        {
          "text": "इ॒मं रा॒तं सु॒तं पि᳚ब ॥",
          "begin": 5.333,
          "end": 7.48
        }
      ]
    },
    {
      "id": "8.32.22",
      "mandala": 8,
      "sukta": 32,
      "rik": 22,
      "padas": [
        {
          "text": "इ॒हि ति॒स्रः प᳚रा॒वत᳚ इ॒हि पञ्च॒ जनाँ॒ अति᳚ ।",
          "begin": 0.0,
          "end": 3.712
        },
        {
          "text": "धेना᳚ इन्द्राव॒चाक᳚शत् ॥",
          "begin": 3.712,
          "end": 7.44
        }
      ]
    },
    {
      "id": "8.32.23",
      "mandala": 8,
      "sukta": 32,
      "rik": 23,
      "padas": [
        {
          "text": "सूर्यो᳚ र॒श्मिं यथा᳚ सृ॒जा त्वा᳚ यच्छन्तु मे॒ गिरः᳚ ।",
          "begin": 0.0,
          "end": 6.799
        },
        {
          "text": "नि॒म्नमापो॒ न स॒ध्र्य᳚क् ॥",
          "begin": 6.799,
          "end": 9.08
        }
      ]
    },
    {
      "id": "8.32.24",
      "mandala": 8,
      "sukta": 32,
      "rik": 24,
      "padas": [
        {
          "text": "अध्व᳚र्य॒वा तु हि षि॒ञ्च सोमं᳚ वी॒राय᳚ शि॒प्रिणे᳚ ।",
          "begin": 0.0,
          "end": 5.619
        },
        {
          "text": "भरा᳚ सु॒तस्य᳚ पी॒तये᳚ ॥",
          "begin": 5.619,
          "end": 8.48
        }
      ]
    },
    {
      "id": "8.32.25",
      "mandala": 8,
      "sukta": 32,
      "rik": 25,
      "padas": [
        {
          "text": "य उ॒द्नः फ᳚लि॒गं भि॒नन्न्य१॒᳚क्सिन्धूँ᳚र॒वासृ᳚जत् ।",
          "begin": 0.0,
          "end": 4.036
        },
        {
          "text": "यो गोषु᳚ प॒क्वं धा॒रय᳚त् ॥",
          "begin": 4.036,
          "end": 8.44
        }
      ]
    },
    {
      "id": "8.32.26",
      "mandala": 8,
      "sukta": 32,
      "rik": 26,
      "padas": [
        {
          "text": "अह᳚न्वृ॒त्रमृची᳚षम और्णवा॒भम᳚ही॒शुव᳚म् ।",
          "begin": 0.0,
          "end": 4.946
        },
        {
          "text": "हि॒मेना᳚विध्य॒दर्बु᳚दम् ॥",
          "begin": 4.946,
          "end": 8.4
        }
      ]
    },
    {
      "id": "8.32.27",
      "mandala": 8,
      "sukta": 32,
      "rik": 27,
      "padas": [
        {
          "text": "प्र व᳚ उ॒ग्राय᳚ नि॒ष्टुरे॑षा᳚ळ्हाय प्रस॒क्षिणे᳚ ।",
          "begin": 0.0,
          "end": 5.206
        },
        {
          "text": "दे॒वत्तं॒ ब्रह्म᳚ गायत ॥",
          "begin": 5.206,
          "end": 7.76
        }
      ]
    },
    {
      "id": "8.32.28",
      "mandala": 8,
      "sukta": 32,
      "rik": 28,
      "padas": [
        {
          "text": "यो विश्वा᳚न्य॒भि व्र॒ता सोम᳚स्य॒ मदे॒ अन्ध᳚सः ।",
          "begin": 0.0,
          "end": 6.472
        },
        {
          "text": "इन्द्रो᳚ दे॒वेषु॒ चेत᳚ति ॥",
          "begin": 6.472,
          "end": 8.52
        }
      ]
    },
    {
      "id": "8.32.29",
      "mandala": 8,
      "sukta": 32,
      "rik": 29,
      "padas": [
        {
          "text": "इ॒ह त्या स᳚ध॒माद्या॒ हरी॒ हिर᳚ण्यकेश्या ।",
          "begin": 0.0,
          "end": 5.254
        },
        {
          "text": "वो॒ळ्हाम॒भि प्रयो᳚ हि॒तम् ॥",
          "begin": 5.254,
          "end": 8.36
        }
      ]
    },
    {
      "id": "8.32.30",
      "mandala": 8,
      "sukta": 32,
      "rik": 30,
      "padas": [
        {
          "text": "अ॒र्वाञ्चं᳚ त्वा पुरुष्टुत प्रि॒यमे᳚धस्तुता॒ हरी᳚ ।",
          "begin": 0.0,
          "end": 6.983
        },
        {
          "text": "सो॒म॒पेया᳚य वक्षतः ॥",
          "begin": 6.983,
          "end": 11.44
        }
      ]
    }
  ]
}