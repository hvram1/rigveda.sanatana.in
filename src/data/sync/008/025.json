{
  "mandala": 8,
  "sukta": 25,
  "verses": [
    {
      "id": "8.25.1",
      "mandala": 8,
      "sukta": 25,
      "rik": 1,
      "padas": [
        {
          "text": "ता वां॒ विश्व᳚स्य गो॒पा दे॒वा दे॒वेषु᳚ य॒ज्ञिया᳚ ।",
          "begin": 0.0,
          "end": 8.471
        },
        {
          "text": "ऋ॒तावा᳚ना यजसे पू॒तद᳚क्षसा ॥",
          "begin": 8.471,
          "end": 12.8
        }
      ]
    },
    {
      "id": "8.25.2",
      "mandala": 8,
      "sukta": 25,
      "rik": 2,
      "padas": [
        {
          "text": "मि॒त्रा तना॒ न र॒थ्या॒३॒᳚ वरु᳚णो॒ यश्च᳚ सु॒क्रतु᳚ः ।",
          "begin": 0.0,
          "end": 7.103
        },
        {
          "text": "स॒नात्सु᳚जा॒ता तन᳚या धृ॒तव्र᳚ता ॥",
          "begin": 7.103,
          "end": 10.68
        }
      ]
    },
    {
      "id": "8.25.3",
      "mandala": 8,
      "sukta": 25,
      "rik": 3,
      "padas": [
        {
          "text": "ता मा॒ता वि॒श्ववे᳚दसासु॒र्या᳚य॒ प्रम᳚हसा ।",
          "begin": 0.0,
          "end": 5.99
        },
        {
          "text": "म॒ही ज᳚जा॒नादि᳚तिरृ॒ताव᳚री ॥",
          "begin": 5.99,
          "end": 8.96
        }
      ]
    },
    {
      "id": "8.25.4",
      "mandala": 8,
      "sukta": 25,
      "rik": 4,
      "padas": [
        {
          "text": "म॒हान्ता᳚ मि॒त्रावरु᳚णा स॒म्राजा᳚ दे॒वावसु᳚रा ।",
          "begin": 0.0,
          "end": 6.166
        },
        {
          "text": "ऋ॒तावा᳚नावृ॒तमा घो᳚षतो बृ॒हत् ॥",
          "begin": 6.166,
          "end": 11.0
        }
      ]
    },
    {
      "id": "8.25.5",
      "mandala": 8,
      "sukta": 25,
      "rik": 5,
      "padas": [
        {
          "text": "नपा᳚ता॒ शव᳚सो म॒हः सू॒नू दक्ष᳚स्य सु॒क्रतू᳚ ।",
          "begin": 0.0,
          "end": 6.195
        },
        {
          "text": "सृ॒प्रदा᳚नू इ॒षो वास्त्वधि᳚ क्षितः ॥",
          "begin": 6.195,
          "end": 10.6
        }
      ]
    },
    {
      "id": "8.25.6",
      "mandala": 8,
      "sukta": 25,
      "rik": 6,
      "padas": [
        {
          "text": "सं या दानू᳚नि ये॒मथु᳚र्दि॒व्याः पार्थि᳚वी॒रिषः᳚ ।",
          "begin": 0.0,
          "end": 6.383
        },
        {
          "text": "नभ᳚स्वती॒रा वां᳚ चरन्तु वृ॒ष्टयः᳚ ॥",
          "begin": 6.383,
          "end": 10.6
        }
      ]
    },
    {
      "id": "8.25.7",
      "mandala": 8,
      "sukta": 25,
      "rik": 7,
      "padas": [
        {
          "text": "अधि॒ या बृ᳚ह॒तो दि॒वो॒३॒᳚॑भि यू॒थेव॒ पश्य᳚तः ।",
          "begin": 0.0,
          "end": 6.713
        },
        {
          "text": "ऋ॒तावा᳚ना स॒म्राजा॒ नम᳚से हि॒ता ॥",
          "begin": 6.713,
          "end": 10.88
        }
      ]
    },
    {
      "id": "8.25.8",
      "mandala": 8,
      "sukta": 25,
      "rik": 8,
      "padas": [
        {
          "text": "ऋ॒तावा᳚ना॒ नि षे᳚दतु॒ः साम्रा᳚ज्याय सु॒क्रतू᳚ ।",
          "begin": 0.0,
          "end": 5.891
        },
        {
          "text": "धृ॒तव्र᳚ता क्ष॒त्रिया᳚ क्ष॒त्रमा᳚शतुः ॥",
          "begin": 5.891,
          "end": 11.48
        }
      ]
    },
    {
      "id": "8.25.9",
      "mandala": 8,
      "sukta": 25,
      "rik": 9,
      "padas": [
        {
          "text": "अ॒क्ष्णश्चि᳚द्गातु॒वित्त᳚रानुल्ब॒णेन॒ चक्ष᳚सा ।",
          "begin": 0.0,
          "end": 5.189
        },
        {
          "text": "नि चि᳚न्मि॒षन्ता᳚ निचि॒रा नि चि᳚क्यतुः ॥",
          "begin": 5.189,
          "end": 9.28
        }
      ]
    },
    {
      "id": "8.25.10",
      "mandala": 8,
      "sukta": 25,
      "rik": 10,
      "padas": [
        {
          "text": "उ॒त नो᳚ दे॒व्यदि᳚तिरुरु॒ष्यतां॒ नास᳚त्या ।",
          "begin": 0.0,
          "end": 5.491
        },
        {
          "text": "उ॒रु॒ष्यन्तु᳚ म॒रुतो᳚ वृ॒द्धश᳚वसः ॥",
          "begin": 5.491,
          "end": 8.96
        }
      ]
    },
    {
      "id": "8.25.11",
      "mandala": 8,
      "sukta": 25,
      "rik": 11,
      "padas": [
        {
          "text": "ते नो᳚ ना॒वमु᳚रुष्यत॒ दिवा॒ नक्तं᳚ सुदानवः ।",
          "begin": 0.0,
          "end": 5.891
        },
        {
          "text": "अरि᳚ष्यन्तो॒ नि पा॒युभि᳚ः सचेमहि ॥",
          "begin": 5.891,
          "end": 9.16
        }
      ]
    },
    {
      "id": "8.25.12",
      "mandala": 8,
      "sukta": 25,
      "rik": 12,
      "padas": [
        {
          "text": "अघ्न᳚ते॒ विष्ण᳚वे व॒यमरि᳚ष्यन्तः सु॒दान᳚वे ।",
          "begin": 0.0,
          "end": 4.807
        },
        {
          "text": "श्रु॒धि स्व᳚यावन्त्सिन्धो पू॒र्वचि᳚त्तये ॥",
          "begin": 4.807,
          "end": 8.84
        }
      ]
    },
    {
      "id": "8.25.13",
      "mandala": 8,
      "sukta": 25,
      "rik": 13,
      "padas": [
        {
          "text": "तद्वार्यं᳚ वृणीमहे॒ वरि᳚ष्ठं गोप॒यत्य᳚म् ।",
          "begin": 0.0,
          "end": 4.844
        },
        {
          "text": "मि॒त्रो यत्पान्ति॒ वरु᳚णो॒ यद᳚र्य॒मा ॥",
          "begin": 4.844,
          "end": 9.72
        }
      ]
    },
    {
      "id": "8.25.14",
      "mandala": 8,
      "sukta": 25,
      "rik": 14,
      "padas": [
        {
          "text": "उ॒त न॒ः सिन्धु᳚र॒पां तन्म॒रुत॒स्तद॒श्विना᳚ ।",
          "begin": 0.0,
          "end": 5.156
        },
        {
          "text": "इन्द्रो॒ विष्णु᳚र्मी॒ढ्वांसः᳚ स॒जोष᳚सः ॥",
          "begin": 5.156,
          "end": 9.52
        }
      ]
    },
    {
      "id": "8.25.15",
      "mandala": 8,
      "sukta": 25,
      "rik": 15,
      "padas": [
        {
          "text": "ते हि ष्मा᳚ व॒नुषो॒ नरो॒॑भिमा᳚तिं॒ कय᳚स्य चित् ।",
          "begin": 0.0,
          "end": 5.364
        },
        {
          "text": "ति॒ग्मं न क्षोदः᳚ प्रति॒घ्नन्ति॒ भूर्ण᳚यः ॥",
          "begin": 5.364,
          "end": 9.68
        }
      ]
    },
    {
      "id": "8.25.16",
      "mandala": 8,
      "sukta": 25,
      "rik": 16,
      "padas": [
        {
          "text": "अ॒यमेक᳚ इ॒त्था पु॒रूरु च᳚ष्टे॒ वि वि॒श्पति᳚ः ।",
          "begin": 0.0,
          "end": 5.082
        },
        {
          "text": "तस्य᳚ व्र॒तान्यनु᳚ वश्चरामसि ॥",
          "begin": 5.082,
          "end": 8.64
        }
      ]
    },
    {
      "id": "8.25.17",
      "mandala": 8,
      "sukta": 25,
      "rik": 17,
      "padas": [
        {
          "text": "अनु॒ पूर्वा᳚ण्यो॒क्या᳚ साम्रा॒ज्यस्य᳚ सश्चिम ।",
          "begin": 0.0,
          "end": 6.593
        },
        {
          "text": "मि॒त्रस्य᳚ व्र॒ता वरु᳚णस्य दीर्घ॒श्रुत् ॥",
          "begin": 6.593,
          "end": 10.96
        }
      ]
    },
    {
      "id": "8.25.18",
      "mandala": 8,
      "sukta": 25,
      "rik": 18,
      "padas": [
        {
          "text": "परि॒ यो र॒श्मिना᳚ दि॒वो॑न्ता᳚न्म॒मे पृ᳚थि॒व्याः ।",
          "begin": 0.0,
          "end": 5.671
        },
        {
          "text": "उ॒भे आ प᳚प्रौ॒ रोद᳚सी महि॒त्वा ॥",
          "begin": 5.671,
          "end": 9.88
        }
      ]
    },
    {
      "id": "8.25.19",
      "mandala": 8,
      "sukta": 25,
      "rik": 19,
      "padas": [
        {
          "text": "उदु॒ ष्य श᳚र॒णे दि॒वो ज्योति᳚रयंस्त॒ सूर्य᳚ः ।",
          "begin": 0.0,
          "end": 5.174
        },
        {
          "text": "अ॒ग्निर्न शु॒क्रः स᳚मिधा॒न आहु᳚तः ॥",
          "begin": 5.174,
          "end": 9.28
        }
      ]
    },
    {
      "id": "8.25.20",
      "mandala": 8,
      "sukta": 25,
      "rik": 20,
      "padas": [
        {
          "text": "वचो᳚ दी॒र्घप्र᳚सद्म॒नीशे॒ वाज᳚स्य॒ गोम᳚तः ।",
          "begin": 0.0,
          "end": 5.948
        },
        {
          "text": "ईशे॒ हि पि॒त्वो᳚॑वि॒षस्य᳚ दा॒वने᳚ ॥",
          "begin": 5.948,
          "end": 10.2
        }
      ]
    },
    {
      "id": "8.25.21",
      "mandala": 8,
      "sukta": 25,
      "rik": 21,
      "padas": [
        {
          "text": "तत्सूर्यं॒ रोद᳚सी उ॒भे दो॒षा वस्तो॒रुप᳚ ब्रुवे ।",
          "begin": 0.0,
          "end": 6.491
        },
        {
          "text": "भो॒जेष्व॒स्माँ अ॒भ्युच्च᳚रा॒ सदा᳚ ॥",
          "begin": 6.491,
          "end": 10.6
        }
      ]
    },
    {
      "id": "8.25.22",
      "mandala": 8,
      "sukta": 25,
      "rik": 22,
      "padas": [
        {
          "text": "ऋ॒ज्रमु᳚क्ष॒ण्याय᳚ने रज॒तं हर᳚याणे ।",
          "begin": 0.0,
          "end": 4.884
        },
        {
          "text": "रथं᳚ यु॒क्तम᳚सनाम सु॒षाम᳚णि ॥",
          "begin": 4.884,
          "end": 8.12
        }
      ]
    },
    {
      "id": "8.25.23",
      "mandala": 8,
      "sukta": 25,
      "rik": 23,
      "padas": [
        {
          "text": "ता मे॒ अश्व्या᳚नां॒ हरी᳚णां नि॒तोश᳚ना ।",
          "begin": 0.0,
          "end": 5.744
        },
        {
          "text": "उ॒तो नु कृत्व्या᳚नां नृ॒वाह᳚सा ॥",
          "begin": 5.744,
          "end": 9.68
        }
      ]
    },
    {
      "id": "8.25.24",
      "mandala": 8,
      "sukta": 25,
      "rik": 24,
      "padas": [
        {
          "text": "स्मद᳚भीशू॒ कशा᳚वन्ता॒ विप्रा॒ नवि᳚ष्ठया म॒ती ।",
          "begin": 0.0,
          "end": 6.215
        },
        {
          "text": "म॒हो वा॒जिना॒वर्व᳚न्ता॒ सचा᳚सनम् ॥",
          "begin": 6.215,
          "end": 12.16
        }
      ]
    }
  ]
}