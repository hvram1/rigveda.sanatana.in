{
  "mandala": 8,
  "sukta": 30,
  "verses": [
    {
      "id": "8.30.1",
      "mandala": 8,
      "sukta": 30,
      "rik": 1,
      "padas": [
        {
          "text": "न॒हि वो॒ अस्त्य᳚र्भ॒को देवा᳚सो॒ न कु᳚मार॒कः ।",
          "begin": 0.0,
          "end": 7.987
        },
        {
          "text": "विश्वे᳚ स॒तोम᳚हान्त॒ इत् ॥",
          "begin": 7.987,
          "end": 11.48
        }
      ]
    },
    {
      "id": "8.30.2",
      "mandala": 8,
      "sukta": 30,
      "rik": 2,
      "padas": [
        {
          "text": "इति᳚ स्तु॒तासो᳚ असथा रिशादसो॒ ये स्थ त्रय᳚श्च त्रिं॒शच्च᳚ ।",
          "begin": 0.0,
          "end": 7.03
        },
        {
          "text": "मनो᳚र्देवा यज्ञियासः ॥",
          "begin": 7.03,
          "end": 10.4
        }
      ]
    },
    {
      "id": "8.30.3",
      "mandala": 8,
      "sukta": 30,
      "rik": 3,
      "padas": [
        {
          "text": "ते न᳚स्त्राध्वं॒ ते᳚॑वत॒ त उ᳚ नो॒ अधि᳚ वोचत ।",
          "begin": 0.0,
          "end": 3.538
        },
        {
          "text": "मा नः᳚ प॒थः पित्र्या᳚न्मान॒वादधि᳚ दू॒रं नै᳚ष्ट परा॒वतः᳚ ॥",
          "begin": 3.538,
          "end": 11.64
        }
      ]
    },
    {
      "id": "8.30.4",
      "mandala": 8,
      "sukta": 30,
      "rik": 4,
      "padas": [
        {
          "text": "ये दे᳚वास इ॒ह स्थन॒ विश्वे᳚ वैश्वान॒रा उ॒त ।",
          "begin": 0.0,
          "end": 4.794
        },
        {
          "text": "अ॒स्मभ्यं॒ शर्म᳚ स॒प्रथो॒ गवे॑श्वा᳚य यच्छत ॥",
          "begin": 4.794,
          "end": 11.96
        }
      ]
    }
  ]
}