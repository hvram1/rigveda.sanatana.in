{
  "mandala": 8,
  "sukta": 5,
  "verses": [
    {
      "id": "8.5.1",
      "mandala": 8,
      "sukta": 5,
      "rik": 1,
      "padas": [
        {
          "text": "दू॒रादि॒हेव॒ यत्स॒त्य᳚रु॒णप्सु॒रशि᳚श्वितत् ।",
          "begin": 0.0,
          "end": 4.915
        },
        {
          "text": "वि भा॒नुं वि॒श्वधा᳚तनत् ॥",
          "begin": 4.915,
          "end": 8.64
        }
      ]
    },
    {
      "id": "8.5.2",
      "mandala": 8,
      "sukta": 5,
      "rik": 2,
      "padas": [
        {
          "text": "नृ॒वद्द᳚स्रा मनो॒युजा॒ रथे᳚न पृथु॒पाज᳚सा ।",
          "begin": 0.0,
          "end": 5.74
        },
        {
          "text": "सचे᳚थे अश्विनो॒षस᳚म् ॥",
          "begin": 5.74,
          "end": 9.24
        }
      ]
    },
    {
      "id": "8.5.3",
      "mandala": 8,
      "sukta": 5,
      "rik": 3,
      "padas": [
        {
          "text": "यु॒वाभ्यां᳚ वाजिनीवसू॒ प्रति॒ स्तोमा᳚ अदृक्षत ।",
          "begin": 0.0,
          "end": 8.161
        },
        {
          "text": "वाचं᳚ दू॒तो यथो᳚हिषे ॥",
          "begin": 8.161,
          "end": 10.36
        }
      ]
    },
    {
      "id": "8.5.4",
      "mandala": 8,
      "sukta": 5,
      "rik": 4,
      "padas": [
        {
          "text": "पु॒रु॒प्रि॒या ण᳚ ऊ॒तये᳚ पुरुम॒न्द्रा पु᳚रू॒वसू᳚ ।",
          "begin": 0.0,
          "end": 5.996
        },
        {
          "text": "स्तु॒षे कण्वा᳚सो अ॒श्विना᳚ ॥",
          "begin": 5.996,
          "end": 9.64
        }
      ]
    },
    {
      "id": "8.5.5",
      "mandala": 8,
      "sukta": 5,
      "rik": 5,
      "padas": [
        {
          "text": "मंहि᳚ष्ठा वाज॒सात᳚मे॒षय᳚न्ता शु॒भस्पती᳚ ।",
          "begin": 0.0,
          "end": 5.429
        },
        {
          "text": "गन्ता᳚रा दा॒शुषो᳚ गृ॒हम् ॥",
          "begin": 5.429,
          "end": 9.76
        }
      ]
    },
    {
      "id": "8.5.6",
      "mandala": 8,
      "sukta": 5,
      "rik": 6,
      "padas": [
        {
          "text": "ता सु᳚दे॒वाय᳚ दा॒शुषे᳚ सुमे॒धामवि᳚तारिणीम् ।",
          "begin": 0.0,
          "end": 6.011
        },
        {
          "text": "घृ॒तैर्गव्यू᳚तिमुक्षतम् ॥",
          "begin": 6.011,
          "end": 9.24
        }
      ]
    },
    {
      "id": "8.5.7",
      "mandala": 8,
      "sukta": 5,
      "rik": 7,
      "padas": [
        {
          "text": "आ न॒ः स्तोम॒मुप᳚ द्र॒वत्तूयं᳚ श्ये॒नेभि᳚रा॒शुभि᳚ः ।",
          "begin": 0.0,
          "end": 5.764
        },
        {
          "text": "या॒तमश्वे᳚भिरश्विना ॥",
          "begin": 5.764,
          "end": 9.4
        }
      ]
    },
    {
      "id": "8.5.8",
      "mandala": 8,
      "sukta": 5,
      "rik": 8,
      "padas": [
        {
          "text": "येभि᳚स्ति॒स्रः प᳚रा॒वतो᳚ दि॒वो विश्वा᳚नि रोच॒ना ।",
          "begin": 0.0,
          "end": 6.225
        },
        {
          "text": "त्रीँर॒क्तून्प᳚रि॒दीय᳚थः ॥",
          "begin": 6.225,
          "end": 10.16
        }
      ]
    },
    {
      "id": "8.5.9",
      "mandala": 8,
      "sukta": 5,
      "rik": 9,
      "padas": [
        {
          "text": "उ॒त नो॒ गोम᳚ती॒रिष᳚ उ॒त सा॒तीर᳚हर्विदा ।",
          "begin": 0.0,
          "end": 4.965
        },
        {
          "text": "वि प॒थः सा॒तये᳚ सितम् ॥",
          "begin": 4.965,
          "end": 7.88
        }
      ]
    },
    {
      "id": "8.5.10",
      "mandala": 8,
      "sukta": 5,
      "rik": 10,
      "padas": [
        {
          "text": "आ नो॒ गोम᳚न्तमश्विना सु॒वीरं᳚ सु॒रथं᳚ र॒यिम् ।",
          "begin": 0.0,
          "end": 6.967
        },
        {
          "text": "वो॒ळ्हमश्वा᳚वती॒रिषः᳚ ॥",
          "begin": 6.967,
          "end": 10.36
        }
      ]
    },
    {
      "id": "8.5.11",
      "mandala": 8,
      "sukta": 5,
      "rik": 11,
      "padas": [
        {
          "text": "वा॒वृ॒धा॒ना शु᳚भस्पती दस्रा॒ हिर᳚ण्यवर्तनी ।",
          "begin": 0.0,
          "end": 5.255
        },
        {
          "text": "पिब᳚तं सो॒म्यं मधु᳚ ॥",
          "begin": 5.255,
          "end": 8.2
        }
      ]
    },
    {
      "id": "8.5.12",
      "mandala": 8,
      "sukta": 5,
      "rik": 12,
      "padas": [
        {
          "text": "अ॒स्मभ्यं᳚ वाजिनीवसू म॒घव᳚द्भ्यश्च स॒प्रथः᳚ ।",
          "begin": 0.0,
          "end": 5.974
        },
        {
          "text": "छ॒र्दिर्य᳚न्त॒मदा᳚भ्यम् ॥",
          "begin": 5.974,
          "end": 9.24
        }
      ]
    },
    {
      "id": "8.5.13",
      "mandala": 8,
      "sukta": 5,
      "rik": 13,
      "padas": [
        {
          "text": "नि षु ब्रह्म॒ जना᳚नां॒ यावि᳚ष्टं॒ तूय॒मा ग᳚तम् ।",
          "begin": 0.0,
          "end": 5.814
        },
        {
          "text": "मो ष्व१॒᳚न्याँ उपा᳚रतम् ॥",
          "begin": 5.814,
          "end": 10.2
        }
      ]
    },
    {
      "id": "8.5.14",
      "mandala": 8,
      "sukta": 5,
      "rik": 14,
      "padas": [
        {
          "text": "अ॒स्य पि᳚बतमश्विना यु॒वं मद᳚स्य॒ चारु᳚णः ।",
          "begin": 0.0,
          "end": 3.72
        },
        {
          "text": "मध्वो᳚ रा॒तस्य᳚ धिष्ण्या ॥",
          "begin": 3.72,
          "end": 8.2
        }
      ]
    },
    {
      "id": "8.5.15",
      "mandala": 8,
      "sukta": 5,
      "rik": 15,
      "padas": [
        {
          "text": "अ॒स्मे आ व᳚हतं र॒यिं श॒तव᳚न्तं सह॒स्रिण᳚म् ।",
          "begin": 0.0,
          "end": 6.013
        },
        {
          "text": "पु॒रु॒क्षुं वि॒श्वधा᳚यसम् ॥",
          "begin": 6.013,
          "end": 9.08
        }
      ]
    },
    {
      "id": "8.5.16",
      "mandala": 8,
      "sukta": 5,
      "rik": 16,
      "padas": [
        {
          "text": "पु॒रु॒त्रा चि॒द्धि वां᳚ नरा वि॒ह्वय᳚न्ते मनी॒षिणः᳚ ।",
          "begin": 0.0,
          "end": 5.117
        },
        {
          "text": "वा॒घद्भि᳚रश्वि॒ना ग᳚तम् ॥",
          "begin": 5.117,
          "end": 8.96
        }
      ]
    },
    {
      "id": "8.5.17",
      "mandala": 8,
      "sukta": 5,
      "rik": 17,
      "padas": [
        {
          "text": "जना᳚सो वृ॒क्तब᳚र्हिषो ह॒विष्म᳚न्तो अरं॒कृतः᳚ ।",
          "begin": 0.0,
          "end": 6.693
        },
        {
          "text": "यु॒वां ह᳚वन्ते अश्विना ॥",
          "begin": 6.693,
          "end": 9.84
        }
      ]
    },
    {
      "id": "8.5.18",
      "mandala": 8,
      "sukta": 5,
      "rik": 18,
      "padas": [
        {
          "text": "अ॒स्माक᳚म॒द्य वा᳚म॒यं स्तोमो॒ वाहि᳚ष्ठो॒ अन्त᳚मः ।",
          "begin": 0.0,
          "end": 6.754
        },
        {
          "text": "यु॒वाभ्यां᳚ भूत्वश्विना ॥",
          "begin": 6.754,
          "end": 10.28
        }
      ]
    },
    {
      "id": "8.5.19",
      "mandala": 8,
      "sukta": 5,
      "rik": 19,
      "padas": [
        {
          "text": "यो ह᳚ वां॒ मधु᳚नो॒ दृति॒राहि᳚तो रथ॒चर्ष᳚णे ।",
          "begin": 0.0,
          "end": 5.298
        },
        {
          "text": "ततः᳚ पिबतमश्विना ॥",
          "begin": 5.298,
          "end": 7.52
        }
      ]
    },
    {
      "id": "8.5.20",
      "mandala": 8,
      "sukta": 5,
      "rik": 20,
      "padas": [
        {
          "text": "तेन᳚ नो वाजिनीवसू॒ पश्वे᳚ तो॒काय॒ शं गवे᳚ ।",
          "begin": 0.0,
          "end": 6.481
        },
        {
          "text": "वह᳚तं॒ पीव᳚री॒रिषः᳚ ॥",
          "begin": 6.481,
          "end": 9.08
        }
      ]
    },
    {
      "id": "8.5.21",
      "mandala": 8,
      "sukta": 5,
      "rik": 21,
      "padas": [
        {
          "text": "उ॒त नो᳚ दि॒व्या इष᳚ उ॒त सिन्धूँ᳚रहर्विदा ।",
          "begin": 0.0,
          "end": 4.203
        },
        {
          "text": "अप॒ द्वारे᳚व वर्षथः ॥",
          "begin": 4.203,
          "end": 9.0
        }
      ]
    },
    {
      "id": "8.5.22",
      "mandala": 8,
      "sukta": 5,
      "rik": 22,
      "padas": [
        {
          "text": "क॒दा वां᳚ तौ॒ग्र्यो वि᳚धत्समु॒द्रे ज᳚हि॒तो न᳚रा ।",
          "begin": 0.0,
          "end": 6.074
        },
        {
          "text": "यद्वां॒ रथो॒ विभि॒ष्पता᳚त् ॥",
          "begin": 6.074,
          "end": 9.4
        }
      ]
    },
    {
      "id": "8.5.23",
      "mandala": 8,
      "sukta": 5,
      "rik": 23,
      "padas": [
        {
          "text": "यु॒वं कण्वा᳚य नासत्या॒ ऋपि᳚रिप्ताय ह॒र्म्ये ।",
          "begin": 0.0,
          "end": 5.62
        },
        {
          "text": "शश्व᳚दू॒तीर्द᳚शस्यथः ॥",
          "begin": 5.62,
          "end": 8.44
        }
      ]
    },
    {
      "id": "8.5.24",
      "mandala": 8,
      "sukta": 5,
      "rik": 24,
      "padas": [
        {
          "text": "ताभि॒रा या᳚तमू॒तिभि॒र्नव्य᳚सीभिः सुश॒स्तिभि᳚ः ।",
          "begin": 0.0,
          "end": 5.09
        },
        {
          "text": "यद्वां᳚ वृषण्वसू हु॒वे ॥",
          "begin": 5.09,
          "end": 9.32
        }
      ]
    },
    {
      "id": "8.5.25",
      "mandala": 8,
      "sukta": 5,
      "rik": 25,
      "padas": [
        {
          "text": "यथा᳚ चि॒त्कण्व॒माव᳚तं प्रि॒यमे᳚धमुपस्तु॒तम् ।",
          "begin": 0.0,
          "end": 6.664
        },
        {
          "text": "अत्रिं᳚ शि॒ञ्जार᳚मश्विना ॥",
          "begin": 6.664,
          "end": 9.32
        }
      ]
    },
    {
      "id": "8.5.26",
      "mandala": 8,
      "sukta": 5,
      "rik": 26,
      "padas": [
        {
          "text": "यथो॒त कृत्व्ये॒ धनें॒॑शुं गोष्व॒गस्त्य᳚म् ।",
          "begin": 0.0,
          "end": 4.943
        },
        {
          "text": "यथा॒ वाजे᳚षु॒ सोभ᳚रिम् ॥",
          "begin": 4.943,
          "end": 8.24
        }
      ]
    },
    {
      "id": "8.5.27",
      "mandala": 8,
      "sukta": 5,
      "rik": 27,
      "padas": [
        {
          "text": "ए॒ताव᳚द्वां वृषण्वसू॒ अतो᳚ वा॒ भूयो᳚ अश्विना ।",
          "begin": 0.0,
          "end": 6.796
        },
        {
          "text": "गृ॒णन्त᳚ः सु॒म्नमी᳚महे ॥",
          "begin": 6.796,
          "end": 9.92
        }
      ]
    },
    {
      "id": "8.5.28",
      "mandala": 8,
      "sukta": 5,
      "rik": 28,
      "padas": [
        {
          "text": "रथं॒ हिर᳚ण्यवन्धुरं॒ हिर᳚ण्याभीशुमश्विना ।",
          "begin": 0.0,
          "end": 6.268
        },
        {
          "text": "आ हि स्थाथो᳚ दिवि॒स्पृश᳚म् ॥",
          "begin": 6.268,
          "end": 9.72
        }
      ]
    },
    {
      "id": "8.5.29",
      "mandala": 8,
      "sukta": 5,
      "rik": 29,
      "padas": [
        {
          "text": "हि॒र॒ण्ययी᳚ वां॒ रभि᳚री॒षा अक्षो᳚ हिर॒ण्ययः᳚ ।",
          "begin": 0.0,
          "end": 5.487
        },
        {
          "text": "उ॒भा च॒क्रा हि᳚र॒ण्यया᳚ ॥",
          "begin": 5.487,
          "end": 9.92
        }
      ]
    },
    {
      "id": "8.5.30",
      "mandala": 8,
      "sukta": 5,
      "rik": 30,
      "padas": [
        {
          "text": "तेन᳚ नो वाजिनीवसू परा॒वत᳚श्चि॒दा ग᳚तम् ।",
          "begin": 0.0,
          "end": 4.811
        },
        {
          "text": "उपे॒मां सु᳚ष्टु॒तिं मम᳚ ॥",
          "begin": 4.811,
          "end": 7.92
        }
      ]
    },
    {
      "id": "8.5.31",
      "mandala": 8,
      "sukta": 5,
      "rik": 31,
      "padas": [
        {
          "text": "आ व᳚हेथे परा॒कात्पू॒र्वीर॒श्नन्ता᳚वश्विना ।",
          "begin": 0.0,
          "end": 6.488
        },
        {
          "text": "इषो॒ दासी᳚रमर्त्या ॥",
          "begin": 6.488,
          "end": 9.44
        }
      ]
    },
    {
      "id": "8.5.32",
      "mandala": 8,
      "sukta": 5,
      "rik": 32,
      "padas": [
        {
          "text": "आ नो᳚ द्यु॒म्नैरा श्रवो᳚भि॒रा रा॒या या᳚तमश्विना ।",
          "begin": 0.0,
          "end": 7.256
        },
        {
          "text": "पुरु᳚श्चन्द्रा॒ नास᳚त्या ॥",
          "begin": 7.256,
          "end": 10.08
        }
      ]
    },
    {
      "id": "8.5.33",
      "mandala": 8,
      "sukta": 5,
      "rik": 33,
      "padas": [
        {
          "text": "एह वां᳚ प्रुषि॒तप्स᳚वो॒ वयो᳚ वहन्तु प॒र्णिनः᳚ ।",
          "begin": 0.0,
          "end": 6.586
        },
        {
          "text": "अच्छा᳚ स्वध्व॒रं जन᳚म् ॥",
          "begin": 6.586,
          "end": 9.76
        }
      ]
    },
    {
      "id": "8.5.34",
      "mandala": 8,
      "sukta": 5,
      "rik": 34,
      "padas": [
        {
          "text": "रथं᳚ वा॒मनु᳚गायसं॒ य इ॒षा वर्त᳚ते स॒ह ।",
          "begin": 0.0,
          "end": 5.586
        },
        {
          "text": "न च॒क्रम॒भि बा᳚धते ॥",
          "begin": 5.586,
          "end": 8.28
        }
      ]
    },
    {
      "id": "8.5.35",
      "mandala": 8,
      "sukta": 5,
      "rik": 35,
      "padas": [
        {
          "text": "हि॒र॒ण्यये᳚न॒ रथे᳚न द्र॒वत्पा᳚णिभि॒रश्वै᳚ः ।",
          "begin": 0.0,
          "end": 6.796
        },
        {
          "text": "धीज᳚वना॒ नास᳚त्या ॥",
          "begin": 6.796,
          "end": 9.4
        }
      ]
    },
    {
      "id": "8.5.36",
      "mandala": 8,
      "sukta": 5,
      "rik": 36,
      "padas": [
        {
          "text": "यु॒वं मृ॒गं जा᳚गृ॒वांसं॒ स्वद᳚थो वा वृषण्वसू ।",
          "begin": 0.0,
          "end": 5.564
        },
        {
          "text": "ता नः᳚ पृङ्क्तमि॒षा र॒यिम् ॥",
          "begin": 5.564,
          "end": 8.96
        }
      ]
    },
    {
      "id": "8.5.37",
      "mandala": 8,
      "sukta": 5,
      "rik": 37,
      "padas": [
        {
          "text": "ता मे᳚ अश्विना सनी॒नां वि॒द्यातं॒ नवा᳚नाम् ।",
          "begin": 0.0,
          "end": 6.429
        },
        {
