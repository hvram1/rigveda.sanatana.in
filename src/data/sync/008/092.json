{
  "mandala": 8,
  "sukta": 92,
  "verses": [
    {
      "id": "8.92.1",
      "mandala": 8,
      "sukta": 92,
      "rik": 1,
      "padas": [
        {
          "text": "पान्त॒मा वो॒ अन्ध᳚स॒ इन्द्र᳚म॒भि प्र गा᳚यत ।",
          "begin": 0.0,
          "end": 9.516
        },
        {
          "text": "वि॒श्वा॒साहं᳚ श॒तक्र᳚तुं॒ मंहि᳚ष्ठं चर्षणी॒नाम् ॥",
          "begin": 9.516,
          "end": 15.04
        }
      ]
    },
    {
      "id": "8.92.2",
      "mandala": 8,
      "sukta": 92,
      "rik": 2,
      "padas": [
        {
          "text": "पु॒रु॒हू॒तं पु᳚रुष्टु॒तं गा᳚था॒न्यं१॒᳚ सन᳚श्रुतम् ।",
          "begin": 0.0,
          "end": 6.163
        },
        {
          "text": "इन्द्र॒ इति᳚ ब्रवीतन ॥",
          "begin": 6.163,
          "end": 8.36
        }
      ]
    },
    {
      "id": "8.92.3",
      "mandala": 8,
      "sukta": 92,
      "rik": 3,
      "padas": [
        {
          "text": "इन्द्र॒ इन्नो᳚ म॒हानां᳚ दा॒ता वाजा᳚नां नृ॒तुः ।",
          "begin": 0.0,
          "end": 6.508
        },
        {
          "text": "म॒हाँ अ᳚भि॒ज्ञ्वा य᳚मत् ॥",
          "begin": 6.508,
          "end": 9.08
        }
      ]
    },
    {
      "id": "8.92.4",
      "mandala": 8,
      "sukta": 92,
      "rik": 4,
      "padas": [
        {
          "text": "अपा᳚दु शि॒प्र्यन्ध᳚सः सु॒दक्ष᳚स्य प्रहो॒षिणः᳚ ।",
          "begin": 0.0,
          "end": 5.367
        },
        {
          "text": "इन्दो॒रिन्द्रो॒ यवा᳚शिरः ॥",
          "begin": 5.367,
          "end": 8.32
        }
      ]
    },
    {
      "id": "8.92.5",
      "mandala": 8,
      "sukta": 92,
      "rik": 5,
      "padas": [
        {
          "text": "तम्व॒भि प्रार्च॒तेन्द्रं॒ सोम᳚स्य पी॒तये᳚ ।",
          "begin": 0.0,
          "end": 4.99
        },
        {
          "text": "तदिद्ध्य᳚स्य॒ वर्ध᳚नम् ॥",
          "begin": 4.99,
          "end": 7.12
        }
      ]
    },
    {
      "id": "8.92.6",
      "mandala": 8,
      "sukta": 92,
      "rik": 6,
      "padas": [
        {
          "text": "अ॒स्य पी॒त्वा मदा᳚नां दे॒वो दे॒वस्यौज᳚सा ।",
          "begin": 0.0,
          "end": 5.474
        },
        {
          "text": "विश्वा॒भि भुव᳚ना भुवत् ॥",
          "begin": 5.474,
          "end": 7.96
        }
      ]
    },
    {
      "id": "8.92.7",
      "mandala": 8,
      "sukta": 92,
      "rik": 7,
      "padas": [
        {
          "text": "त्यमु᳚ वः सत्रा॒साहं॒ विश्वा᳚सु गी॒र्ष्वाय᳚तम् ।",
          "begin": 0.0,
          "end": 5.85
        },
        {
          "text": "आ च्या᳚वयस्यू॒तये᳚ ॥",
          "begin": 5.85,
          "end": 8.72
        }
      ]
    },
    {
      "id": "8.92.8",
      "mandala": 8,
      "sukta": 92,
      "rik": 8,
      "padas": [
        {
          "text": "यु॒ध्मं सन्त᳚मन॒र्वाणं᳚ सोम॒पामन᳚पच्युतम् ।",
          "begin": 0.0,
          "end": 5.021
        },
        {
          "text": "नर᳚मवा॒र्यक्र᳚तुम् ॥",
          "begin": 5.021,
          "end": 7.44
        }
      ]
    },
    {
      "id": "8.92.9",
      "mandala": 8,
      "sukta": 92,
      "rik": 9,
      "padas": [
        {
          "text": "शिक्षा᳚ ण इन्द्र रा॒य आ पु॒रु वि॒द्वाँ ऋ᳚चीषम ।",
          "begin": 0.0,
          "end": 6.147
        },
        {
          "text": "अवा᳚ न॒ः पार्ये॒ धने᳚ ॥",
          "begin": 6.147,
          "end": 8.48
        }
      ]
    },
    {
      "id": "8.92.10",
      "mandala": 8,
      "sukta": 92,
      "rik": 10,
      "padas": [
        {
          "text": "अत᳚श्चिदिन्द्र ण॒ उपा या᳚हि श॒तवा᳚जया ।",
          "begin": 0.0,
          "end": 5.095
        },
        {
          "text": "इ॒षा स॒हस्र᳚वाजया ॥",
          "begin": 5.095,
          "end": 7.16
        }
      ]
    },
    {
      "id": "8.92.11",
      "mandala": 8,
      "sukta": 92,
      "rik": 11,
      "padas": [
        {
          "text": "अया᳚म॒ धीव᳚तो॒ धियो॑र्व᳚द्भिः शक्र गोदरे ।",
          "begin": 0.0,
          "end": 5.343
        },
        {
          "text": "जये᳚म पृ॒त्सु व᳚ज्रिवः ॥",
          "begin": 5.343,
          "end": 7.96
        }
      ]
    },
    {
      "id": "8.92.12",
      "mandala": 8,
      "sukta": 92,
      "rik": 12,
      "padas": [
        {
          "text": "व॒यमु᳚ त्वा शतक्रतो॒ गावो॒ न यव᳚से॒ष्वा ।",
          "begin": 0.0,
          "end": 4.98
        },
        {
          "text": "उ॒क्थेषु᳚ रणयामसि ॥",
          "begin": 4.98,
          "end": 7.0
        }
      ]
    },
    {
      "id": "8.92.13",
      "mandala": 8,
      "sukta": 92,
      "rik": 13,
      "padas": [
        {
          "text": "विश्वा॒ हि म᳚र्त्यत्व॒नानु᳚का॒मा श᳚तक्रतो ।",
          "begin": 0.0,
          "end": 4.658
        },
        {
          "text": "अग᳚न्म वज्रिन्ना॒शसः᳚ ॥",
          "begin": 4.658,
          "end": 7.32
        }
      ]
    },
    {
      "id": "8.92.14",
      "mandala": 8,
      "sukta": 92,
      "rik": 14,
      "padas": [
        {
          "text": "त्वे सु पु᳚त्र शव॒सो॑वृ᳚त्र॒न्काम᳚कातयः ।",
          "begin": 0.0,
          "end": 4.263
        },
        {
          "text": "न त्वामि॒न्द्राति᳚ रिच्यते ॥",
          "begin": 4.263,
          "end": 6.92
        }
      ]
    },
    {
      "id": "8.92.15",
      "mandala": 8,
      "sukta": 92,
      "rik": 15,
      "padas": [
        {
          "text": "स नो᳚ वृष॒न्त्सनि᳚ष्ठया॒ सं घो॒रया᳚ द्रवि॒त्न्वा ।",
          "begin": 0.0,
          "end": 5.7
        },
        {
          "text": "धि॒यावि᳚ड्ढि॒ पुरं᳚ध्या ॥",
          "begin": 5.7,
          "end": 8.44
        }
      ]
    },
    {
      "id": "8.92.16",
      "mandala": 8,
      "sukta": 92,
      "rik": 16,
      "padas": [
        {
          "text": "यस्ते᳚ नू॒नं श᳚तक्रत॒विन्द्र᳚ द्यु॒म्नित᳚मो॒ मदः᳚ ।",
          "begin": 0.0,
          "end": 5.704
        },
        {
          "text": "तेन᳚ नू॒नं मदे᳚ मदेः ॥",
          "begin": 5.704,
          "end": 8.64
        }
      ]
    },
    {
      "id": "8.92.17",
      "mandala": 8,
      "sukta": 92,
      "rik": 17,
      "padas": [
        {
          "text": "यस्ते᳚ चि॒त्रश्र᳚वस्तमो॒ य इ᳚न्द्र वृत्र॒हन्त᳚मः ।",
          "begin": 0.0,
          "end": 5.881
        },
        {
          "text": "य ओ᳚जो॒दात᳚मो॒ मदः᳚ ॥",
          "begin": 5.881,
          "end": 8.96
        }
      ]
    },
    {
      "id": "8.92.18",
      "mandala": 8,
      "sukta": 92,
      "rik": 18,
      "padas": [
        {
          "text": "वि॒द्मा हि यस्ते᳚ अद्रिव॒स्त्वाद᳚त्तः सत्य सोमपाः ।",
          "begin": 0.0,
          "end": 5.652
        },
        {
          "text": "विश्वा᳚सु दस्म कृ॒ष्टिषु᳚ ॥",
          "begin": 5.652,
          "end": 8.72
        }
      ]
    },
    {
      "id": "8.92.19",
      "mandala": 8,
      "sukta": 92,
      "rik": 19,
      "padas": [
        {
          "text": "इन्द्रा᳚य॒ मद्व᳚ने सु॒तं परि᳚ ष्टोभन्तु नो॒ गिरः᳚ ।",
          "begin": 0.0,
          "end": 5.758
        },
        {
          "text": "अ॒र्कम᳚र्चन्तु का॒रवः᳚ ॥",
          "begin": 5.758,
          "end": 8.2
        }
      ]
    },
    {
      "id": "8.92.20",
      "mandala": 8,
      "sukta": 92,
      "rik": 20,
      "padas": [
        {
          "text": "यस्मि॒न्विश्वा॒ अधि॒ श्रियो॒ रण᳚न्ति स॒प्त सं॒सदः᳚ ।",
          "begin": 0.0,
          "end": 5.777
        },
        {
          "text": "इन्द्रं᳚ सु॒ते ह᳚वामहे ॥",
          "begin": 5.777,
          "end": 8.4
        }
      ]
    },
    {
      "id": "8.92.21",
      "mandala": 8,
      "sukta": 92,
      "rik": 21,
      "padas": [
        {
          "text": "त्रिक᳚द्रुकेषु॒ चेत᳚नं दे॒वासो᳚ य॒ज्ञम᳚त्नत ।",
          "begin": 0.0,
          "end": 5.373
        },
        {
          "text": "तमिद्व᳚र्धन्तु नो॒ गिरः᳚ ॥",
          "begin": 5.373,
          "end": 7.92
        }
      ]
    },
    {
      "id": "8.92.22",
      "mandala": 8,
      "sukta": 92,
      "rik": 22,
      "padas": [
        {
          "text": "आ त्वा᳚ विश॒न्त्विन्द᳚वः समु॒द्रमि᳚व॒ सिन्ध᳚वः ।",
          "begin": 0.0,
          "end": 5.361
        },
        {
          "text": "न त्वामि॒न्द्राति᳚ रिच्यते ॥",
          "begin": 5.361,
          "end": 7.96
        }
      ]
    },
    {
      "id": "8.92.23",
      "mandala": 8,
      "sukta": 92,
      "rik": 23,
      "padas": [
        {
          "text": "वि॒व्यक्थ᳚ महि॒ना वृ᳚षन्भ॒क्षं सोम᳚स्य जागृवे ।",
          "begin": 0.0,
          "end": 5.178
        },
        {
          "text": "य इ᳚न्द्र ज॒ठरे᳚षु ते ॥",
          "begin": 5.178,
          "end": 7.68
        }
      ]
    },
    {
      "id": "8.92.24",
      "mandala": 8,
      "sukta": 92,
      "rik": 24,
      "padas": [
        {
          "text": "अरं᳚ त इन्द्र कु॒क्षये॒ सोमो᳚ भवतु वृत्रहन् ।",
          "begin": 0.0,
          "end": 5.864
        },
        {
          "text": "अरं॒ धाम᳚भ्य॒ इन्द᳚वः ॥",
          "begin": 5.864,
          "end": 8.2
        }
      ]
    },
    {
      "id": "8.92.25",
      "mandala": 8,
      "sukta": 92,
      "rik": 25,
      "padas": [
        {
          "text": "अर॒मश्वा᳚य गायति श्रु॒तक᳚क्षो॒ अरं॒ गवे᳚ ।",
          "begin": 0.0,
          "end": 5.834
        },
        {
          "text": "अर॒मिन्द्र᳚स्य॒ धाम्ने᳚ ॥",
          "begin": 5.834,
          "end": 8.56
        }
      ]
    },
    {
      "id": "8.92.26",
      "mandala": 8,
      "sukta": 92,
      "rik": 26,
      "padas": [
        {
          "text": "अरं॒ हि ष्म᳚ सु॒तेषु᳚ ण॒ः सोमे᳚ष्विन्द्र॒ भूष᳚सि ।",
          "begin": 0.0,
          "end": 6.079
        },
        {
          "text": "अरं᳚ ते शक्र दा॒वने᳚ ॥",
          "begin": 6.079,
          "end": 8.92
        }
      ]
    },
    {
      "id": "8.92.27",
      "mandala": 8,
      "sukta": 92,
      "rik": 27,
      "padas": [
        {
          "text": "प॒रा॒कात्ता᳚च्चिदद्रिव॒स्त्वां न᳚क्षन्त नो॒ गिरः᳚ ।",
          "begin": 0.0,
          "end": 5.922
        },
        {
          "text": "अरं᳚ गमाम ते व॒यम् ॥",
          "begin": 5.922,
          "end": 8.72
        }
      ]
    },
    {
      "id": "8.92.28",
      "mandala": 8,
      "sukta": 92,
      "rik": 28,
      "padas": [
        {
          "text": "ए॒वा ह्यसि᳚ वीर॒युरे॒वा शूर᳚ उ॒त स्थि॒रः ।",
          "begin": 0.0,
          "end": 5.272
        },
        {
          "text": "ए॒वा ते॒ राध्यं॒ मनः᳚ ॥",
          "begin": 5.272,
          "end": 8.0
        }
      ]
    },
    {
      "id": "8.92.29",
      "mandala": 8,
      "sukta": 92,
      "rik": 29,
      "padas": [
        {
          "text": "ए॒वा रा॒तिस्तु᳚वीमघ॒ विश्वे᳚भिर्धायि धा॒तृभि᳚ः ।",
          "begin": 0.0,
          "end": 6.007
        },
        {
          "text": "अधा᳚ चिदिन्द्र मे॒ सचा᳚ ॥",
          "begin": 6.007,
          "end": 8.96
        }
      ]
    },
    {
      "id": "8.92.30",
      "mandala": 8,
      "sukta": 92,
      "rik": 30,
      "padas": [
        {
          "text": "मो षु ब्र॒ह्मेव᳚ तन्द्र॒युर्भुवो᳚ वाजानां पते ।",
          "begin": 0.0,
          "end": 5.939
        },
        {
          "text": "मत्स्वा᳚ सु॒तस्य॒ गोम᳚तः ॥",
          "begin": 5.939,
          "end": 8.76
        }
      ]
    },
    {
      "id": "8.92.31",
      "mandala": 8,
      "sukta": 92,
      "rik": 31,
      "padas": [
        {
          "text": "मा न᳚ इन्द्रा॒भ्या॒३॒᳚दिश॒ः सूरो᳚ अ॒क्तुष्वा य᳚मन् ।",
          "begin": 0.0,
          "end": 7.79
        },
        {
          "text": "त्वा यु॒जा व᳚नेम॒ तत् ॥",
          "begin": 7.79,
          "end": 10.2
        }
      ]
    },
    {
      "id": "8.92.32",
      "mandala": 8,
      "sukta": 92,
      "rik": 32,
      "padas": [
        {
          "text": "त्वयेदि᳚न्द्र यु॒जा व॒यं प्रति᳚ ब्रुवीमहि॒ स्पृधः᳚ ।",
          "begin": 0.0,
          "end": 5.241
        },
        {
          "text": "त्वम॒स्माकं॒ तव᳚ स्मसि ॥",
          "begin": 5.241,
          "end": 7.72
        }
      ]
    },
    {
      "id": "8.92.33",
      "mandala": 8,
      "sukta": 92,
      "rik": 33,
      "padas": [
        {
          "text": "त्वामिद्धि त्वा॒यवो᳚॑नु॒नोनु᳚वत॒श्चरा᳚न् ।",
          "begin": 0.0,
          "end": 5.61
        },
        {
          "text": "सखा᳚य इन्द्र का॒रवः᳚ ॥",
          "begin": 5.61,
          "end": 9.96
        }
      ]
    }
  ]
}