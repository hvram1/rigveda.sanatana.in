{
  "mandala": 8,
  "sukta": 13,
  "verses": [
    {
      "id": "8.13.1",
      "mandala": 8,
      "sukta": 13,
      "rik": 1,
      "padas": [
        {
          "text": "इन्द्र᳚ः सु॒तेषु॒ सोमे᳚षु॒ क्रतुं᳚ पुनीत उ॒क्थ्य᳚म् ।",
          "begin": 0.0,
          "end": 11.012
        },
        {
          "text": "वि॒दे वृ॒धस्य॒ दक्ष᳚सो म॒हान्हि षः ॥",
          "begin": 11.012,
          "end": 17.12
        }
      ]
    },
    {
      "id": "8.13.2",
      "mandala": 8,
      "sukta": 13,
      "rik": 2,
      "padas": [
        {
          "text": "स प्र᳚थ॒मे व्यो᳚मनि दे॒वानां॒ सद᳚ने वृ॒धः ।",
          "begin": 0.0,
          "end": 4.936
        },
        {
          "text": "सु॒पा॒रः सु॒श्रव᳚स्तम॒ः सम᳚प्सु॒जित् ॥",
          "begin": 4.936,
          "end": 8.28
        }
      ]
    },
    {
      "id": "8.13.3",
      "mandala": 8,
      "sukta": 13,
      "rik": 3,
      "padas": [
        {
          "text": "तम᳚ह्वे॒ वाज᳚सातय॒ इन्द्रं॒ भरा᳚य शु॒ष्मिण᳚म् ।",
          "begin": 0.0,
          "end": 5.077
        },
        {
          "text": "भवा᳚ नः सु॒म्ने अन्त᳚म॒ः सखा᳚ वृ॒धे ॥",
          "begin": 5.077,
          "end": 9.8
        }
      ]
    },
    {
      "id": "8.13.4",
      "mandala": 8,
      "sukta": 13,
      "rik": 4,
      "padas": [
        {
          "text": "इ॒यं त᳚ इन्द्र गिर्वणो रा॒तिः क्ष᳚रति सुन्व॒तः ।",
          "begin": 0.0,
          "end": 4.863
        },
        {
          "text": "म॒न्दा॒नो अ॒स्य ब॒र्हिषो॒ वि रा᳚जसि ॥",
          "begin": 4.863,
          "end": 8.56
        }
      ]
    },
    {
      "id": "8.13.5",
      "mandala": 8,
      "sukta": 13,
      "rik": 5,
      "padas": [
        {
          "text": "नू॒नं तदि᳚न्द्र दद्धि नो॒ यत्त्वा᳚ सु॒न्वन्त॒ ईम᳚हे ।",
          "begin": 0.0,
          "end": 5.709
        },
        {
          "text": "र॒यिं न᳚श्चि॒त्रमा भ᳚रा स्व॒र्विद᳚म् ॥",
          "begin": 5.709,
          "end": 9.32
        }
      ]
    },
    {
      "id": "8.13.6",
      "mandala": 8,
      "sukta": 13,
      "rik": 6,
      "padas": [
        {
          "text": "स्तो॒ता यत्ते॒ विच᳚र्षणिरतिप्रश॒र्धय॒द्गिरः᳚ ।",
          "begin": 0.0,
          "end": 4.998
        },
        {
          "text": "व॒या इ॒वानु᳚ रोहते जु॒षन्त॒ यत् ॥",
          "begin": 4.998,
          "end": 8.56
        }
      ]
    },
    {
      "id": "8.13.7",
      "mandala": 8,
      "sukta": 13,
      "rik": 7,
      "padas": [
        {
          "text": "प्र॒त्न॒वज्ज᳚नया॒ गिरः᳚ शृणु॒धी ज᳚रि॒तुर्हव᳚म् ।",
          "begin": 0.0,
          "end": 4.212
        },
        {
          "text": "मदे᳚मदे ववक्षिथा सु॒कृत्व᳚ने ॥",
          "begin": 4.212,
          "end": 7.72
        }
      ]
    },
    {
      "id": "8.13.8",
      "mandala": 8,
      "sukta": 13,
      "rik": 8,
      "padas": [
        {
          "text": "क्रीळ᳚न्त्यस्य सू॒नृता॒ आपो॒ न प्र॒वता᳚ य॒तीः ।",
          "begin": 0.0,
          "end": 5.543
        },
        {
          "text": "अ॒या धि॒या य उ॒च्यते॒ पति᳚र्दि॒वः ॥",
          "begin": 5.543,
          "end": 9.32
        }
      ]
    },
    {
      "id": "8.13.9",
      "mandala": 8,
      "sukta": 13,
      "rik": 9,
      "padas": [
        {
          "text": "उ॒तो पति॒र्य उ॒च्यते᳚ कृष्टी॒नामेक॒ इद्व॒शी ।",
          "begin": 0.0,
          "end": 4.913
        },
        {
          "text": "न॒मो॒वृ॒धैर᳚व॒स्युभि᳚ः सु॒ते र᳚ण ॥",
          "begin": 4.913,
          "end": 8.24
        }
      ]
    },
    {
      "id": "8.13.10",
      "mandala": 8,
      "sukta": 13,
      "rik": 10,
      "padas": [
        {
          "text": "स्तु॒हि श्रु॒तं वि᳚प॒श्चितं॒ हरी॒ यस्य᳚ प्रस॒क्षिणा᳚ ।",
          "begin": 0.0,
          "end": 7.246
        },
        {
          "text": "गन्ता᳚रा दा॒शुषो᳚ गृ॒हं न᳚म॒स्विनः᳚ ॥",
          "begin": 7.246,
          "end": 9.64
        }
      ]
    },
    {
      "id": "8.13.11",
      "mandala": 8,
      "sukta": 13,
      "rik": 11,
      "padas": [
        {
          "text": "तू॒तु॒जा॒नो म᳚हेम॒ते॑श्वे᳚भिः प्रुषि॒तप्सु᳚भिः ।",
          "begin": 0.0,
          "end": 4.879
        },
        {
          "text": "आ या᳚हि य॒ज्ञमा॒शुभि॒ः शमिद्धि ते᳚ ॥",
          "begin": 4.879,
          "end": 9.32
        }
      ]
    },
    {
      "id": "8.13.12",
      "mandala": 8,
      "sukta": 13,
      "rik": 12,
      "padas": [
        {
          "text": "इन्द्र᳚ शविष्ठ सत्पते र॒यिं गृ॒णत्सु᳚ धारय ।",
          "begin": 0.0,
          "end": 4.552
        },
        {
          "text": "श्रवः᳚ सू॒रिभ्यो᳚ अ॒मृतं᳚ वसुत्व॒नम् ॥",
          "begin": 4.552,
          "end": 8.64
        }
      ]
    },
    {
      "id": "8.13.13",
      "mandala": 8,
      "sukta": 13,
      "rik": 13,
      "padas": [
        {
          "text": "हवे᳚ त्वा॒ सूर॒ उदि᳚ते॒ हवे᳚ म॒ध्यंदि᳚ने दि॒वः ।",
          "begin": 0.0,
          "end": 5.732
        },
        {
          "text": "जु॒षा॒ण इ᳚न्द्र॒ सप्ति᳚भिर्न॒ आ ग᳚हि ॥",
          "begin": 5.732,
          "end": 9.32
        }
      ]
    },
    {
      "id": "8.13.14",
      "mandala": 8,
      "sukta": 13,
      "rik": 14,
      "padas": [
        {
          "text": "आ तू ग᳚हि॒ प्र तु द्र᳚व॒ मत्स्वा᳚ सु॒तस्य॒ गोम᳚तः ।",
          "begin": 0.0,
          "end": 5.399
        },
        {
          "text": "तन्तुं᳚ तनुष्व पू॒र्व्यं यथा᳚ वि॒दे ॥",
          "begin": 5.399,
          "end": 9.6
        }
      ]
    },
    {
      "id": "8.13.15",
      "mandala": 8,
      "sukta": 13,
      "rik": 15,
      "padas": [
        {
          "text": "यच्छ॒क्रासि᳚ परा॒वति॒ यद᳚र्वा॒वति᳚ वृत्रहन् ।",
          "begin": 0.0,
          "end": 5.09
        },
        {
          "text": "यद्वा᳚ समु॒द्रे अन्ध᳚सो॑वि॒तेद᳚सि ॥",
          "begin": 5.09,
          "end": 8.88
        }
      ]
    },
    {
      "id": "8.13.16",
      "mandala": 8,
      "sukta": 13,
      "rik": 16,
      "padas": [
        {
          "text": "इन्द्रं᳚ वर्धन्तु नो॒ गिर॒ इन्द्रं᳚ सु॒तास॒ इन्द᳚वः ।",
          "begin": 0.0,
          "end": 5.719
        },
        {
          "text": "इन्द्रे᳚ ह॒विष्म᳚ती॒र्विशो᳚ अराणिषुः ॥",
          "begin": 5.719,
          "end": 9.88
        }
      ]
    },
    {
      "id": "8.13.17",
      "mandala": 8,
      "sukta": 13,
      "rik": 17,
      "padas": [
        {
          "text": "तमिद्विप्रा᳚ अव॒स्यवः᳚ प्र॒वत्व᳚तीभिरू॒तिभि᳚ः ।",
          "begin": 0.0,
          "end": 4.599
        },
        {
          "text": "इन्द्रं᳚ क्षो॒णीर᳚वर्धयन्व॒या इ᳚व ॥",
          "begin": 4.599,
          "end": 9.56
        }
      ]
    },
    {
      "id": "8.13.18",
      "mandala": 8,
      "sukta": 13,
      "rik": 18,
      "padas": [
        {
          "text": "त्रिक᳚द्रुकेषु॒ चेत᳚नं दे॒वासो᳚ य॒ज्ञम᳚त्नत ।",
          "begin": 0.0,
          "end": 5.101
        },
        {
          "text": "तमिद्व᳚र्धन्तु नो॒ गिरः᳚ स॒दावृ᳚धम् ॥",
          "begin": 5.101,
          "end": 8.48
        }
      ]
    },
    {
      "id": "8.13.19",
      "mandala": 8,
      "sukta": 13,
      "rik": 19,
      "padas": [
        {
          "text": "स्तो॒ता यत्ते॒ अनु᳚व्रत उ॒क्थान्यृ᳚तु॒था द॒धे ।",
          "begin": 0.0,
          "end": 5.683
        },
        {
          "text": "शुचि᳚ः पाव॒क उ᳚च्यते॒ सो अद्भु᳚तः ॥",
          "begin": 5.683,
          "end": 9.52
        }
      ]
    },
    {
      "id": "8.13.20",
      "mandala": 8,
      "sukta": 13,
      "rik": 20,
      "padas": [
        {
          "text": "तदिद्रु॒द्रस्य᳚ चेतति य॒ह्वं प्र॒त्नेषु॒ धाम᳚सु ।",
          "begin": 0.0,
          "end": 4.814
        },
        {
          "text": "मनो॒ यत्रा॒ वि तद्द॒धुर्विचे᳚तसः ॥",
          "begin": 4.814,
          "end": 8.72
        }
      ]
    },
    {
      "id": "8.13.21",
      "mandala": 8,
      "sukta": 13,
      "rik": 21,
      "padas": [
        {
          "text": "यदि᳚ मे स॒ख्यमा॒वर᳚ इ॒मस्य᳚ पा॒ह्यन्ध᳚सः ।",
          "begin": 0.0,
          "end": 4.823
        },
        {
          "text": "येन॒ विश्वा॒ अति॒ द्विषो॒ अता᳚रिम ॥",
          "begin": 4.823,
          "end": 8.84
        }
      ]
    },
    {
      "id": "8.13.22",
      "mandala": 8,
      "sukta": 13,
      "rik": 22,
      "padas": [
        {
          "text": "क॒दा त᳚ इन्द्र गिर्वणः स्तो॒ता भ᳚वाति॒ शंत᳚मः ।",
          "begin": 0.0,
          "end": 4.029
        },
        {
          "text": "क॒दा नो॒ गव्ये॒ अश्व्ये॒ वसौ᳚ दधः ॥",
          "begin": 4.029,
          "end": 8.88
        }
      ]
    },
    {
      "id": "8.13.23",
      "mandala": 8,
      "sukta": 13,
      "rik": 23,
      "padas": [
        {
          "text": "उ॒त ते॒ सुष्टु᳚ता॒ हरी॒ वृष᳚णा वहतो॒ रथ᳚म् ।",
          "begin": 0.0,
          "end": 4.562
        },
        {
          "text": "अ॒जु॒र्यस्य᳚ म॒दिन्त᳚मं॒ यमीम᳚हे ॥",
          "begin": 4.562,
          "end": 7.96
        }
      ]
    },
    {
      "id": "8.13.24",
      "mandala": 8,
      "sukta": 13,
      "rik": 24,
      "padas": [
        {
          "text": "तमी᳚महे पुरुष्टु॒तं य॒ह्वं प्र॒त्नाभि᳚रू॒तिभि᳚ः ।",
          "begin": 0.0,
          "end": 5.142
        },
        {
          "text": "नि ब॒र्हिषि᳚ प्रि॒ये स᳚द॒दध᳚ द्वि॒ता ॥",
          "begin": 5.142,
          "end": 8.72
        }
      ]
    },
    {
      "id": "8.13.25",
      "mandala": 8,
      "sukta": 13,
      "rik": 25,
      "padas": [
        {
          "text": "वर्ध᳚स्वा॒ सु पु᳚रुष्टुत॒ ऋषि᳚ष्टुताभिरू॒तिभि᳚ः ।",
          "begin": 0.0,
          "end": 4.823
        },
        {
          "text": "धु॒क्षस्व᳚ पि॒प्युषी॒मिष॒मवा᳚ च नः ॥",
          "begin": 4.823,
          "end": 8.28
        }
      ]
    },
    {
      "id": "8.13.26",
      "mandala": 8,
      "sukta": 13,
      "rik": 26,
      "padas": [
        {
          "text": "इन्द्र॒ त्वम᳚वि॒तेद᳚सी॒त्था स्तु᳚व॒तो अ᳚द्रिवः ।",
          "begin": 0.0,
          "end": 5.273
        },
        {
          "text": "ऋ॒तादि᳚यर्मि ते॒ धियं᳚ मनो॒युज᳚म् ॥",
          "begin": 5.273,
          "end": 9.08
        }
      ]
    },
    {
      "id": "8.13.27",
      "mandala": 8,
      "sukta": 13,
      "rik": 27,
      "padas": [
        {
          "text": "इ॒ह त्या स᳚ध॒माद्या᳚ युजा॒नः सोम᳚पीतये ।",
          "begin": 0.0,
          "end": 4.704
        },
        {
          "text": "हरी᳚ इन्द्र प्र॒तद्व᳚सू अ॒भि स्व᳚र ॥",
          "begin": 4.704,
          "end": 9.56
        }
      ]
    },
    {
      "id": "8.13.28",
      "mandala": 8,
      "sukta": 13,
      "rik": 28,
      "padas": [
        {
          "text": "अ॒भि स्व᳚रन्तु॒ ये तव᳚ रु॒द्रासः᳚ सक्षत॒ श्रिय᳚म् ।",
          "begin": 0.0,
          "end": 5.624
        },
        {
          "text": "उ॒तो म॒रुत्व᳚ती॒र्विशो᳚ अ॒भि प्रयः᳚ ॥",
          "begin": 5.624,
          "end": 9.6
        }
      ]
    },
    {
      "id": "8.13.29",
      "mandala": 8,
      "sukta": 13,
      "rik": 29,
      "padas": [
        {
          "text": "इ॒मा अ᳚स्य॒ प्रतू᳚र्तयः प॒दं जु᳚षन्त॒ यद्दि॒वि ।",
          "begin": 0.0,
          "end": 5.884
        },
        {
          "text": "नाभा᳚ य॒ज्ञस्य॒ सं द᳚धु॒र्यथा᳚ वि॒दे ॥",
          "begin": 5.884,
          "end": 10.36
        }
      ]
    },
    {
      "id": "8.13.30",
      "mandala": 8,
      "sukta": 13,
      "rik": 30,
      "padas": [
        {
          "text": "अ॒यं दी॒र्घाय॒ चक्ष᳚से॒ प्राचि᳚ प्रय॒त्य᳚ध्व॒रे ।",
          "begin": 0.0,
          "end": 4.995
        },
        {
          "text": "मिमी᳚ते य॒ज्ञमा᳚नु॒षग्वि॒चक्ष्य᳚ ॥",
          "begin": 4.995,
          "end": 9.32
        }
      ]
    },
    {
      "id": "8.13.31",
      "mandala": 8,
      "sukta": 13,
      "rik": 31,
      "padas": [
        {
          "text": "वृषा॒यमि᳚न्द्र ते॒ रथ᳚ उ॒तो ते॒ वृष᳚णा॒ हरी᳚ ।",
          "begin": 0.0,
          "end": 5.098
        },
        {
          "text": "वृषा॒ त्वं श᳚तक्रतो॒ वृषा॒ हवः᳚ ॥",
          "begin": 5.098,
          "end": 9.0
        }
      ]
    },
    {
      "id": "8.13.32",
      "mandala": 8,
      "sukta": 13,
      "rik": 32,
      "padas": [
        {
          "text": "वृषा॒ ग्रावा॒ वृषा॒ मदो॒ वृषा॒ सोमो᳚ अ॒यं सु॒तः ।",
          "begin": 0.0,
          "end": 5.581
        },
        {
          "text": "वृषा᳚ य॒ज्ञो यमिन्व᳚सि॒ वृषा॒ हवः᳚ ॥",
          "begin": 5.581,
          "end": 9.8
        }
      ]
    },
    {
      "id": "8.13.33",
      "mandala": 8,
      "sukta": 13,
      "rik": 33,
      "padas": [
        {
          "text": "वृषा᳚ त्वा॒ वृष᳚णं हुवे॒ वज्रि᳚ञ्चि॒त्राभि᳚रू॒तिभि᳚ः ।",
          "begin": 0.0,
          "end": 7.115
        },
        {
          "text": "वा॒वन्थ॒ हि प्रति᳚ष्टुतिं॒ वृषा॒ हवः᳚ ॥",
          "begin": 7.115,
          "end": 15.96
        }
      ]
    }
  ]
}