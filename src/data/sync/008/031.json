{
  "mandala": 8,
  "sukta": 31,
  "verses": [
    {
      "id": "8.31.1",
      "mandala": 8,
      "sukta": 31,
      "rik": 1,
      "padas": [
        {
          "text": "यो यजा᳚ति॒ यजा᳚त॒ इत्सु॒नव᳚च्च॒ पचा᳚ति च ।",
          "begin": 0.0,
          "end": 6.788
        },
        {
          "text": "ब्र॒ह्मेदिन्द्र᳚स्य चाकनत् ॥",
          "begin": 6.788,
          "end": 10.44
        }
      ]
    },
    {
      "id": "8.31.2",
      "mandala": 8,
      "sukta": 31,
      "rik": 2,
      "padas": [
        {
          "text": "पु॒रो॒ळाशं॒ यो अ᳚स्मै॒ सोमं॒ रर᳚त आ॒शिर᳚म् ।",
          "begin": 0.0,
          "end": 5.429
        },
        {
          "text": "पादित्तं श॒क्रो अंह᳚सः ॥",
          "begin": 5.429,
          "end": 8.44
        }
      ]
    },
    {
      "id": "8.31.3",
      "mandala": 8,
      "sukta": 31,
      "rik": 3,
      "padas": [
        {
          "text": "तस्य᳚ द्यु॒माँ अ᳚स॒द्रथो᳚ दे॒वजू᳚त॒ः स शू᳚शुवत् ।",
          "begin": 0.0,
          "end": 6.662
        },
        {
          "text": "विश्वा᳚ व॒न्वन्न᳚मि॒त्रिया᳚ ॥",
          "begin": 6.662,
          "end": 10.16
        }
      ]
    },
    {
      "id": "8.31.4",
      "mandala": 8,
      "sukta": 31,
      "rik": 4,
      "padas": [
        {
          "text": "अस्य᳚ प्र॒जाव᳚ती गृ॒हे॑स᳚श्चन्ती दि॒वेदि᳚वे ।",
          "begin": 0.0,
          "end": 5.026
        },
        {
          "text": "इळा᳚ धेनु॒मती᳚ दुहे ॥",
          "begin": 5.026,
          "end": 8.04
        }
      ]
    },
    {
      "id": "8.31.5",
      "mandala": 8,
      "sukta": 31,
      "rik": 5,
      "padas": [
        {
          "text": "या दम्प᳚ती॒ सम᳚नसा सुनु॒त आ च॒ धाव᳚तः ।",
          "begin": 0.0,
          "end": 4.796
        },
        {
          "text": "देवा᳚सो॒ नित्य᳚या॒शिरा᳚ ॥",
          "begin": 4.796,
          "end": 8.24
        }
      ]
    },
    {
      "id": "8.31.6",
      "mandala": 8,
      "sukta": 31,
      "rik": 6,
      "padas": [
        {
          "text": "प्रति᳚ प्राश॒व्याँ᳚ इतः स॒म्यञ्चा᳚ ब॒र्हिरा᳚शाते ।",
          "begin": 0.0,
          "end": 6.623
        },
        {
          "text": "न ता वाजे᳚षु वायतः ॥",
          "begin": 6.623,
          "end": 9.56
        }
      ]
    },
    {
      "id": "8.31.7",
      "mandala": 8,
      "sukta": 31,
      "rik": 7,
      "padas": [
        {
          "text": "न दे॒वाना॒मपि᳚ ह्नुतः सुम॒तिं न जु᳚गुक्षतः ।",
          "begin": 0.0,
          "end": 5.209
        },
        {
          "text": "श्रवो᳚ बृ॒हद्वि᳚वासतः ॥",
          "begin": 5.209,
          "end": 8.04
        }
      ]
    },
    {
      "id": "8.31.8",
      "mandala": 8,
      "sukta": 31,
      "rik": 8,
      "padas": [
        {
          "text": "पु॒त्रिणा॒ ता कु᳚मा॒रिणा॒ विश्व॒मायु॒र्व्य᳚श्नुतः ।",
          "begin": 0.0,
          "end": 4.904
        },
        {
          "text": "उ॒भा हिर᳚ण्यपेशसा ॥",
          "begin": 4.904,
          "end": 7.52
        }
      ]
    },
    {
      "id": "8.31.9",
      "mandala": 8,
      "sukta": 31,
      "rik": 9,
      "padas": [
        {
          "text": "वी॒तिहो᳚त्रा कृ॒तद्व᳚सू दश॒स्यन्ता॒मृता᳚य॒ कम् ।",
          "begin": 0.0,
          "end": 5.932
        },
        {
          "text": "समूधो᳚ रोम॒शं ह᳚तो दे॒वेषु᳚ कृणुतो॒ दुवः᳚ ॥",
          "begin": 5.932,
          "end": 10.84
        }
      ]
    },
    {
      "id": "8.31.10",
      "mandala": 8,
      "sukta": 31,
      "rik": 10,
      "padas": [
        {
          "text": "आ शर्म॒ पर्व᳚तानां वृणी॒महे᳚ न॒दीना᳚म् ।",
          "begin": 0.0,
          "end": 6.101
        },
        {
          "text": "आ विष्णो᳚ः सचा॒भुवः᳚ ॥",
          "begin": 6.101,
          "end": 9.12
        }
      ]
    },
    {
      "id": "8.31.11",
      "mandala": 8,
      "sukta": 31,
      "rik": 11,
      "padas": [
        {
          "text": "ऐतु᳚ पू॒षा र॒यिर्भगः᳚ स्व॒स्ति स᳚र्व॒धात᳚मः ।",
          "begin": 0.0,
          "end": 5.411
        },
        {
          "text": "उ॒रुरध्वा᳚ स्व॒स्तये᳚ ॥",
          "begin": 5.411,
          "end": 8.36
        }
      ]
    },
    {
      "id": "8.31.12",
      "mandala": 8,
      "sukta": 31,
      "rik": 12,
      "padas": [
        {
          "text": "अ॒रम᳚तिरन॒र्वणो॒ विश्वो᳚ दे॒वस्य॒ मन᳚सा ।",
          "begin": 0.0,
          "end": 5.54
        },
        {
          "text": "आ॒दि॒त्याना᳚मने॒ह इत् ॥",
          "begin": 5.54,
          "end": 8.4
        }
      ]
    },
    {
      "id": "8.31.13",
      "mandala": 8,
      "sukta": 31,
      "rik": 13,
      "padas": [
        {
          "text": "यथा᳚ नो मि॒त्रो अ᳚र्य॒मा वरु᳚ण॒ः सन्ति᳚ गो॒पाः ।",
          "begin": 0.0,
          "end": 6.509
        },
        {
          "text": "सु॒गा ऋ॒तस्य॒ पन्था᳚ः ॥",
          "begin": 6.509,
          "end": 9.4
        }
      ]
    },
    {
      "id": "8.31.14",
      "mandala": 8,
      "sukta": 31,
      "rik": 14,
      "padas": [
        {
          "text": "अ॒ग्निं वः᳚ पू॒र्व्यं गि॒रा दे॒वमी᳚ळे॒ वसू᳚नाम् ।",
          "begin": 0.0,
          "end": 5.767
        },
        {
          "text": "स॒प॒र्यन्त᳚ः पुरुप्रि॒यं मि॒त्रं न क्षे᳚त्र॒साध᳚सम् ॥",
          "begin": 5.767,
          "end": 10.8
        }
      ]
    },
    {
      "id": "8.31.15",
      "mandala": 8,
      "sukta": 31,
      "rik": 15,
      "padas": [
        {
          "text": "म॒क्षू दे॒वव᳚तो॒ रथ॒ः शूरो᳚ वा पृ॒त्सु कासु᳚ चित् ।",
          "begin": 0.0,
          "end": 5.472
        },
        {
          "text": "दे॒वानां॒ य इन्मनो॒ यज᳚मान॒ इय᳚क्षत्य॒भीदय᳚ज्वनो भुवत् ॥",
          "begin": 5.472,
          "end": 10.68
        }
      ]
    },
    {
      "id": "8.31.16",
      "mandala": 8,
      "sukta": 31,
      "rik": 16,
      "padas": [
        {
          "text": "न य᳚जमान रिष्यसि॒ न सु᳚न्वान॒ न दे᳚वयो ।",
          "begin": 0.0,
          "end": 3.723
        },
        {
          "text": "दे॒वानां॒ य इन्मनो॒ यज᳚मान॒ इय᳚क्षत्य॒भीदय᳚ज्वनो भुवत् ॥",
          "begin": 3.723,
          "end": 10.6
        }
      ]
    },
    {
      "id": "8.31.17",
      "mandala": 8,
      "sukta": 31,
      "rik": 17,
      "padas": [
        {
          "text": "नकि॒ष्टं कर्म᳚णा नश॒न्न प्र यो᳚ष॒न्न यो᳚षति ।",
          "begin": 0.0,
          "end": 5.584
        },
        {
          "text": "दे॒वानां॒ य इन्मनो॒ यज᳚मान॒ इय᳚क्षत्य॒भीदय᳚ज्वनो भुवत् ॥",
          "begin": 5.584,
          "end": 11.04
        }
      ]
    },
    {
      "id": "8.31.18",
      "mandala": 8,
      "sukta": 31,
      "rik": 18,
      "padas": [
        {
          "text": "अस॒दत्र᳚ सु॒वीर्य᳚मु॒त त्यदा॒श्वश्व्य᳚म् ।",
          "begin": 0.0,
          "end": 4.254
        },
        {
          "text": "दे॒वानां॒ य इन्मनो॒ यज᳚मान॒ इय᳚क्षत्य॒भीदय᳚ज्वनो भुवत् ॥",
          "begin": 4.254,
          "end": 12.32
        }
      ]
    }
  ]
}