{
  "mandala": 8,
  "sukta": 18,
  "verses": [
    {
      "id": "8.18.1",
      "mandala": 8,
      "sukta": 18,
      "rik": 1,
      "padas": [
        {
          "text": "इ॒दं ह᳚ नू॒नमे᳚षां सु॒म्नं भि᳚क्षेत॒ मर्त्य᳚ः ।",
          "begin": 0.0,
          "end": 7.414
        },
        {
          "text": "आ॒दि॒त्याना॒मपू᳚र्व्यं॒ सवी᳚मनि ॥",
          "begin": 7.414,
          "end": 13.8
        }
      ]
    },
    {
      "id": "8.18.2",
      "mandala": 8,
      "sukta": 18,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒न॒र्वाणो॒ ह्ये᳚षां॒ पन्था᳚ आदि॒त्याना᳚म् ।",
          "begin": 0.0,
          "end": 6.436
        },
        {
          "text": "अद᳚ब्धा॒ः सन्ति᳚ पा॒यवः᳚ सुगे॒वृधः᳚ ॥",
          "begin": 6.436,
          "end": 10.16
        }
      ]
    },
    {
      "id": "8.18.3",
      "mandala": 8,
      "sukta": 18,
      "rik": 3,
      "padas": [
        {
          "text": "तत्सु नः᳚ सवि॒ता भगो॒ वरु᳚णो मि॒त्रो अ᳚र्य॒मा ।",
          "begin": 0.0,
          "end": 5.431
        },
        {
          "text": "शर्म᳚ यच्छन्तु स॒प्रथो॒ यदीम᳚हे ॥",
          "begin": 5.431,
          "end": 8.8
        }
      ]
    },
    {
      "id": "8.18.4",
      "mandala": 8,
      "sukta": 18,
      "rik": 4,
      "padas": [
        {
          "text": "दे॒वेभि᳚र्देव्यदि॒ते॑रि᳚ष्टभर्म॒न्ना ग᳚हि ।",
          "begin": 0.0,
          "end": 4.679
        },
        {
          "text": "स्मत्सू॒रिभि᳚ः पुरुप्रिये सु॒शर्म᳚भिः ॥",
          "begin": 4.679,
          "end": 8.24
        }
      ]
    },
    {
      "id": "8.18.5",
      "mandala": 8,
      "sukta": 18,
      "rik": 5,
      "padas": [
        {
          "text": "ते हि पु॒त्रासो॒ अदि᳚तेर्वि॒दुर्द्वेषां᳚सि॒ योत᳚वे ।",
          "begin": 0.0,
          "end": 6.337
        },
        {
          "text": "अं॒होश्चि᳚दुरु॒चक्र᳚यो॑ने॒हसः᳚ ॥",
          "begin": 6.337,
          "end": 9.4
        }
      ]
    },
    {
      "id": "8.18.6",
      "mandala": 8,
      "sukta": 18,
      "rik": 6,
      "padas": [
        {
          "text": "अदि᳚तिर्नो॒ दिवा᳚ प॒शुमदि᳚ति॒र्नक्त॒मद्व᳚याः ।",
          "begin": 0.0,
          "end": 5.017
        },
        {
          "text": "अदि᳚तिः पा॒त्वंह᳚सः स॒दावृ᳚धा ॥",
          "begin": 5.017,
          "end": 8.28
        }
      ]
    },
    {
      "id": "8.18.7",
      "mandala": 8,
      "sukta": 18,
      "rik": 7,
      "padas": [
        {
          "text": "उ॒त स्या नो॒ दिवा᳚ म॒तिरदि᳚तिरू॒त्या ग᳚मत् ।",
          "begin": 0.0,
          "end": 5.254
        },
        {
          "text": "सा शंता᳚ति॒ मय᳚स्कर॒दप॒ स्रिधः᳚ ॥",
          "begin": 5.254,
          "end": 9.12
        }
      ]
    },
    {
      "id": "8.18.8",
      "mandala": 8,
      "sukta": 18,
      "rik": 8,
      "padas": [
        {
          "text": "उ॒त त्या दैव्या᳚ भि॒षजा॒ शं नः᳚ करतो अ॒श्विना᳚ ।",
          "begin": 0.0,
          "end": 6.413
        },
        {
          "text": "यु॒यु॒याता᳚मि॒तो रपो॒ अप॒ स्रिधः᳚ ॥",
          "begin": 6.413,
          "end": 10.52
        }
      ]
    },
    {
      "id": "8.18.9",
      "mandala": 8,
      "sukta": 18,
      "rik": 9,
      "padas": [
        {
          "text": "शम॒ग्निर॒ग्निभि᳚ः कर॒च्छं न᳚स्तपतु॒ सूर्य᳚ः ।",
          "begin": 0.0,
          "end": 4.995
        },
        {
          "text": "शं वातो᳚ वात्वर॒पा अप॒ स्रिधः᳚ ॥",
          "begin": 4.995,
          "end": 9.0
        }
      ]
    },
    {
      "id": "8.18.10",
      "mandala": 8,
      "sukta": 18,
      "rik": 10,
      "padas": [
        {
          "text": "अपामी᳚वा॒मप॒ स्रिध॒मप᳚ सेधत दुर्म॒तिम् ।",
          "begin": 0.0,
          "end": 4.958
        },
        {
          "text": "आदि᳚त्यासो यु॒योत᳚ना नो॒ अंह᳚सः ॥",
          "begin": 4.958,
          "end": 9.0
        }
      ]
    },
    {
      "id": "8.18.11",
      "mandala": 8,
      "sukta": 18,
      "rik": 11,
      "padas": [
        {
          "text": "यु॒योता॒ शरु᳚म॒स्मदाँ आदि᳚त्यास उ॒ताम᳚तिम् ।",
          "begin": 0.0,
          "end": 4.699
        },
        {
          "text": "ऋध॒ग्द्वेषः᳚ कृणुत विश्ववेदसः ॥",
          "begin": 4.699,
          "end": 8.6
        }
      ]
    },
    {
      "id": "8.18.12",
      "mandala": 8,
      "sukta": 18,
      "rik": 12,
      "padas": [
        {
          "text": "तत्सु न॒ः शर्म᳚ यच्छ॒तादि᳚त्या॒ यन्मुमो᳚चति ।",
          "begin": 0.0,
          "end": 5.317
        },
        {
          "text": "एन᳚स्वन्तं चि॒देन᳚सः सुदानवः ॥",
          "begin": 5.317,
          "end": 8.56
        }
      ]
    },
    {
      "id": "8.18.13",
      "mandala": 8,
      "sukta": 18,
      "rik": 13,
      "padas": [
        {
          "text": "यो न॒ः कश्चि॒द्रिरि᳚क्षति रक्ष॒स्त्वेन॒ मर्त्य᳚ः ।",
          "begin": 0.0,
          "end": 5.196
        },
        {
          "text": "स्वैः ष एवै᳚ रिरिषीष्ट॒ युर्जनः᳚ ॥",
          "begin": 5.196,
          "end": 9.2
        }
      ]
    },
    {
      "id": "8.18.14",
      "mandala": 8,
      "sukta": 18,
      "rik": 14,
      "padas": [
        {
          "text": "समित्तम॒घम᳚श्नवद्दु॒ःशंसं॒ मर्त्यं᳚ रि॒पुम् ।",
          "begin": 0.0,
          "end": 5.373
        },
        {
          "text": "यो अ᳚स्म॒त्रा दु॒र्हणा᳚वाँ॒ उप᳚ द्व॒युः ॥",
          "begin": 5.373,
          "end": 10.2
        }
      ]
    },
    {
      "id": "8.18.15",
      "mandala": 8,
      "sukta": 18,
      "rik": 15,
      "padas": [
        {
          "text": "पा॒क॒त्रा स्थ᳚न देवा हृ॒त्सु जा᳚नीथ॒ मर्त्य᳚म् ।",
          "begin": 0.0,
          "end": 5.805
        },
        {
          "text": "उप᳚ द्व॒युं चाद्व᳚युं च वसवः ॥",
          "begin": 5.805,
          "end": 9.16
        }
      ]
    },
    {
      "id": "8.18.16",
      "mandala": 8,
      "sukta": 18,
      "rik": 16,
      "padas": [
        {
          "text": "आ शर्म॒ पर्व᳚ताना॒मोतापां वृ᳚णीमहे ।",
          "begin": 0.0,
          "end": 5.085
        },
        {
          "text": "द्यावा᳚क्षामा॒रे अ॒स्मद्रप᳚स्कृतम् ॥",
          "begin": 5.085,
          "end": 9.44
        }
      ]
    },
    {
      "id": "8.18.17",
      "mandala": 8,
      "sukta": 18,
      "rik": 17,
      "padas": [
        {
          "text": "ते नो᳚ भ॒द्रेण॒ शर्म᳚णा यु॒ष्माकं᳚ ना॒वा व᳚सवः ।",
          "begin": 0.0,
          "end": 6.444
        },
        {
          "text": "अति॒ विश्वा᳚नि दुरि॒ता पि᳚पर्तन ॥",
          "begin": 6.444,
          "end": 9.88
        }
      ]
    },
    {
      "id": "8.18.18",
      "mandala": 8,
      "sukta": 18,
      "rik": 18,
      "padas": [
        {
          "text": "तु॒चे तना᳚य॒ तत्सु नो॒ द्राघी᳚य॒ आयु᳚र्जी॒वसे᳚ ।",
          "begin": 0.0,
          "end": 6.616
        },
        {
          "text": "आदि᳚त्यासः सुमहसः कृ॒णोत᳚न ॥",
          "begin": 6.616,
          "end": 9.8
        }
      ]
    },
    {
      "id": "8.18.19",
      "mandala": 8,
      "sukta": 18,
      "rik": 19,
      "padas": [
        {
          "text": "य॒ज्ञो ही॒ळो वो॒ अन्त᳚र॒ आदि᳚त्या॒ अस्ति᳚ मृ॒ळत᳚ ।",
          "begin": 0.0,
          "end": 6.291
        },
        {
          "text": "यु॒ष्मे इद्वो॒ अपि᳚ ष्मसि सजा॒त्ये᳚ ॥",
          "begin": 6.291,
          "end": 10.36
        }
      ]
    },
    {
      "id": "8.18.20",
      "mandala": 8,
      "sukta": 18,
      "rik": 20,
      "padas": [
        {
          "text": "बृ॒हद्वरू᳚थं म॒रुतां᳚ दे॒वं त्रा॒तार᳚म॒श्विना᳚ ।",
          "begin": 0.0,
          "end": 6.173
        },
        {
          "text": "मि॒त्रमी᳚महे॒ वरु᳚णं स्व॒स्तये᳚ ॥",
          "begin": 6.173,
          "end": 10.76
        }
      ]
    },
    {
      "id": "8.18.21",
      "mandala": 8,
      "sukta": 18,
      "rik": 21,
      "padas": [
        {
          "text": "अ॒ने॒हो मि᳚त्रार्यमन्नृ॒वद्व᳚रुण॒ शंस्य᳚म् ।",
          "begin": 0.0,
          "end": 4.619
        },
        {
          "text": "त्रि॒वरू᳚थं मरुतो यन्त नश्छ॒र्दिः ॥",
          "begin": 4.619,
          "end": 9.2
        }
      ]
    },
    {
      "id": "8.18.22",
      "mandala": 8,
      "sukta": 18,
      "rik": 22,
      "padas": [
        {
          "text": "ये चि॒द्धि मृ॒त्युब᳚न्धव॒ आदि᳚त्या॒ मन᳚व॒ः स्मसि᳚ ।",
          "begin": 0.0,
          "end": 5.665
        },
        {
          "text": "प्र सू न॒ आयु᳚र्जी॒वसे᳚ तिरेतन ॥",
          "begin": 5.665,
          "end": 10.92
        }
      ]
    }
  ]
}