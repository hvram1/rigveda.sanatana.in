{
  "mandala": 8,
  "sukta": 34,
  "verses": [
    {
      "id": "8.34.1",
      "mandala": 8,
      "sukta": 34,
      "rik": 1,
      "padas": [
        {
          "text": "एन्द्र᳚ याहि॒ हरि᳚भि॒रुप॒ कण्व᳚स्य सुष्टु॒तिम् ।",
          "begin": 0.0,
          "end": 7.319
        },
        {
          "text": "दि॒वो अ॒मुष्य॒ शास᳚तो॒ दिवं᳚ य॒य दि᳚वावसो ॥",
          "begin": 7.319,
          "end": 12.12
        }
      ]
    },
    {
      "id": "8.34.2",
      "mandala": 8,
      "sukta": 34,
      "rik": 2,
      "padas": [
        {
          "text": "आ त्वा॒ ग्रावा॒ वद᳚न्नि॒ह सो॒मी घोषे᳚ण यच्छतु ।",
          "begin": 0.0,
          "end": 5.989
        },
        {
          "text": "दि॒वो अ॒मुष्य॒ शास᳚तो॒ दिवं᳚ य॒य दि᳚वावसो ॥",
          "begin": 5.989,
          "end": 10.72
        }
      ]
    },
    {
      "id": "8.34.3",
      "mandala": 8,
      "sukta": 34,
      "rik": 3,
      "padas": [
        {
          "text": "अत्रा॒ वि ने॒मिरे᳚षा॒मुरां॒ न धू᳚नुते॒ वृकः᳚ ।",
          "begin": 0.0,
          "end": 5.772
        },
        {
          "text": "दि॒वो अ॒मुष्य॒ शास᳚तो॒ दिवं᳚ य॒य दि᳚वावसो ॥",
          "begin": 5.772,
          "end": 10.0
        }
      ]
    },
    {
      "id": "8.34.4",
      "mandala": 8,
      "sukta": 34,
      "rik": 4,
      "padas": [
        {
          "text": "आ त्वा॒ कण्वा᳚ इ॒हाव᳚से॒ हव᳚न्ते॒ वाज᳚सातये ।",
          "begin": 0.0,
          "end": 6.375
        },
        {
          "text": "दि॒वो अ॒मुष्य॒ शास᳚तो॒ दिवं᳚ य॒य दि᳚वावसो ॥",
          "begin": 6.375,
          "end": 10.72
        }
      ]
    },
    {
      "id": "8.34.5",
      "mandala": 8,
      "sukta": 34,
      "rik": 5,
      "padas": [
        {
          "text": "दधा᳚मि ते सु॒तानां॒ वृष्णे॒ न पू᳚र्व॒पाय्य᳚म् ।",
          "begin": 0.0,
          "end": 6.056
        },
        {
          "text": "दि॒वो अ॒मुष्य॒ शास᳚तो॒ दिवं᳚ य॒य दि᳚वावसो ॥",
          "begin": 6.056,
          "end": 10.52
        }
      ]
    },
    {
      "id": "8.34.6",
      "mandala": 8,
      "sukta": 34,
      "rik": 6,
      "padas": [
        {
          "text": "स्मत्पु᳚रंधिर्न॒ आ ग᳚हि वि॒श्वतो᳚धीर्न ऊ॒तये᳚ ।",
          "begin": 0.0,
          "end": 5.533
        },
        {
          "text": "दि॒वो अ॒मुष्य॒ शास᳚तो॒ दिवं᳚ य॒य दि᳚वावसो ॥",
          "begin": 5.533,
          "end": 9.72
        }
      ]
    },
    {
      "id": "8.34.7",
      "mandala": 8,
      "sukta": 34,
      "rik": 7,
      "padas": [
        {
          "text": "आ नो᳚ याहि महेमते॒ सह᳚स्रोते॒ शता᳚मघ ।",
          "begin": 0.0,
          "end": 5.341
        },
        {
          "text": "दि॒वो अ॒मुष्य॒ शास᳚तो॒ दिवं᳚ य॒य दि᳚वावसो ॥",
          "begin": 5.341,
          "end": 9.72
        }
      ]
    },
    {
      "id": "8.34.8",
      "mandala": 8,
      "sukta": 34,
      "rik": 8,
      "padas": [
        {
          "text": "आ त्वा॒ होता॒ मनु᳚र्हितो देव॒त्रा व᳚क्ष॒दीड्य᳚ः ।",
          "begin": 0.0,
          "end": 5.553
        },
        {
          "text": "दि॒वो अ॒मुष्य॒ शास᳚तो॒ दिवं᳚ य॒य दि᳚वावसो ॥",
          "begin": 5.553,
          "end": 9.76
        }
      ]
    },
    {
      "id": "8.34.9",
      "mandala": 8,
      "sukta": 34,
      "rik": 9,
      "padas": [
        {
          "text": "आ त्वा᳚ मद॒च्युता॒ हरी᳚ श्ये॒नं प॒क्षेव᳚ वक्षतः ।",
          "begin": 0.0,
          "end": 6.228
        },
        {
          "text": "दि॒वो अ॒मुष्य॒ शास᳚तो॒ दिवं᳚ य॒य दि᳚वावसो ॥",
          "begin": 6.228,
          "end": 10.56
        }
      ]
    },
    {
      "id": "8.34.10",
      "mandala": 8,
      "sukta": 34,
      "rik": 10,
      "padas": [
        {
          "text": "आ या᳚ह्य॒र्य आ परि॒ स्वाहा॒ सोम᳚स्य पी॒तये᳚ ।",
          "begin": 0.0,
          "end": 5.821
        },
        {
          "text": "दि॒वो अ॒मुष्य॒ शास᳚तो॒ दिवं᳚ य॒य दि᳚वावसो ॥",
          "begin": 5.821,
          "end": 10.08
        }
      ]
    },
    {
      "id": "8.34.11",
      "mandala": 8,
      "sukta": 34,
      "rik": 11,
      "padas": [
        {
          "text": "आ नो᳚ या॒ह्युप᳚श्रुत्यु॒क्थेषु᳚ रणया इ॒ह ।",
          "begin": 0.0,
          "end": 5.114
        },
        {
          "text": "दि॒वो अ॒मुष्य॒ शास᳚तो॒ दिवं᳚ य॒य दि᳚वावसो ॥",
          "begin": 5.114,
          "end": 9.52
        }
      ]
    },
    {
      "id": "8.34.12",
      "mandala": 8,
      "sukta": 34,
      "rik": 12,
      "padas": [
        {
          "text": "सरू᳚पै॒रा सु नो᳚ गहि॒ सम्भृ᳚तै॒ः सम्भृ᳚ताश्वः ।",
          "begin": 0.0,
          "end": 5.795
        },
        {
          "text": "दि॒वो अ॒मुष्य॒ शास᳚तो॒ दिवं᳚ य॒य दि᳚वावसो ॥",
          "begin": 5.795,
          "end": 10.08
        }
      ]
    },
    {
      "id": "8.34.13",
      "mandala": 8,
      "sukta": 34,
      "rik": 13,
      "padas": [
        {
          "text": "आ या᳚हि॒ पर्व᳚तेभ्यः समु॒द्रस्याधि᳚ वि॒ष्टपः᳚ ।",
          "begin": 0.0,
          "end": 5.613
        },
        {
          "text": "दि॒वो अ॒मुष्य॒ शास᳚तो॒ दिवं᳚ य॒य दि᳚वावसो ॥",
          "begin": 5.613,
          "end": 9.92
        }
      ]
    },
    {
      "id": "8.34.14",
      "mandala": 8,
      "sukta": 34,
      "rik": 14,
      "padas": [
        {
          "text": "आ नो॒ गव्या॒न्यश्व्या᳚ स॒हस्रा᳚ शूर दर्दृहि ।",
          "begin": 0.0,
          "end": 5.753
        },
        {
          "text": "दि॒वो अ॒मुष्य॒ शास᳚तो॒ दिवं᳚ य॒य दि᳚वावसो ॥",
          "begin": 5.753,
          "end": 10.16
        }
      ]
    },
    {
      "id": "8.34.15",
      "mandala": 8,
      "sukta": 34,
      "rik": 15,
      "padas": [
        {
          "text": "आ नः᳚ सहस्र॒शो भ᳚रा॒युता᳚नि श॒तानि᳚ च ।",
          "begin": 0.0,
          "end": 4.879
        },
        {
          "text": "दि॒वो अ॒मुष्य॒ शास᳚तो॒ दिवं᳚ य॒य दि᳚वावसो ॥",
          "begin": 4.879,
          "end": 9.08
        }
      ]
    },
    {
      "id": "8.34.16",
      "mandala": 8,
      "sukta": 34,
      "rik": 16,
      "padas": [
        {
          "text": "आ यदिन्द्र᳚श्च॒ दद्व᳚हे स॒हस्रं॒ वसु᳚रोचिषः ।",
          "begin": 0.0,
          "end": 5.122
        },
        {
          "text": "ओजि᳚ष्ठ॒मश्व्यं᳚ प॒शुम् ॥",
          "begin": 5.122,
          "end": 7.6
        }
      ]
    },
    {
      "id": "8.34.17",
      "mandala": 8,
      "sukta": 34,
      "rik": 17,
      "padas": [
        {
          "text": "य ऋ॒ज्रा वात᳚रंहसो॑रु॒षासो᳚ रघु॒ष्यदः᳚ ।",
          "begin": 0.0,
          "end": 5.047
        },
        {
          "text": "भ्राज᳚न्ते॒ सूर्या᳚ इव ॥",
          "begin": 5.047,
          "end": 8.64
        }
      ]
    },
    {
      "id": "8.34.18",
      "mandala": 8,
      "sukta": 34,
      "rik": 18,
      "padas": [
        {
          "text": "पारा᳚वतस्य रा॒तिषु᳚ द्र॒वच्च᳚क्रेष्वा॒शुषु᳚ ।",
          "begin": 0.0,
          "end": 6.522
        },
        {
          "text": "तिष्ठं॒ वन᳚स्य॒ मध्य॒ आ ॥",
          "begin": 6.522,
          "end": 13.24
        }
      ]
    }
  ]
}