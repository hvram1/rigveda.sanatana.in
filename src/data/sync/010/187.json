  "verses": [
    {
      "id": "10.187.1",
      "mandala": 10,
      "sukta": 187,
      "rik": 1,
      "padas": [
        {
          "text": "प्राग्नये॒ वाच᳚मीरय वृष॒भाय᳚ क्षिती॒नाम् ।",
          "begin": 0.0,
          "end": 7.542
        },
        {
          "text": "स नः᳚ पर्ष॒दति॒ द्विषः᳚ ॥",
          "begin": 7.542,
          "end": 10.52
        }
      ]
    },
    {
      "id": "10.187.2",
      "mandala": 10,
      "sukta": 187,
      "rik": 2,
      "padas": [
        {
          "text": "यः पर᳚स्याः परा॒वत᳚स्ति॒रो धन्वा᳚ति॒रोच᳚ते ।",
          "begin": 0.0,
          "end": 5.85
        },
        {
          "text": "स नः᳚ पर्ष॒दति॒ द्विषः᳚ ॥",
          "begin": 5.85,
          "end": 8.52
        }
      ]
    },
    {
      "id": "10.187.3",
      "mandala": 10,
      "sukta": 187,
      "rik": 3,
      "padas": [
        {
          "text": "यो रक्षां᳚सि नि॒जूर्व᳚ति॒ वृषा᳚ शु॒क्रेण᳚ शो॒चिषा᳚ ।",
          "begin": 0.0,
          "end": 6.639
        },
        {
          "text": "स नः᳚ पर्ष॒दति॒ द्विषः᳚ ॥",
          "begin": 6.639,
          "end": 9.2
        }
      ]
    },
    {
      "id": "10.187.4",
      "mandala": 10,
      "sukta": 187,
      "rik": 4,
      "padas": [
        {
          "text": "यो विश्वा॒भि वि॒पश्य᳚ति॒ भुव᳚ना॒ सं च॒ पश्य᳚ति ।",
          "begin": 0.0,
          "end": 5.161
        },
        {
          "text": "स नः᳚ पर्ष॒दति॒ द्विषः᳚ ॥",
          "begin": 5.161,
          "end": 7.68
        }
      ]
    },
    {
      "id": "10.187.5",
      "mandala": 10,
      "sukta": 187,
      "rik": 5,
      "padas": [
        {
          "text": "यो अ॒स्य पा॒रे रज᳚सः शु॒क्रो अ॒ग्निरजा᳚यत ।",
          "begin": 0.0,
          "end": 6.74
        },
        {
          "text": "स नः᳚ पर्ष॒दति॒ द्विषः᳚ ॥",
          "begin": 6.74,
          "end": 11.24
        }
      ]
    }
  ]
}