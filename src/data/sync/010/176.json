{
  "mandala": 10,
  "sukta": 176,
  "verses": [
    {
      "id": "10.176.1",
      "mandala": 10,
      "sukta": 176,
      "rik": 1,
      "padas": [
        {
          "text": "प्र सू॒नव᳚ ऋभू॒णां बृ॒हन्न᳚वन्त वृ॒जना᳚ ।",
          "begin": 0.0,
          "end": 7.228
        },
        {
          "text": "क्षामा॒ ये वि॒श्वधा᳚य॒सो॑श्न᳚न्धे॒नुं न मा॒तर᳚म् ॥",
          "begin": 7.228,
          "end": 13.12
        }
      ]
    },
    {
      "id": "10.176.2",
      "mandala": 10,
      "sukta": 176,
      "rik": 2,
      "padas": [
        {
          "text": "प्र दे॒वं दे॒व्या धि॒या भर᳚ता जा॒तवे᳚दसम् ।",
          "begin": 0.0,
          "end": 5.902
        },
        {
          "text": "ह॒व्या नो᳚ वक्षदानु॒षक् ॥",
          "begin": 5.902,
          "end": 8.8
        }
      ]
    },
    {
      "id": "10.176.3",
      "mandala": 10,
      "sukta": 176,
      "rik": 3,
      "padas": [
        {
          "text": "अ॒यमु॒ ष्य प्र दे᳚व॒युर्होता᳚ य॒ज्ञाय᳚ नीयते ।",
          "begin": 0.0,
          "end": 5.922
        },
        {
          "text": "रथो॒ न योर॒भीवृ᳚तो॒ घृणी᳚वाञ्चेतति॒ त्मना᳚ ॥",
          "begin": 5.922,
          "end": 10.8
        }
      ]
    },
    {
      "id": "10.176.4",
      "mandala": 10,
      "sukta": 176,
      "rik": 4,
      "padas": [
        {
          "text": "अ॒यम॒ग्निरु᳚रुष्यत्य॒मृता᳚दिव॒ जन्म᳚नः ।",
          "begin": 0.0,
          "end": 5.334
        },
        {
          "text": "सह᳚सश्चि॒त्सही᳚यान्दे॒वो जी॒वात᳚वे कृ॒तः ॥",
          "begin": 5.334,
          "end": 11.88
        }
      ]
    }
  ]
}