{
  "mandala": 10,
  "sukta": 85,
  "verses": [
    {
      "id": "10.85.1",
      "mandala": 10,
      "sukta": 85,
      "rik": 1,
      "padas": [
        {
          "text": "स॒त्येनोत्त᳚भिता॒ भूमि॒ः सूर्ये॒णोत्त᳚भिता॒ द्यौः ।",
          "begin": 0.0,
          "end": 5.992
        },
        {
          "text": "ऋ॒तेना᳚दि॒त्यास्ति᳚ष्ठन्ति दि॒वि सोमो॒ अधि᳚ श्रि॒तः ॥",
          "begin": 5.992,
          "end": 12.48
        }
      ]
    },
    {
      "id": "10.85.2",
      "mandala": 10,
      "sukta": 85,
      "rik": 2,
      "padas": [
        {
          "text": "सोमे᳚नादि॒त्या ब॒लिन॒ः सोमे᳚न पृथि॒वी म॒ही ।",
          "begin": 0.0,
          "end": 5.992
        },
        {
          "text": "अथो॒ नक्ष᳚त्राणामे॒षामु॒पस्थे॒ सोम॒ आहि᳚तः ॥",
          "begin": 5.992,
          "end": 10.64
        }
      ]
    },
    {
      "id": "10.85.3",
      "mandala": 10,
      "sukta": 85,
      "rik": 3,
      "padas": [
        {
          "text": "सोमं᳚ मन्यते पपि॒वान्यत्स᳚म्पिं॒षन्त्योष᳚धिम् ।",
          "begin": 0.0,
          "end": 6.203
        },
        {
          "text": "सोमं॒ यं ब्र॒ह्माणो᳚ वि॒दुर्न तस्या᳚श्नाति॒ कश्च॒न ॥",
          "begin": 6.203,
          "end": 11.44
        }
      ]
    },
    {
      "id": "10.85.4",
      "mandala": 10,
      "sukta": 85,
      "rik": 4,
      "padas": [
        {
          "text": "आ॒च्छद्वि᳚धानैर्गुपि॒तो बार्ह᳚तैः सोम रक्षि॒तः ।",
          "begin": 0.0,
          "end": 5.965
        },
        {
          "text": "ग्राव्णा॒मिच्छृ॒ण्वन्ति᳚ष्ठसि॒ न ते᳚ अश्नाति॒ पार्थि᳚वः ॥",
          "begin": 5.965,
          "end": 11.2
        }
      ]
    },
    {
      "id": "10.85.5",
      "mandala": 10,
      "sukta": 85,
      "rik": 5,
      "padas": [
        {
          "text": "यत्त्वा᳚ देव प्र॒पिब᳚न्ति॒ तत॒ आ प्या᳚यसे॒ पुनः᳚ ।",
          "begin": 0.0,
          "end": 6.26
        },
        {
          "text": "वा॒युः सोम᳚स्य रक्षि॒ता समा᳚नां॒ मास॒ आकृ᳚तिः ॥",
          "begin": 6.26,
          "end": 11.64
        }
      ]
    },
    {
      "id": "10.85.6",
      "mandala": 10,
      "sukta": 85,
      "rik": 6,
      "padas": [
        {
          "text": "रैभ्या᳚सीदनु॒देयी᳚ नाराशं॒सी न्योच᳚नी ।",
          "begin": 0.0,
          "end": 6.396
        },
        {
          "text": "सू॒र्याया᳚ भ॒द्रमिद्वासो॒ गाथ᳚यैति॒ परि᳚ष्कृतम् ॥",
          "begin": 6.396,
          "end": 11.56
        }
      ]
    },
    {
      "id": "10.85.7",
      "mandala": 10,
      "sukta": 85,
      "rik": 7,
      "padas": [
        {
          "text": "चित्ति᳚रा उप॒बर्ह᳚णं॒ चक्षु᳚रा अ॒भ्यञ्ज᳚नम् ।",
          "begin": 0.0,
          "end": 5.317
        },
        {
          "text": "द्यौर्भूमि॒ः कोश᳚ आसी॒द्यदया᳚त्सू॒र्या पति᳚म् ॥",
          "begin": 5.317,
          "end": 10.68
        }
      ]
    },
    {
      "id": "10.85.8",
      "mandala": 10,
      "sukta": 85,
      "rik": 8,
      "padas": [
        {
          "text": "स्तोमा᳚ आसन्प्रति॒धयः᳚ कु॒रीरं॒ छन्द᳚ ओप॒शः ।",
          "begin": 0.0,
          "end": 5.811
        },
        {
          "text": "सू॒र्याया᳚ अ॒श्विना᳚ व॒राग्निरा᳚सीत्पुरोग॒वः ॥",
          "begin": 5.811,
          "end": 11.44
        }
      ]
    },
    {
      "id": "10.85.9",
      "mandala": 10,
      "sukta": 85,
      "rik": 9,
      "padas": [
        {
          "text": "सोमो᳚ वधू॒युर᳚भवद॒श्विना᳚स्तामु॒भा व॒रा ।",
          "begin": 0.0,
          "end": 6.059
        },
        {
          "text": "सू॒र्यां यत्पत्ये॒ शंस᳚न्तीं॒ मन᳚सा सवि॒ताद᳚दात् ॥",
          "begin": 6.059,
          "end": 11.92
        }
      ]
    },
    {
      "id": "10.85.10",
      "mandala": 10,
      "sukta": 85,
      "rik": 10,
      "padas": [
        {
          "text": "मनो᳚ अस्या॒ अन᳚ आसी॒द्द्यौरा᳚सीदु॒त च्छ॒दिः ।",
          "begin": 0.0,
          "end": 6.026
        },
        {
          "text": "शु॒क्राव᳚न॒ड्वाहा᳚वास्तां॒ यदया᳚त्सू॒र्या गृ॒हम् ॥",
          "begin": 6.026,
          "end": 12.6
        }
      ]
    },
    {
      "id": "10.85.11",
      "mandala": 10,
      "sukta": 85,
      "rik": 11,
      "padas": [
        {
          "text": "ऋ॒क्सा॒माभ्या᳚म॒भिहि᳚तौ॒ गावौ᳚ ते साम॒नावि᳚तः ।",
          "begin": 0.0,
          "end": 6.65
        },
        {
          "text": "श्रोत्रं᳚ ते च॒क्रे आ᳚स्तां दि॒वि पन्था᳚श्चराचा॒रः ॥",
          "begin": 6.65,
          "end": 13.32
        }
      ]
    },
    {
      "id": "10.85.12",
      "mandala": 10,
      "sukta": 85,
      "rik": 12,
      "padas": [
        {
          "text": "शुची᳚ ते च॒क्रे या॒त्या व्या॒नो अक्ष॒ आह᳚तः ।",
          "begin": 0.0,
          "end": 6.343
        },
        {
          "text": "अनो᳚ मन॒स्मयं᳚ सू॒र्यारो᳚हत्प्रय॒ती पति᳚म् ॥",
          "begin": 6.343,
          "end": 12.52
        }
      ]
    },
    {
      "id": "10.85.13",
      "mandala": 10,
      "sukta": 85,
      "rik": 13,
      "padas": [
        {
          "text": "सू॒र्याया᳚ वह॒तुः प्रागा᳚त्सवि॒ता यम॒वासृ᳚जत् ।",
          "begin": 0.0,
          "end": 6.593
        },
        {
          "text": "अ॒घासु᳚ हन्यन्ते॒ गावो॑र्जु᳚न्यो॒ः पर्यु᳚ह्यते ॥",
          "begin": 6.593,
          "end": 11.56
        }
      ]
    },
    {
      "id": "10.85.14",
      "mandala": 10,
      "sukta": 85,
      "rik": 14,
      "padas": [
        {
          "text": "यद᳚श्विना पृ॒च्छमा᳚ना॒वया᳚तं त्रिच॒क्रेण᳚ वह॒तुं सू॒र्याया᳚ः ।",
          "begin": 0.0,
          "end": 7.698
        },
        {
          "text": "विश्वे᳚ दे॒वा अनु॒ तद्वा᳚मजानन्पु॒त्रः पि॒तरा᳚ववृणीत पू॒षा ॥",
          "begin": 7.698,
          "end": 15.52
        }
      ]
    },
    {
      "id": "10.85.15",
      "mandala": 10,
      "sukta": 85,
      "rik": 15,
      "padas": [
        {
          "text": "यदया᳚तं शुभस्पती वरे॒यं सू॒र्यामुप᳚ ।",
          "begin": 0.0,
          "end": 6.061
        },
        {
          "text": "क्वैकं᳚ च॒क्रं वा᳚मासी॒त्क्व᳚ दे॒ष्ट्राय᳚ तस्थथुः ॥",
          "begin": 6.061,
          "end": 11.96
        }
      ]
    },
    {
      "id": "10.85.16",
      "mandala": 10,
      "sukta": 85,
      "rik": 16,
      "padas": [
        {
          "text": "द्वे ते᳚ च॒क्रे सू᳚र्ये ब्र॒ह्माण᳚ ऋतु॒था वि᳚दुः ।",
          "begin": 0.0,
          "end": 3.677
        },
        {
          "text": "अथैकं᳚ च॒क्रं यद्गुहा॒ तद᳚द्धा॒तय॒ इद्वि᳚दुः ॥",
          "begin": 3.677,
          "end": 11.36
        }
      ]
    },
    {
      "id": "10.85.17",
      "mandala": 10,
      "sukta": 85,
      "rik": 17,
      "padas": [
        {
          "text": "सू॒र्यायै᳚ दे॒वेभ्यो᳚ मि॒त्राय॒ वरु᳚णाय च ।",
          "begin": 0.0,
          "end": 5.958
        },
        {
          "text": "ये भू॒तस्य॒ प्रचे᳚तस इ॒दं तेभ्यो᳚॑करं॒ नमः᳚ ॥",
          "begin": 5.958,
          "end": 11.68
        }
      ]
    },
    {
      "id": "10.85.18",
      "mandala": 10,
      "sukta": 85,
      "rik": 18,
      "padas": [
        {
          "text": "पू॒र्वा॒प॒रं च᳚रतो मा॒ययै॒तौ शिशू॒ क्रीळ᳚न्तौ॒ परि᳚ यातो अध्व॒रम् ।",
          "begin": 0.0,
          "end": 8.211
        },
        {
          "text": "विश्वा᳚न्य॒न्यो भुव᳚नाभि॒चष्ट᳚ ऋ॒तूँर॒न्यो वि॒दध᳚ज्जायते॒ पुनः᳚ ॥",
          "begin": 8.211,
          "end": 14.76
        }
      ]
    },
    {
      "id": "10.85.19",
      "mandala": 10,
      "sukta": 85,
      "rik": 19,
      "padas": [
        {
          "text": "नवो᳚नवो भवति॒ जाय᳚मा॒नो॑ह्नां᳚ के॒तुरु॒षसा᳚मे॒त्यग्र᳚म् ।",
          "begin": 0.0,
          "end": 8.297
        },
        {
          "text": "भा॒गं दे॒वेभ्यो॒ वि द᳚धात्या॒यन्प्र च॒न्द्रमा᳚स्तिरते दी॒र्घमायु᳚ः ॥",
          "begin": 8.297,
          "end": 15.56
        }
      ]
    },
    {
      "id": "10.85.20",
      "mandala": 10,
      "sukta": 85,
      "rik": 20,
      "padas": [
        {
          "text": "सु॒किं॒शु॒कं श᳚ल्म॒लिं वि॒श्वरू᳚पं॒ हिर᳚ण्यवर्णं सु॒वृतं᳚ सुच॒क्रम् ।",
          "begin": 0.0,
          "end": 8.33
        },
        {
          "text": "आ रो᳚ह सूर्ये अ॒मृत᳚स्य लो॒कं स्यो॒नं पत्ये᳚ वह॒तुं कृ᳚णुष्व ॥",
          "begin": 8.33,
          "end": 15.4
        }
      ]
    },
    {
      "id": "10.85.21",
      "mandala": 10,
      "sukta": 85,
      "rik": 21,
      "padas": [
        {
          "text": "उदी॒र्ष्वात॒ः पति᳚वती॒ ह्ये॒३॒᳚षा वि॒श्वाव᳚सुं॒ नम᳚सा गी॒र्भिरी᳚ळे ।",
          "begin": 0.0,
          "end": 9.019
        },
        {
          "text": "अ॒न्यामि᳚च्छ पितृ॒षदं॒ व्य᳚क्तां॒ स ते᳚ भा॒गो ज॒नुषा॒ तस्य᳚ विद्धि ॥",
          "begin": 9.019,
          "end": 16.16
        }
      ]
    },
    {
      "id": "10.85.22",
      "mandala": 10,
      "sukta": 85,
      "rik": 22,
      "padas": [
        {
          "text": "उदी॒र्ष्वातो᳚ विश्वावसो॒ नम᳚सेळा महे त्वा ।",
          "begin": 0.0,
          "end": 6.815
        },
        {
          "text": "अ॒न्यामि᳚च्छ प्रफ॒र्व्यं१॒᳚ सं जा॒यां पत्या᳚ सृज ॥",
          "begin": 6.815,
          "end": 12.52
        }
      ]
    },
    {
      "id": "10.85.23",
      "mandala": 10,
      "sukta": 85,
      "rik": 23,
      "padas": [
        {
          "text": "अ॒नृ॒क्ष॒रा ऋ॒जवः᳚ सन्तु॒ पन्था॒ येभि॒ः सखा᳚यो॒ यन्ति᳚ नो वरे॒यम् ।",
          "begin": 0.0,
          "end": 7.822
        },
        {
          "text": "सम᳚र्य॒मा सं भगो᳚ नो निनीया॒त्सं जा᳚स्प॒त्यं सु॒यम᳚मस्तु देवाः ॥",
          "begin": 7.822,
          "end": 15.28
        }
      ]
    },
    {
      "id": "10.85.24",
      "mandala": 10,
      "sukta": 85,
      "rik": 24,
      "padas": [
        {
          "text": "प्र त्वा᳚ मुञ्चामि॒ वरु᳚णस्य॒ पाशा॒द्येन॒ त्वाब᳚ध्नात्सवि॒ता सु॒शेवः᳚ ।",
          "begin": 0.0,
          "end": 7.476
        },
        {
          "text": "ऋ॒तस्य॒ योनौ᳚ सुकृ॒तस्य᳚ लो॒के॑रि᳚ष्टां त्वा स॒ह पत्या᳚ दधामि ॥",
          "begin": 7.476,
          "end": 14.56
        }
      ]
    },
    {
      "id": "10.85.25",
      "mandala": 10,
      "sukta": 85,
      "rik": 25,
      "padas": [
        {
          "text": "प्रेतो मु॒ञ्चामि॒ नामुतः᳚ सुब॒द्धाम॒मुत᳚स्करम् ।",
          "begin": 0.0,
          "end": 5.786
        },
        {
          "text": "यथे॒यमि᳚न्द्र मीढ्वः सुपु॒त्रा सु॒भगास᳚ति ॥",
          "begin": 5.786,
          "end": 9.88
        }
      ]
    },
    {
      "id": "10.85.26",
      "mandala": 10,
      "sukta": 85,
      "rik": 26,
      "padas": [
        {
          "text": "पू॒षा त्वे॒तो न᳚यतु हस्त॒गृह्या॒श्विना᳚ त्वा॒ प्र व᳚हतां॒ रथे᳚न ।",
          "begin": 0.0,
          "end": 8.22
        },
        {
          "text": "गृ॒हान्ग᳚च्छ गृ॒हप᳚त्नी॒ यथासो᳚ व॒शिनी॒ त्वं वि॒दथ॒मा व᳚दासि ॥",
          "begin": 8.22,
          "end": 14.44
        }
      ]
    },
    {
      "id": "10.85.27",
      "mandala": 10,
      "sukta": 85,
      "rik": 27,
      "padas": [
        {
          "text": "इ॒ह प्रि॒यं प्र॒जया᳚ ते॒ समृ᳚ध्यताम॒स्मिन्गृ॒हे गार्ह᳚पत्याय जागृहि ।",
          "begin": 0.0,
          "end": 8.196
        },
        {
          "text": "ए॒ना पत्या᳚ त॒न्वं१॒᳚ सं सृ᳚ज॒स्वाधा॒ जिव्री᳚ वि॒दथ॒मा व᳚दाथः ॥",
          "begin": 8.196,
          "end": 15.16
        }
      ]
    },
    {
      "id": "10.85.28",
      "mandala": 10,
      "sukta": 85,
      "rik": 28,
      "padas": [
        {
          "text": "नी॒ल॒लो॒हि॒तं भ᳚वति कृ॒त्यास॒क्तिर्व्य᳚ज्यते ।",
          "begin": 0.0,
          "end": 5.619
        },
        {
          "text": "एध᳚न्ते अस्या ज्ञा॒तय॒ः पति᳚र्ब॒न्धेषु᳚ बध्यते ॥",
          "begin": 5.619,
          "end": 10.92
        }
      ]
    },
    {
      "id": "10.85.29",
      "mandala": 10,
      "sukta": 85,
      "rik": 29,
      "padas": [
        {
          "text": "परा᳚ देहि शामु॒ल्यं᳚ ब्र॒ह्मभ्यो॒ वि भ᳚जा॒ वसु᳚ ।",
          "begin": 0.0,
          "end": 6.037
        },
        {
          "text": "कृ॒त्यैषा प॒द्वती᳚ भू॒त्व्या जा॒या वि᳚शते॒ पति᳚म् ॥",
          "begin": 6.037,
          "end": 11.64
        }
      ]
    },
    {
      "id": "10.85.30",
      "mandala": 10,
      "sukta": 85,
      "rik": 30,
      "padas": [
        {
          "text": "अ॒श्री॒रा त॒नूर्भ᳚वति॒ रुश᳚ती पा॒पया᳚मु॒या ।",
          "begin": 0.0,
          "end": 5.503
        },
        {
          "text": "पति॒र्यद्व॒ध्वो॒३॒᳚ वास᳚सा॒ स्वमङ्ग᳚मभि॒धित्स᳚ते ॥",
          "begin": 5.503,
          "end": 12.48
        }
      ]
    },
    {
      "id": "10.85.31",
      "mandala": 10,
      "sukta": 85,
      "rik": 31,
      "padas": [
        {
          "text": "ये व॒ध्व᳚श्च॒न्द्रं व᳚ह॒तुं यक्ष्मा॒ यन्ति॒ जना॒दनु᳚ ।",
          "begin": 0.0,
          "end": 5.719
        },
        {
          "text": "पुन॒स्तान्य॒ज्ञिया᳚ दे॒वा नय᳚न्तु॒ यत॒ आग᳚ताः ॥",
          "begin": 5.719,
          "end": 11.44
        }
      ]
    },
    {
      "id": "10.85.32",
      "mandala": 10,
      "sukta": 85,
      "rik": 32,
      "padas": [
        {
          "text": "मा वि᳚दन्परिप॒न्थिनो॒ य आ॒सीद᳚न्ति॒ दम्प᳚ती ।",
          "begin": 0.0,
          "end": 4.882
        },
        {
          "text": "सु॒गेभि᳚र्दु॒र्गमती᳚ता॒मप᳚ द्रा॒न्त्वरा᳚तयः ॥",
          "begin": 4.882,
          "end": 11.24
        }
      ]
    },
    {
      "id": "10.85.33",
      "mandala": 10,
      "sukta": 85,
      "rik": 33,
      "padas": [
        {
          "text": "सु॒म॒ङ्ग॒लीरि॒यं व॒धूरि॒मां स॒मेत॒ पश्य᳚त ।",
          "begin": 0.0,
          "end": 5.934
        },
        {
          "text": "सौभा᳚ग्यमस्यै द॒त्त्वायाथास्तं॒ वि परे᳚तन ॥",
          "begin": 5.934,
          "end": 11.68
        }
      ]
    },
    {
      "id": "10.85.34",
      "mandala": 10,
      "sukta": 85,
      "rik": 34,
      "padas": [
        {
          "text": "तृ॒ष्टमे॒तत्कटु᳚कमे॒तद᳚पा॒ष्ठव᳚द्वि॒षव॒न्नैतदत्त᳚वे ।",
          "begin": 0.0,
          "end": 6.565
        },
        {
          "text": "सू॒र्यां यो ब्र॒ह्मा वि॒द्यात्स इद्वाधू᳚यमर्हति ॥",
          "begin": 6.565,
          "end": 12.48
        }
      ]
    },
    {
      "id": "10.85.35",
      "mandala": 10,
      "sukta": 85,
      "rik": 35,
      "padas": [
        {
          "text": "आ॒शस᳚नं वि॒शस᳚न॒मथो᳚ अधिवि॒कर्त᳚नम् ।",
          "begin": 0.0,
          "end": 5.365
        },
        {
          "text": "सू॒र्याया᳚ः पश्य रू॒पाणि॒ तानि᳚ ब्र॒ह्मा तु शु᳚न्धति ॥",
          "begin": 5.365,
          "end": 11.28
        }
      ]
    },
    {
      "id": "10.85.36",
      "mandala": 10,
      "sukta": 85,
      "rik": 36,
      "padas": [
        {
          "text": "गृ॒भ्णामि᳚ ते सौभग॒त्वाय॒ हस्तं॒ मया॒ पत्या᳚ ज॒रद᳚ष्टि॒र्यथासः᳚ ।",
          "begin": 0.0,
          "end": 7.5
        },
        {
          "text": "भगो᳚ अर्य॒मा स᳚वि॒ता पुरं᳚धि॒र्मह्यं᳚ त्वादु॒र्गार्ह᳚पत्याय दे॒वाः ॥",
          "begin": 7.5,
          "end": 15.56
        }
      ]
    },
    {
      "id": "10.85.37",
      "mandala": 10,
      "sukta": 85,
      "rik": 37,
      "padas": [
        {
          "text": "तां पू᳚षञ्छि॒वत᳚मा॒मेर᳚यस्व॒ यस्यां॒ बीजं᳚ मनु॒ष्या॒३॒᳚ वप᳚न्ति ।",
          "begin": 0.0,
          "end": 10.663
        },
        {
          "text": "या न᳚ ऊ॒रू उ᳚श॒ती वि॒श्रया᳚ते॒ यस्या᳚मु॒शन्त᳚ः प्र॒हरा᳚म॒ शेप᳚म् ॥",
          "begin": 10.663,
          "end": 18.72
        }
      ]
    },
    {
      "id": "10.85.38",
      "mandala": 10,
      "sukta": 85,
      "rik": 38,
      "padas": [
        {
          "text": "तुभ्य॒मग्रे॒ पर्य᳚वहन्त्सू॒र्यां व᳚ह॒तुना᳚ स॒ह ।",
          "begin": 0.0,
          "end": 5.894
        },
        {
          "text": "पुन॒ः पति᳚भ्यो जा॒यां दा अ᳚ग्ने प्र॒जया᳚ स॒ह ॥",
          "begin": 5.894,
          "end": 12.04
        }
      ]
    },
    {
      "id": "10.85.39",
      "mandala": 10,
      "sukta": 85,
      "rik": 39,
      "padas": [
        {
          "text": "पुन॒ः पत्नी᳚म॒ग्निर᳚दा॒दायु᳚षा स॒ह वर्च᳚सा ।",
          "begin": 0.0,
          "end": 3.426
        },
        {
          "text": "दी॒र्घायु᳚रस्या॒ यः पति॒र्जीवा᳚ति श॒रदः᳚ श॒तम् ॥",
          "begin": 3.426,
          "end": 11.12
        }
      ]
    },
    {
      "id": "10.85.40",
      "mandala": 10,
      "sukta": 85,
      "rik": 40,
      "padas": [
        {
          "text": "सोमः᳚ प्रथ॒मो वि᳚विदे गन्ध॒र्वो वि᳚विद॒ उत्त᳚रः ।",
          "begin": 0.0,
          "end": 5.955
        },
        {
          "text": "तृ॒तीयो᳚ अ॒ग्निष्टे॒ पति᳚स्तु॒रीय᳚स्ते मनुष्य॒जाः ॥",
          "begin": 5.955,
          "end": 11.28
        }
      ]
    },
    {
      "id": "10.85.41",
      "mandala": 10,
      "sukta": 85,
      "rik": 41,
      "padas": [
        {
          "text": "सोमो᳚ ददद्गन्ध॒र्वाय᳚ गन्ध॒र्वो द᳚दद॒ग्नये᳚ ।",
          "begin": 0.0,
          "end": 6.497
        },
        {
          "text": "र॒यिं च᳚ पु॒त्राँश्चा᳚दाद॒ग्निर्मह्य॒मथो᳚ इ॒माम् ॥",
          "begin": 6.497,
          "end": 12.64
        }
      ]
    },
    {
      "id": "10.85.42",
      "mandala": 10,
      "sukta": 85,
      "rik": 42,
      "padas": [
        {
          "text": "इ॒हैव स्तं॒ मा वि यौ᳚ष्टं॒ विश्व॒मायु॒र्व्य᳚श्नुतम् ।",
          "begin": 0.0,
          "end": 5.84
        },
        {
          "text": "क्रीळ᳚न्तौ पु॒त्रैर्नप्तृ᳚भि॒र्मोद᳚मानौ॒ स्वे गृ॒हे ॥",
          "begin": 5.84,
          "end": 12.2
        }
      ]
    },
    {
      "id": "10.85.43",
      "mandala": 10,
      "sukta": 85,
      "rik": 43,
      "padas": [
        {
          "text": "आ नः᳚ प्र॒जां ज᳚नयतु प्र॒जाप᳚तिराजर॒साय॒ सम᳚नक्त्वर्य॒मा ।",
          "begin": 0.0,
          "end": 4.439
        },
        {
          "text": "अदु᳚र्मङ्गलीः पतिलो॒कमा वि᳚श॒ शं नो᳚ भव द्वि॒पदे॒ शं चतु᳚ष्पदे ॥",
          "begin": 4.439,
          "end": 14.2
        }
      ]
    },
    {
      "id": "10.85.44",
      "mandala": 10,
      "sukta": 85,
      "rik": 44,
      "padas": [
        {
          "text": "अघो᳚रचक्षु॒रप᳚तिघ्न्येधि शि॒वा प॒शुभ्य᳚ः सु॒मना᳚ः सु॒वर्चा᳚ः ।",
          "begin": 0.0,
          "end": 7.746
        },
        {
          "text": "वी॒र॒सूर्दे॒वका᳚मा स्यो॒ना शं नो᳚ भव द्वि॒पदे॒ शं चतु᳚ष्पदे ॥",
          "begin": 7.746,
          "end": 15.36
        }
      ]
    },
    {
      "id": "10.85.45",
      "mandala": 10,
      "sukta": 85,
      "rik": 45,
      "padas": [
        {
          "text": "इ॒मां त्वमि᳚न्द्र मीढ्वः सुपु॒त्रां सु॒भगां᳚ कृणु ।",
          "begin": 0.0,
          "end": 7.136
        },
        {
          "text": "दशा᳚स्यां पु॒त्राना धे᳚हि॒ पति᳚मेकाद॒शं कृ᳚धि ॥",
          "begin": 7.136,
          "end": 12.84
        }
      ]
    },
    {
      "id": "10.85.46",
      "mandala": 10,
      "sukta": 85,
      "rik": 46,
      "padas": [
        {
          "text": "स॒म्राज्ञी॒ श्वशु᳚रे भव स॒म्राज्ञी᳚ श्व॒श्र्वां भ᳚व ।",
          "begin": 0.0,
          "end": 7.07
        },
        {
          "text": "नना᳚न्दरि स॒म्राज्ञी᳚ भव स॒म्राज्ञी॒ अधि᳚ दे॒वृषु᳚ ॥",
          "begin": 7.07,
          "end": 13.56
        }
      ]
    },
    {
      "id": "10.85.47",
      "mandala": 10,
      "sukta": 85,
      "rik": 47,
      "padas": [
        {
          "text": "सम᳚ञ्जन्तु॒ विश्वे᳚ दे॒वाः समापो॒ हृद᳚यानि नौ ।",
          "begin": 0.0,
          "end": 6.794
        },
        {
          "text": "सं मा᳚त॒रिश्वा॒ सं धा॒ता समु॒ देष्ट्री᳚ दधातु नौ ॥",
          "begin": 6.794,
          "end": 12.8
        }
      ]
    }
  ]
}