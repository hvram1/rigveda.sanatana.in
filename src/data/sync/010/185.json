      "padas": [
        {
          "text": "महि᳚ त्री॒णामवो᳚॑स्तु द्यु॒क्षं मि॒त्रस्या᳚र्य॒म्णः ।",
          "begin": 0.0,
          "end": 8.595
        },
        {
          "text": "दु॒रा॒धर्षं॒ वरु᳚णस्य ॥",
          "begin": 8.595,
          "end": 11.44
        }
      ]
    },
    {
      "id": "10.185.2",
      "mandala": 10,
      "sukta": 185,
      "rik": 2,
      "padas": [
        {
          "text": "न॒हि तेषा᳚म॒मा च॒न नाध्व᳚सु वार॒णेषु᳚ ।",
          "begin": 0.0,
          "end": 5.979
        },
        {
          "text": "ईशे᳚ रि॒पुर॒घशं᳚सः ॥",
          "begin": 5.979,
          "end": 8.88
        }
      ]
    },
    {
      "id": "10.185.3",
      "mandala": 10,
      "sukta": 185,
      "rik": 3,
      "padas": [
        {
          "text": "यस्मै᳚ पु॒त्रासो॒ अदि᳚ते॒ः प्र जी॒वसे॒ मर्त्या᳚य ।",
          "begin": 0.0,
          "end": 6.703
        },
        {
          "text": "ज्योति॒र्यच्छ॒न्त्यज᳚स्रम् ॥",
          "begin": 6.703,
          "end": 11.48
        }
      ]
    }
  ]
}