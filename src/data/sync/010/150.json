{
  "mandala": 10,
  "sukta": 150,
  "verses": [
    {
      "id": "10.150.1",
      "mandala": 10,
      "sukta": 150,
      "rik": 1,
      "padas": [
        {
          "text": "समि᳚द्धश्चि॒त्समि᳚ध्यसे दे॒वेभ्यो᳚ हव्यवाहन ।",
          "begin": 0.0,
          "end": 5.719
        },
        {
          "text": "आ॒दि॒त्यै रु॒द्रैर्वसु᳚भिर्न॒ आ ग᳚हि मृळी॒काय᳚ न॒ आ ग᳚हि ॥",
          "begin": 5.719,
          "end": 14.96
        }
      ]
    },
    {
      "id": "10.150.2",
      "mandala": 10,
      "sukta": 150,
      "rik": 2,
      "padas": [
        {
          "text": "इ॒मं य॒ज्ञमि॒दं वचो᳚ जुजुषा॒ण उ॒पाग᳚हि ।",
          "begin": 0.0,
          "end": 4.784
        },
        {
          "text": "मर्ता᳚सस्त्वा समिधान हवामहे मृळी॒काय᳚ हवामहे ॥",
          "begin": 4.784,
          "end": 11.2
        }
      ]
    },
    {
      "id": "10.150.3",
      "mandala": 10,
      "sukta": 150,
      "rik": 3,
      "padas": [
        {
          "text": "त्वामु᳚ जा॒तवे᳚दसं वि॒श्ववा᳚रं गृणे धि॒या ।",
          "begin": 0.0,
          "end": 5.549
        },
        {
          "text": "अग्ने᳚ दे॒वाँ आ व᳚ह नः प्रि॒यव्र᳚तान्मृळी॒काय᳚ प्रि॒यव्र᳚तान् ॥",
          "begin": 5.549,
          "end": 13.24
        }
      ]
    },
    {
      "id": "10.150.4",
      "mandala": 10,
      "sukta": 150,
      "rik": 4,
      "padas": [
        {
          "text": "अ॒ग्निर्दे॒वो दे॒वाना᳚मभवत्पु॒रोहि᳚तो॒॑ग्निं म᳚नु॒ष्या॒३॒᳚ ऋष᳚य॒ः समी᳚धिरे ।",
          "begin": 0.0,
          "end": 10.332
        },
        {
          "text": "अ॒ग्निं म॒हो धन᳚साताव॒हं हु᳚वे मृळी॒कं धन᳚सातये ॥",
          "begin": 10.332,
          "end": 15.56
        }
      ]
    },
    {
      "id": "10.150.5",
      "mandala": 10,
      "sukta": 150,
      "rik": 5,
      "padas": [
        {
          "text": "अ॒ग्निरत्रिं᳚ भ॒रद्वा᳚जं॒ गवि᳚ष्ठिरं॒ प्राव᳚न्न॒ः कण्वं᳚ त्र॒सद᳚स्युमाह॒वे ।",
          "begin": 0.0,
          "end": 8.692
        },
        {
          "text": "अ॒ग्निं वसि᳚ष्ठो हवते पु॒रोहि᳚तो मृळी॒काय᳚ पु॒रोहि᳚तः ॥",
          "begin": 8.692,
          "end": 17.08
        }
      ]
    }
  ]
}