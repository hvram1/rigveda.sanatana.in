{
  "mandala": 10,
  "sukta": 156,
  "verses": [
    {
      "id": "10.156.1",
      "mandala": 10,
      "sukta": 156,
      "rik": 1,
      "padas": [
        {
          "text": "अ॒ग्निं हि᳚न्वन्तु नो॒ धिय॒ः सप्ति᳚मा॒शुमि᳚वा॒जिषु᳚ ।",
          "begin": 0.0,
          "end": 6.57
        },
        {
          "text": "तेन᳚ जेष्म॒ धनं᳚धनम् ॥",
          "begin": 6.57,
          "end": 9.92
        }
      ]
    },
    {
      "id": "10.156.2",
      "mandala": 10,
      "sukta": 156,
      "rik": 2,
      "padas": [
        {
          "text": "यया॒ गा आ॒करा᳚महे॒ सेन᳚याग्ने॒ तवो॒त्या ।",
          "begin": 0.0,
          "end": 5.089
        },
        {
          "text": "तां नो᳚ हिन्व म॒घत्त᳚ये ॥",
          "begin": 5.089,
          "end": 9.48
        }
      ]
    },
    {
      "id": "10.156.3",
      "mandala": 10,
      "sukta": 156,
      "rik": 3,
      "padas": [
        {
          "text": "आग्ने᳚ स्थू॒रं र॒यिं भ᳚र पृ॒थुं गोम᳚न्तम॒श्विन᳚म् ।",
          "begin": 0.0,
          "end": 5.741
        },
        {
          "text": "अ॒ङ्ग्धि खं व॒र्तया᳚ प॒णिम् ॥",
          "begin": 5.741,
          "end": 9.96
        }
      ]
    },
    {
      "id": "10.156.4",
      "mandala": 10,
      "sukta": 156,
      "rik": 4,
      "padas": [
        {
          "text": "अग्ने॒ नक्ष᳚त्रम॒जर॒मा सूर्यं᳚ रोहयो दि॒वि ।",
          "begin": 0.0,
          "end": 5.997
        },
        {
          "text": "दध॒ज्ज्योति॒र्जने᳚भ्यः ॥",
          "begin": 5.997,
          "end": 8.8
        }
      ]
    },
    {
      "id": "10.156.5",
      "mandala": 10,
      "sukta": 156,
      "rik": 5,
      "padas": [
        {
          "text": "अग्ने᳚ के॒तुर्वि॒शाम᳚सि॒ प्रेष्ठ॒ः श्रेष्ठ᳚ उपस्थ॒सत् ।",
          "begin": 0.0,
          "end": 8.535
        },
        {
          "text": "बोधा᳚ स्तो॒त्रे वयो॒ दध᳚त् ॥",
          "begin": 8.535,
          "end": 12.92
        }
      ]
    }
  ]
}