{
  "mandala": 10,
  "sukta": 119,
  "verses": [
    {
      "id": "10.119.1",
      "mandala": 10,
      "sukta": 119,
      "rik": 1,
      "padas": [
        {
          "text": "इति॒ वा इति᳚ मे॒ मनो॒ गामश्वं᳚ सनुया॒मिति᳚ ।",
          "begin": 0.0,
          "end": 8.284
        },
        {
          "text": "कु॒वित्सोम॒स्यापा॒मिति᳚ ॥",
          "begin": 8.284,
          "end": 11.16
        }
      ]
    },
    {
      "id": "10.119.2",
      "mandala": 10,
      "sukta": 119,
      "rik": 2,
      "padas": [
        {
          "text": "प्र वाता᳚ इव॒ दोध᳚त॒ उन्मा᳚ पी॒ता अ᳚यंसत ।",
          "begin": 0.0,
          "end": 5.792
        },
        {
          "text": "कु॒वित्सोम॒स्यापा॒मिति᳚ ॥",
          "begin": 5.792,
          "end": 8.6
        }
      ]
    },
    {
      "id": "10.119.3",
      "mandala": 10,
      "sukta": 119,
      "rik": 3,
      "padas": [
        {
          "text": "उन्मा᳚ पी॒ता अ᳚यंसत॒ रथ॒मश्वा᳚ इवा॒शवः᳚ ।",
          "begin": 0.0,
          "end": 6.128
        },
        {
          "text": "कु॒वित्सोम॒स्यापा॒मिति᳚ ॥",
          "begin": 6.128,
          "end": 8.8
        }
      ]
    },
    {
      "id": "10.119.4",
      "mandala": 10,
      "sukta": 119,
      "rik": 4,
      "padas": [
        {
          "text": "उप᳚ मा म॒तिर᳚स्थित वा॒श्रा पु॒त्रमि᳚व प्रि॒यम् ।",
          "begin": 0.0,
          "end": 5.205
        },
        {
          "text": "कु॒वित्सोम॒स्यापा॒मिति᳚ ॥",
          "begin": 5.205,
          "end": 7.64
        }
      ]
    },
    {
      "id": "10.119.5",
      "mandala": 10,
      "sukta": 119,
      "rik": 5,
      "padas": [
        {
          "text": "अ॒हं तष्टे᳚व व॒न्धुरं॒ पर्य᳚चामि हृ॒दा म॒तिम् ।",
          "begin": 0.0,
          "end": 5.549
        },
        {
          "text": "कु॒वित्सोम॒स्यापा॒मिति᳚ ॥",
          "begin": 5.549,
          "end": 8.08
        }
      ]
    },
    {
      "id": "10.119.6",
      "mandala": 10,
      "sukta": 119,
      "rik": 6,
      "padas": [
        {
          "text": "न॒हि मे᳚ अक्षि॒पच्च॒नाच्छा᳚न्त्सु॒ः पञ्च᳚ कृ॒ष्टयः᳚ ।",
          "begin": 0.0,
          "end": 6.362
        },
        {
          "text": "कु॒वित्सोम॒स्यापा॒मिति᳚ ॥",
          "begin": 6.362,
          "end": 9.0
        }
      ]
    },
    {
      "id": "10.119.7",
      "mandala": 10,
      "sukta": 119,
      "rik": 7,
      "padas": [
        {
          "text": "न॒हि मे॒ रोद᳚सी उ॒भे अ॒न्यं प॒क्षं च॒न प्रति᳚ ।",
          "begin": 0.0,
          "end": 5.901
        },
        {
          "text": "कु॒वित्सोम॒स्यापा॒मिति᳚ ॥",
          "begin": 5.901,
          "end": 8.48
        }
      ]
    },
    {
      "id": "10.119.8",
      "mandala": 10,
      "sukta": 119,
      "rik": 8,
      "padas": [
        {
          "text": "अ॒भि द्यां म᳚हि॒ना भु᳚वम॒भी॒३॒᳚मां पृ᳚थि॒वीं म॒हीम् ।",
          "begin": 0.0,
          "end": 7.539
        },
        {
          "text": "कु॒वित्सोम॒स्यापा॒मिति᳚ ॥",
          "begin": 7.539,
          "end": 10.28
        }
      ]
    },
    {
      "id": "10.119.9",
      "mandala": 10,
      "sukta": 119,
      "rik": 9,
      "padas": [
        {
          "text": "हन्ता॒हं पृ᳚थि॒वीमि॒मां नि द᳚धानी॒ह वे॒ह वा᳚ ।",
          "begin": 0.0,
          "end": 5.725
        },
        {
          "text": "कु॒वित्सोम॒स्यापा॒मिति᳚ ॥",
          "begin": 5.725,
          "end": 8.64
        }
      ]
    },
    {
      "id": "10.119.10",
      "mandala": 10,
      "sukta": 119,
      "rik": 10,
      "padas": [
        {
          "text": "ओ॒षमित्पृ᳚थि॒वीम॒हं ज॒ङ्घना᳚नी॒ह वे॒ह वा᳚ ।",
          "begin": 0.0,
          "end": 6.012
        },
        {
          "text": "कु॒वित्सोम॒स्यापा॒मिति᳚ ॥",
          "begin": 6.012,
          "end": 8.72
        }
      ]
    },
    {
      "id": "10.119.11",
      "mandala": 10,
      "sukta": 119,
      "rik": 11,
      "padas": [
        {
          "text": "दि॒वि मे᳚ अ॒न्यः प॒क्षो॒३॒᳚॑धो अ॒न्यम᳚चीकृषम् ।",
          "begin": 0.0,
          "end": 7.221
        },
        {
          "text": "कु॒वित्सोम॒स्यापा॒मिति᳚ ॥",
          "begin": 7.221,
          "end": 10.76
        }
      ]
    },
    {
      "id": "10.119.12",
      "mandala": 10,
      "sukta": 119,
      "rik": 12,
      "padas": [
        {
          "text": "अ॒हम᳚स्मि महाम॒हो᳚॑भिन॒भ्यमुदी᳚षितः ।",
          "begin": 0.0,
          "end": 5.606
        },
        {
          "text": "कु॒वित्सोम॒स्यापा॒मिति᳚ ॥",
          "begin": 5.606,
          "end": 8.36
        }
      ]
    },
    {
      "id": "10.119.13",
      "mandala": 10,
      "sukta": 119,
      "rik": 13,
      "padas": [
        {
          "text": "गृ॒हो या॒म्यरं᳚कृतो दे॒वेभ्यो᳚ हव्य॒वाह᳚नः ।",
          "begin": 0.0,
          "end": 6.149
        },
        {
          "text": "कु॒वित्सोम॒स्यापा॒मिति᳚ ॥",
          "begin": 6.149,
          "end": 9.16
        }
      ]
    }
  ]
}