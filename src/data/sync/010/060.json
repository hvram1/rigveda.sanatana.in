{
  "mandala": 10,
  "sukta": 60,
  "verses": [
    {
      "id": "10.60.1",
      "mandala": 10,
      "sukta": 60,
      "rik": 1,
      "padas": [
        {
          "text": "आ जनं᳚ त्वे॒षसं᳚दृशं॒ माही᳚नाना॒मुप᳚स्तुतम् ।",
          "begin": 0.0,
          "end": 6.527
        },
        {
          "text": "अग᳚न्म॒ बिभ्र᳚तो॒ नमः᳚ ॥",
          "begin": 6.527,
          "end": 11.04
        }
      ]
    },
    {
      "id": "10.60.2",
      "mandala": 10,
      "sukta": 60,
      "rik": 2,
      "padas": [
        {
          "text": "अस᳚मातिं नि॒तोश᳚नं त्वे॒षं नि᳚य॒यिनं॒ रथ᳚म् ।",
          "begin": 0.0,
          "end": 5.197
        },
        {
          "text": "भ॒जेर᳚थस्य॒ सत्प᳚तिम् ॥",
          "begin": 5.197,
          "end": 8.24
        }
      ]
    },
    {
      "id": "10.60.3",
      "mandala": 10,
      "sukta": 60,
      "rik": 3,
      "padas": [
        {
          "text": "यो जना᳚न्महि॒षाँ इ᳚वातित॒स्थौ पवी᳚रवान् ।",
          "begin": 0.0,
          "end": 6.464
        },
        {
          "text": "उ॒ताप᳚वीरवान्यु॒धा ॥",
          "begin": 6.464,
          "end": 9.12
        }
      ]
    },
    {
      "id": "10.60.4",
      "mandala": 10,
      "sukta": 60,
      "rik": 4,
      "padas": [
        {
          "text": "यस्ये᳚क्ष्वा॒कुरुप᳚ व्र॒ते रे॒वान्म᳚रा॒य्येध᳚ते ।",
          "begin": 0.0,
          "end": 6.785
        },
        {
          "text": "दि॒वी᳚व॒ पञ्च᳚ कृ॒ष्टयः᳚ ॥",
          "begin": 6.785,
          "end": 9.68
        }
      ]
    },
    {
      "id": "10.60.5",
      "mandala": 10,
      "sukta": 60,
      "rik": 5,
      "padas": [
        {
          "text": "इन्द्र᳚ क्ष॒त्रास᳚मातिषु॒ रथ᳚प्रोष्ठेषु धारय ।",
          "begin": 0.0,
          "end": 5.489
        },
        {
          "text": "दि॒वी᳚व॒ सूर्यं᳚ दृ॒शे ॥",
          "begin": 5.489,
          "end": 8.44
        }
      ]
    },
    {
      "id": "10.60.6",
      "mandala": 10,
      "sukta": 60,
      "rik": 6,
      "padas": [
        {
          "text": "अ॒गस्त्य᳚स्य॒ नद्भ्य॒ः सप्ती᳚ युनक्षि॒ रोहि᳚ता ।",
          "begin": 0.0,
          "end": 4.894
        },
        {
          "text": "प॒णीन्न्य᳚क्रमीर॒भि विश्वा᳚न्राजन्नरा॒धसः᳚ ॥",
          "begin": 4.894,
          "end": 10.0
        }
      ]
    },
    {
      "id": "10.60.7",
      "mandala": 10,
      "sukta": 60,
      "rik": 7,
      "padas": [
        {
          "text": "अ॒यं मा॒तायं पि॒तायं जी॒वातु॒राग᳚मत् ।",
          "begin": 0.0,
          "end": 5.111
        },
        {
          "text": "इ॒दं तव᳚ प्र॒सर्प᳚णं॒ सुब᳚न्ध॒वेहि॒ निरि᳚हि ॥",
          "begin": 5.111,
          "end": 9.36
        }
      ]
    },
    {
      "id": "10.60.8",
      "mandala": 10,
      "sukta": 60,
      "rik": 8,
      "padas": [
        {
          "text": "यथा᳚ यु॒गं व᳚र॒त्रया॒ नह्य᳚न्ति ध॒रुणा᳚य॒ कम् ।",
          "begin": 0.0,
          "end": 6.457
        },
        {
          "text": "ए॒वा दा᳚धार ते॒ मनो᳚ जी॒वात᳚वे॒ न मृ॒त्यवे॑थो᳚ अरि॒ष्टता᳚तये ॥",
          "begin": 6.457,
          "end": 13.92
        }
      ]
    },
    {
      "id": "10.60.9",
      "mandala": 10,
      "sukta": 60,
      "rik": 9,
      "padas": [
        {
          "text": "यथे॒यं पृ᳚थि॒वी म॒ही दा॒धारे॒मान्वन॒स्पती᳚न् ।",
          "begin": 0.0,
          "end": 3.401
        },
        {
          "text": "ए॒वा दा᳚धार ते॒ मनो᳚ जी॒वात᳚वे॒ न मृ॒त्यवे॑थो᳚ अरि॒ष्टता᳚तये ॥",
          "begin": 3.401,
          "end": 13.72
        }
      ]
    },
    {
      "id": "10.60.10",
      "mandala": 10,
      "sukta": 60,
      "rik": 10,
      "padas": [
        {
          "text": "य॒माद॒हं वै᳚वस्व॒तात्सु॒बन्धो॒र्मन॒ आभ᳚रम् ।",
          "begin": 0.0,
          "end": 6.498
        },
        {
          "text": "जी॒वात᳚वे॒ न मृ॒त्यवे॑थो᳚ अरि॒ष्टता᳚तये ॥",
          "begin": 6.498,
          "end": 11.44
        }
      ]
    },
    {
      "id": "10.60.11",
      "mandala": 10,
      "sukta": 60,
      "rik": 11,
      "padas": [
        {
          "text": "न्य१॒᳚ग्वातो॑व᳚ वाति॒ न्य᳚क्तपति॒ सूर्य᳚ः ।",
          "begin": 0.0,
          "end": 6.36
        },
        {
          "text": "नी॒चीन᳚म॒घ्न्या दु᳚हे॒ न्य᳚ग्भवतु ते॒ रपः᳚ ॥",
          "begin": 6.36,
          "end": 10.84
        }
      ]
    },
    {
      "id": "10.60.12",
      "mandala": 10,
      "sukta": 60,
      "rik": 12,
      "padas": [
        {
          "text": "अ॒यं मे॒ हस्तो॒ भग᳚वान॒यं मे॒ भग᳚वत्तरः ।",
          "begin": 0.0,
          "end": 6.672
        },
        {
          "text": "अ॒यं मे᳚ वि॒श्वभे᳚षजो॒॑यं शि॒वाभि᳚मर्शनः ॥",
          "begin": 6.672,
          "end": 12.16
        }
      ]
    }
  ]
}