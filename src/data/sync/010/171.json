  "verses": [
    {
      "id": "10.171.1",
      "mandala": 10,
      "sukta": 171,
      "rik": 1,
      "padas": [
        {
          "text": "त्वं त्यमि॒टतो॒ रथ॒मिन्द्र॒ प्रावः᳚ सु॒ताव᳚तः ।",
          "begin": 0.0,
          "end": 6.313
        },
        {
          "text": "अशृ᳚णोः सो॒मिनो॒ हव᳚म् ॥",
          "begin": 6.313,
          "end": 9.24
        }
      ]
    },
    {
      "id": "10.171.2",
      "mandala": 10,
      "sukta": 171,
      "rik": 2,
      "padas": [
        {
          "text": "त्वं म॒खस्य॒ दोध᳚त॒ः शिरो॑व᳚ त्व॒चो भ᳚रः ।",
          "begin": 0.0,
          "end": 4.837
        },
        {
          "text": "अग᳚च्छः सो॒मिनो᳚ गृ॒हम् ॥",
          "begin": 4.837,
          "end": 7.52
        }
      ]
    },
    {
      "id": "10.171.3",
      "mandala": 10,
      "sukta": 171,
      "rik": 3,
      "padas": [
        {
          "text": "त्वं त्यमि᳚न्द्र॒ मर्त्य᳚मास्त्रबु॒ध्नाय᳚ वे॒न्यम् ।",
          "begin": 0.0,
          "end": 5.19
        },
        {
          "text": "मुहु᳚ः श्रथ्ना मन॒स्यवे᳚ ॥",
          "begin": 5.19,
          "end": 7.96
        }
      ]
    },
    {
      "id": "10.171.4",
      "mandala": 10,
      "sukta": 171,
      "rik": 4,
      "padas": [
        {
          "text": "त्वं त्यमि᳚न्द्र॒ सूर्यं᳚ प॒श्चा सन्तं᳚ पु॒रस्कृ᳚धि ।",
          "begin": 0.0,
          "end": 10.053
        },
        {
          "text": "दे॒वानां᳚ चित्ति॒रो वश᳚म् ॥",
          "begin": 10.053,
          "end": 13.0
        }
      ]
    }
  ]
}