{
  "mandala": 10,
  "sukta": 174,
  "verses": [
    {
      "id": "10.174.1",
      "mandala": 10,
      "sukta": 174,
      "rik": 1,
      "padas": [
        {
          "text": "अ॒भी॒व॒र्तेन᳚ ह॒विषा॒ येनेन्द्रो᳚ अभिवावृ॒ते ।",
          "begin": 0.0,
          "end": 7.374
        },
        {
          "text": "तेना॒स्मान्ब्र᳚ह्मणस्पते॒॑भि रा॒ष्ट्राय᳚ वर्तय ॥",
          "begin": 7.374,
          "end": 12.84
        }
      ]
    },
    {
      "id": "10.174.2",
      "mandala": 10,
      "sukta": 174,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒भि॒वृत्य᳚ स॒पत्ना᳚न॒भि या नो॒ अरा᳚तयः ।",
          "begin": 0.0,
          "end": 5.643
        },
        {
          "text": "अ॒भि पृ᳚त॒न्यन्तं᳚ तिष्ठा॒भि यो न᳚ इर॒स्यति᳚ ॥",
          "begin": 5.643,
          "end": 10.04
        }
      ]
    },
    {
      "id": "10.174.3",
      "mandala": 10,
      "sukta": 174,
      "rik": 3,
      "padas": [
        {
          "text": "अ॒भि त्वा᳚ दे॒वः स᳚वि॒ताभि सोमो᳚ अवीवृतत् ।",
          "begin": 0.0,
          "end": 5.918
        },
        {
          "text": "अ॒भि त्वा॒ विश्वा᳚ भू॒तान्य᳚भीव॒र्तो यथास᳚सि ॥",
          "begin": 5.918,
          "end": 10.64
        }
      ]
    },
    {
      "id": "10.174.4",
      "mandala": 10,
      "sukta": 174,
      "rik": 4,
      "padas": [
        {
          "text": "येनेन्द्रो᳚ ह॒विषा᳚ कृ॒त्व्यभ᳚वद्द्यु॒म्न्यु᳚त्त॒मः ।",
          "begin": 0.0,
          "end": 5.876
        },
        {
          "text": "इ॒दं तद᳚क्रि देवा असप॒त्नः किला᳚भुवम् ॥",
          "begin": 5.876,
          "end": 10.32
        }
      ]
    },
    {
      "id": "10.174.5",
      "mandala": 10,
      "sukta": 174,
      "rik": 5,
      "padas": [
        {
          "text": "अ॒स॒प॒त्नः स᳚पत्न॒हाभिरा᳚ष्ट्रो विषास॒हिः ।",
          "begin": 0.0,
          "end": 5.313
        },
        {
          "text": "यथा॒हमे᳚षां भू॒तानां᳚ वि॒राजा᳚नि॒ जन᳚स्य च ॥",
          "begin": 5.313,
          "end": 14.0
        }
      ]
    }
  ]
}