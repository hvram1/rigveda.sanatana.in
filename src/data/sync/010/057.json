{
  "mandala": 10,
  "sukta": 57,
  "verses": [
    {
      "id": "10.57.1",
      "mandala": 10,
      "sukta": 57,
      "rik": 1,
      "padas": [
        {
          "text": "मा प्र गा᳚म प॒थो व॒यं मा य॒ज्ञादि᳚न्द्र सो॒मिनः᳚ ।",
          "begin": 0.0,
          "end": 7.711
        },
        {
          "text": "मान्तः स्थु᳚र्नो॒ अरा᳚तयः ॥",
          "begin": 7.711,
          "end": 11.56
        }
      ]
    },
    {
      "id": "10.57.2",
      "mandala": 10,
      "sukta": 57,
      "rik": 2,
      "padas": [
        {
          "text": "यो य॒ज्ञस्य᳚ प्र॒साध᳚न॒स्तन्तु᳚र्दे॒वेष्वात᳚तः ।",
          "begin": 0.0,
          "end": 7.073
        },
        {
          "text": "तमाहु᳚तं नशीमहि ॥",
          "begin": 7.073,
          "end": 8.64
        }
      ]
    },
    {
      "id": "10.57.3",
      "mandala": 10,
      "sukta": 57,
      "rik": 3,
      "padas": [
        {
          "text": "मनो॒ न्वा हु᳚वामहे नाराशं॒सेन॒ सोमे᳚न ।",
          "begin": 0.0,
          "end": 6.961
        },
        {
          "text": "पि॒तॄ॒णां च॒ मन्म᳚भिः ॥",
          "begin": 6.961,
          "end": 8.8
        }
      ]
    },
    {
      "id": "10.57.4",
      "mandala": 10,
      "sukta": 57,
      "rik": 4,
      "padas": [
        {
          "text": "आ त᳚ एतु॒ मन॒ः पुन॒ः क्रत्वे॒ दक्षा᳚य जी॒वसे᳚ ।",
          "begin": 0.0,
          "end": 6.262
        },
        {
          "text": "ज्योक्च॒ सूर्यं᳚ दृ॒शे ॥",
          "begin": 6.262,
          "end": 9.2
        }
      ]
    },
    {
      "id": "10.57.5",
      "mandala": 10,
      "sukta": 57,
      "rik": 5,
      "padas": [
        {
          "text": "पुन᳚र्नः पितरो॒ मनो॒ ददा᳚तु॒ दैव्यो॒ जनः᳚ ।",
          "begin": 0.0,
          "end": 6.414
        },
        {
          "text": "जी॒वं व्रातं᳚ सचेमहि ॥",
          "begin": 6.414,
          "end": 9.24
        }
      ]
    },
    {
      "id": "10.57.6",
      "mandala": 10,
      "sukta": 57,
      "rik": 6,
      "padas": [
        {
          "text": "व॒यं सो᳚म व्र॒ते तव॒ मन᳚स्त॒नूषु॒ बिभ्र᳚तः ।",
          "begin": 0.0,
          "end": 6.773
        },
        {
          "text": "प्र॒जाव᳚न्तः सचेमहि ॥",
          "begin": 6.773,
          "end": 9.64
        }
      ]
    }
  ]
}