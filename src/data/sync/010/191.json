{
  "mandala": 10,
  "sukta": 191,
  "verses": [
    {
      "id": "10.191.1",
      "mandala": 10,
      "sukta": 191,
      "rik": 1,
      "padas": [
        {
          "text": "संस॒मिद्यु᳚वसे वृष॒न्नग्ने॒ विश्वा᳚न्य॒र्य आ ।",
          "begin": 0.0,
          "end": 7.822
        },
        {
          "text": "इ॒ळस्प॒दे समि᳚ध्यसे॒ स नो॒ वसू॒न्या भ᳚र ॥",
          "begin": 7.822,
          "end": 12.48
        }
      ]
    },
    {
      "id": "10.191.2",
      "mandala": 10,
      "sukta": 191,
      "rik": 2,
      "padas": [
        {
          "text": "सं ग᳚च्छध्वं॒ सं व᳚दध्वं॒ सं वो॒ मनां᳚सि जानताम् ।",
          "begin": 0.0,
          "end": 6.216
        },
        {
          "text": "दे॒वा भा॒गं यथा॒ पूर्वे᳚ संजाना॒ना उ॒पास᳚ते ॥",
          "begin": 6.216,
          "end": 12.8
        }
      ]
    },
    {
      "id": "10.191.3",
      "mandala": 10,
      "sukta": 191,
      "rik": 3,
      "padas": [
        {
          "text": "स॒मा॒नो मन्त्र॒ः समि᳚तिः समा॒नी स᳚मा॒नं मनः᳚ स॒ह चि॒त्तमे᳚षाम् ।",
          "begin": 0.0,
          "end": 8.549
        },
        {
          "text": "स॒मा॒नं मन्त्र᳚म॒भि म᳚न्त्रये वः समा॒नेन᳚ वो ह॒विषा᳚ जुहोमि ॥",
          "begin": 8.549,
          "end": 14.68
        }
      ]
    },
    {
      "id": "10.191.4",
      "mandala": 10,
      "sukta": 191,
      "rik": 4,
      "padas": [
        {
          "text": "स॒मा॒नी व॒ आकू᳚तिः समा॒ना हृद᳚यानि वः ।",
          "begin": 0.0,
          "end": 6.54
        },
        {
          "text": "स॒मा॒नम᳚स्तु वो॒ मनो॒ यथा᳚ व॒ः सुस॒हास᳚ति ॥",
          "begin": 6.54,
          "end": 11.8
        }
      ]
    }
  ]
}