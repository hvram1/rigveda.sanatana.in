{
  "mandala": 10,
  "sukta": 118,
  "verses": [
    {
      "id": "10.118.1",
      "mandala": 10,
      "sukta": 118,
      "rik": 1,
      "padas": [
        {
          "text": "अग्ने॒ हंसि॒ न्य१॒᳚त्रिणं॒ दीद्य॒न्मर्त्ये॒ष्वा ।",
          "begin": 0.0,
          "end": 9.033
        },
        {
          "text": "स्वे क्षये᳚ शुचिव्रत ॥",
          "begin": 9.033,
          "end": 12.48
        }
      ]
    },
    {
      "id": "10.118.2",
      "mandala": 10,
      "sukta": 118,
      "rik": 2,
      "padas": [
        {
          "text": "उत्ति᳚ष्ठसि॒ स्वा᳚हुतो घृ॒तानि॒ प्रति᳚ मोदसे ।",
          "begin": 0.0,
          "end": 5.498
        },
        {
          "text": "यत्त्वा॒ स्रुचः᳚ स॒मस्थि᳚रन् ॥",
          "begin": 5.498,
          "end": 8.4
        }
      ]
    },
    {
      "id": "10.118.3",
      "mandala": 10,
      "sukta": 118,
      "rik": 3,
      "padas": [
        {
          "text": "स आहु᳚तो॒ वि रो᳚चते॒॑ग्निरी॒ळेन्यो᳚ गि॒रा ।",
          "begin": 0.0,
          "end": 5.401
        },
        {
          "text": "स्रु॒चा प्रती᳚कमज्यते ॥",
          "begin": 5.401,
          "end": 8.72
        }
      ]
    },
    {
      "id": "10.118.4",
      "mandala": 10,
      "sukta": 118,
      "rik": 4,
      "padas": [
        {
          "text": "घृ॒तेना॒ग्निः सम᳚ज्यते॒ मधु᳚प्रतीक॒ आहु᳚तः ।",
          "begin": 0.0,
          "end": 6.555
        },
        {
          "text": "रोच᳚मानो वि॒भाव᳚सुः ॥",
          "begin": 6.555,
          "end": 8.36
        }
      ]
    },
    {
      "id": "10.118.5",
      "mandala": 10,
      "sukta": 118,
      "rik": 5,
      "padas": [
        {
          "text": "जर᳚माण॒ः समि᳚ध्यसे दे॒वेभ्यो᳚ हव्यवाहन ।",
          "begin": 0.0,
          "end": 6.096
        },
        {
          "text": "तं त्वा᳚ हवन्त॒ मर्त्या᳚ः ॥",
          "begin": 6.096,
          "end": 9.4
        }
      ]
    },
    {
      "id": "10.118.6",
      "mandala": 10,
      "sukta": 118,
      "rik": 6,
      "padas": [
        {
          "text": "तं म᳚र्ता॒ अम᳚र्त्यं घृ॒तेना॒ग्निं स᳚पर्यत ।",
          "begin": 0.0,
          "end": 6.559
        },
        {
          "text": "अदा᳚भ्यं गृ॒हप᳚तिम् ॥",
          "begin": 6.559,
          "end": 8.32
        }
      ]
    },
    {
      "id": "10.118.7",
      "mandala": 10,
      "sukta": 118,
      "rik": 7,
      "padas": [
        {
          "text": "अदा᳚भ्येन शो॒चिषाग्ने॒ रक्ष॒स्त्वं द᳚ह ।",
          "begin": 0.0,
          "end": 5.628
        },
        {
          "text": "गो॒पा ऋ॒तस्य᳚ दीदिहि ॥",
          "begin": 5.628,
          "end": 8.36
        }
      ]
    },
    {
      "id": "10.118.8",
      "mandala": 10,
      "sukta": 118,
      "rik": 8,
      "padas": [
        {
          "text": "स त्वम᳚ग्ने॒ प्रती᳚केन॒ प्रत्यो᳚ष यातुधा॒न्य᳚ः ।",
          "begin": 0.0,
          "end": 6.859
        },
        {
          "text": "उ॒रु॒क्षये᳚षु॒ दीद्य᳚त् ॥",
          "begin": 6.859,
          "end": 9.52
        }
      ]
    },
    {
      "id": "10.118.9",
      "mandala": 10,
      "sukta": 118,
      "rik": 9,
      "padas": [
        {
          "text": "तं त्वा᳚ गी॒र्भिरु᳚रु॒क्षया᳚ हव्य॒वाहं॒ समी᳚धिरे ।",
          "begin": 0.0,
          "end": 9.068
        },
        {
          "text": "यजि᳚ष्ठं॒ मानु᳚षे॒ जने᳚ ॥",
          "begin": 9.068,
          "end": 13.8
        }
      ]
    }
  ]
}