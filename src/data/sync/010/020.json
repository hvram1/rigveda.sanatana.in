    {
      "id": "10.20.2",
      "mandala": 10,
      "sukta": 20,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒ग्निमी᳚ळे भु॒जां यवि᳚ष्ठं शा॒सा मि॒त्रं दु॒र्धरी᳚तुम् ।",
          "begin": 0.0,
          "end": 6.427
        },
        {
          "text": "यस्य॒ धर्म॒न्त्स्व१॒᳚रेनी᳚ः सप॒र्यन्ति᳚ मा॒तुरूधः᳚ ॥",
          "begin": 6.427,
          "end": 12.24
        }
      ]
    },
    {
      "id": "10.20.3",
      "mandala": 10,
      "sukta": 20,
      "rik": 3,
      "padas": [
        {
          "text": "यमा॒सा कृ॒पनी᳚ळं भा॒साके᳚तुं व॒र्धय᳚न्ति ।",
          "begin": 0.0,
          "end": 6.572
        },
        {
          "text": "भ्राज᳚ते॒ श्रेणि᳚दन् ॥",
          "begin": 6.572,
          "end": 9.2
        }
      ]
    },
    {
      "id": "10.20.4",
      "mandala": 10,
      "sukta": 20,
      "rik": 4,
      "padas": [
        {
          "text": "अ॒र्यो वि॒शां गा॒तुरे᳚ति॒ प्र यदान᳚ड्दि॒वो अन्ता᳚न् ।",
          "begin": 0.0,
          "end": 6.307
        },
        {
          "text": "क॒विर॒भ्रं दीद्या᳚नः ॥",
          "begin": 6.307,
          "end": 9.76
        }
      ]
    },
    {
      "id": "10.20.5",
      "mandala": 10,
      "sukta": 20,
      "rik": 5,
      "padas": [
        {
          "text": "जु॒षद्ध॒व्या मानु᳚षस्यो॒र्ध्वस्त᳚स्था॒वृभ्वा᳚ य॒ज्ञे ।",
          "begin": 0.0,
          "end": 5.875
        },
        {
          "text": "मि॒न्वन्त्सद्म᳚ पु॒र ए᳚ति ॥",
          "begin": 5.875,
          "end": 9.56
        }
      ]
    },
    {
      "id": "10.20.6",
      "mandala": 10,
      "sukta": 20,
      "rik": 6,
      "padas": [
        {
          "text": "स हि क्षेमो᳚ ह॒विर्य॒ज्ञः श्रु॒ष्टीद᳚स्य गा॒तुरे᳚ति ।",
          "begin": 0.0,
          "end": 5.726
        },
        {
          "text": "अ॒ग्निं दे॒वा वाशी᳚मन्तम् ॥",
          "begin": 5.726,
          "end": 9.76
        }
      ]
    },
    {
      "id": "10.20.7",
      "mandala": 10,
      "sukta": 20,
      "rik": 7,
      "padas": [
        {
          "text": "य॒ज्ञा॒साहं॒ दुव᳚ इषे॒॑ग्निं पूर्व᳚स्य॒ शेव᳚स्य ।",
          "begin": 0.0,
          "end": 5.861
        },
        {
          "text": "अद्रे᳚ः सू॒नुमा॒युमा᳚हुः ॥",
          "begin": 5.861,
          "end": 9.12
        }
      ]
    },
    {
      "id": "10.20.8",
      "mandala": 10,
      "sukta": 20,
      "rik": 8,
      "padas": [
        {
          "text": "नरो॒ ये के चा॒स्मदा विश्वेत्ते वा॒म आ स्यु᳚ः ।",
          "begin": 0.0,
          "end": 5.799
        },
        {
          "text": "अ॒ग्निं ह॒विषा॒ वर्ध᳚न्तः ॥",
          "begin": 5.799,
          "end": 9.48
        }
      ]
    },
    {
      "id": "10.20.9",
      "mandala": 10,
      "sukta": 20,
      "rik": 9,
      "padas": [
        {
          "text": "कृ॒ष्णः श्वे॒तो᳚॑रु॒षो यामो᳚ अस्य ब्र॒ध्न ऋ॒ज्र उ॒त शोणो॒ यश᳚स्वान् ।",
          "begin": 0.0,
          "end": 7.931
        },
        {
          "text": "हिर᳚ण्यरूपं॒ जनि᳚ता जजान ॥",
          "begin": 7.931,
          "end": 11.04
        }
      ]
    },
    {
      "id": "10.20.10",
      "mandala": 10,
      "sukta": 20,
      "rik": 10,
      "padas": [
        {
          "text": "ए॒वा ते᳚ अग्ने विम॒दो म᳚नी॒षामूर्जो᳚ नपाद॒मृते᳚भिः स॒जोषा᳚ः ।",
          "begin": 0.0,
          "end": 8.417
        },
        {
          "text": "गिर॒ आ व᳚क्षत्सुम॒तीरि᳚या॒न इष॒मूर्जं᳚ सुक्षि॒तिं विश्व॒माभा᳚ः ॥",
          "begin": 8.417,
          "end": 17.84
        }
      ]
    }
  ]
