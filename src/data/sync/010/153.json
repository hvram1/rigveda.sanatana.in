{
  "mandala": 10,
  "sukta": 153,
  "verses": [
    {
      "id": "10.153.1",
      "mandala": 10,
      "sukta": 153,
      "rik": 1,
      "padas": [
        {
          "text": "ई॒ङ्खय᳚न्तीरप॒स्युव॒ इन्द्रं᳚ जा॒तमुपा᳚सते ।",
          "begin": 0.0,
          "end": 8.397
        },
        {
          "text": "भे॒जा॒नासः᳚ सु॒वीर्य᳚म् ॥",
          "begin": 8.397,
          "end": 11.8
        }
      ]
    },
    {
      "id": "10.153.2",
      "mandala": 10,
      "sukta": 153,
      "rik": 2,
      "padas": [
        {
          "text": "त्वमि᳚न्द्र॒ बला॒दधि॒ सह᳚सो जा॒त ओज᳚सः ।",
          "begin": 0.0,
          "end": 4.797
        },
        {
          "text": "त्वं वृ᳚ष॒न्वृषेद᳚सि ॥",
          "begin": 4.797,
          "end": 7.2
        }
      ]
    },
    {
      "id": "10.153.3",
      "mandala": 10,
      "sukta": 153,
      "rik": 3,
      "padas": [
        {
          "text": "त्वमि᳚न्द्रासि वृत्र॒हा व्य१॒᳚न्तरि᳚क्षमतिरः ।",
          "begin": 0.0,
          "end": 5.872
        },
        {
          "text": "उद्द्याम᳚स्तभ्ना॒ ओज᳚सा ॥",
          "begin": 5.872,
          "end": 9.08
        }
      ]
    },
    {
      "id": "10.153.4",
      "mandala": 10,
      "sukta": 153,
      "rik": 4,
      "padas": [
        {
          "text": "त्वमि᳚न्द्र स॒जोष᳚सम॒र्कं बि᳚भर्षि बा॒ह्वोः ।",
          "begin": 0.0,
          "end": 5.853
        },
        {
          "text": "वज्रं॒ शिशा᳚न॒ ओज᳚सा ॥",
          "begin": 5.853,
          "end": 8.64
        }
      ]
    },
    {
      "id": "10.153.5",
      "mandala": 10,
      "sukta": 153,
      "rik": 5,
      "padas": [
        {
          "text": "त्वमि᳚न्द्राभि॒भूर᳚सि॒ विश्वा᳚ जा॒तान्योज᳚सा ।",
          "begin": 0.0,
          "end": 6.353
        },
        {
          "text": "स विश्वा॒ भुव॒ आभ᳚वः ॥",
          "begin": 6.353,
          "end": 13.16
        }
      ]
    }
  ]
}