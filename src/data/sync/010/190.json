      "rik": 1,
      "padas": [
        {
          "text": "ऋ॒तं च᳚ स॒त्यं चा॒भी᳚द्धा॒त्तप॒सो॑ध्य᳚जायत ।",
          "begin": 0.0,
          "end": 7.499
        },
        {
          "text": "ततो॒ रात्र्य᳚जायत॒ ततः᳚ समु॒द्रो अ᳚र्ण॒वः ॥",
          "begin": 7.499,
          "end": 13.08
        }
      ]
    },
    {
      "id": "10.190.2",
      "mandala": 10,
      "sukta": 190,
      "rik": 2,
      "padas": [
        {
          "text": "स॒मु॒द्राद᳚र्ण॒वादधि᳚ संवत्स॒रो अ᳚जायत ।",
          "begin": 0.0,
          "end": 6.268
        },
        {
          "text": "अ॒हो॒रा॒त्राणि᳚ वि॒दध॒द्विश्व᳚स्य मिष॒तो व॒शी ॥",
          "begin": 6.268,
          "end": 11.08
        }
      ]
    },
    {
      "id": "10.190.3",
      "mandala": 10,
      "sukta": 190,
      "rik": 3,
      "padas": [
        {
          "text": "सू॒र्या॒च॒न्द्र॒मसौ᳚ धा॒ता य᳚थापू॒र्वम᳚कल्पयत् ।",
          "begin": 0.0,
          "end": 7.211
        },
        {
          "text": "दिवं᳚ च पृथि॒वीं चा॒न्तरि᳚क्ष॒मथो॒ स्वः᳚ ॥",
          "begin": 7.211,
          "end": 13.16
        }
      ]
    }
  ]
}