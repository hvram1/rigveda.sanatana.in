      "rik": 1,
      "padas": [
        {
          "text": "प्र नू॒नं जा॒तवे᳚दस॒मश्वं᳚ हिनोत वा॒जिन᳚म् ।",
          "begin": 0.0,
          "end": 7.567
        },
        {
          "text": "इ॒दं नो᳚ ब॒र्हिरा॒सदे᳚ ॥",
          "begin": 7.567,
          "end": 11.44
        }
      ]
    },
    {
      "id": "10.188.2",
      "mandala": 10,
      "sukta": 188,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒स्य प्र जा॒तवे᳚दसो॒ विप्र᳚वीरस्य मी॒ळ्हुषः᳚ ।",
          "begin": 0.0,
          "end": 6.034
        },
        {
          "text": "म॒हीमि᳚यर्मि सुष्टु॒तिम् ॥",
          "begin": 6.034,
          "end": 8.56
        }
      ]
    },
    {
      "id": "10.188.3",
      "mandala": 10,
      "sukta": 188,
      "rik": 3,
      "padas": [
        {
          "text": "या रुचो᳚ जा॒तवे᳚दसो देव॒त्रा ह᳚व्य॒वाह᳚नीः ।",
          "begin": 0.0,
          "end": 7.528
        },
        {
          "text": "ताभि᳚र्नो य॒ज्ञमि᳚न्वतु ॥",
          "begin": 7.528,
          "end": 13.0
        }
      ]
    }
  ]
}