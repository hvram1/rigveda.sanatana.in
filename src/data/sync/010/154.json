{
  "mandala": 10,
  "sukta": 154,
  "verses": [
    {
      "id": "10.154.1",
      "mandala": 10,
      "sukta": 154,
      "rik": 1,
      "padas": [
        {
          "text": "सोम॒ एके᳚भ्यः पवते घृ॒तमेक॒ उपा᳚सते ।",
          "begin": 0.0,
          "end": 5.495
        },
        {
          "text": "येभ्यो॒ मधु᳚ प्र॒धाव᳚ति॒ ताँश्चि᳚दे॒वापि᳚ गच्छतात् ॥",
          "begin": 5.495,
          "end": 13.16
        }
      ]
    },
    {
      "id": "10.154.2",
      "mandala": 10,
      "sukta": 154,
      "rik": 2,
      "padas": [
        {
          "text": "तप᳚सा॒ ये अ᳚नाधृ॒ष्यास्तप᳚सा॒ ये स्व᳚र्य॒युः ।",
          "begin": 0.0,
          "end": 6.233
        },
        {
          "text": "तपो॒ ये च᳚क्रि॒रे मह॒स्ताँश्चि᳚दे॒वापि᳚ गच्छतात् ॥",
          "begin": 6.233,
          "end": 11.2
        }
      ]
    },
    {
      "id": "10.154.3",
      "mandala": 10,
      "sukta": 154,
      "rik": 3,
      "padas": [
        {
          "text": "ये युध्य᳚न्ते प्र॒धने᳚षु॒ शूरा᳚सो॒ ये त᳚नू॒त्यजः᳚ ।",
          "begin": 0.0,
          "end": 7.066
        },
        {
          "text": "ये वा᳚ स॒हस्र᳚दक्षिणा॒स्ताँश्चि᳚दे॒वापि᳚ गच्छतात् ॥",
          "begin": 7.066,
          "end": 12.72
        }
      ]
    },
    {
      "id": "10.154.4",
      "mandala": 10,
      "sukta": 154,
      "rik": 4,
      "padas": [
        {
          "text": "ये चि॒त्पूर्व᳚ ऋत॒साप᳚ ऋ॒तावा᳚न ऋता॒वृधः᳚ ।",
          "begin": 0.0,
          "end": 5.486
        },
        {
          "text": "पि॒तॄन्तप᳚स्वतो यम॒ ताँश्चि᳚दे॒वापि᳚ गच्छतात् ॥",
          "begin": 5.486,
          "end": 10.52
        }
      ]
    },
    {
      "id": "10.154.5",
      "mandala": 10,
      "sukta": 154,
      "rik": 5,
      "padas": [
        {
          "text": "स॒हस्र᳚णीथाः क॒वयो॒ ये गो᳚पा॒यन्ति॒ सूर्य᳚म् ।",
          "begin": 0.0,
          "end": 5.625
        },
        {
          "text": "ऋषी॒न्तप᳚स्वतो यम तपो॒जाँ अपि᳚ गच्छतात् ॥",
          "begin": 5.625,
          "end": 12.84
        }
      ]
    }
  ]
}