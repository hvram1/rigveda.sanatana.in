{
  "mandala": 10,
  "sukta": 127,
  "verses": [
    {
      "id": "10.127.1",
      "mandala": 10,
      "sukta": 127,
      "rik": 1,
      "padas": [
        {
          "text": "रात्री॒ व्य᳚ख्यदाय॒ती पु᳚रु॒त्रा दे॒व्य१॒᳚क्षभि᳚ः ।",
          "begin": 0.0,
          "end": 9.34
        },
        {
          "text": "विश्वा॒ अधि॒ श्रियो᳚॑धित ॥",
          "begin": 9.34,
          "end": 13.08
        }
      ]
    },
    {
      "id": "10.127.2",
      "mandala": 10,
      "sukta": 127,
      "rik": 2,
      "padas": [
        {
          "text": "ओर्व᳚प्रा॒ अम᳚र्त्या नि॒वतो᳚ दे॒व्यु१॒᳚द्वतः᳚ ।",
          "begin": 0.0,
          "end": 7.445
        },
        {
          "text": "ज्योति᳚षा बाधते॒ तमः᳚ ॥",
          "begin": 7.445,
          "end": 10.84
        }
      ]
    },
    {
      "id": "10.127.3",
      "mandala": 10,
      "sukta": 127,
      "rik": 3,
      "padas": [
        {
          "text": "निरु॒ स्वसा᳚रमस्कृतो॒षसं᳚ दे॒व्या᳚य॒ती ।",
          "begin": 0.0,
          "end": 7.097
        },
        {
          "text": "अपेदु᳚ हासते॒ तमः᳚ ॥",
          "begin": 7.097,
          "end": 9.92
        }
      ]
    },
    {
      "id": "10.127.4",
      "mandala": 10,
      "sukta": 127,
      "rik": 4,
      "padas": [
        {
          "text": "सा नो᳚ अ॒द्य यस्या᳚ व॒यं नि ते॒ याम॒न्नवि᳚क्ष्महि ।",
          "begin": 0.0,
          "end": 6.582
        },
        {
          "text": "वृ॒क्षे न व᳚स॒तिं वयः᳚ ॥",
          "begin": 6.582,
          "end": 10.04
        }
      ]
    },
    {
      "id": "10.127.5",
      "mandala": 10,
      "sukta": 127,
      "rik": 5,
      "padas": [
        {
          "text": "नि ग्रामा᳚सो अविक्षत॒ नि प॒द्वन्तो॒ नि प॒क्षिणः᳚ ।",
          "begin": 0.0,
          "end": 6.866
        },
        {
          "text": "नि श्ये॒नास᳚श्चिद॒र्थिनः᳚ ॥",
          "begin": 6.866,
          "end": 10.08
        }
      ]
    },
    {
      "id": "10.127.6",
      "mandala": 10,
      "sukta": 127,
      "rik": 6,
      "padas": [
        {
          "text": "या॒वया᳚ वृ॒क्यं१॒᳚ वृकं᳚ य॒वय᳚ स्ते॒नमू᳚र्म्ये ।",
          "begin": 0.0,
          "end": 7.905
        },
        {
          "text": "अथा᳚ नः सु॒तरा᳚ भव ॥",
          "begin": 7.905,
          "end": 11.2
        }
      ]
    },
    {
      "id": "10.127.7",
      "mandala": 10,
      "sukta": 127,
      "rik": 7,
      "padas": [
        {
          "text": "उप᳚ मा॒ पेपि᳚श॒त्तमः᳚ कृ॒ष्णं व्य᳚क्तमस्थित ।",
          "begin": 0.0,
          "end": 4.947
        },
        {
          "text": "उष᳚ ऋ॒णेव᳚ यातय ॥",
          "begin": 4.947,
          "end": 8.28
        }
      ]
    },
    {
      "id": "10.127.8",
      "mandala": 10,
      "sukta": 127,
      "rik": 8,
      "padas": [
        {
          "text": "उप᳚ ते॒ गा इ॒वाक᳚रं वृणी॒ष्व दु᳚हितर्दिवः ।",
          "begin": 0.0,
          "end": 9.159
        },
        {
          "text": "रात्रि॒ स्तोमं॒ न जि॒ग्युषे᳚ ॥",
          "begin": 9.159,
          "end": 14.0
        }
      ]
    }
  ]
}