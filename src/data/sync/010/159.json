{
  "mandala": 10,
  "sukta": 159,
  "verses": [
    {
      "id": "10.159.1",
      "mandala": 10,
      "sukta": 159,
      "rik": 1,
      "padas": [
        {
          "text": "उद॒सौ सूर्यो᳚ अगा॒दुद॒यं मा᳚म॒को भगः᳚ ।",
          "begin": 0.0,
          "end": 7.243
        },
        {
          "text": "अ॒हं तद्वि᳚द्व॒ला पति᳚म॒भ्य᳚साक्षि विषास॒हिः ॥",
          "begin": 7.243,
          "end": 12.36
        }
      ]
    },
    {
      "id": "10.159.2",
      "mandala": 10,
      "sukta": 159,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒हं के॒तुर॒हं मू॒र्धाहमु॒ग्रा वि॒वाच᳚नी ।",
          "begin": 0.0,
          "end": 4.885
        },
        {
          "text": "ममेदनु॒ क्रतुं॒ पति᳚ः सेहा॒नाया᳚ उ॒पाच᳚रेत् ॥",
          "begin": 4.885,
          "end": 10.24
        }
      ]
    },
    {
      "id": "10.159.3",
      "mandala": 10,
      "sukta": 159,
      "rik": 3,
      "padas": [
        {
          "text": "मम᳚ पु॒त्राः श᳚त्रु॒हणो॑थो᳚ मे दुहि॒ता वि॒राट् ।",
          "begin": 0.0,
          "end": 5.58
        },
        {
          "text": "उ॒ताहम᳚स्मि संज॒या पत्यौ᳚ मे॒ श्लोक᳚ उत्त॒मः ॥",
          "begin": 5.58,
          "end": 10.68
        }
      ]
    },
    {
      "id": "10.159.4",
      "mandala": 10,
      "sukta": 159,
      "rik": 4,
      "padas": [
        {
          "text": "येनेन्द्रो᳚ ह॒विषा᳚ कृ॒त्व्यभ᳚वद्द्यु॒म्न्यु᳚त्त॒मः ।",
          "begin": 0.0,
          "end": 5.998
        },
        {
          "text": "इ॒दं तद᳚क्रि देवा असप॒त्ना किला᳚भुवम् ॥",
          "begin": 5.998,
          "end": 10.6
        }
      ]
    },
    {
      "id": "10.159.5",
      "mandala": 10,
      "sukta": 159,
      "rik": 5,
      "padas": [
        {
          "text": "अ॒स॒प॒त्ना स᳚पत्न॒घ्नी जय᳚न्त्यभि॒भूव᳚री ।",
          "begin": 0.0,
          "end": 4.153
        },
        {
          "text": "आवृ᳚क्षम॒न्यासां॒ वर्चो॒ राधो॒ अस्थे᳚यसामिव ॥",
          "begin": 4.153,
          "end": 11.16
        }
      ]
    },
    {
      "id": "10.159.6",
      "mandala": 10,
      "sukta": 159,
      "rik": 6,
      "padas": [
        {
          "text": "सम᳚जैषमि॒मा अ॒हं स॒पत्नी᳚रभि॒भूव᳚री ।",
          "begin": 0.0,
          "end": 6.209
        },
        {
          "text": "यथा॒हम॒स्य वी॒रस्य᳚ वि॒राजा᳚नि॒ जन᳚स्य च ॥",
          "begin": 6.209,
          "end": 13.48
        }
      ]
    }
  ]
}