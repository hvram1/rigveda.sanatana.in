{
  "mandala": 10,
  "sukta": 97,
  "verses": [
    {
      "id": "10.97.1",
      "mandala": 10,
      "sukta": 97,
      "rik": 1,
      "padas": [
        {
          "text": "या ओष᳚धी॒ः पूर्वा᳚ जा॒ता दे॒वेभ्य᳚स्त्रियु॒गं पु॒रा ।",
          "begin": 0.0,
          "end": 5.679
        },
        {
          "text": "मनै॒ नु ब॒भ्रूणा᳚म॒हं श॒तं धामा᳚नि स॒प्त च᳚ ॥",
          "begin": 5.679,
          "end": 13.24
        }
      ]
    },
    {
      "id": "10.97.2",
      "mandala": 10,
      "sukta": 97,
      "rik": 2,
      "padas": [
        {
          "text": "श॒तं वो᳚ अम्ब॒ धामा᳚नि स॒हस्र᳚मु॒त वो॒ रुहः᳚ ।",
          "begin": 0.0,
          "end": 6.426
        },
        {
          "text": "अधा᳚ शतक्रत्वो यू॒यमि॒मं मे᳚ अग॒दं कृ᳚त ॥",
          "begin": 6.426,
          "end": 11.28
        }
      ]
    },
    {
      "id": "10.97.3",
      "mandala": 10,
      "sukta": 97,
      "rik": 3,
      "padas": [
        {
          "text": "ओष᳚धी॒ः प्रति᳚ मोदध्वं॒ पुष्प᳚वतीः प्र॒सूव᳚रीः ।",
          "begin": 0.0,
          "end": 6.169
        },
        {
          "text": "अश्वा᳚ इव स॒जित्व᳚रीर्वी॒रुधः᳚ पारयि॒ष्ण्व᳚ः ॥",
          "begin": 6.169,
          "end": 11.08
        }
      ]
    },
    {
      "id": "10.97.4",
      "mandala": 10,
      "sukta": 97,
      "rik": 4,
      "padas": [
        {
          "text": "ओष᳚धी॒रिति᳚ मातर॒स्तद्वो᳚ देवी॒रुप᳚ ब्रुवे ।",
          "begin": 0.0,
          "end": 6.02
        },
        {
          "text": "स॒नेय॒मश्वं॒ गां वास᳚ आ॒त्मानं॒ तव᳚ पूरुष ॥",
          "begin": 6.02,
          "end": 10.8
        }
      ]
    },
    {
      "id": "10.97.5",
      "mandala": 10,
      "sukta": 97,
      "rik": 5,
      "padas": [
        {
          "text": "अ॒श्व॒त्थे वो᳚ नि॒षद᳚नं प॒र्णे वो᳚ वस॒तिष्कृ॒ता ।",
          "begin": 0.0,
          "end": 6.305
        },
        {
          "text": "गो॒भाज॒ इत्किला᳚सथ॒ यत्स॒नव᳚थ॒ पूरु᳚षम् ॥",
          "begin": 6.305,
          "end": 10.64
        }
      ]
    },
    {
      "id": "10.97.6",
      "mandala": 10,
      "sukta": 97,
      "rik": 6,
      "padas": [
        {
          "text": "यत्रौष᳚धीः स॒मग्म᳚त॒ राजा᳚न॒ः समि᳚ताविव ।",
          "begin": 0.0,
          "end": 5.894
        },
        {
          "text": "विप्र॒ः स उ᳚च्यते भि॒षग्र᳚क्षो॒हामी᳚व॒चात᳚नः ॥",
          "begin": 5.894,
          "end": 10.88
        }
      ]
    },
    {
      "id": "10.97.7",
      "mandala": 10,
      "sukta": 97,
      "rik": 7,
      "padas": [
        {
          "text": "अ॒श्वा॒व॒तीं सो᳚माव॒तीमू॒र्जय᳚न्ती॒मुदो᳚जसम् ।",
          "begin": 0.0,
          "end": 7.339
        },
        {
          "text": "आवि᳚त्सि॒ सर्वा॒ ओष᳚धीर॒स्मा अ᳚रि॒ष्टता᳚तये ॥",
          "begin": 7.339,
          "end": 12.76
        }
      ]
    },
    {
      "id": "10.97.8",
      "mandala": 10,
      "sukta": 97,
      "rik": 8,
      "padas": [
        {
          "text": "उच्छुष्मा॒ ओष᳚धीनां॒ गावो᳚ गो॒ष्ठादि᳚वेरते ।",
          "begin": 0.0,
          "end": 6.797
        },
        {
          "text": "धनं᳚ सनि॒ष्यन्ती᳚नामा॒त्मानं॒ तव᳚ पूरुष ॥",
          "begin": 6.797,
          "end": 11.88
        }
      ]
    },
    {
      "id": "10.97.9",
      "mandala": 10,
      "sukta": 97,
      "rik": 9,
      "padas": [
        {
          "text": "इष्कृ᳚ति॒र्नाम᳚ वो मा॒ताथो᳚ यू॒यं स्थ॒ निष्कृ᳚तीः ।",
          "begin": 0.0,
          "end": 5.769
        },
        {
          "text": "सी॒राः प᳚त॒त्रिणी᳚ः स्थन॒ यदा॒मय᳚ति॒ निष्कृ᳚थ ॥",
          "begin": 5.769,
          "end": 11.0
        }
      ]
    },
    {
      "id": "10.97.10",
      "mandala": 10,
      "sukta": 97,
      "rik": 10,
      "padas": [
        {
          "text": "अति॒ विश्वा᳚ः परि॒ष्ठाः स्ते॒न इ᳚व व्र॒जम᳚क्रमुः ।",
          "begin": 0.0,
          "end": 6.126
        },
        {
          "text": "ओष᳚धी॒ः प्राचु᳚च्यवु॒र्यत्किं च᳚ त॒न्वो॒३॒᳚ रपः᳚ ॥",
          "begin": 6.126,
          "end": 12.72
        }
      ]
    },
    {
      "id": "10.97.11",
      "mandala": 10,
      "sukta": 97,
      "rik": 11,
      "padas": [
        {
          "text": "यदि॒मा वा॒जय᳚न्न॒हमोष᳚धी॒र्हस्त᳚ आद॒धे ।",
          "begin": 0.0,
          "end": 5.996
        },
        {
          "text": "आ॒त्मा यक्ष्म᳚स्य नश्यति पु॒रा जी᳚व॒गृभो᳚ यथा ॥",
          "begin": 5.996,
          "end": 11.32
        }
      ]
    },
    {
      "id": "10.97.12",
      "mandala": 10,
      "sukta": 97,
      "rik": 12,
      "padas": [
        {
          "text": "यस्यौ᳚षधीः प्र॒सर्प॒थाङ्ग᳚मङ्गं॒ परु᳚ष्परुः ।",
          "begin": 0.0,
          "end": 6.253
        },
        {
          "text": "ततो॒ यक्ष्मं॒ वि बा᳚धध्व उ॒ग्रो म᳚ध्यम॒शीरि᳚व ॥",
          "begin": 6.253,
          "end": 11.08
        }
      ]
    },
    {
      "id": "10.97.13",
      "mandala": 10,
      "sukta": 97,
      "rik": 13,
      "padas": [
        {
          "text": "सा॒कं य᳚क्ष्म॒ प्र प᳚त॒ चाषे᳚ण किकिदी॒विना᳚ ।",
          "begin": 0.0,
          "end": 6.085
        },
        {
          "text": "सा॒कं वात᳚स्य॒ ध्राज्या᳚ सा॒कं न᳚श्य नि॒हाक᳚या ॥",
          "begin": 6.085,
          "end": 11.2
        }
      ]
    },
    {
      "id": "10.97.14",
      "mandala": 10,
      "sukta": 97,
      "rik": 14,
      "padas": [
        {
          "text": "अ॒न्या वो᳚ अ॒न्याम᳚वत्व॒न्यान्यस्या॒ उपा᳚वत ।",
          "begin": 0.0,
          "end": 7.007
        },
        {
          "text": "ताः सर्वा᳚ः संविदा॒ना इ॒दं मे॒ प्राव᳚ता॒ वचः᳚ ॥",
          "begin": 7.007,
          "end": 12.72
        }
      ]
    },
    {
      "id": "10.97.15",
      "mandala": 10,
      "sukta": 97,
      "rik": 15,
      "padas": [
        {
          "text": "याः फ॒लिनी॒र्या अ᳚फ॒ला अ᳚पु॒ष्पा याश्च᳚ पु॒ष्पिणी᳚ः ।",
          "begin": 0.0,
          "end": 6.527
        },
        {
          "text": "बृह॒स्पति᳚प्रसूता॒स्ता नो᳚ मुञ्च॒न्त्वंह᳚सः ॥",
          "begin": 6.527,
          "end": 12.32
        }
      ]
    },
    {
      "id": "10.97.16",
      "mandala": 10,
      "sukta": 97,
      "rik": 16,
      "padas": [
        {
          "text": "मु॒ञ्चन्तु᳚ मा शप॒थ्या॒३॒᳚दथो᳚ वरु॒ण्या᳚दु॒त ।",
          "begin": 0.0,
          "end": 8.035
        },
        {
          "text": "अथो᳚ य॒मस्य॒ पड्बी᳚शा॒त्सर्व᳚स्माद्देवकिल्बि॒षात् ॥",
          "begin": 8.035,
          "end": 14.36
        }
      ]
    },
    {
      "id": "10.97.17",
      "mandala": 10,
      "sukta": 97,
      "rik": 17,
      "padas": [
        {
          "text": "अ॒व॒पत᳚न्तीरवदन्दि॒व ओष᳚धय॒स्परि᳚ ।",
          "begin": 0.0,
          "end": 5.177
        },
        {
          "text": "यं जी॒वम॒श्नवा᳚महै॒ न स रि᳚ष्याति॒ पूरु᳚षः ॥",
          "begin": 5.177,
          "end": 10.0
        }
      ]
    },
    {
      "id": "10.97.18",
      "mandala": 10,
      "sukta": 97,
      "rik": 18,
      "padas": [
        {
          "text": "या ओष᳚धी॒ः सोम᳚राज्ञीर्ब॒ह्वीः श॒तवि᳚चक्षणाः ।",
          "begin": 0.0,
          "end": 6.396
        },
        {
          "text": "तासां॒ त्वम᳚स्युत्त॒मारं॒ कामा᳚य॒ शं हृ॒दे ॥",
          "begin": 6.396,
          "end": 11.76
        }
      ]
    },
    {
      "id": "10.97.19",
      "mandala": 10,
      "sukta": 97,
      "rik": 19,
      "padas": [
        {
          "text": "या ओष᳚धी॒ः सोम᳚राज्ञी॒र्विष्ठि᳚ताः पृथि॒वीमनु᳚ ।",
          "begin": 0.0,
          "end": 6.291
        },
        {
          "text": "बृह॒स्पति᳚प्रसूता अ॒स्यै सं द᳚त्त वी॒र्य᳚म् ॥",
          "begin": 6.291,
          "end": 11.12
        }
      ]
    },
    {
      "id": "10.97.20",
      "mandala": 10,
      "sukta": 97,
      "rik": 20,
      "padas": [
        {
          "text": "मा वो᳚ रिषत्खनि॒ता यस्मै᳚ चा॒हं खना᳚मि वः ।",
          "begin": 0.0,
          "end": 6.057
        },
        {
          "text": "द्वि॒पच्चतु᳚ष्पद॒स्माकं॒ सर्व᳚मस्त्वनातु॒रम् ॥",
          "begin": 6.057,
          "end": 10.6
        }
      ]
    },
    {
      "id": "10.97.21",
      "mandala": 10,
      "sukta": 97,
      "rik": 21,
      "padas": [
        {
          "text": "याश्चे॒दमु᳚पशृ॒ण्वन्ति॒ याश्च᳚ दू॒रं परा᳚गताः ।",
          "begin": 0.0,
          "end": 6.434
        },
        {
          "text": "सर्वा᳚ः सं॒गत्य᳚ वीरुधो॒॑स्यै सं द᳚त्त वी॒र्य᳚म् ॥",
          "begin": 6.434,
          "end": 12.04
        }
      ]
    },
    {
      "id": "10.97.22",
      "mandala": 10,
      "sukta": 97,
      "rik": 22,
      "padas": [
        {
          "text": "ओष᳚धय॒ः सं व᳚दन्ते॒ सोमे᳚न स॒ह राज्ञा᳚ ।",
          "begin": 0.0,
          "end": 6.219
        },
        {
          "text": "यस्मै᳚ कृ॒णोति᳚ ब्राह्म॒णस्तं रा᳚जन्पारयामसि ॥",
          "begin": 6.219,
          "end": 11.84
        }
      ]
    },
    {
      "id": "10.97.23",
      "mandala": 10,
      "sukta": 97,
      "rik": 23,
      "padas": [
        {
          "text": "त्वमु᳚त्त॒मास्यो᳚षधे॒ तव᳚ वृ॒क्षा उप᳚स्तयः ।",
          "begin": 0.0,
          "end": 6.556
        },
        {
          "text": "उप᳚स्तिरस्तु॒ सो॒३॒᳚॑स्माकं॒ यो अ॒स्माँ अ᳚भि॒दास᳚ति ॥",
          "begin": 6.556,
          "end": 15.4
        }
      ]
    }
  ]
}