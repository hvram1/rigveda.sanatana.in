  "verses": [
    {
      "id": "10.170.1",
      "mandala": 10,
      "sukta": 170,
      "rik": 1,
      "padas": [
        {
          "text": "वि॒भ्राड्बृ॒हत्पि᳚बतु सो॒म्यं मध्वायु॒र्दध᳚द्य॒ज्ञप᳚ता॒ववि᳚ह्रुतम् ।",
          "begin": 0.0,
          "end": 9.154
        },
        {
          "text": "वात᳚जूतो॒ यो अ᳚भि॒रक्ष᳚ति॒ त्मना᳚ प्र॒जाः पु᳚पोष पुरु॒धा वि रा᳚जति ॥",
          "begin": 9.154,
          "end": 16.24
        }
      ]
    },
    {
      "id": "10.170.2",
      "mandala": 10,
      "sukta": 170,
      "rik": 2,
      "padas": [
        {
          "text": "वि॒भ्राड्बृ॒हत्सुभृ᳚तं वाज॒सात᳚मं॒ धर्म᳚न्दि॒वो ध॒रुणे᳚ स॒त्यमर्पि᳚तम् ।",
          "begin": 0.0,
          "end": 7.184
        },
        {
          "text": "अ॒मि॒त्र॒हा वृ᳚त्र॒हा द᳚स्यु॒हन्त᳚मं॒ ज्योति᳚र्जज्ञे असुर॒हा स᳚पत्न॒हा ॥",
          "begin": 7.184,
          "end": 14.24
        }
      ]
    },
    {
      "id": "10.170.3",
      "mandala": 10,
      "sukta": 170,
      "rik": 3,
      "padas": [
        {
          "text": "इ॒दं श्रेष्ठं॒ ज्योति᳚षां॒ ज्योति᳚रुत्त॒मं वि᳚श्व॒जिद्ध᳚न॒जिदु᳚च्यते बृ॒हत् ।",
          "begin": 0.0,
          "end": 5.913
        },
        {
          "text": "वि॒श्व॒भ्राड्भ्रा॒जो महि॒ सूर्यो᳚ दृ॒श उ॒रु प᳚प्रथे॒ सह॒ ओजो॒ अच्यु᳚तम् ॥",
          "begin": 5.913,
          "end": 14.72
        }
      ]
    },
    {
      "id": "10.170.4",
      "mandala": 10,
      "sukta": 170,
      "rik": 4,
      "padas": [
        {
          "text": "वि॒भ्राज॒ञ्ज्योति᳚षा॒ स्व१॒᳚रग᳚च्छो रोच॒नं दि॒वः ।",
          "begin": 0.0,
          "end": 6.63
        },
        {
          "text": "येने॒मा विश्वा॒ भुव᳚ना॒न्याभृ᳚ता वि॒श्वक᳚र्मणा वि॒श्वदे᳚व्यावता ॥",
          "begin": 6.63,
          "end": 14.12
        }
      ]
    }
  ]
}