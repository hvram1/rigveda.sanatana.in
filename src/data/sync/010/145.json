{
  "mandala": 10,
  "sukta": 145,
  "verses": [
    {
      "id": "10.145.1",
      "mandala": 10,
      "sukta": 145,
      "rik": 1,
      "padas": [
        {
          "text": "इ॒मां ख᳚ना॒म्योष᳚धिं वी॒रुधं॒ बल᳚वत्तमाम् ।",
          "begin": 0.0,
          "end": 3.781
        },
        {
          "text": "यया᳚ स॒पत्नीं॒ बाध᳚ते॒ यया᳚ संवि॒न्दते॒ पति᳚म् ॥",
          "begin": 3.781,
          "end": 13.76
        }
      ]
    },
    {
      "id": "10.145.2",
      "mandala": 10,
      "sukta": 145,
      "rik": 2,
      "padas": [
        {
          "text": "उत्ता᳚नपर्णे॒ सुभ᳚गे॒ देव᳚जूते॒ सह᳚स्वति ।",
          "begin": 0.0,
          "end": 5.779
        },
        {
          "text": "स॒पत्नीं᳚ मे॒ परा᳚ धम॒ पतिं᳚ मे॒ केव᳚लं कुरु ॥",
          "begin": 5.779,
          "end": 11.04
        }
      ]
    },
    {
      "id": "10.145.3",
      "mandala": 10,
      "sukta": 145,
      "rik": 3,
      "padas": [
        {
          "text": "उत्त᳚रा॒हमु᳚त्तर॒ उत्त॒रेदुत्त᳚राभ्यः ।",
          "begin": 0.0,
          "end": 5.276
        },
        {
          "text": "अथा᳚ स॒पत्नी॒ या ममाध᳚रा॒ साध᳚राभ्यः ॥",
          "begin": 5.276,
          "end": 9.92
        }
      ]
    },
    {
      "id": "10.145.4",
      "mandala": 10,
      "sukta": 145,
      "rik": 4,
      "padas": [
        {
          "text": "न॒ह्य᳚स्या॒ नाम᳚ गृ॒भ्णामि॒ नो अ॒स्मिन्र᳚मते॒ जने᳚ ।",
          "begin": 0.0,
          "end": 5.113
        },
        {
          "text": "परा᳚मे॒व प᳚रा॒वतं᳚ स॒पत्नीं᳚ गमयामसि ॥",
          "begin": 5.113,
          "end": 11.28
        }
      ]
    },
    {
      "id": "10.145.5",
      "mandala": 10,
      "sukta": 145,
      "rik": 5,
      "padas": [
        {
          "text": "अ॒हम᳚स्मि॒ सह᳚मा॒नाथ॒ त्वम᳚सि सास॒हिः ।",
          "begin": 0.0,
          "end": 4.954
        },
        {
          "text": "उ॒भे सह᳚स्वती भू॒त्वी स॒पत्नीं᳚ मे सहावहै ॥",
          "begin": 4.954,
          "end": 9.64
        }
      ]
    },
    {
      "id": "10.145.6",
      "mandala": 10,
      "sukta": 145,
      "rik": 6,
      "padas": [
        {
          "text": "उप᳚ ते॑धां॒ सह᳚मानाम॒भि त्वा᳚धां॒ सही᳚यसा ।",
          "begin": 0.0,
          "end": 5.812
        },
        {
          "text": "मामनु॒ प्र ते॒ मनो᳚ व॒त्सं गौरि᳚व धावतु प॒था वारि᳚व धावतु ॥",
          "begin": 5.812,
          "end": 13.88
        }
      ]
    }
  ]
}