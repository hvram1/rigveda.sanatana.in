  "verses": [
    {
      "id": "10.180.1",
      "mandala": 10,
      "sukta": 180,
      "rik": 1,
      "padas": [
        {
          "text": "प्र स᳚साहिषे पुरुहूत॒ शत्रू॒ञ्ज्येष्ठ᳚स्ते॒ शुष्म᳚ इ॒ह रा॒तिर᳚स्तु ।",
          "begin": 0.0,
          "end": 7.842
        },
        {
          "text": "इन्द्रा भ᳚र॒ दक्षि᳚णेना॒ वसू᳚नि॒ पति॒ः सिन्धू᳚नामसि रे॒वती᳚नाम् ॥",
          "begin": 7.842,
          "end": 14.88
        }
      ]
    },
    {
      "id": "10.180.2",
      "mandala": 10,
      "sukta": 180,
      "rik": 2,
      "padas": [
        {
          "text": "मृ॒गो न भी॒मः कु᳚च॒रो गि᳚रि॒ष्ठाः प᳚रा॒वत॒ आ ज᳚गन्था॒ पर᳚स्याः ।",
          "begin": 0.0,
          "end": 6.673
        },
        {
          "text": "सृ॒कं सं॒शाय᳚ प॒विमि᳚न्द्र ति॒ग्मं वि शत्रू᳚न्ताळ्हि॒ वि मृधो᳚ नुदस्व ॥",
          "begin": 6.673,
          "end": 13.52
        }
      ]
    },
    {
      "id": "10.180.3",
      "mandala": 10,
      "sukta": 180,
      "rik": 3,
      "padas": [
        {
          "text": "इन्द्र᳚ क्ष॒त्रम॒भि वा॒ममोजो॑जा᳚यथा वृषभ चर्षणी॒नाम् ।",
          "begin": 0.0,
          "end": 6.549
        },
        {
          "text": "अपा᳚नुदो॒ जन᳚ममित्र॒यन्त᳚मु॒रुं दे॒वेभ्यो᳚ अकृणोरु लो॒कम् ॥",
          "begin": 6.549,
          "end": 14.96
        }
      ]
    }
  ]
}