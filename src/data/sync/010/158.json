{
  "mandala": 10,
  "sukta": 158,
  "verses": [
    {
      "id": "10.158.1",
      "mandala": 10,
      "sukta": 158,
      "rik": 1,
      "padas": [
        {
          "text": "सूर्यो᳚ नो दि॒वस्पा᳚तु॒ वातो᳚ अ॒न्तरि᳚क्षात् ।",
          "begin": 0.0,
          "end": 5.19
        },
        {
          "text": "अ॒ग्निर्न॒ः पार्थि᳚वेभ्यः ॥",
          "begin": 5.19,
          "end": 11.88
        }
      ]
    },
    {
      "id": "10.158.2",
      "mandala": 10,
      "sukta": 158,
      "rik": 2,
      "padas": [
        {
          "text": "जोषा᳚ सवित॒र्यस्य᳚ ते॒ हरः᳚ श॒तं स॒वाँ अर्ह᳚ति ।",
          "begin": 0.0,
          "end": 5.681
        },
        {
          "text": "पा॒हि नो᳚ दि॒द्युत॒ः पत᳚न्त्याः ॥",
          "begin": 5.681,
          "end": 10.32
        }
      ]
    },
    {
      "id": "10.158.3",
      "mandala": 10,
      "sukta": 158,
      "rik": 3,
      "padas": [
        {
          "text": "चक्षु᳚र्नो दे॒वः स᳚वि॒ता चक्षु᳚र्न उ॒त पर्व᳚तः ।",
          "begin": 0.0,
          "end": 6.251
        },
        {
          "text": "चक्षु᳚र्धा॒ता द᳚धातु नः ॥",
          "begin": 6.251,
          "end": 9.0
        }
      ]
    },
    {
      "id": "10.158.4",
      "mandala": 10,
      "sukta": 158,
      "rik": 4,
      "padas": [
        {
          "text": "चक्षु᳚र्नो धेहि॒ चक्षु᳚षे॒ चक्षु᳚र्वि॒ख्यै त॒नूभ्य᳚ः ।",
          "begin": 0.0,
          "end": 6.547
        },
        {
          "text": "सं चे॒दं वि च᳚ पश्येम ॥",
          "begin": 6.547,
          "end": 9.12
        }
      ]
    },
    {
      "id": "10.158.5",
      "mandala": 10,
      "sukta": 158,
      "rik": 5,
      "padas": [
        {
          "text": "सु॒सं॒दृशं᳚ त्वा व॒यं प्रति᳚ पश्येम सूर्य ।",
          "begin": 0.0,
          "end": 5.921
        },
        {
          "text": "वि प᳚श्येम नृ॒चक्ष᳚सः ॥",
          "begin": 5.921,
          "end": 11.12
        }
      ]
    }
  ]
}