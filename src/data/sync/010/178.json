  "verses": [
    {
      "id": "10.178.1",
      "mandala": 10,
      "sukta": 178,
      "rik": 1,
      "padas": [
        {
          "text": "त्यमू॒ षु वा॒जिनं᳚ दे॒वजू᳚तं स॒हावा᳚नं तरु॒तारं॒ रथा᳚नाम् ।",
          "begin": 0.0,
          "end": 9.709
        },
        {
          "text": "अरि᳚ष्टनेमिं पृत॒नाज᳚मा॒शुं स्व॒स्तये॒ तार्क्ष्य᳚मि॒हा हु᳚वेम ॥",
          "begin": 9.709,
          "end": 15.8
        }
      ]
    },
    {
      "id": "10.178.2",
      "mandala": 10,
      "sukta": 178,
      "rik": 2,
      "padas": [
        {
          "text": "इन्द्र᳚स्येव रा॒तिमा॒जोहु᳚वानाः स्व॒स्तये॒ नाव᳚मि॒वा रु᳚हेम ।",
          "begin": 0.0,
          "end": 6.754
        },
        {
          "text": "उर्वी॒ न पृथ्वी॒ बहु᳚ले॒ गभी᳚रे॒ मा वा॒मेतौ॒ मा परे᳚तौ रिषाम ॥",
          "begin": 6.754,
          "end": 13.48
        }
      ]
    },
    {
      "id": "10.178.3",
      "mandala": 10,
      "sukta": 178,
      "rik": 3,
      "padas": [
        {
          "text": "स॒द्यश्चि॒द्यः शव᳚सा॒ पञ्च᳚ कृ॒ष्टीः सूर्य᳚ इव॒ ज्योति᳚षा॒पस्त॒तान᳚ ।",
          "begin": 0.0,
          "end": 7.673
        },
        {
          "text": "स॒ह॒स्र॒साः श᳚त॒सा अ᳚स्य॒ रंहि॒र्न स्मा᳚ वरन्ते युव॒तिं न शर्या᳚म् ॥",
          "begin": 7.673,
          "end": 17.56
        }
      ]
    }
  ]
}