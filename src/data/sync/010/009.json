{
  "mandala": 10,
  "sukta": 9,
  "verses": [
    {
      "id": "10.9.1",
      "mandala": 10,
      "sukta": 9,
      "rik": 1,
      "padas": [
        {
          "text": "आपो॒ हि ष्ठा म᳚यो॒भुव॒स्ता न᳚ ऊ॒र्जे द᳚धातन ।",
          "begin": 0.0,
          "end": 6.926
        },
        {
          "text": "म॒हे रणा᳚य॒ चक्ष᳚से ॥",
          "begin": 6.926,
          "end": 10.08
        }
      ]
    },
    {
      "id": "10.9.2",
      "mandala": 10,
      "sukta": 9,
      "rik": 2,
      "padas": [
        {
          "text": "यो वः᳚ शि॒वत᳚मो॒ रस॒स्तस्य᳚ भाजयते॒ह नः᳚ ।",
          "begin": 0.0,
          "end": 5.326
        },
        {
          "text": "उ॒श॒तीरि᳚व मा॒तरः᳚ ॥",
          "begin": 5.326,
          "end": 7.6
        }
      ]
    },
    {
      "id": "10.9.3",
      "mandala": 10,
      "sukta": 9,
      "rik": 3,
      "padas": [
        {
          "text": "तस्मा॒ अरं᳚ गमाम वो॒ यस्य॒ क्षया᳚य॒ जिन्व᳚थ ।",
          "begin": 0.0,
          "end": 6.333
        },
        {
          "text": "आपो᳚ ज॒नय᳚था च नः ॥",
          "begin": 6.333,
          "end": 8.96
        }
      ]
    },
    {
      "id": "10.9.4",
      "mandala": 10,
      "sukta": 9,
      "rik": 4,
      "padas": [
        {
          "text": "शं नो᳚ दे॒वीर॒भिष्ट᳚य॒ आपो᳚ भवन्तु पी॒तये᳚ ।",
          "begin": 0.0,
          "end": 6.045
        },
        {
          "text": "शं योर॒भि स्र᳚वन्तु नः ॥",
          "begin": 6.045,
          "end": 8.64
        }
      ]
    },
    {
      "id": "10.9.5",
      "mandala": 10,
      "sukta": 9,
      "rik": 5,
      "padas": [
        {
          "text": "ईशा᳚ना॒ वार्या᳚णां॒ क्षय᳚न्तीश्चर्षणी॒नाम् ।",
          "begin": 0.0,
          "end": 6.187
        },
        {
          "text": "अ॒पो या᳚चामि भेष॒जम् ॥",
          "begin": 6.187,
          "end": 9.84
        }
      ]
    },
    {
      "id": "10.9.6",
      "mandala": 10,
      "sukta": 9,
      "rik": 6,
      "padas": [
        {
          "text": "अ॒प्सु मे॒ सोमो᳚ अब्रवीद॒न्तर्विश्वा᳚नि भेष॒जा ।",
          "begin": 0.0,
          "end": 6.503
        },
        {
          "text": "अ॒ग्निं च᳚ वि॒श्वश᳚म्भुवम् ॥",
          "begin": 6.503,
          "end": 9.4
        }
      ]
    },
    {
      "id": "10.9.7",
      "mandala": 10,
      "sukta": 9,
      "rik": 7,
      "padas": [
        {
          "text": "आपः᳚ पृणी॒त भे᳚ष॒जं वरू᳚थं त॒न्वे॒३॒᳚ मम᳚ ।",
          "begin": 0.0,
          "end": 8.148
        },
        {
          "text": "ज्योक्च॒ सूर्यं᳚ दृ॒शे ॥",
          "begin": 8.148,
          "end": 11.28
        }
      ]
    },
    {
      "id": "10.9.8",
      "mandala": 10,
      "sukta": 9,
      "rik": 8,
      "padas": [
        {
          "text": "इ॒दमा᳚प॒ः प्र व᳚हत॒ यत्किं च᳚ दुरि॒तं मयि᳚ ।",
          "begin": 0.0,
          "end": 4.494
        },
        {
          "text": "यद्वा॒हम᳚भिदु॒द्रोह॒ यद्वा᳚ शे॒प उ॒तानृ᳚तम् ॥",
          "begin": 4.494,
          "end": 9.88
        }
      ]
    },
    {
      "id": "10.9.9",
      "mandala": 10,
      "sukta": 9,
      "rik": 9,
      "padas": [
        {
          "text": "आपो᳚ अ॒द्यान्व᳚चारिषं॒ रसे᳚न॒ सम᳚गस्महि ।",
          "begin": 0.0,
          "end": 5.583
        },
        {
          "text": "पय᳚स्वानग्न॒ आ ग᳚हि॒ तं मा॒ सं सृ᳚ज॒ वर्च᳚सा ॥",
          "begin": 5.583,
          "end": 12.16
        }
      ]
    }
  ]
}