      "padas": [
        {
          "text": "प्र वो᳚ ग्रावाणः सवि॒ता दे॒वः सु᳚वतु॒ धर्म᳚णा ।",
          "begin": 0.0,
          "end": 6.82
        },
        {
          "text": "धू॒र्षु यु᳚ज्यध्वं सुनु॒त ॥",
          "begin": 6.82,
          "end": 9.6
        }
      ]
    },
    {
      "id": "10.175.2",
      "mandala": 10,
      "sukta": 175,
      "rik": 2,
      "padas": [
        {
          "text": "ग्रावा᳚णो॒ अप᳚ दु॒च्छुना॒मप᳚ सेधत दुर्म॒तिम् ।",
          "begin": 0.0,
          "end": 5.902
        },
        {
          "text": "उ॒स्राः क᳚र्तन भेष॒जम् ॥",
          "begin": 5.902,
          "end": 8.48
        }
      ]
    },
    {
      "id": "10.175.3",
      "mandala": 10,
      "sukta": 175,
      "rik": 3,
      "padas": [
        {
          "text": "ग्रावा᳚ण॒ उप᳚रे॒ष्वा म᳚ही॒यन्ते᳚ स॒जोष᳚सः ।",
          "begin": 0.0,
          "end": 5.4
        },
        {
          "text": "वृष्णे॒ दध᳚तो॒ वृष्ण्य᳚म् ॥",
          "begin": 5.4,
          "end": 8.88
        }
      ]
    },
    {
      "id": "10.175.4",
      "mandala": 10,
      "sukta": 175,
      "rik": 4,
      "padas": [
        {
          "text": "ग्रावा᳚णः सवि॒ता नु वो᳚ दे॒वः सु᳚वतु॒ धर्म᳚णा ।",
          "begin": 0.0,
          "end": 7.179
        },
        {
          "text": "यज᳚मानाय सुन्व॒ते ॥",
          "begin": 7.179,
          "end": 10.24
        }
      ]
    }
  ]
