{
  "mandala": 1,
  "sukta": 25,
  "verses": [
    {
      "id": "1.25.1",
      "mandala": 1,
      "sukta": 25,
      "rik": 1,
      "padas": [
        {
          "text": "यच्चि॒द्धि ते॒ विशो᳚ यथा॒ प्र दे᳚व वरुण व्र॒तम् ।",
          "begin": 0.0,
          "end": 7.982
        },
        {
          "text": "मि॒नी॒मसि॒ द्यवि᳚द्यवि ॥",
          "begin": 7.982,
          "end": 11.16
        }
      ]
    },
    {
      "id": "1.25.2",
      "mandala": 1,
      "sukta": 25,
      "rik": 2,
      "padas": [
        {
          "text": "मा नो᳚ व॒धाय᳚ ह॒त्नवे᳚ जिहीळा॒नस्य᳚ रीरधः ।",
          "begin": 0.0,
          "end": 8.063
        },
        {
          "text": "मा हृ᳚णा॒नस्य᳚ म॒न्यवे᳚ ॥",
          "begin": 8.063,
          "end": 12.08
        }
      ]
    },
    {
      "id": "1.25.3",
      "mandala": 1,
      "sukta": 25,
      "rik": 3,
      "padas": [
        {
          "text": "वि मृ᳚ळी॒काय᳚ ते॒ मनो᳚ र॒थीरश्वं॒ न संदि᳚तम् ।",
          "begin": 0.0,
          "end": 7.534
        },
        {
          "text": "गी॒र्भिर्व᳚रुण सीमहि ॥",
          "begin": 7.534,
          "end": 10.92
        }
      ]
    },
    {
      "id": "1.25.4",
      "mandala": 1,
      "sukta": 25,
      "rik": 4,
      "padas": [
        {
          "text": "परा॒ हि मे॒ विम᳚न्यव॒ः पत᳚न्ति॒ वस्य᳚इष्टये ।",
          "begin": 0.0,
          "end": 7.579
        },
        {
          "text": "वयो॒ न व᳚स॒तीरुप᳚ ॥",
          "begin": 7.579,
          "end": 10.56
        }
      ]
    },
    {
      "id": "1.25.5",
      "mandala": 1,
      "sukta": 25,
      "rik": 5,
      "padas": [
        {
          "text": "क॒दा क्ष᳚त्र॒श्रियं॒ नर॒मा वरु᳚णं करामहे ।",
          "begin": 0.0,
          "end": 6.954
        },
        {
          "text": "मृ॒ळी॒कायो᳚रु॒चक्ष᳚सम् ॥",
          "begin": 6.954,
          "end": 10.96
        }
      ]
    },
    {
      "id": "1.25.6",
      "mandala": 1,
      "sukta": 25,
      "rik": 6,
      "padas": [
        {
          "text": "तदित्स᳚मा॒नमा᳚शाते॒ वेन᳚न्ता॒ न प्र यु᳚च्छतः ।",
          "begin": 0.0,
          "end": 8.556
        },
        {
          "text": "धृ॒तव्र᳚ताय दा॒शुषे᳚ ॥",
          "begin": 8.556,
          "end": 12.4
        }
      ]
    },
    {
      "id": "1.25.7",
      "mandala": 1,
      "sukta": 25,
      "rik": 7,
      "padas": [
        {
          "text": "वेदा॒ यो वी॒नां प॒दम॒न्तरि᳚क्षेण॒ पत᳚ताम् ।",
          "begin": 0.0,
          "end": 7.705
        },
        {
          "text": "वेद᳚ ना॒वः स᳚मु॒द्रियः᳚ ॥",
          "begin": 7.705,
          "end": 11.6
        }
      ]
    },
    {
      "id": "1.25.8",
      "mandala": 1,
      "sukta": 25,
      "rik": 8,
      "padas": [
        {
          "text": "वेद᳚ मा॒सो धृ॒तव्र᳚तो॒ द्वाद᳚श प्र॒जाव᳚तः ।",
          "begin": 0.0,
          "end": 7.548
        },
        {
          "text": "वेदा॒ य उ᳚प॒जाय᳚ते ॥",
          "begin": 7.548,
          "end": 10.96
        }
      ]
    },
    {
      "id": "1.25.9",
      "mandala": 1,
      "sukta": 25,
      "rik": 9,
      "padas": [
        {
          "text": "वेद॒ वात᳚स्य वर्त॒निमु॒रोरृ॒ष्वस्य᳚ बृह॒तः ।",
          "begin": 0.0,
          "end": 7.159
        },
        {
          "text": "वेदा॒ ये अ॒ध्यास᳚ते ॥",
          "begin": 7.159,
          "end": 10.96
        }
      ]
    },
    {
      "id": "1.25.10",
      "mandala": 1,
      "sukta": 25,
      "rik": 10,
      "padas": [
        {
          "text": "नि ष᳚साद धृ॒तव्र᳚तो॒ वरु᳚णः प॒स्त्या॒३॒᳚स्वा ।",
          "begin": 0.0,
          "end": 9.509
        },
        {
          "text": "साम्रा᳚ज्याय सु॒क्रतु᳚ः ॥",
          "begin": 9.509,
          "end": 14.68
        }
      ]
    },
    {
      "id": "1.25.11",
      "mandala": 1,
      "sukta": 25,
      "rik": 11,
      "padas": [
        {
          "text": "अतो॒ विश्वा॒न्यद्भु᳚ता चिकि॒त्वाँ अ॒भि प᳚श्यति ।",
          "begin": 0.0,
          "end": 7.463
        },
        {
          "text": "कृ॒तानि॒ या च॒ कर्त्वा᳚ ॥",
          "begin": 7.463,
          "end": 11.16
        }
      ]
    },
    {
      "id": "1.25.12",
      "mandala": 1,
      "sukta": 25,
      "rik": 12,
      "padas": [
        {
          "text": "स नो᳚ वि॒श्वाहा᳚ सु॒क्रतु᳚रादि॒त्यः सु॒पथा᳚ करत् ।",
          "begin": 0.0,
          "end": 8.88
        },
        {
          "text": "प्र ण॒ आयूं᳚षि तारिषत् ॥",
          "begin": 8.88,
          "end": 13.12
        }
      ]
    },
    {
      "id": "1.25.13",
      "mandala": 1,
      "sukta": 25,
      "rik": 13,
      "padas": [
        {
          "text": "बिभ्र᳚द्द्रा॒पिं हि᳚र॒ण्ययं॒ वरु᳚णो वस्त नि॒र्णिज᳚म् ।",
          "begin": 0.0,
          "end": 6.881
        },
        {
          "text": "परि॒ स्पशो॒ नि षे᳚दिरे ॥",
          "begin": 6.881,
          "end": 10.76
        }
      ]
    },
    {
      "id": "1.25.14",
      "mandala": 1,
      "sukta": 25,
      "rik": 14,
      "padas": [
        {
          "text": "न यं दिप्स᳚न्ति दि॒प्सवो॒ न द्रुह्वा᳚णो॒ जना᳚नाम् ।",
          "begin": 0.0,
          "end": 9.109
        },
        {
          "text": "न दे॒वम॒भिमा᳚तयः ॥",
          "begin": 9.109,
          "end": 12.64
        }
      ]
    },
    {
      "id": "1.25.15",
      "mandala": 1,
      "sukta": 25,
      "rik": 15,
      "padas": [
        {
          "text": "उ॒त यो मानु᳚षे॒ष्वा यश᳚श्च॒क्रे असा॒म्या ।",
          "begin": 0.0,
          "end": 7.525
        },
        {
          "text": "अ॒स्माक᳚मु॒दरे॒ष्वा ॥",
          "begin": 7.525,
          "end": 10.8
        }
      ]
    },
    {
      "id": "1.25.16",
      "mandala": 1,
      "sukta": 25,
      "rik": 16,
      "padas": [
        {
          "text": "परा᳚ मे यन्ति धी॒तयो॒ गावो॒ न गव्यू᳚ती॒रनु᳚ ।",
          "begin": 0.0,
          "end": 8.403
        },
        {
          "text": "इ॒च्छन्ती᳚रुरु॒चक्ष᳚सम् ॥",
          "begin": 8.403,
          "end": 12.36
        }
      ]
    },
    {
      "id": "1.25.17",
      "mandala": 1,
      "sukta": 25,
      "rik": 17,
      "padas": [
        {
          "text": "सं नु वो᳚चावहै॒ पुन॒र्यतो᳚ मे॒ मध्वाभृ᳚तम् ।",
          "begin": 0.0,
          "end": 7.99
        },
        {
          "text": "होते᳚व॒ क्षद᳚से प्रि॒यम् ॥",
          "begin": 7.99,
          "end": 12.28
        }
      ]
    },
    {
      "id": "1.25.18",
      "mandala": 1,
      "sukta": 25,
      "rik": 18,
      "padas": [
        {
          "text": "दर्शं॒ नु वि॒श्वद᳚र्शतं॒ दर्शं॒ रथ॒मधि॒ क्षमि᳚ ।",
          "begin": 0.0,
          "end": 6.886
        },
        {
          "text": "ए॒ता जु᳚षत मे॒ गिरः᳚ ॥",
          "begin": 6.886,
          "end": 10.12
        }
      ]
    },
    {
      "id": "1.25.19",
      "mandala": 1,
      "sukta": 25,
      "rik": 19,
      "padas": [
        {
          "text": "इ॒मं मे᳚ वरुण श्रुधी॒ हव᳚म॒द्या च᳚ मृळय ।",
          "begin": 0.0,
          "end": 6.21
        },
        {
          "text": "त्वाम᳚व॒स्युरा च᳚के ॥",
          "begin": 6.21,
          "end": 9.76
        }
      ]
    },
    {
      "id": "1.25.20",
      "mandala": 1,
      "sukta": 25,
      "rik": 20,
      "padas": [
        {
          "text": "त्वं विश्व᳚स्य मेधिर दि॒वश्च॒ ग्मश्च᳚ राजसि ।",
          "begin": 0.0,
          "end": 6.153
        },
        {
          "text": "स याम᳚नि॒ प्रति᳚ श्रुधि ॥",
          "begin": 6.153,
          "end": 9.24
        }
      ]
    },
    {
      "id": "1.25.21",
      "mandala": 1,
      "sukta": 25,
      "rik": 21,
      "padas": [
        {
          "text": "उदु᳚त्त॒मं मु᳚मुग्धि नो॒ वि पाशं᳚ मध्य॒मं चृ᳚त ।",
          "begin": 0.0,
          "end": 8.459
        },
        {
          "text": "अवा᳚ध॒मानि᳚ जी॒वसे᳚ ॥",
          "begin": 8.459,
          "end": 11.8
        }
      ]
    }
  ]
}