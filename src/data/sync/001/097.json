{
  "mandala": 1,
  "sukta": 97,
  "verses": [
    {
      "id": "1.97.1",
      "mandala": 1,
      "sukta": 97,
      "rik": 1,
      "padas": [
        {
          "text": "अप᳚ न॒ः शोशु᳚चद॒घमग्ने᳚ शुशु॒ग्ध्या र॒यिम् ।",
          "begin": 0.0,
          "end": 7.052
        },
        {
          "text": "अप᳚ न॒ः शोशु᳚चद॒घम् ॥",
          "begin": 7.052,
          "end": 9.84
        }
      ]
    },
    {
      "id": "1.97.2",
      "mandala": 1,
      "sukta": 97,
      "rik": 2,
      "padas": [
        {
          "text": "सु॒क्षे॒त्रि॒या सु᳚गातु॒या व᳚सू॒या च᳚ यजामहे ।",
          "begin": 0.0,
          "end": 6.525
        },
        {
          "text": "अप᳚ न॒ः शोशु᳚चद॒घम् ॥",
          "begin": 6.525,
          "end": 9.08
        }
      ]
    },
    {
      "id": "1.97.3",
      "mandala": 1,
      "sukta": 97,
      "rik": 3,
      "padas": [
        {
          "text": "प्र यद्भन्दि᳚ष्ठ एषां॒ प्रास्माका᳚सश्च सू॒रयः᳚ ।",
          "begin": 0.0,
          "end": 7.891
        },
        {
          "text": "अप᳚ न॒ः शोशु᳚चद॒घम् ॥",
          "begin": 7.891,
          "end": 10.52
        }
      ]
    },
    {
      "id": "1.97.4",
      "mandala": 1,
      "sukta": 97,
      "rik": 4,
      "padas": [
        {
          "text": "प्र यत्ते᳚ अग्ने सू॒रयो॒ जाये᳚महि॒ प्र ते᳚ व॒यम् ।",
          "begin": 0.0,
          "end": 8.175
        },
        {
          "text": "अप᳚ न॒ः शोशु᳚चद॒घम् ॥",
          "begin": 8.175,
          "end": 10.64
        }
      ]
    },
    {
      "id": "1.97.5",
      "mandala": 1,
      "sukta": 97,
      "rik": 5,
      "padas": [
        {
          "text": "प्र यद॒ग्नेः सह᳚स्वतो वि॒श्वतो॒ यन्ति᳚ भा॒नवः᳚ ।",
          "begin": 0.0,
          "end": 6.798
        },
        {
          "text": "अप᳚ न॒ः शोशु᳚चद॒घम् ॥",
          "begin": 6.798,
          "end": 9.12
        }
      ]
    },
    {
      "id": "1.97.6",
      "mandala": 1,
      "sukta": 97,
      "rik": 6,
      "padas": [
        {
          "text": "त्वं हि वि᳚श्वतोमुख वि॒श्वतः᳚ परि॒भूरसि᳚ ।",
          "begin": 0.0,
          "end": 5.333
        },
        {
          "text": "अप᳚ न॒ः शोशु᳚चद॒घम् ॥",
          "begin": 5.333,
          "end": 7.76
        }
      ]
    },
    {
      "id": "1.97.7",
      "mandala": 1,
      "sukta": 97,
      "rik": 7,
      "padas": [
        {
          "text": "द्विषो᳚ नो विश्वतोमु॒खाति᳚ ना॒वेव᳚ पारय ।",
          "begin": 0.0,
          "end": 6.176
        },
        {
          "text": "अप᳚ न॒ः शोशु᳚चद॒घम् ॥",
          "begin": 6.176,
          "end": 8.64
        }
      ]
    },
    {
      "id": "1.97.8",
      "mandala": 1,
      "sukta": 97,
      "rik": 8,
      "padas": [
        {
          "text": "स न॒ः सिन्धु᳚मिव ना॒वयाति᳚ पर्षा स्व॒स्तये᳚ ।",
          "begin": 0.0,
          "end": 6.649
        },
        {
          "text": "अप᳚ न॒ः शोशु᳚चद॒घम् ॥",
          "begin": 6.649,
          "end": 10.68
        }
      ]
    }
  ]
}