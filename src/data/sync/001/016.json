{
  "mandala": 1,
  "sukta": 16,
  "verses": [
    {
      "id": "1.16.1",
      "mandala": 1,
      "sukta": 16,
      "rik": 1,
      "padas": [
        {
          "text": "आ त्वा᳚ वहन्तु॒ हर᳚यो॒ वृष᳚णं॒ सोम᳚पीतये ।",
          "begin": 0.0,
          "end": 8.449
        },
        {
          "text": "इन्द्र᳚ त्वा॒ सूर᳚चक्षसः ॥",
          "begin": 8.449,
          "end": 12.52
        }
      ]
    },
    {
      "id": "1.16.2",
      "mandala": 1,
      "sukta": 16,
      "rik": 2,
      "padas": [
        {
          "text": "इ॒मा धा॒ना घृ᳚त॒स्नुवो॒ हरी᳚ इ॒होप᳚ वक्षतः ।",
          "begin": 0.0,
          "end": 8.554
        },
        {
          "text": "इन्द्रं᳚ सु॒खत᳚मे॒ रथे᳚ ॥",
          "begin": 8.554,
          "end": 12.2
        }
      ]
    },
    {
      "id": "1.16.3",
      "mandala": 1,
      "sukta": 16,
      "rik": 3,
      "padas": [
        {
          "text": "इन्द्रं᳚ प्रा॒तर्ह᳚वामह॒ इन्द्रं᳚ प्रय॒त्य᳚ध्व॒रे ।",
          "begin": 0.0,
          "end": 8.078
        },
        {
          "text": "इन्द्रं॒ सोम᳚स्य पी॒तये᳚ ॥",
          "begin": 8.078,
          "end": 12.32
        }
      ]
    },
    {
      "id": "1.16.4",
      "mandala": 1,
      "sukta": 16,
      "rik": 4,
      "padas": [
        {
          "text": "उप᳚ नः सु॒तमा ग᳚हि॒ हरि᳚भिरिन्द्र के॒शिभि᳚ः ।",
          "begin": 0.0,
          "end": 6.534
        },
        {
          "text": "सु॒ते हि त्वा॒ हवा᳚महे ॥",
          "begin": 6.534,
          "end": 10.56
        }
      ]
    },
    {
      "id": "1.16.5",
      "mandala": 1,
      "sukta": 16,
      "rik": 5,
      "padas": [
        {
          "text": "सेमं न॒ः स्तोम॒मा ग॒ह्युपे॒दं सव᳚नं सु॒तम् ।",
          "begin": 0.0,
          "end": 7.219
        },
        {
          "text": "गौ॒रो न तृ᳚षि॒तः पि᳚ब ॥",
          "begin": 7.219,
          "end": 10.68
        }
      ]
    },
    {
      "id": "1.16.6",
      "mandala": 1,
      "sukta": 16,
      "rik": 6,
      "padas": [
        {
          "text": "इ॒मे सोमा᳚स॒ इन्द᳚वः सु॒तासो॒ अधि᳚ ब॒र्हिषि᳚ ।",
          "begin": 0.0,
          "end": 8.039
        },
        {
          "text": "ताँ इ᳚न्द्र॒ सह᳚से पिब ॥",
          "begin": 8.039,
          "end": 12.72
        }
      ]
    },
    {
      "id": "1.16.7",
      "mandala": 1,
      "sukta": 16,
      "rik": 7,
      "padas": [
        {
          "text": "अ॒यं ते॒ स्तोमो᳚ अग्रि॒यो हृ᳚दि॒स्पृग᳚स्तु॒ शंत᳚मः ।",
          "begin": 0.0,
          "end": 8.566
        },
        {
          "text": "अथा॒ सोमं᳚ सु॒तं पि᳚ब ॥",
          "begin": 8.566,
          "end": 12.56
        }
      ]
    },
    {
      "id": "1.16.8",
      "mandala": 1,
      "sukta": 16,
      "rik": 8,
      "padas": [
        {
          "text": "विश्व॒मित्सव᳚नं सु॒तमिन्द्रो॒ मदा᳚य गच्छति ।",
          "begin": 0.0,
          "end": 7.284
        },
        {
          "text": "वृ॒त्र॒हा सोम᳚पीतये ॥",
          "begin": 7.284,
          "end": 10.8
        }
      ]
    },
    {
      "id": "1.16.9",
      "mandala": 1,
      "sukta": 16,
      "rik": 9,
      "padas": [
        {
          "text": "सेमं न॒ः काम॒मा पृ᳚ण॒ गोभि॒रश्वै᳚ः शतक्रतो ।",
          "begin": 0.0,
          "end": 8.555
        },
        {
          "text": "स्तवा᳚म त्वा स्वा॒ध्य᳚ः ॥",
          "begin": 8.555,
          "end": 15.8
        }
      ]
    }
  ]
}