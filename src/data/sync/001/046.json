{
  "mandala": 1,
  "sukta": 46,
  "verses": [
    {
      "id": "1.46.1",
      "mandala": 1,
      "sukta": 46,
      "rik": 1,
      "padas": [
        {
          "text": "ए॒षो उ॒षा अपू᳚र्व्या॒ व्यु᳚च्छति प्रि॒या दि॒वः ।",
          "begin": 0.0,
          "end": 8.463
        },
        {
          "text": "स्तु॒षे वा᳚मश्विना बृ॒हत् ॥",
          "begin": 8.463,
          "end": 12.84
        }
      ]
    },
    {
      "id": "1.46.2",
      "mandala": 1,
      "sukta": 46,
      "rik": 2,
      "padas": [
        {
          "text": "या द॒स्रा सिन्धु᳚मातरा मनो॒तरा᳚ रयी॒णाम् ।",
          "begin": 0.0,
          "end": 8.144
        },
        {
          "text": "धि॒या दे॒वा व᳚सु॒विदा᳚ ॥",
          "begin": 8.144,
          "end": 12.12
        }
      ]
    },
    {
      "id": "1.46.3",
      "mandala": 1,
      "sukta": 46,
      "rik": 3,
      "padas": [
        {
          "text": "व॒च्यन्ते᳚ वां ककु॒हासो᳚ जू॒र्णाया॒मधि᳚ वि॒ष्टपि᳚ ।",
          "begin": 0.0,
          "end": 9.303
        },
        {
          "text": "यद्वां॒ रथो॒ विभि॒ष्पता᳚त् ॥",
          "begin": 9.303,
          "end": 14.12
        }
      ]
    },
    {
      "id": "1.46.4",
      "mandala": 1,
      "sukta": 46,
      "rik": 4,
      "padas": [
        {
          "text": "ह॒विषा᳚ जा॒रो अ॒पां पिप᳚र्ति॒ पपु᳚रिर्नरा ।",
          "begin": 0.0,
          "end": 7.782
        },
        {
          "text": "पि॒ता कुट᳚स्य चर्ष॒णिः ॥",
          "begin": 7.782,
          "end": 11.24
        }
      ]
    },
    {
      "id": "1.46.5",
      "mandala": 1,
      "sukta": 46,
      "rik": 5,
      "padas": [
        {
          "text": "आ॒दा॒रो वां᳚ मती॒नां नास᳚त्या मतवचसा ।",
          "begin": 0.0,
          "end": 7.661
        },
        {
          "text": "पा॒तं सोम᳚स्य धृष्णु॒या ॥",
          "begin": 7.661,
          "end": 12.2
        }
      ]
    },
    {
      "id": "1.46.6",
      "mandala": 1,
      "sukta": 46,
      "rik": 6,
      "padas": [
        {
          "text": "या न॒ः पीप᳚रदश्विना॒ ज्योति᳚ष्मती॒ तम᳚स्ति॒रः ।",
          "begin": 0.0,
          "end": 7.887
        },
        {
          "text": "ताम॒स्मे रा᳚साथा॒मिष᳚म् ॥",
          "begin": 7.887,
          "end": 12.76
        }
      ]
    },
    {
      "id": "1.46.7",
      "mandala": 1,
      "sukta": 46,
      "rik": 7,
      "padas": [
        {
          "text": "आ नो᳚ ना॒वा म᳚ती॒नां या॒तं पा॒राय॒ गन्त᳚वे ।",
          "begin": 0.0,
          "end": 8.89
        },
        {
          "text": "यु॒ञ्जाथा᳚मश्विना॒ रथ᳚म् ॥",
          "begin": 8.89,
          "end": 13.92
        }
      ]
    },
    {
      "id": "1.46.8",
      "mandala": 1,
      "sukta": 46,
      "rik": 8,
      "padas": [
        {
          "text": "अ॒रित्रं᳚ वां दि॒वस्पृ॒थु ती॒र्थे सिन्धू᳚नां॒ रथः᳚ ।",
          "begin": 0.0,
          "end": 9.4
        },
        {
          "text": "धि॒या यु᳚युज्र॒ इन्द᳚वः ॥",
          "begin": 9.4,
          "end": 12.88
        }
      ]
    },
    {
      "id": "1.46.9",
      "mandala": 1,
      "sukta": 46,
      "rik": 9,
      "padas": [
        {
          "text": "दि॒वस्क᳚ण्वास॒ इन्द᳚वो॒ वसु॒ सिन्धू᳚नां प॒दे ।",
          "begin": 0.0,
          "end": 8.123
        },
        {
          "text": "स्वं व॒व्रिं कुह᳚ धित्सथः ॥",
          "begin": 8.123,
          "end": 12.12
        }
      ]
    },
    {
      "id": "1.46.10",
      "mandala": 1,
      "sukta": 46,
      "rik": 10,
      "padas": [
        {
          "text": "अभू᳚दु॒ भा उ᳚ अं॒शवे॒ हिर᳚ण्यं॒ प्रति॒ सूर्य᳚ः ।",
          "begin": 0.0,
          "end": 8.536
        },
        {
          "text": "व्य᳚ख्यज्जि॒ह्वयासि᳚तः ॥",
          "begin": 8.536,
          "end": 12.2
        }
      ]
    },
    {
      "id": "1.46.11",
      "mandala": 1,
      "sukta": 46,
      "rik": 11,
      "padas": [
        {
          "text": "अभू᳚दु पा॒रमेत᳚वे॒ पन्था᳚ ऋ॒तस्य᳚ साधु॒या ।",
          "begin": 0.0,
          "end": 8.382
        },
        {
          "text": "अद᳚र्शि॒ वि स्रु॒तिर्दि॒वः ॥",
          "begin": 8.382,
          "end": 11.96
        }
      ]
    },
    {
      "id": "1.46.12",
      "mandala": 1,
      "sukta": 46,
      "rik": 12,
      "padas": [
        {
          "text": "तत्त॒दिद॒श्विनो॒रवो᳚ जरि॒ता प्रति᳚ भूषति ।",
          "begin": 0.0,
          "end": 7.468
        },
        {
          "text": "मदे॒ सोम᳚स्य॒ पिप्र᳚तोः ॥",
          "begin": 7.468,
          "end": 11.52
        }
      ]
    },
    {
      "id": "1.46.13",
      "mandala": 1,
      "sukta": 46,
      "rik": 13,
      "padas": [
        {
          "text": "वा॒व॒सा॒ना वि॒वस्व᳚ति॒ सोम᳚स्य पी॒त्या गि॒रा ।",
          "begin": 0.0,
          "end": 7.628
        },
        {
          "text": "म॒नु॒ष्वच्छ᳚म्भू॒ आ ग᳚तम् ॥",
          "begin": 7.628,
          "end": 12.04
        }
      ]
    },
    {
      "id": "1.46.14",
      "mandala": 1,
      "sukta": 46,
      "rik": 14,
      "padas": [
        {
          "text": "यु॒वोरु॒षा अनु॒ श्रियं॒ परि᳚ज्मनोरु॒पाच᳚रत् ।",
          "begin": 0.0,
          "end": 7.583
        },
        {
          "text": "ऋ॒ता व᳚नथो अ॒क्तुभि᳚ः ॥",
          "begin": 7.583,
          "end": 11.6
        }
      ]
    },
    {
      "id": "1.46.15",
      "mandala": 1,
      "sukta": 46,
      "rik": 15,
      "padas": [
        {
          "text": "उ॒भा पि᳚बतमश्विनो॒भा न॒ः शर्म᳚ यच्छतम् ।",
          "begin": 0.0,
          "end": 6.672
        },
        {
          "text": "अ॒वि॒द्रि॒याभि᳚रू॒तिभि᳚ः ॥",
          "begin": 6.672,
          "end": 10.6
        }
      ]
    }
  ]
}