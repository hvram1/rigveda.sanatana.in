{
  "mandala": 1,
  "sukta": 5,
  "verses": [
    {
      "id": "1.5.1",
      "mandala": 1,
      "sukta": 5,
      "rik": 1,
      "padas": [
        {
          "text": "आ त्वेता॒ नि षी᳚द॒तेन्द्र᳚म॒भि प्र गा᳚यत ।",
          "begin": 0.0,
          "end": 6.063
        },
        {
          "text": "सखा᳚य॒ः स्तोम᳚वाहसः ॥",
          "begin": 6.063,
          "end": 13.04
        }
      ]
    },
    {
      "id": "1.5.2",
      "mandala": 1,
      "sukta": 5,
      "rik": 2,
      "padas": [
        {
          "text": "पु॒रू॒तमं᳚ पुरू॒णामीशा᳚नं॒ वार्या᳚णाम् ।",
          "begin": 0.0,
          "end": 8.92
        },
        {
          "text": "इन्द्रं॒ सोमे॒ सचा᳚ सु॒ते ॥",
          "begin": 8.92,
          "end": 13.68
        }
      ]
    },
    {
      "id": "1.5.3",
      "mandala": 1,
      "sukta": 5,
      "rik": 3,
      "padas": [
        {
          "text": "स घा᳚ नो॒ योग॒ आ भु᳚व॒त्स रा॒ये स पुरं᳚ध्याम् ।",
          "begin": 0.0,
          "end": 8.718
        },
        {
          "text": "गम॒द्वाजे᳚भि॒रा स नः᳚ ॥",
          "begin": 8.718,
          "end": 13.6
        }
      ]
    },
    {
      "id": "1.5.4",
      "mandala": 1,
      "sukta": 5,
      "rik": 4,
      "padas": [
        {
          "text": "यस्य᳚ सं॒स्थे न वृ॒ण्वते॒ हरी᳚ स॒मत्सु॒ शत्र᳚वः ।",
          "begin": 0.0,
          "end": 8.008
        },
        {
          "text": "तस्मा॒ इन्द्रा᳚य गायत ॥",
          "begin": 8.008,
          "end": 12.76
        }
      ]
    },
    {
      "id": "1.5.5",
      "mandala": 1,
      "sukta": 5,
      "rik": 5,
      "padas": [
        {
          "text": "सु॒त॒पाव्ने᳚ सु॒ता इ॒मे शुच᳚यो यन्ति वी॒तये᳚ ।",
          "begin": 0.0,
          "end": 8.662
        },
        {
          "text": "सोमा᳚सो॒ दध्या᳚शिरः ॥",
          "begin": 8.662,
          "end": 13.56
        }
      ]
    },
    {
      "id": "1.5.6",
      "mandala": 1,
      "sukta": 5,
      "rik": 6,
      "padas": [
        {
          "text": "त्वं सु॒तस्य᳚ पी॒तये᳚ स॒द्यो वृ॒द्धो अ᳚जायथाः ।",
          "begin": 0.0,
          "end": 8.533
        },
        {
          "text": "इन्द्र॒ ज्यैष्ठ्या᳚य सुक्रतो ॥",
          "begin": 8.533,
          "end": 12.72
        }
      ]
    },
    {
      "id": "1.5.7",
      "mandala": 1,
      "sukta": 5,
      "rik": 7,
      "padas": [
        {
          "text": "आ त्वा᳚ विशन्त्वा॒शव॒ः सोमा᳚स इन्द्र गिर्वणः ।",
          "begin": 0.0,
          "end": 8.915
        },
        {
          "text": "शं ते᳚ सन्तु॒ प्रचे᳚तसे ॥",
          "begin": 8.915,
          "end": 14.04
        }
      ]
    },
    {
      "id": "1.5.8",
      "mandala": 1,
      "sukta": 5,
      "rik": 8,
      "padas": [
        {
          "text": "त्वां स्तोमा᳚ अवीवृध॒न्त्वामु॒क्था श᳚तक्रतो ।",
          "begin": 0.0,
          "end": 8.216
        },
        {
          "text": "त्वां व᳚र्धन्तु नो॒ गिरः᳚ ॥",
          "begin": 8.216,
          "end": 12.72
        }
      ]
    },
    {
      "id": "1.5.9",
      "mandala": 1,
      "sukta": 5,
      "rik": 9,
      "padas": [
        {
          "text": "अक्षि᳚तोतिः सनेदि॒मं वाज॒मिन्द्र᳚ः सह॒स्रिण᳚म् ।",
          "begin": 0.0,
          "end": 7.67
        },
        {
          "text": "यस्मि॒न्विश्वा᳚नि॒ पौंस्या᳚ ॥",
          "begin": 7.67,
          "end": 12.48
        }
      ]
    },
    {
      "id": "1.5.10",
      "mandala": 1,
      "sukta": 5,
      "rik": 10,
      "padas": [
        {
          "text": "मा नो॒ मर्ता᳚ अ॒भि द्रु᳚हन्त॒नूना᳚मिन्द्र गिर्वणः ।",
          "begin": 0.0,
          "end": 9.817
        },
        {
          "text": "ईशा᳚नो यवया व॒धम् ॥",
          "begin": 9.817,
          "end": 15.2
        }
      ]
    }
  ]
}