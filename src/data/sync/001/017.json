{
  "mandala": 1,
  "sukta": 17,
  "verses": [
    {
      "id": "1.17.1",
      "mandala": 1,
      "sukta": 17,
      "rik": 1,
      "padas": [
        {
          "text": "इन्द्रा॒वरु᳚णयोर॒हं स॒म्राजो॒रव॒ आ वृ᳚णे ।",
          "begin": 0.0,
          "end": 8.14
        },
        {
          "text": "ता नो᳚ मृळात ई॒दृशे᳚ ॥",
          "begin": 8.14,
          "end": 12.96
        }
      ]
    },
    {
      "id": "1.17.2",
      "mandala": 1,
      "sukta": 17,
      "rik": 2,
      "padas": [
        {
          "text": "गन्ता᳚रा॒ हि स्थो॑व᳚से॒ हवं॒ विप्र᳚स्य॒ माव᳚तः ।",
          "begin": 0.0,
          "end": 7.951
        },
        {
          "text": "ध॒र्तारा᳚ चर्षणी॒नाम् ॥",
          "begin": 7.951,
          "end": 12.24
        }
      ]
    },
    {
      "id": "1.17.3",
      "mandala": 1,
      "sukta": 17,
      "rik": 3,
      "padas": [
        {
          "text": "अ॒नु॒का॒मं त᳚र्पयेथा॒मिन्द्रा᳚वरुण रा॒य आ ।",
          "begin": 0.0,
          "end": 8.067
        },
        {
          "text": "ता वां॒ नेदि᳚ष्ठमीमहे ॥",
          "begin": 8.067,
          "end": 12.4
        }
      ]
    },
    {
      "id": "1.17.4",
      "mandala": 1,
      "sukta": 17,
      "rik": 4,
      "padas": [
        {
          "text": "यु॒वाकु॒ हि शची᳚नां यु॒वाकु᳚ सुमती॒नाम् ।",
          "begin": 0.0,
          "end": 7.815
        },
        {
          "text": "भू॒याम᳚ वाज॒दाव्ना᳚म् ॥",
          "begin": 7.815,
          "end": 12.28
        }
      ]
    },
    {
      "id": "1.17.5",
      "mandala": 1,
      "sukta": 17,
      "rik": 5,
      "padas": [
        {
          "text": "इन्द्र᳚ः सहस्र॒दाव्नां॒ वरु᳚ण॒ः शंस्या᳚नाम् ।",
          "begin": 0.0,
          "end": 7.856
        },
        {
          "text": "क्रतु᳚र्भवत्यु॒क्थ्य᳚ः ॥",
          "begin": 7.856,
          "end": 11.72
        }
      ]
    },
    {
      "id": "1.17.6",
      "mandala": 1,
      "sukta": 17,
      "rik": 6,
      "padas": [
        {
          "text": "तयो॒रिदव᳚सा व॒यं स॒नेम॒ नि च᳚ धीमहि ।",
          "begin": 0.0,
          "end": 6.028
        },
        {
          "text": "स्यादु॒त प्र॒रेच᳚नम् ॥",
          "begin": 6.028,
          "end": 9.16
        }
      ]
    },
    {
      "id": "1.17.7",
      "mandala": 1,
      "sukta": 17,
      "rik": 7,
      "padas": [
        {
          "text": "इन्द्रा᳚वरुण वाम॒हं हु॒वे चि॒त्राय॒ राध᳚से ।",
          "begin": 0.0,
          "end": 7.389
        },
        {
          "text": "अ॒स्मान्त्सु जि॒ग्युष᳚स्कृतम् ॥",
          "begin": 7.389,
          "end": 11.48
        }
      ]
    },
    {
      "id": "1.17.8",
      "mandala": 1,
      "sukta": 17,
      "rik": 8,
      "padas": [
        {
          "text": "इन्द्रा᳚वरुण॒ नू नु वां॒ सिषा᳚सन्तीषु धी॒ष्वा ।",
          "begin": 0.0,
          "end": 8.779
        },
        {
          "text": "अ॒स्मभ्यं॒ शर्म᳚ यच्छतम् ॥",
          "begin": 8.779,
          "end": 12.44
        }
      ]
    },
    {
      "id": "1.17.9",
      "mandala": 1,
      "sukta": 17,
      "rik": 9,
      "padas": [
        {
          "text": "प्र वा᳚मश्नोतु सुष्टु॒तिरिन्द्रा᳚वरुण॒ यां हु॒वे ।",
          "begin": 0.0,
          "end": 11.914
        },
        {
          "text": "यामृ॒धाथे᳚ स॒धस्तु᳚तिम् ॥",
          "begin": 11.914,
          "end": 14.96
        }
      ]
    }
  ]
}