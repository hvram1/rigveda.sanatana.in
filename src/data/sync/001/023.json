{
  "mandala": 1,
  "sukta": 23,
  "verses": [
    {
      "id": "1.23.1",
      "mandala": 1,
      "sukta": 23,
      "rik": 1,
      "padas": [
        {
          "text": "ती॒व्राः सोमा᳚स॒ आ ग᳚ह्या॒शीर्व᳚न्तः सु॒ता इ॒मे ।",
          "begin": 0.0,
          "end": 11.355
        },
        {
          "text": "वायो॒ तान्प्रस्थि᳚तान्पिब ॥",
          "begin": 11.355,
          "end": 16.6
        }
      ]
    },
    {
      "id": "1.23.2",
      "mandala": 1,
      "sukta": 23,
      "rik": 2,
      "padas": [
        {
          "text": "उ॒भा दे॒वा दि᳚वि॒स्पृशे᳚न्द्रवा॒यू ह᳚वामहे ।",
          "begin": 0.0,
          "end": 7.117
        },
        {
          "text": "अ॒स्य सोम᳚स्य पी॒तये᳚ ॥",
          "begin": 7.117,
          "end": 12.28
        }
      ]
    },
    {
      "id": "1.23.3",
      "mandala": 1,
      "sukta": 23,
      "rik": 3,
      "padas": [
        {
          "text": "इ॒न्द्र॒वा॒यू म᳚नो॒जुवा॒ विप्रा᳚ हवन्त ऊ॒तये᳚ ।",
          "begin": 0.0,
          "end": 8.658
        },
        {
          "text": "स॒ह॒स्रा॒क्षा धि॒यस्पती᳚ ॥",
          "begin": 8.658,
          "end": 13.32
        }
      ]
    },
    {
      "id": "1.23.4",
      "mandala": 1,
      "sukta": 23,
      "rik": 4,
      "padas": [
        {
          "text": "मि॒त्रं व॒यं ह᳚वामहे॒ वरु᳚णं॒ सोम᳚पीतये ।",
          "begin": 0.0,
          "end": 6.022
        },
        {
          "text": "ज॒ज्ञा॒ना पू॒तद᳚क्षसा ॥",
          "begin": 6.022,
          "end": 11.08
        }
      ]
    },
    {
      "id": "1.23.5",
      "mandala": 1,
      "sukta": 23,
      "rik": 5,
      "padas": [
        {
          "text": "ऋ॒तेन॒ यावृ᳚ता॒वृधा᳚वृ॒तस्य॒ ज्योति᳚ष॒स्पती᳚ ।",
          "begin": 0.0,
          "end": 8.101
        },
        {
          "text": "ता मि॒त्रावरु᳚णा हुवे ॥",
          "begin": 8.101,
          "end": 12.16
        }
      ]
    },
    {
      "id": "1.23.6",
      "mandala": 1,
      "sukta": 23,
      "rik": 6,
      "padas": [
        {
          "text": "वरु᳚णः प्रावि॒ता भु᳚वन्मि॒त्रो विश्वा᳚भिरू॒तिभि᳚ः ।",
          "begin": 0.0,
          "end": 8.221
        },
        {
          "text": "कर᳚तां नः सु॒राध᳚सः ॥",
          "begin": 8.221,
          "end": 11.96
        }
      ]
    },
    {
      "id": "1.23.7",
      "mandala": 1,
      "sukta": 23,
      "rik": 7,
      "padas": [
        {
          "text": "म॒रुत्व᳚न्तं हवामह॒ इन्द्र॒मा सोम᳚पीतये ।",
          "begin": 0.0,
          "end": 7.939
        },
        {
          "text": "स॒जूर्ग॒णेन᳚ तृम्पतु ॥",
          "begin": 7.939,
          "end": 11.84
        }
      ]
    },
    {
      "id": "1.23.8",
      "mandala": 1,
      "sukta": 23,
      "rik": 8,
      "padas": [
        {
          "text": "इन्द्र᳚ज्येष्ठा॒ मरु᳚द्गणा॒ देवा᳚स॒ः पूष᳚रातयः ।",
          "begin": 0.0,
          "end": 8.961
        },
        {
          "text": "विश्वे॒ मम᳚ श्रुता॒ हव᳚म् ॥",
          "begin": 8.961,
          "end": 13.2
        }
      ]
    },
    {
      "id": "1.23.9",
      "mandala": 1,
      "sukta": 23,
      "rik": 9,
      "padas": [
        {
          "text": "ह॒त वृ॒त्रं सु᳚दानव॒ इन्द्रे᳚ण॒ सह᳚सा यु॒जा ।",
          "begin": 0.0,
          "end": 7.657
        },
        {
          "text": "मा नो᳚ दु॒ःशंस᳚ ईशत ॥",
          "begin": 7.657,
          "end": 12.04
        }
      ]
    },
    {
      "id": "1.23.10",
      "mandala": 1,
      "sukta": 23,
      "rik": 10,
      "padas": [
        {
          "text": "विश्वा᳚न्दे॒वान्ह᳚वामहे म॒रुत॒ः सोम᳚पीतये ।",
          "begin": 0.0,
          "end": 8.13
        },
        {
          "text": "उ॒ग्रा हि पृश्नि᳚मातरः ॥",
          "begin": 8.13,
          "end": 12.48
        }
      ]
    },
    {
      "id": "1.23.11",
      "mandala": 1,
      "sukta": 23,
      "rik": 11,
      "padas": [
        {
          "text": "जय᳚तामिव तन्य॒तुर्म॒रुता᳚मेति धृष्णु॒या ।",
          "begin": 0.0,
          "end": 7.753
        },
        {
          "text": "यच्छुभं᳚ या॒थना᳚ नरः ॥",
          "begin": 7.753,
          "end": 12.68
        }
      ]
    },
    {
      "id": "1.23.12",
      "mandala": 1,
      "sukta": 23,
      "rik": 12,
      "padas": [
        {
          "text": "ह॒स्का॒राद्वि॒द्युत॒स्पर्यतो᳚ जा॒ता अ᳚वन्तु नः ।",
          "begin": 0.0,
          "end": 8.917
        },
        {
          "text": "म॒रुतो᳚ मृळयन्तु नः ॥",
          "begin": 8.917,
          "end": 12.92
        }
      ]
    },
    {
      "id": "1.23.13",
      "mandala": 1,
      "sukta": 23,
      "rik": 13,
      "padas": [
        {
          "text": "आ पू᳚षञ्चि॒त्रब᳚र्हिष॒माघृ᳚णे ध॒रुणं᳚ दि॒वः ।",
          "begin": 0.0,
          "end": 9.164
        },
        {
          "text": "आजा᳚ न॒ष्टं यथा᳚ प॒शुम् ॥",
          "begin": 9.164,
          "end": 14.12
        }
      ]
    },
    {
      "id": "1.23.14",
      "mandala": 1,
      "sukta": 23,
      "rik": 14,
      "padas": [
        {
          "text": "पू॒षा राजा᳚न॒माघृ᳚णि॒रप᳚गूळ्हं॒ गुहा᳚ हि॒तम् ।",
          "begin": 0.0,
          "end": 8.986
        },
        {
          "text": "अवि᳚न्दच्चि॒त्रब᳚र्हिषम् ॥",
          "begin": 8.986,
          "end": 13.12
        }
      ]
    },
    {
      "id": "1.23.15",
      "mandala": 1,
      "sukta": 23,
      "rik": 15,
      "padas": [
        {
          "text": "उ॒तो स मह्य॒मिन्दु᳚भि॒ः षड्यु॒क्ताँ अ᳚नु॒सेषि᳚धत् ।",
          "begin": 0.0,
          "end": 7.5
        },
        {
          "text": "गोभि॒र्यवं॒ न च᳚र्कृषत् ॥",
          "begin": 7.5,
          "end": 11.52
        }
      ]
    },
    {
      "id": "1.23.16",
      "mandala": 1,
      "sukta": 23,
      "rik": 16,
      "padas": [
        {
          "text": "अ॒म्बयो᳚ य॒न्त्यध्व᳚भिर्जा॒मयो᳚ अध्वरीय॒ताम् ।",
          "begin": 0.0,
          "end": 8.299
        },
        {
          "text": "पृ॒ञ्च॒तीर्मधु᳚ना॒ पयः᳚ ॥",
          "begin": 8.299,
          "end": 12.92
        }
      ]
    },
    {
      "id": "1.23.17",
      "mandala": 1,
      "sukta": 23,
      "rik": 17,
      "padas": [
        {
          "text": "अ॒मूर्या उप॒ सूर्ये॒ याभि᳚र्वा॒ सूर्य᳚ः स॒ह ।",
          "begin": 0.0,
          "end": 8.271
        },
        {
          "text": "ता नो᳚ हिन्वन्त्वध्व॒रम् ॥",
          "begin": 8.271,
          "end": 13.04
        }
      ]
    },
    {
      "id": "1.23.18",
      "mandala": 1,
      "sukta": 23,
      "rik": 18,
      "padas": [
        {
          "text": "अ॒पो दे॒वीरुप᳚ ह्वये॒ यत्र॒ गाव॒ः पिब᳚न्ति नः ।",
          "begin": 0.0,
          "end": 7.991
        },
        {
          "text": "सिन्धु᳚भ्य॒ः कर्त्वं᳚ ह॒विः ॥",
          "begin": 7.991,
          "end": 12.48
        }
      ]
    },
    {
      "id": "1.23.19",
      "mandala": 1,
      "sukta": 23,
      "rik": 19,
      "padas": [
        {
          "text": "अ॒प्स्व१॒᳚न्तर॒मृत᳚म॒प्सु भे᳚ष॒जम॒पामु॒त प्रश᳚स्तये ।",
          "begin": 0.0,
          "end": 8.914
        },
        {
          "text": "देवा॒ भव᳚त वा॒जिनः᳚ ॥",
          "begin": 8.914,
          "end": 13.0
        }
      ]
    },
    {
      "id": "1.23.20",
      "mandala": 1,
      "sukta": 23,
      "rik": 20,
      "padas": [
        {
          "text": "अ॒प्सु मे॒ सोमो᳚ अब्रवीद॒न्तर्विश्वा᳚नि भेष॒जा ।",
          "begin": 0.0,
          "end": 8.697
        },
        {
          "text": "अ॒ग्निं च᳚ वि॒श्वश᳚म्भुव॒माप᳚श्च वि॒श्वभे᳚षजीः ॥",
          "begin": 8.697,
          "end": 16.12
        }
      ]
    },
    {
      "id": "1.23.21",
      "mandala": 1,
      "sukta": 23,
      "rik": 21,
      "padas": [
        {
          "text": "आपः᳚ पृणी॒त भे᳚ष॒जं वरू᳚थं त॒न्वे॒३॒᳚ मम᳚ ।",
          "begin": 0.0,
          "end": 10.581
        },
        {
          "text": "ज्योक्च॒ सूर्यं᳚ दृ॒शे ॥",
          "begin": 10.581,
          "end": 15.12
        }
      ]
    },
    {
      "id": "1.23.22",
      "mandala": 1,
      "sukta": 23,
      "rik": 22,
      "padas": [
        {
          "text": "इ॒दमा᳚प॒ः प्र व᳚हत॒ यत्किं च᳚ दुरि॒तं मयि᳚ ।",
          "begin": 0.0,
          "end": 6.437
        },
        {
          "text": "यद्वा॒हम᳚भिदु॒द्रोह॒ यद्वा᳚ शे॒प उ॒तानृ᳚तम् ॥",
          "begin": 6.437,
          "end": 13.6
        }
      ]
    },
    {
      "id": "1.23.23",
      "mandala": 1,
      "sukta": 23,
      "rik": 23,
      "padas": [
        {
          "text": "आपो᳚ अ॒द्यान्व᳚चारिषं॒ रसे᳚न॒ सम᳚गस्महि ।",
          "begin": 0.0,
          "end": 8.31
        },
        {
          "text": "पय᳚स्वानग्न॒ आ ग᳚हि॒ तं मा॒ सं सृ᳚ज॒ वर्च᳚सा ॥",
          "begin": 8.31,
          "end": 15.16
        }
      ]
    },
    {
      "id": "1.23.24",
      "mandala": 1,
      "sukta": 23,
      "rik": 24,
      "padas": [
        {
          "text": "सं मा᳚ग्ने॒ वर्च᳚सा सृज॒ सं प्र॒जया॒ समायु᳚षा ।",
          "begin": 0.0,
          "end": 8.54
        },
        {
          "text": "वि॒द्युर्मे᳚ अस्य दे॒वा इन्द्रो᳚ विद्यात्स॒ह ऋषि᳚भिः ॥",
          "begin": 8.54,
          "end": 19.2
        }
      ]
    }
  ]
}