  "verses": [
    {
      "id": "1.107.1",
      "mandala": 1,
      "sukta": 107,
      "rik": 1,
      "padas": [
        {
          "text": "य॒ज्ञो दे॒वानां॒ प्रत्ये᳚ति सु॒म्नमादि᳚त्यासो॒ भव᳚ता मृळ॒यन्त᳚ः ।",
          "begin": 0.0,
          "end": 10.144
        },
        {
          "text": "आ वो॒॑र्वाची᳚ सुम॒तिर्व᳚वृत्यादं॒होश्चि॒द्या व᳚रिवो॒वित्त॒रास᳚त् ॥",
          "begin": 10.144,
          "end": 18.56
        }
      ]
    },
    {
      "id": "1.107.2",
      "mandala": 1,
      "sukta": 107,
      "rik": 2,
      "padas": [
        {
          "text": "उप᳚ नो दे॒वा अव॒सा ग᳚म॒न्त्वङ्गि᳚रसां॒ साम᳚भिः स्तू॒यमा᳚नाः ।",
          "begin": 0.0,
          "end": 8.798
        },
        {
          "text": "इन्द्र᳚ इन्द्रि॒यैर्म॒रुतो᳚ म॒रुद्भि᳚रादि॒त्यैर्नो॒ अदि᳚ति॒ः शर्म᳚ यंसत् ॥",
          "begin": 8.798,
          "end": 17.24
        }
      ]
    },
    {
      "id": "1.107.3",
      "mandala": 1,
      "sukta": 107,
      "rik": 3,
      "padas": [
        {
          "text": "तन्न॒ इन्द्र॒स्तद्वरु᳚ण॒स्तद॒ग्निस्तद᳚र्य॒मा तत्स᳚वि॒ता चनो᳚ धात् ।",
          "begin": 0.0,
          "end": 8.965
        },
        {
          "text": "तन्नो᳚ मि॒त्रो वरु᳚णो मामहन्ता॒मदि᳚ति॒ः सिन्धु᳚ः पृथि॒वी उ॒त द्यौः ॥",
          "begin": 8.965,
          "end": 18.84
        }
      ]
    }
  ]
}