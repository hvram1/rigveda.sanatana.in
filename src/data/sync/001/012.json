{
  "mandala": 1,
  "sukta": 12,
  "verses": [
    {
      "id": "1.12.1",
      "mandala": 1,
      "sukta": 12,
      "rik": 1,
      "padas": [
        {
          "text": "अ॒ग्निं दू॒तं वृ᳚णीमहे॒ होता᳚रं वि॒श्ववे᳚दसम् ।",
          "begin": 0.0,
          "end": 9.923
        },
        {
          "text": "अ॒स्य य॒ज्ञस्य᳚ सु॒क्रतु᳚म् ॥",
          "begin": 9.923,
          "end": 14.36
        }
      ]
    },
    {
      "id": "1.12.2",
      "mandala": 1,
      "sukta": 12,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒ग्निम᳚ग्निं॒ हवी᳚मभि॒ः सदा᳚ हवन्त वि॒श्पति᳚म् ।",
          "begin": 0.0,
          "end": 7.501
        },
        {
          "text": "ह॒व्य॒वाहं᳚ पुरुप्रि॒यम् ॥",
          "begin": 7.501,
          "end": 13.0
        }
      ]
    },
    {
      "id": "1.12.3",
      "mandala": 1,
      "sukta": 12,
      "rik": 3,
      "padas": [
        {
          "text": "अग्ने᳚ दे॒वाँ इ॒हा व᳚ह जज्ञा॒नो वृ॒क्तब᳚र्हिषे ।",
          "begin": 0.0,
          "end": 11.632
        },
        {
          "text": "असि॒ होता᳚ न॒ ईड्य᳚ः ॥",
          "begin": 11.632,
          "end": 13.68
        }
      ]
    },
    {
      "id": "1.12.4",
      "mandala": 1,
      "sukta": 12,
      "rik": 4,
      "padas": [
        {
          "text": "ताँ उ᳚श॒तो वि बो᳚धय॒ यद᳚ग्ने॒ यासि᳚ दू॒त्य᳚म् ।",
          "begin": 0.0,
          "end": 9.079
        },
        {
          "text": "दे॒वैरा स᳚त्सि ब॒र्हिषि᳚ ॥",
          "begin": 9.079,
          "end": 13.08
        }
      ]
    },
    {
      "id": "1.12.5",
      "mandala": 1,
      "sukta": 12,
      "rik": 5,
      "padas": [
        {
          "text": "घृता᳚हवन दीदिव॒ः प्रति᳚ ष्म॒ रिष᳚तो दह ।",
          "begin": 0.0,
          "end": 5.776
        },
        {
          "text": "अग्ने॒ त्वं र᳚क्ष॒स्विनः᳚ ॥",
          "begin": 5.776,
          "end": 11.56
        }
      ]
    },
    {
      "id": "1.12.6",
      "mandala": 1,
      "sukta": 12,
      "rik": 6,
      "padas": [
        {
          "text": "अ॒ग्निना॒ग्निः समि᳚ध्यते क॒विर्गृ॒हप᳚ति॒र्युवा᳚ ।",
          "begin": 0.0,
          "end": 7.804
        },
        {
          "text": "ह॒व्य॒वाड्जु॒ह्वा᳚स्यः ॥",
          "begin": 7.804,
          "end": 12.16
        }
      ]
    },
    {
      "id": "1.12.7",
      "mandala": 1,
      "sukta": 12,
      "rik": 7,
      "padas": [
        {
          "text": "क॒विम॒ग्निमुप᳚ स्तुहि स॒त्यध᳚र्माणमध्व॒रे ।",
          "begin": 0.0,
          "end": 7.29
        },
        {
          "text": "दे॒वम᳚मीव॒चात᳚नम् ॥",
          "begin": 7.29,
          "end": 10.52
        }
      ]
    },
    {
      "id": "1.12.8",
      "mandala": 1,
      "sukta": 12,
      "rik": 8,
      "padas": [
        {
          "text": "यस्त्वाम᳚ग्ने ह॒विष्प᳚तिर्दू॒तं दे᳚व सप॒र्यति᳚ ।",
          "begin": 0.0,
          "end": 7.763
        },
        {
          "text": "तस्य᳚ स्म प्रावि॒ता भ᳚व ॥",
          "begin": 7.763,
          "end": 11.48
        }
      ]
    },
    {
      "id": "1.12.9",
      "mandala": 1,
      "sukta": 12,
      "rik": 9,
      "padas": [
        {
          "text": "यो अ॒ग्निं दे॒ववी᳚तये ह॒विष्माँ᳚ आ॒विवा᳚सति ।",
          "begin": 0.0,
          "end": 9.588
        },
        {
          "text": "तस्मै᳚ पावक मृळय ॥",
          "begin": 9.588,
          "end": 13.48
        }
      ]
    },
    {
      "id": "1.12.10",
      "mandala": 1,
      "sukta": 12,
      "rik": 10,
      "padas": [
        {
          "text": "स नः᳚ पावक दीदि॒वो॑ग्ने᳚ दे॒वाँ इ॒हा व᳚ह ।",
          "begin": 0.0,
          "end": 8.073
        },
        {
          "text": "उप᳚ य॒ज्ञं ह॒विश्च᳚ नः ॥",
          "begin": 8.073,
          "end": 11.68
        }
      ]
    },
    {
      "id": "1.12.11",
      "mandala": 1,
      "sukta": 12,
      "rik": 11,
      "padas": [
        {
          "text": "स न॒ः स्तवा᳚न॒ आ भ᳚र गाय॒त्रेण॒ नवी᳚यसा ।",
          "begin": 0.0,
          "end": 8.019
        },
        {
          "text": "र॒यिं वी॒रव᳚ती॒मिष᳚म् ॥",
          "begin": 8.019,
          "end": 12.28
        }
      ]
    },
    {
      "id": "1.12.12",
      "mandala": 1,
      "sukta": 12,
      "rik": 12,
      "padas": [
        {
          "text": "अग्ने᳚ शु॒क्रेण᳚ शो॒चिषा॒ विश्वा᳚भिर्दे॒वहू᳚तिभिः ।",
          "begin": 0.0,
          "end": 9.865
        },
        {
          "text": "इ॒मं स्तोमं᳚ जुषस्व नः ॥",
          "begin": 9.865,
          "end": 15.4
        }
      ]
    }
  ]
}