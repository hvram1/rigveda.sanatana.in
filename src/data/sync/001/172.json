      "rik": 1,
      "padas": [
        {
          "text": "चि॒त्रो वो᳚॑स्तु॒ याम᳚श्चि॒त्र ऊ॒ती सु᳚दानवः ।",
          "begin": 0.0,
          "end": 7.745
        },
        {
          "text": "मरु᳚तो॒ अहि᳚भानवः ॥",
          "begin": 7.745,
          "end": 10.6
        }
      ]
    },
    {
      "id": "1.172.2",
      "mandala": 1,
      "sukta": 172,
      "rik": 2,
      "padas": [
        {
          "text": "आ॒रे सा वः᳚ सुदानवो॒ मरु᳚त ऋञ्ज॒ती शरु᳚ः ।",
          "begin": 0.0,
          "end": 6.733
        },
        {
          "text": "आ॒रे अश्मा॒ यमस्य᳚थ ॥",
          "begin": 6.733,
          "end": 9.8
        }
      ]
    },
    {
      "id": "1.172.3",
      "mandala": 1,
      "sukta": 172,
      "rik": 3,
      "padas": [
        {
          "text": "तृ॒ण॒स्क॒न्दस्य॒ नु विश॒ः परि᳚ वृङ्क्त सुदानवः ।",
          "begin": 0.0,
          "end": 6.812
        },
        {
          "text": "ऊ॒र्ध्वान्न᳚ः कर्त जी॒वसे᳚ ॥",
          "begin": 6.812,
          "end": 11.2
        }
      ]
    }
  ]
}