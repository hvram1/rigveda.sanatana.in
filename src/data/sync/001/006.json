{
  "mandala": 1,
  "sukta": 6,
  "verses": [
    {
      "id": "1.6.1",
      "mandala": 1,
      "sukta": 6,
      "rik": 1,
      "padas": [
        {
          "text": "यु॒ञ्जन्ति᳚ ब्र॒ध्नम᳚रु॒षं चर᳚न्तं॒ परि᳚ त॒स्थुषः᳚ ।",
          "begin": 0.0,
          "end": 7.656
        },
        {
          "text": "रोच᳚न्ते रोच॒ना दि॒वि ॥",
          "begin": 7.656,
          "end": 13.6
        }
      ]
    },
    {
      "id": "1.6.2",
      "mandala": 1,
      "sukta": 6,
      "rik": 2,
      "padas": [
        {
          "text": "यु॒ञ्जन्त्य᳚स्य॒ काम्या॒ हरी॒ विप᳚क्षसा॒ रथे᳚ ।",
          "begin": 0.0,
          "end": 6.949
        },
        {
          "text": "शोणा᳚ धृ॒ष्णू नृ॒वाह᳚सा ॥",
          "begin": 6.949,
          "end": 12.2
        }
      ]
    },
    {
      "id": "1.6.3",
      "mandala": 1,
      "sukta": 6,
      "rik": 3,
      "padas": [
        {
          "text": "के॒तुं कृ॒ण्वन्न᳚के॒तवे॒ पेशो᳚ मर्या अपे॒शसे᳚ ।",
          "begin": 0.0,
          "end": 9.133
        },
        {
          "text": "समु॒षद्भि᳚रजायथाः ॥",
          "begin": 9.133,
          "end": 12.56
        }
      ]
    },
    {
      "id": "1.6.4",
      "mandala": 1,
      "sukta": 6,
      "rik": 4,
      "padas": [
        {
          "text": "आदह᳚ स्व॒धामनु॒ पुन᳚र्गर्भ॒त्वमे᳚रि॒रे ।",
          "begin": 0.0,
          "end": 7.188
        },
        {
          "text": "दधा᳚ना॒ नाम᳚ य॒ज्ञिय᳚म् ॥",
          "begin": 7.188,
          "end": 12.04
        }
      ]
    },
    {
      "id": "1.6.5",
      "mandala": 1,
      "sukta": 6,
      "rik": 5,
      "padas": [
        {
          "text": "वी॒ळु चि᳚दारुज॒त्नुभि॒र्गुहा᳚ चिदिन्द्र॒ वह्नि᳚भिः ।",
          "begin": 0.0,
          "end": 7.875
        },
        {
          "text": "अवि᳚न्द उ॒स्रिया॒ अनु᳚ ॥",
          "begin": 7.875,
          "end": 11.96
        }
      ]
    },
    {
      "id": "1.6.6",
      "mandala": 1,
      "sukta": 6,
      "rik": 6,
      "padas": [
        {
          "text": "दे॒व॒यन्तो॒ यथा᳚ म॒तिमच्छा᳚ वि॒दद्व᳚सुं॒ गिरः᳚ ।",
          "begin": 0.0,
          "end": 8.673
        },
        {
          "text": "म॒हाम᳚नूषत श्रु॒तम् ॥",
          "begin": 8.673,
          "end": 12.08
        }
      ]
    },
    {
      "id": "1.6.7",
      "mandala": 1,
      "sukta": 6,
      "rik": 7,
      "padas": [
        {
          "text": "इन्द्रे᳚ण॒ सं हि दृक्ष᳚से संजग्मा॒नो अबि᳚भ्युषा ।",
          "begin": 0.0,
          "end": 8.456
        },
        {
          "text": "म॒न्दू स᳚मा॒नव᳚र्चसा ॥",
          "begin": 8.456,
          "end": 12.56
        }
      ]
    },
    {
      "id": "1.6.8",
      "mandala": 1,
      "sukta": 6,
      "rik": 8,
      "padas": [
        {
          "text": "अ॒न॒व॒द्यैर॒भिद्यु᳚भिर्म॒खः सह᳚स्वदर्चति ।",
          "begin": 0.0,
          "end": 6.613
        },
        {
          "text": "ग॒णैरिन्द्र᳚स्य॒ काम्यै᳚ः ॥",
          "begin": 6.613,
          "end": 11.16
        }
      ]
    },
    {
      "id": "1.6.9",
      "mandala": 1,
      "sukta": 6,
      "rik": 9,
      "padas": [
        {
          "text": "अतः᳚ परिज्म॒न्ना ग᳚हि दि॒वो वा᳚ रोच॒नादधि᳚ ।",
          "begin": 0.0,
          "end": 7.928
        },
        {
          "text": "सम᳚स्मिन्नृञ्जते॒ गिरः᳚ ॥",
          "begin": 7.928,
          "end": 11.12
        }
      ]
    },
    {
      "id": "1.6.10",
      "mandala": 1,
      "sukta": 6,
      "rik": 10,
      "padas": [
        {
          "text": "इ॒तो वा᳚ सा॒तिमीम᳚हे दि॒वो वा॒ पार्थि᳚वा॒दधि᳚ ।",
          "begin": 0.0,
          "end": 8.482
        },
        {
          "text": "इन्द्रं᳚ म॒हो वा॒ रज᳚सः ॥",
          "begin": 8.482,
          "end": 14.0
        }
      ]
    }
  ]
}