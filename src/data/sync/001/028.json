{
  "mandala": 1,
  "sukta": 28,
  "verses": [
    {
      "id": "1.28.1",
      "mandala": 1,
      "sukta": 28,
      "rik": 1,
      "padas": [
        {
          "text": "यत्र॒ ग्रावा᳚ पृ॒थुबु᳚ध्न ऊ॒र्ध्वो भव᳚ति॒ सोत᳚वे ।",
          "begin": 0.0,
          "end": 7.673
        },
        {
          "text": "उ॒लूख᳚लसुताना॒मवेद्वि᳚न्द्र जल्गुलः ॥",
          "begin": 7.673,
          "end": 13.8
        }
      ]
    },
    {
      "id": "1.28.2",
      "mandala": 1,
      "sukta": 28,
      "rik": 2,
      "padas": [
        {
          "text": "यत्र॒ द्वावि᳚व ज॒घना᳚धिषव॒ण्या᳚ कृ॒ता ।",
          "begin": 0.0,
          "end": 4.613
        },
        {
          "text": "उ॒लूख᳚लसुताना॒मवेद्वि᳚न्द्र जल्गुलः ॥",
          "begin": 4.613,
          "end": 12.68
        }
      ]
    },
    {
      "id": "1.28.3",
      "mandala": 1,
      "sukta": 28,
      "rik": 3,
      "padas": [
        {
          "text": "यत्र॒ नार्य᳚पच्य॒वमु᳚पच्य॒वं च॒ शिक्ष᳚ते ।",
          "begin": 0.0,
          "end": 6.335
        },
        {
          "text": "उ॒लूख᳚लसुताना॒मवेद्वि᳚न्द्र जल्गुलः ॥",
          "begin": 6.335,
          "end": 12.08
        }
      ]
    },
    {
      "id": "1.28.4",
      "mandala": 1,
      "sukta": 28,
      "rik": 4,
      "padas": [
        {
          "text": "यत्र॒ मन्थां᳚ विब॒ध्नते᳚ र॒श्मीन्यमि᳚त॒वा इ᳚व ।",
          "begin": 0.0,
          "end": 8.072
        },
        {
          "text": "उ॒लूख᳚लसुताना॒मवेद्वि᳚न्द्र जल्गुलः ॥",
          "begin": 8.072,
          "end": 14.04
        }
      ]
    },
    {
      "id": "1.28.5",
      "mandala": 1,
      "sukta": 28,
      "rik": 5,
      "padas": [
        {
          "text": "यच्चि॒द्धि त्वं गृ॒हेगृ᳚ह॒ उलू᳚खलक यु॒ज्यसे᳚ ।",
          "begin": 0.0,
          "end": 6.832
        },
        {
          "text": "इ॒ह द्यु॒मत्त᳚मं वद॒ जय᳚तामिव दुन्दु॒भिः ॥",
          "begin": 6.832,
          "end": 12.16
        }
      ]
    },
    {
      "id": "1.28.6",
      "mandala": 1,
      "sukta": 28,
      "rik": 6,
      "padas": [
        {
          "text": "उ॒त स्म᳚ ते वनस्पते॒ वातो॒ वि वा॒त्यग्र॒मित् ।",
          "begin": 0.0,
          "end": 7.366
        },
        {
          "text": "अथो॒ इन्द्रा᳚य॒ पात᳚वे सु॒नु सोम᳚मुलूखल ॥",
          "begin": 7.366,
          "end": 13.0
        }
      ]
    },
    {
      "id": "1.28.7",
      "mandala": 1,
      "sukta": 28,
      "rik": 7,
      "padas": [
        {
          "text": "आ॒य॒जी वा᳚ज॒सात᳚मा॒ ता ह्यु१॒᳚च्चा वि᳚जर्भृ॒तः ।",
          "begin": 0.0,
          "end": 8.814
        },
        {
          "text": "हरी᳚ इ॒वान्धां᳚सि॒ बप्स᳚ता ॥",
          "begin": 8.814,
          "end": 14.12
        }
      ]
    },
    {
      "id": "1.28.8",
      "mandala": 1,
      "sukta": 28,
      "rik": 8,
      "padas": [
        {
          "text": "ता नो᳚ अ॒द्य व᳚नस्पती ऋ॒ष्वावृ॒ष्वेभि᳚ः सो॒तृभि᳚ः ।",
          "begin": 0.0,
          "end": 8.365
        },
        {
          "text": "इन्द्रा᳚य॒ मधु᳚मत्सुतम् ॥",
          "begin": 8.365,
          "end": 12.2
        }
      ]
    },
    {
      "id": "1.28.9",
      "mandala": 1,
      "sukta": 28,
      "rik": 9,
      "padas": [
        {
          "text": "उच्छि॒ष्टं च॒म्वो᳚र्भर॒ सोमं᳚ प॒वित्र॒ आ सृ᳚ज ।",
          "begin": 0.0,
          "end": 7.488
        },
        {
          "text": "नि धे᳚हि॒ गोरधि᳚ त्व॒चि ॥",
          "begin": 7.488,
          "end": 11.2
        }
      ]
    }
  ]
}