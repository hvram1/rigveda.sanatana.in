  "verses": [
    {
      "id": "1.106.1",
      "mandala": 1,
      "sukta": 106,
      "rik": 1,
      "padas": [
        {
          "text": "इन्द्रं᳚ मि॒त्रं वरु᳚णम॒ग्निमू॒तये॒ मारु᳚तं॒ शर्धो॒ अदि᳚तिं हवामहे ।",
          "begin": 0.0,
          "end": 10.064
        },
        {
          "text": "रथं॒ न दु॒र्गाद्व᳚सवः सुदानवो॒ विश्व᳚स्मान्नो॒ अंह᳚सो॒ निष्पि᳚पर्तन ॥",
          "begin": 10.064,
          "end": 19.04
        }
      ]
    },
    {
      "id": "1.106.2",
      "mandala": 1,
      "sukta": 106,
      "rik": 2,
      "padas": [
        {
          "text": "त आ᳚दित्या॒ आ ग᳚ता स॒र्वता᳚तये भू॒त दे᳚वा वृत्र॒तूर्ये᳚षु श॒म्भुवः᳚ ।",
          "begin": 0.0,
          "end": 11.56
        },
        {
          "text": "रथं॒ न दु॒र्गाद्व᳚सवः सुदानवो॒ विश्व᳚स्मान्नो॒ अंह᳚सो॒ निष्पि᳚पर्तन ॥",
          "begin": 11.56,
          "end": 20.56
        }
      ]
    },
    {
      "id": "1.106.3",
      "mandala": 1,
      "sukta": 106,
      "rik": 3,
      "padas": [
        {
          "text": "अव᳚न्तु नः पि॒तरः᳚ सुप्रवाच॒ना उ॒त दे॒वी दे॒वपु᳚त्रे ऋता॒वृधा᳚ ।",
          "begin": 0.0,
          "end": 9.129
        },
        {
          "text": "रथं॒ न दु॒र्गाद्व᳚सवः सुदानवो॒ विश्व᳚स्मान्नो॒ अंह᳚सो॒ निष्पि᳚पर्तन ॥",
          "begin": 9.129,
          "end": 17.88
        }
      ]
    },
    {
      "id": "1.106.4",
      "mandala": 1,
      "sukta": 106,
      "rik": 4,
      "padas": [
        {
          "text": "नरा॒शंसं᳚ वा॒जिनं᳚ वा॒जय᳚न्नि॒ह क्ष॒यद्वी᳚रं पू॒षणं᳚ सु॒म्नैरी᳚महे ।",
          "begin": 0.0,
          "end": 10.208
        },
        {
          "text": "रथं॒ न दु॒र्गाद्व᳚सवः सुदानवो॒ विश्व᳚स्मान्नो॒ अंह᳚सो॒ निष्पि᳚पर्तन ॥",
          "begin": 10.208,
          "end": 20.08
        }
      ]
    },
    {
      "id": "1.106.5",
      "mandala": 1,
      "sukta": 106,
      "rik": 5,
      "padas": [
        {
          "text": "बृह᳚स्पते॒ सद॒मिन्न᳚ः सु॒गं कृ᳚धि॒ शं योर्यत्ते॒ मनु᳚र्हितं॒ तदी᳚महे ।",
          "begin": 0.0,
          "end": 7.988
        },
        {
          "text": "रथं॒ न दु॒र्गाद्व᳚सवः सुदानवो॒ विश्व᳚स्मान्नो॒ अंह᳚सो॒ निष्पि᳚पर्तन ॥",
          "begin": 7.988,
          "end": 17.32
        }
      ]
    },
    {
      "id": "1.106.6",
      "mandala": 1,
      "sukta": 106,
      "rik": 6,
      "padas": [
        {
          "text": "इन्द्रं॒ कुत्सो᳚ वृत्र॒हणं॒ शची॒पतिं᳚ का॒टे निबा᳚ळ्ह॒ ऋषि᳚रह्वदू॒तये᳚ ।",
          "begin": 0.0,
          "end": 10.418
        },
        {
          "text": "रथं॒ न दु॒र्गाद्व᳚सवः सुदानवो॒ विश्व᳚स्मान्नो॒ अंह᳚सो॒ निष्पि᳚पर्तन ॥",
          "begin": 10.418,
          "end": 18.64
        }
      ]
    },
    {
      "id": "1.106.7",
      "mandala": 1,
      "sukta": 106,
      "rik": 7,
      "padas": [
        {
          "text": "दे॒वैर्नो᳚ दे॒व्यदि᳚ति॒र्नि पा᳚तु दे॒वस्त्रा॒ता त्रा᳚यता॒मप्र᳚युच्छन् ।",
          "begin": 0.0,
          "end": 14.562
        },
        {
          "text": "तन्नो᳚ मि॒त्रो वरु᳚णो मामहन्ता॒मदि᳚ति॒ः सिन्धु᳚ः पृथि॒वी उ॒त द्यौः ॥",
          "begin": 14.562,
          "end": 20.6
        }
      ]
    }
  ]
}