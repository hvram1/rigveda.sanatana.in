{
  "mandala": 1,
  "sukta": 2,
  "verses": [
    {
      "id": "1.2.1",
      "mandala": 1,
      "sukta": 2,
      "rik": 1,
      "padas": [
        {
          "text": "वाय॒वा या᳚हि दर्शते॒मे सोमा॒ अरं᳚कृताः ।",
          "begin": 0.0,
          "end": 8.725
        },
        {
          "text": "तेषां᳚ पाहि श्रु॒धी हव᳚म् ॥",
          "begin": 8.725,
          "end": 14.52
        }
      ]
    },
    {
      "id": "1.2.2",
      "mandala": 1,
      "sukta": 2,
      "rik": 2,
      "padas": [
        {
          "text": "वाय᳚ उ॒क्थेभि᳚र्जरन्ते॒ त्वामच्छा᳚ जरि॒तारः᳚ ।",
          "begin": 0.0,
          "end": 8.653
        },
        {
          "text": "सु॒तसो᳚मा अह॒र्विदः᳚ ॥",
          "begin": 8.653,
          "end": 13.32
        }
      ]
    },
    {
      "id": "1.2.3",
      "mandala": 1,
      "sukta": 2,
      "rik": 3,
      "padas": [
        {
          "text": "वायो॒ तव᳚ प्रपृञ्च॒ती धेना᳚ जिगाति दा॒शुषे᳚ ।",
          "begin": 0.0,
          "end": 8.952
        },
        {
          "text": "उ॒रू॒ची सोम᳚पीतये ॥",
          "begin": 8.952,
          "end": 12.6
        }
      ]
    },
    {
      "id": "1.2.4",
      "mandala": 1,
      "sukta": 2,
      "rik": 4,
      "padas": [
        {
          "text": "इन्द्र᳚वायू इ॒मे सु॒ता उप॒ प्रयो᳚भि॒रा ग᳚तम् ।",
          "begin": 0.0,
          "end": 8.562
        },
        {
          "text": "इन्द᳚वो वामु॒शन्ति॒ हि ॥",
          "begin": 8.562,
          "end": 12.4
        }
      ]
    },
    {
      "id": "1.2.5",
      "mandala": 1,
      "sukta": 2,
      "rik": 5,
      "padas": [
        {
          "text": "वाय॒विन्द्र᳚श्च चेतथः सु॒तानां᳚ वाजिनीवसू ।",
          "begin": 0.0,
          "end": 8.163
        },
        {
          "text": "तावा या᳚त॒मुप᳚ द्र॒वत् ॥",
          "begin": 8.163,
          "end": 13.16
        }
      ]
    },
    {
      "id": "1.2.6",
      "mandala": 1,
      "sukta": 2,
      "rik": 6,
      "padas": [
        {
          "text": "वाय॒विन्द्र᳚श्च सुन्व॒त आ या᳚त॒मुप᳚ निष्कृ॒तम् ।",
          "begin": 0.0,
          "end": 7.754
        },
        {
          "text": "म॒क्ष्वि१॒᳚त्था धि॒या न᳚रा ॥",
          "begin": 7.754,
          "end": 12.24
        }
      ]
    },
    {
      "id": "1.2.7",
      "mandala": 1,
      "sukta": 2,
      "rik": 7,
      "padas": [
        {
          "text": "मि॒त्रं हु᳚वे पू॒तद᳚क्षं॒ वरु᳚णं च रि॒शाद᳚सम् ।",
          "begin": 0.0,
          "end": 7.461
        },
        {
          "text": "धियं᳚ घृ॒ताचीं॒ साध᳚न्ता ॥",
          "begin": 7.461,
          "end": 12.8
        }
      ]
    },
    {
      "id": "1.2.8",
      "mandala": 1,
      "sukta": 2,
      "rik": 8,
      "padas": [
        {
          "text": "ऋ॒तेन᳚ मित्रावरुणावृतावृधावृतस्पृशा ।",
          "begin": 0.0,
          "end": 6.716
        },
        {
          "text": "क्रतुं᳚ बृ॒हन्त᳚माशाथे ॥",
          "begin": 6.716,
          "end": 11.48
        }
      ]
    },
    {
      "id": "1.2.9",
      "mandala": 1,
      "sukta": 2,
      "rik": 9,
      "padas": [
        {
          "text": "क॒वी नो᳚ मि॒त्रावरु᳚णा तुविजा॒ता उ᳚रु॒क्षया᳚ ।",
          "begin": 0.0,
          "end": 9.393
        },
        {
          "text": "दक्षं᳚ दधाते अ॒पस᳚म् ॥",
          "begin": 9.393,
          "end": 14.6
        }
      ]
    }
  ]
}