{
  "mandala": 1,
  "sukta": 49,
  "verses": [
    {
      "id": "1.49.1",
      "mandala": 1,
      "sukta": 49,
      "rik": 1,
      "padas": [
        {
          "text": "उषो᳚ भ॒द्रेभि॒रा ग᳚हि दि॒वश्चि᳚द्रोच॒नादधि᳚ ।",
          "begin": 0.0,
          "end": 7.996
        },
        {
          "text": "वह᳚न्त्वरु॒णप्स᳚व॒ उप᳚ त्वा सो॒मिनो᳚ गृ॒हम् ॥",
          "begin": 7.996,
          "end": 14.68
        }
      ]
    },
    {
      "id": "1.49.2",
      "mandala": 1,
      "sukta": 49,
      "rik": 2,
      "padas": [
        {
          "text": "सु॒पेश᳚सं सु॒खं रथं॒ यम॒ध्यस्था᳚ उष॒स्त्वम् ।",
          "begin": 0.0,
          "end": 7.573
        },
        {
          "text": "तेना᳚ सु॒श्रव᳚सं॒ जनं॒ प्रावा॒द्य दु᳚हितर्दिवः ॥",
          "begin": 7.573,
          "end": 15.12
        }
      ]
    },
    {
      "id": "1.49.3",
      "mandala": 1,
      "sukta": 49,
      "rik": 3,
      "padas": [
        {
          "text": "वय᳚श्चित्ते पत॒त्रिणो᳚ द्वि॒पच्चतु᳚ष्पदर्जुनि ।",
          "begin": 0.0,
          "end": 11.203
        },
        {
          "text": "उष॒ः प्रार᳚न्नृ॒तूँरनु᳚ दि॒वो अन्ते᳚भ्य॒स्परि᳚ ॥",
          "begin": 11.203,
          "end": 15.68
        }
      ]
    },
    {
      "id": "1.49.4",
      "mandala": 1,
      "sukta": 49,
      "rik": 4,
      "padas": [
        {
          "text": "व्यु॒च्छन्ती॒ हि र॒श्मिभि॒र्विश्व᳚मा॒भासि᳚ रोच॒नम् ।",
          "begin": 0.0,
          "end": 7.69
        },
        {
          "text": "तां त्वामु᳚षर्वसू॒यवो᳚ गी॒र्भिः कण्वा᳚ अहूषत ॥",
          "begin": 7.69,
          "end": 18.28
        }
      ]
    }
  ]
}