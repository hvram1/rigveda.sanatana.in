{
  "mandala": 1,
  "sukta": 13,
  "verses": [
    {
      "id": "1.13.1",
      "mandala": 1,
      "sukta": 13,
      "rik": 1,
      "padas": [
        {
          "text": "सुस᳚मिद्धो न॒ आ व᳚ह दे॒वाँ अ᳚ग्ने ह॒विष्म᳚ते ।",
          "begin": 0.0,
          "end": 8.543
        },
        {
          "text": "होतः᳚ पावक॒ यक्षि᳚ च ॥",
          "begin": 8.543,
          "end": 12.24
        }
      ]
    },
    {
      "id": "1.13.2",
      "mandala": 1,
      "sukta": 13,
      "rik": 2,
      "padas": [
        {
          "text": "मधु᳚मन्तं तनूनपाद्य॒ज्ञं दे॒वेषु᳚ नः कवे ।",
          "begin": 0.0,
          "end": 8.856
        },
        {
          "text": "अ॒द्या कृ᳚णुहि वी॒तये᳚ ॥",
          "begin": 8.856,
          "end": 12.04
        }
      ]
    },
    {
      "id": "1.13.3",
      "mandala": 1,
      "sukta": 13,
      "rik": 3,
      "padas": [
        {
          "text": "नरा॒शंस᳚मि॒ह प्रि॒यम॒स्मिन्य॒ज्ञ उप᳚ ह्वये ।",
          "begin": 0.0,
          "end": 6.889
        },
        {
          "text": "मधु᳚जिह्वं हवि॒ष्कृत᳚म् ॥",
          "begin": 6.889,
          "end": 10.88
        }
      ]
    },
    {
      "id": "1.13.4",
      "mandala": 1,
      "sukta": 13,
      "rik": 4,
      "padas": [
        {
          "text": "अग्ने᳚ सु॒खत᳚मे॒ रथे᳚ दे॒वाँ ई᳚ळि॒त आ व᳚ह ।",
          "begin": 0.0,
          "end": 9.184
        },
        {
          "text": "असि॒ होता॒ मनु᳚र्हितः ॥",
          "begin": 9.184,
          "end": 12.92
        }
      ]
    },
    {
      "id": "1.13.5",
      "mandala": 1,
      "sukta": 13,
      "rik": 5,
      "padas": [
        {
          "text": "स्तृ॒णी॒त ब॒र्हिरा᳚नु॒षग्घृ॒तपृ᳚ष्ठं मनीषिणः ।",
          "begin": 0.0,
          "end": 7.431
        },
        {
          "text": "यत्रा॒मृत᳚स्य॒ चक्ष᳚णम् ॥",
          "begin": 7.431,
          "end": 11.04
        }
      ]
    },
    {
      "id": "1.13.6",
      "mandala": 1,
      "sukta": 13,
      "rik": 6,
      "padas": [
        {
          "text": "वि श्र᳚यन्तामृता॒वृधो॒ द्वारो᳚ दे॒वीर᳚स॒श्चतः᳚ ।",
          "begin": 0.0,
          "end": 9.654
        },
        {
          "text": "अ॒द्या नू॒नं च॒ यष्ट᳚वे ॥",
          "begin": 9.654,
          "end": 12.84
        }
      ]
    },
    {
      "id": "1.13.7",
      "mandala": 1,
      "sukta": 13,
      "rik": 7,
      "padas": [
        {
          "text": "नक्तो॒षासा᳚ सु॒पेश᳚सा॒स्मिन्य॒ज्ञ उप᳚ ह्वये ।",
          "begin": 0.0,
          "end": 8.164
        },
        {
          "text": "इ॒दं नो᳚ ब॒र्हिरा॒सदे᳚ ॥",
          "begin": 8.164,
          "end": 12.76
        }
      ]
    },
    {
      "id": "1.13.8",
      "mandala": 1,
      "sukta": 13,
      "rik": 8,
      "padas": [
        {
          "text": "ता सु᳚जि॒ह्वा उप᳚ ह्वये॒ होता᳚रा॒ दैव्या᳚ क॒वी ।",
          "begin": 0.0,
          "end": 9.218
        },
        {
          "text": "य॒ज्ञं नो᳚ यक्षतामि॒मम् ॥",
          "begin": 9.218,
          "end": 13.6
        }
      ]
    },
    {
      "id": "1.13.9",
      "mandala": 1,
      "sukta": 13,
      "rik": 9,
      "padas": [
        {
          "text": "इळा॒ सर᳚स्वती म॒ही ति॒स्रो दे॒वीर्म᳚यो॒भुवः᳚ ।",
          "begin": 0.0,
          "end": 8.153
        },
        {
          "text": "ब॒र्हिः सी᳚दन्त्व॒स्रिधः᳚ ॥",
          "begin": 8.153,
          "end": 12.64
        }
      ]
    },
    {
      "id": "1.13.10",
      "mandala": 1,
      "sukta": 13,
      "rik": 10,
      "padas": [
        {
          "text": "इ॒ह त्वष्टा᳚रमग्रि॒यं वि॒श्वरू᳚प॒मुप᳚ ह्वये ।",
          "begin": 0.0,
          "end": 8.367
        },
        {
          "text": "अ॒स्माक᳚मस्तु॒ केव᳚लः ॥",
          "begin": 8.367,
          "end": 12.0
        }
      ]
    },
    {
      "id": "1.13.11",
      "mandala": 1,
      "sukta": 13,
      "rik": 11,
      "padas": [
        {
          "text": "अव᳚ सृजा वनस्पते॒ देव᳚ दे॒वेभ्यो᳚ ह॒विः ।",
          "begin": 0.0,
          "end": 7.02
        },
        {
          "text": "प्र दा॒तुर᳚स्तु॒ चेत᳚नम् ॥",
          "begin": 7.02,
          "end": 10.92
        }
      ]
    },
    {
      "id": "1.13.12",
      "mandala": 1,
      "sukta": 13,
      "rik": 12,
      "padas": [
        {
          "text": "स्वाहा᳚ य॒ज्ञं कृ᳚णोत॒नेन्द्रा᳚य॒ यज्व᳚नो गृ॒हे ।",
          "begin": 0.0,
          "end": 9.232
        },
        {
          "text": "तत्र᳚ दे॒वाँ उप᳚ ह्वये ॥",
          "begin": 9.232,
          "end": 14.92
        }
      ]
    }
  ]
}