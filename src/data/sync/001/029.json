  "verses": [
    {
      "id": "1.29.1",
      "mandala": 1,
      "sukta": 29,
      "rik": 1,
      "padas": [
        {
          "text": "यच्चि॒द्धि स᳚त्य सोमपा अनाश॒स्ता इ᳚व॒ स्मसि᳚ ।",
          "begin": 0.0,
          "end": 7.536
        },
        {
          "text": "आ तू न᳚ इन्द्र शंसय॒ गोष्वश्वे᳚षु शु॒भ्रिषु᳚ स॒हस्रे᳚षु तुवीमघ ॥",
          "begin": 7.536,
          "end": 17.0
        }
      ]
    },
    {
      "id": "1.29.2",
      "mandala": 1,
      "sukta": 29,
      "rik": 2,
      "padas": [
        {
          "text": "शिप्रि᳚न्वाजानां पते॒ शची᳚व॒स्तव᳚ दं॒सना᳚ ।",
          "begin": 0.0,
          "end": 8.007
        },
        {
          "text": "आ तू न᳚ इन्द्र शंसय॒ गोष्वश्वे᳚षु शु॒भ्रिषु᳚ स॒हस्रे᳚षु तुवीमघ ॥",
          "begin": 8.007,
          "end": 17.24
        }
      ]
    },
    {
      "id": "1.29.3",
      "mandala": 1,
      "sukta": 29,
      "rik": 3,
      "padas": [
        {
          "text": "नि ष्वा᳚पया मिथू॒दृशा᳚ स॒स्तामबु᳚ध्यमाने ।",
          "begin": 0.0,
          "end": 8.502
        },
        {
          "text": "आ तू न᳚ इन्द्र शंसय॒ गोष्वश्वे᳚षु शु॒भ्रिषु᳚ स॒हस्रे᳚षु तुवीमघ ॥",
          "begin": 8.502,
          "end": 17.0
        }
      ]
    },
    {
      "id": "1.29.4",
      "mandala": 1,
      "sukta": 29,
      "rik": 4,
      "padas": [
        {
          "text": "स॒सन्तु॒ त्या अरा᳚तयो॒ बोध᳚न्तु शूर रा॒तयः᳚ ।",
          "begin": 0.0,
          "end": 8.338
        },
        {
          "text": "आ तू न᳚ इन्द्र शंसय॒ गोष्वश्वे᳚षु शु॒भ्रिषु᳚ स॒हस्रे᳚षु तुवीमघ ॥",
          "begin": 8.338,
          "end": 17.6
        }
      ]
    },
    {
      "id": "1.29.5",
      "mandala": 1,
      "sukta": 29,
      "rik": 5,
      "padas": [
        {
          "text": "समि᳚न्द्र गर्द॒भं मृ᳚ण नु॒वन्तं᳚ पा॒पया᳚मु॒या ।",
          "begin": 0.0,
          "end": 8.306
        },
        {
          "text": "आ तू न᳚ इन्द्र शंसय॒ गोष्वश्वे᳚षु शु॒भ्रिषु᳚ स॒हस्रे᳚षु तुवीमघ ॥",
          "begin": 8.306,
          "end": 17.04
        }
      ]
    },
    {
      "id": "1.29.6",
      "mandala": 1,
      "sukta": 29,
      "rik": 6,
      "padas": [
        {
          "text": "पता᳚ति कुण्डृ॒णाच्या᳚ दू॒रं वातो॒ वना॒दधि᳚ ।",
          "begin": 0.0,
          "end": 7.844
        },
        {
          "text": "आ तू न᳚ इन्द्र शंसय॒ गोष्वश्वे᳚षु शु॒भ्रिषु᳚ स॒हस्रे᳚षु तुवीमघ ॥",
          "begin": 7.844,
          "end": 16.88
        }
      ]
    },
    {
      "id": "1.29.7",
      "mandala": 1,
      "sukta": 29,
      "rik": 7,
      "padas": [
        {
          "text": "सर्वं᳚ परिक्रो॒शं ज᳚हि ज॒म्भया᳚ कृकदा॒श्व᳚म् ।",
          "begin": 0.0,
          "end": 8.054
        },
        {
          "text": "आ तू न᳚ इन्द्र शंसय॒ गोष्वश्वे᳚षु शु॒भ्रिषु᳚ स॒हस्रे᳚षु तुवीमघ ॥",
          "begin": 8.054,
          "end": 17.04
        }
      ]
    }
  ]
}