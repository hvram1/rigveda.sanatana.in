{
  "mandala": 1,
  "sukta": 75,
  "verses": [
    {
      "id": "1.75.1",
      "mandala": 1,
      "sukta": 75,
      "rik": 1,
      "padas": [
        {
          "text": "जु॒षस्व᳚ स॒प्रथ᳚स्तमं॒ वचो᳚ दे॒वप्स᳚रस्तमम् ।",
          "begin": 0.0,
          "end": 7.459
        },
        {
          "text": "ह॒व्या जुह्वा᳚न आ॒सनि᳚ ॥",
          "begin": 7.459,
          "end": 12.0
        }
      ]
    },
    {
      "id": "1.75.2",
      "mandala": 1,
      "sukta": 75,
      "rik": 2,
      "padas": [
        {
          "text": "अथा᳚ ते अङ्गिरस्त॒माग्ने᳚ वेधस्तम प्रि॒यम् ।",
          "begin": 0.0,
          "end": 8.587
        },
        {
          "text": "वो॒चेम॒ ब्रह्म᳚ सान॒सि ॥",
          "begin": 8.587,
          "end": 12.32
        }
      ]
    },
    {
      "id": "1.75.3",
      "mandala": 1,
      "sukta": 75,
      "rik": 3,
      "padas": [
        {
          "text": "कस्ते᳚ जा॒मिर्जना᳚ना॒मग्ने॒ को दा॒श्व᳚ध्वरः ।",
          "begin": 0.0,
          "end": 7.97
        },
        {
          "text": "को ह॒ कस्मि᳚न्नसि श्रि॒तः ॥",
          "begin": 7.97,
          "end": 12.16
        }
      ]
    },
    {
      "id": "1.75.4",
      "mandala": 1,
      "sukta": 75,
      "rik": 4,
      "padas": [
        {
          "text": "त्वं जा॒मिर्जना᳚ना॒मग्ने᳚ मि॒त्रो अ᳚सि प्रि॒यः ।",
          "begin": 0.0,
          "end": 5.752
        },
        {
          "text": "सखा॒ सखि᳚भ्य॒ ईड्य᳚ः ॥",
          "begin": 5.752,
          "end": 11.28
        }
      ]
    },
    {
      "id": "1.75.5",
      "mandala": 1,
      "sukta": 75,
      "rik": 5,
      "padas": [
        {
          "text": "यजा᳚ नो मि॒त्रावरु᳚णा॒ यजा᳚ दे॒वाँ ऋ॒तं बृ॒हत् ।",
          "begin": 0.0,
          "end": 8.084
        },
        {
          "text": "अग्ने॒ यक्षि॒ स्वं दम᳚म् ॥",
          "begin": 8.084,
          "end": 14.2
        }
      ]
    }
  ]
}