  "verses": [
    {
      "id": "1.150.1",
      "mandala": 1,
      "sukta": 150,
      "rik": 1,
      "padas": [
        {
          "text": "पु॒रु त्वा᳚ दा॒श्वान्वो᳚चे॒॑रिर᳚ग्ने॒ तव᳚ स्वि॒दा ।",
          "begin": 0.0,
          "end": 7.785
        },
        {
          "text": "तो॒दस्ये᳚व शर॒ण आ म॒हस्य᳚ ॥",
          "begin": 7.785,
          "end": 12.24
        }
      ]
    },
    {
      "id": "1.150.2",
      "mandala": 1,
      "sukta": 150,
      "rik": 2,
      "padas": [
        {
          "text": "व्य᳚नि॒नस्य᳚ ध॒निनः᳚ प्रहो॒षे चि॒दर᳚रुषः ।",
          "begin": 0.0,
          "end": 5.397
        },
        {
          "text": "क॒दा च॒न प्र॒जिग᳚तो॒ अदे᳚वयोः ॥",
          "begin": 5.397,
          "end": 10.24
        }
      ]
    },
    {
      "id": "1.150.3",
      "mandala": 1,
      "sukta": 150,
      "rik": 3,
      "padas": [
        {
          "text": "स च॒न्द्रो वि᳚प्र॒ मर्त्यो᳚ म॒हो व्राध᳚न्तमो दि॒वि ।",
          "begin": 0.0,
          "end": 7.415
        },
        {
          "text": "प्रप्रेत्ते᳚ अग्ने व॒नुषः᳚ स्याम ॥",
          "begin": 7.415,
          "end": 13.88
        }
      ]
    }
  ]
}