{
  "mandala": 1,
  "sukta": 1,
  "verses": [
    {
      "id": "1.1.1",
      "mandala": 1,
      "sukta": 1,
      "rik": 1,
      "padas": [
        {
          "text": "अ॒ग्निमी᳚ळे पु॒रोहि᳚तं य॒ज्ञस्य᳚ दे॒वमृ॒त्विज᳚म् ।",
          "begin": 0.0,
          "end": 6.272
        },
        {
          "text": "होता᳚रं रत्न॒धात᳚मम् ॥",
          "begin": 6.272,
          "end": 13.8
        }
      ]
    },
    {
      "id": "1.1.2",
      "mandala": 1,
      "sukta": 1,
      "rik": 2,
      "padas": [
        {
          "text": "अ॒ग्निः पूर्वे᳚भि॒र्ऋषि᳚भि॒रीड्यो॒ नूत᳚नैरु॒त ।",
          "begin": 0.0,
          "end": 8.262
        },
        {
          "text": "स दे॒वाँ एह व᳚क्षति ॥",
          "begin": 8.262,
          "end": 12.52
        }
      ]
    },
    {
      "id": "1.1.3",
      "mandala": 1,
      "sukta": 1,
      "rik": 3,
      "padas": [
        {
          "text": "अ॒ग्निना᳚ र॒यिम᳚श्नव॒त्पोष᳚मे॒व दि॒वेदि᳚वे ।",
          "begin": 0.0,
          "end": 8.291
        },
        {
          "text": "य॒शसं᳚ वी॒रव᳚त्तमम् ॥",
          "begin": 8.291,
          "end": 12.08
        }
      ]
    },
    {
      "id": "1.1.4",
      "mandala": 1,
      "sukta": 1,
      "rik": 4,
      "padas": [
        {
          "text": "अग्ने॒ यं य॒ज्ञम᳚ध्व॒रं वि॒श्वतः᳚ परि॒भूरसि᳚ ।",
          "begin": 0.0,
          "end": 7.055
        },
        {
          "text": "स इद्दे॒वेषु᳚ गच्छति ॥",
          "begin": 7.055,
          "end": 10.68
        }
      ]
    },
    {
      "id": "1.1.5",
      "mandala": 1,
      "sukta": 1,
      "rik": 5,
      "padas": [
        {
          "text": "अ॒ग्निर्होता᳚ क॒विक्र᳚तुः स॒त्यश्चि॒त्रश्र᳚वस्तमः ।",
          "begin": 0.0,
          "end": 8.218
        },
        {
          "text": "दे॒वो दे॒वेभि॒रा ग᳚मत् ॥",
          "begin": 8.218,
          "end": 12.32
        }
      ]
    },
    {
      "id": "1.1.6",
      "mandala": 1,
      "sukta": 1,
      "rik": 6,
      "padas": [
        {
          "text": "यद॒ङ्ग दा॒शुषे॒ त्वमग्ने᳚ भ॒द्रं क᳚रि॒ष्यसि᳚ ।",
          "begin": 0.0,
          "end": 8.126
        },
        {
          "text": "तवेत्तत्स॒त्यम᳚ङ्गिरः ॥",
          "begin": 8.126,
          "end": 12.76
        }
      ]
    },
    {
      "id": "1.1.7",
      "mandala": 1,
      "sukta": 1,
      "rik": 7,
      "padas": [
        {
          "text": "उप᳚ त्वाग्ने दि॒वेदि᳚वे॒ दोषा᳚वस्तर्धि॒या व॒यम् ।",
          "begin": 0.0,
          "end": 9.01
        },
        {
          "text": "नमो॒ भर᳚न्त॒ एम᳚सि ॥",
          "begin": 9.01,
          "end": 12.8
        }
      ]
    },
    {
      "id": "1.1.8",
      "mandala": 1,
      "sukta": 1,
      "rik": 8,
      "padas": [
        {
          "text": "राज᳚न्तमध्व॒राणां᳚ गो॒पामृ॒तस्य॒ दीदि᳚विम् ।",
          "begin": 0.0,
          "end": 8.943
        },
        {
          "text": "वर्ध᳚मानं॒ स्वे दमे᳚ ॥",
          "begin": 8.943,
          "end": 13.4
        }
      ]
    },
    {
      "id": "1.1.9",
      "mandala": 1,
      "sukta": 1,
      "rik": 9,
      "padas": [
        {
          "text": "स नः᳚ पि॒तेव᳚ सू॒नवे॑ग्ने᳚ सूपाय॒नो भ᳚व ।",
          "begin": 0.0,
          "end": 7.866
        },
        {
          "text": "सच᳚स्वा नः स्व॒स्तये᳚ ॥",
          "begin": 7.866,
          "end": 13.0
        }
      ]
    }
  ]
}