{
  "mandala": 1,
  "sukta": 15,
  "verses": [
    {
      "id": "1.15.1",
      "mandala": 1,
      "sukta": 15,
      "rik": 1,
      "padas": [
        {
          "text": "इन्द्र॒ सोमं॒ पिब᳚ ऋ॒तुना त्वा᳚ विश॒न्त्विन्द᳚वः ।",
          "begin": 0.0,
          "end": 8.269
        },
        {
          "text": "म॒त्स॒रास॒स्तदो᳚कसः ॥",
          "begin": 8.269,
          "end": 12.2
        }
      ]
    },
    {
      "id": "1.15.2",
      "mandala": 1,
      "sukta": 15,
      "rik": 2,
      "padas": [
        {
          "text": "मरु᳚त॒ः पिब᳚त ऋ॒तुना᳚ पो॒त्राद्य॒ज्ञं पु᳚नीतन ।",
          "begin": 0.0,
          "end": 8.422
        },
        {
          "text": "यू॒यं हि ष्ठा सु᳚दानवः ॥",
          "begin": 8.422,
          "end": 12.12
        }
      ]
    },
    {
      "id": "1.15.3",
      "mandala": 1,
      "sukta": 15,
      "rik": 3,
      "padas": [
        {
          "text": "अ॒भि य॒ज्ञं गृ᳚णीहि नो॒ ग्नावो॒ नेष्ट॒ः पिब᳚ ऋ॒तुना᳚ ।",
          "begin": 0.0,
          "end": 8.201
        },
        {
          "text": "त्वं हि र᳚त्न॒धा असि᳚ ॥",
          "begin": 8.201,
          "end": 11.8
        }
      ]
    },
    {
      "id": "1.15.4",
      "mandala": 1,
      "sukta": 15,
      "rik": 4,
      "padas": [
        {
          "text": "अग्ने᳚ दे॒वाँ इ॒हा व᳚ह सा॒दया॒ योनि᳚षु त्रि॒षु ।",
          "begin": 0.0,
          "end": 7.07
        },
        {
          "text": "परि᳚ भूष॒ पिब᳚ ऋ॒तुना᳚ ॥",
          "begin": 7.07,
          "end": 11.96
        }
      ]
    },
    {
      "id": "1.15.5",
      "mandala": 1,
      "sukta": 15,
      "rik": 5,
      "padas": [
        {
          "text": "ब्राह्म᳚णादिन्द्र॒ राध᳚स॒ः पिबा॒ सोम᳚मृ॒तूँरनु᳚ ।",
          "begin": 0.0,
          "end": 7.753
        },
        {
          "text": "तवेद्धि स॒ख्यमस्तृ᳚तम् ॥",
          "begin": 7.753,
          "end": 11.36
        }
      ]
    },
    {
      "id": "1.15.6",
      "mandala": 1,
      "sukta": 15,
      "rik": 6,
      "padas": [
        {
          "text": "यु॒वं दक्षं᳚ धृतव्रत॒ मित्रा᳚वरुण दू॒ळभ᳚म् ।",
          "begin": 0.0,
          "end": 8.358
        },
        {
          "text": "ऋ॒तुना᳚ य॒ज्ञमा᳚शाथे ॥",
          "begin": 8.358,
          "end": 13.28
        }
      ]
    },
    {
      "id": "1.15.7",
      "mandala": 1,
      "sukta": 15,
      "rik": 7,
      "padas": [
        {
          "text": "द्र॒वि॒णो॒दा द्रवि᳚णसो॒ ग्राव᳚हस्तासो अध्व॒रे ।",
          "begin": 0.0,
          "end": 8.148
        },
        {
          "text": "य॒ज्ञेषु᳚ दे॒वमी᳚ळते ॥",
          "begin": 8.148,
          "end": 12.56
        }
      ]
    },
    {
      "id": "1.15.8",
      "mandala": 1,
      "sukta": 15,
      "rik": 8,
      "padas": [
        {
          "text": "द्र॒वि॒णो॒दा द᳚दातु नो॒ वसू᳚नि॒ यानि᳚ शृण्वि॒रे ।",
          "begin": 0.0,
          "end": 7.874
        },
        {
          "text": "दे॒वेषु॒ ता व᳚नामहे ॥",
          "begin": 7.874,
          "end": 10.96
        }
      ]
    },
    {
      "id": "1.15.9",
      "mandala": 1,
      "sukta": 15,
      "rik": 9,
      "padas": [
        {
          "text": "द्र॒वि॒णो॒दाः पि᳚पीषति जु॒होत॒ प्र च᳚ तिष्ठत ।",
          "begin": 0.0,
          "end": 5.896
        },
        {
          "text": "ने॒ष्ट्रादृ॒तुभि᳚रिष्यत ॥",
          "begin": 5.896,
          "end": 10.12
        }
      ]
    },
    {
      "id": "1.15.10",
      "mandala": 1,
      "sukta": 15,
      "rik": 10,
      "padas": [
        {
          "text": "यत्त्वा᳚ तु॒रीय᳚मृ॒तुभि॒र्द्रवि᳚णोदो॒ यजा᳚महे ।",
          "begin": 0.0,
          "end": 7.908
        },
        {
          "text": "अध᳚ स्मा नो द॒दिर्भ᳚व ॥",
          "begin": 7.908,
          "end": 11.12
        }
      ]
    },
    {
      "id": "1.15.11",
      "mandala": 1,
      "sukta": 15,
      "rik": 11,
      "padas": [
        {
          "text": "अश्वि᳚ना॒ पिब᳚तं॒ मधु॒ दीद्य᳚ग्नी शुचिव्रता ।",
          "begin": 0.0,
          "end": 7.044
        },
        {
          "text": "ऋ॒तुना᳚ यज्ञवाहसा ॥",
          "begin": 7.044,
          "end": 11.0
        }
      ]
    },
    {
      "id": "1.15.12",
      "mandala": 1,
      "sukta": 15,
      "rik": 12,
      "padas": [
        {
          "text": "गार्ह᳚पत्येन सन्त्य ऋ॒तुना᳚ यज्ञ॒नीर᳚सि ।",
          "begin": 0.0,
          "end": 7.435
        },
        {
          "text": "दे॒वान्दे᳚वय॒ते य᳚ज ॥",
          "begin": 7.435,
          "end": 13.32
        }
      ]
    }
  ]
}