{
  "mandala": 1,
  "sukta": 22,
  "verses": [
    {
      "id": "1.22.1",
      "mandala": 1,
      "sukta": 22,
      "rik": 1,
      "padas": [
        {
          "text": "प्रा॒त॒र्युजा॒ वि बो᳚धया॒श्विना॒वेह ग᳚च्छताम् ।",
          "begin": 0.0,
          "end": 8.621
        },
        {
          "text": "अ॒स्य सोम᳚स्य पी॒तये᳚ ॥",
          "begin": 8.621,
          "end": 12.64
        }
      ]
    },
    {
      "id": "1.22.2",
      "mandala": 1,
      "sukta": 22,
      "rik": 2,
      "padas": [
        {
          "text": "या सु॒रथा᳚ र॒थीत᳚मो॒भा दे॒वा दि᳚वि॒स्पृशा᳚ ।",
          "begin": 0.0,
          "end": 7.967
        },
        {
          "text": "अ॒श्विना॒ ता ह᳚वामहे ॥",
          "begin": 7.967,
          "end": 11.4
        }
      ]
    },
    {
      "id": "1.22.3",
      "mandala": 1,
      "sukta": 22,
      "rik": 3,
      "padas": [
        {
          "text": "या वां॒ कशा॒ मधु᳚म॒त्यश्वि᳚ना सू॒नृता᳚वती ।",
          "begin": 0.0,
          "end": 7.511
        },
        {
          "text": "तया᳚ य॒ज्ञं मि᳚मिक्षतम् ॥",
          "begin": 7.511,
          "end": 11.8
        }
      ]
    },
    {
      "id": "1.22.4",
      "mandala": 1,
      "sukta": 22,
      "rik": 4,
      "padas": [
        {
          "text": "न॒हि वा॒मस्ति᳚ दूर॒के यत्रा॒ रथे᳚न॒ गच्छ᳚थः ।",
          "begin": 0.0,
          "end": 7.618
        },
        {
          "text": "अश्वि᳚ना सो॒मिनो᳚ गृ॒हम् ॥",
          "begin": 7.618,
          "end": 11.8
        }
      ]
    },
    {
      "id": "1.22.5",
      "mandala": 1,
      "sukta": 22,
      "rik": 5,
      "padas": [
        {
          "text": "हिर᳚ण्यपाणिमू॒तये᳚ सवि॒तार॒मुप᳚ ह्वये ।",
          "begin": 0.0,
          "end": 7.847
        },
        {
          "text": "स चेत्ता᳚ दे॒वता᳚ प॒दम् ॥",
          "begin": 7.847,
          "end": 13.12
        }
      ]
    },
    {
      "id": "1.22.6",
      "mandala": 1,
      "sukta": 22,
      "rik": 6,
      "padas": [
        {
          "text": "अ॒पां नपा᳚त॒मव᳚से सवि॒तार॒मुप᳚ स्तुहि ।",
          "begin": 0.0,
          "end": 7.756
        },
        {
          "text": "तस्य᳚ व्र॒तान्यु᳚श्मसि ॥",
          "begin": 7.756,
          "end": 11.44
        }
      ]
    },
    {
      "id": "1.22.7",
      "mandala": 1,
      "sukta": 22,
      "rik": 7,
      "padas": [
        {
          "text": "वि॒भ॒क्तारं᳚ हवामहे॒ वसो᳚श्चि॒त्रस्य॒ राध᳚सः ।",
          "begin": 0.0,
          "end": 8.197
        },
        {
          "text": "स॒वि॒तारं᳚ नृ॒चक्ष᳚सम् ॥",
          "begin": 8.197,
          "end": 12.08
        }
      ]
    },
    {
      "id": "1.22.8",
      "mandala": 1,
      "sukta": 22,
      "rik": 8,
      "padas": [
        {
          "text": "सखा᳚य॒ आ नि षी᳚दत सवि॒ता स्तोम्यो॒ नु नः᳚ ।",
          "begin": 0.0,
          "end": 8.707
        },
        {
          "text": "दाता॒ राधां᳚सि शुम्भति ॥",
          "begin": 8.707,
          "end": 13.16
        }
      ]
    },
    {
      "id": "1.22.9",
      "mandala": 1,
      "sukta": 22,
      "rik": 9,
      "padas": [
        {
          "text": "अग्ने॒ पत्नी᳚रि॒हा व᳚ह दे॒वाना᳚मुश॒तीरुप᳚ ।",
          "begin": 0.0,
          "end": 8.182
        },
        {
          "text": "त्वष्टा᳚रं॒ सोम᳚पीतये ॥",
          "begin": 8.182,
          "end": 12.6
        }
      ]
    },
    {
      "id": "1.22.10",
      "mandala": 1,
      "sukta": 22,
      "rik": 10,
      "padas": [
        {
          "text": "आ ग्ना अ᳚ग्न इ॒हाव᳚से॒ होत्रां᳚ यविष्ठ॒ भार᳚तीम् ।",
          "begin": 0.0,
          "end": 11.284
        },
        {
          "text": "वरू᳚त्रीं धि॒षणां᳚ वह ॥",
          "begin": 11.284,
          "end": 14.72
        }
      ]
    },
    {
      "id": "1.22.11",
      "mandala": 1,
      "sukta": 22,
      "rik": 11,
      "padas": [
        {
          "text": "अ॒भि नो᳚ दे॒वीरव᳚सा म॒हः शर्म᳚णा नृ॒पत्नी᳚ः ।",
          "begin": 0.0,
          "end": 8.34
        },
        {
          "text": "अच्छि᳚न्नपत्राः सचन्ताम् ॥",
          "begin": 8.34,
          "end": 13.44
        }
      ]
    },
    {
      "id": "1.22.12",
      "mandala": 1,
      "sukta": 22,
      "rik": 12,
      "padas": [
        {
          "text": "इ॒हेन्द्रा॒णीमुप᳚ ह्वये वरुणा॒नीं स्व॒स्तये᳚ ।",
          "begin": 0.0,
          "end": 8.465
        },
        {
          "text": "अ॒ग्नायीं॒ सोम᳚पीतये ॥",
          "begin": 8.465,
          "end": 12.6
        }
      ]
    },
    {
      "id": "1.22.13",
      "mandala": 1,
      "sukta": 22,
      "rik": 13,
      "padas": [
        {
          "text": "म॒ही द्यौः पृ᳚थि॒वी च᳚ न इ॒मं य॒ज्ञं मि᳚मिक्षताम् ।",
          "begin": 0.0,
          "end": 5.266
        },
        {
          "text": "पि॒पृ॒तां नो॒ भरी᳚मभिः ॥",
          "begin": 5.266,
          "end": 12.28
        }
      ]
    },
    {
      "id": "1.22.14",
      "mandala": 1,
      "sukta": 22,
      "rik": 14,
      "padas": [
        {
          "text": "तयो॒रिद्घृ॒तव॒त्पयो॒ विप्रा᳚ रिहन्ति धी॒तिभि᳚ः ।",
          "begin": 0.0,
          "end": 7.738
        },
        {
          "text": "ग॒न्ध॒र्वस्य᳚ ध्रु॒वे प॒दे ॥",
          "begin": 7.738,
          "end": 11.96
        }
      ]
    },
    {
      "id": "1.22.15",
      "mandala": 1,
      "sukta": 22,
      "rik": 15,
      "padas": [
        {
          "text": "स्यो॒ना पृ᳚थिवि भवानृक्ष॒रा नि॒वेश᳚नी ।",
          "begin": 0.0,
          "end": 6.409
        },
        {
          "text": "यच्छा᳚ न॒ः शर्म᳚ स॒प्रथः᳚ ॥",
          "begin": 6.409,
          "end": 11.04
        }
      ]
    },
    {
      "id": "1.22.16",
      "mandala": 1,
      "sukta": 22,
      "rik": 16,
      "padas": [
        {
          "text": "अतो᳚ दे॒वा अ᳚वन्तु नो॒ यतो॒ विष्णु᳚र्विचक्र॒मे ।",
          "begin": 0.0,
          "end": 8.804
        },
        {
          "text": "पृ॒थि॒व्याः स॒प्त धाम᳚भिः ॥",
          "begin": 8.804,
          "end": 13.24
        }
      ]
    },
    {
      "id": "1.22.17",
      "mandala": 1,
      "sukta": 22,
      "rik": 17,
      "padas": [
        {
          "text": "इ॒दं विष्णु॒र्वि च᳚क्रमे त्रे॒धा नि द᳚धे प॒दम् ।",
          "begin": 0.0,
          "end": 7.51
        },
        {
          "text": "समू᳚ळ्हमस्य पांसु॒रे ॥",
          "begin": 7.51,
          "end": 12.12
        }
      ]
    },
    {
      "id": "1.22.18",
      "mandala": 1,
      "sukta": 22,
      "rik": 18,
      "padas": [
        {
          "text": "त्रीणि᳚ प॒दा वि च᳚क्रमे॒ विष्णु᳚र्गो॒पा अदा᳚भ्यः ।",
          "begin": 0.0,
          "end": 10.598
        },
        {
          "text": "अतो॒ धर्मा᳚णि धा॒रय᳚न् ॥",
          "begin": 10.598,
          "end": 12.96
        }
      ]
    },
    {
      "id": "1.22.19",
      "mandala": 1,
      "sukta": 22,
      "rik": 19,
      "padas": [
        {
          "text": "विष्णो॒ः कर्मा᳚णि पश्यत॒ यतो᳚ व्र॒तानि᳚ पस्प॒शे ।",
          "begin": 0.0,
          "end": 8.928
        },
        {
          "text": "इन्द्र᳚स्य॒ युज्य॒ः सखा᳚ ॥",
          "begin": 8.928,
          "end": 13.12
        }
      ]
    },
    {
      "id": "1.22.20",
      "mandala": 1,
      "sukta": 22,
      "rik": 20,
      "padas": [
        {
          "text": "तद्विष्णो᳚ः पर॒मं प॒दं सदा᳚ पश्यन्ति सू॒रयः᳚ ।",
          "begin": 0.0,
          "end": 9.058
        },
        {
          "text": "दि॒वी᳚व॒ चक्षु॒रात᳚तम् ॥",
          "begin": 9.058,
          "end": 13.16
        }
      ]
    },
    {
      "id": "1.22.21",
      "mandala": 1,
      "sukta": 22,
      "rik": 21,
      "padas": [
        {
          "text": "तद्विप्रा᳚सो विप॒न्यवो᳚ जागृ॒वांस॒ः समि᳚न्धते ।",
          "begin": 0.0,
          "end": 10.204
        },
        {
          "text": "विष्णो॒र्यत्प᳚र॒मं प॒दम् ॥",
          "begin": 10.204,
          "end": 15.48
        }
      ]
    }
  ]
}