  "verses": [
    {
      "id": "1.19.1",
      "mandala": 1,
      "sukta": 19,
      "rik": 1,
      "padas": [
        {
          "text": "प्रति॒ त्यं चारु᳚मध्व॒रं गो᳚पी॒थाय॒ प्र हू᳚यसे ।",
          "begin": 0.0,
          "end": 9.243
        },
        {
          "text": "म॒रुद्भि᳚रग्न॒ आ ग᳚हि ॥",
          "begin": 9.243,
          "end": 12.64
        }
      ]
    },
    {
      "id": "1.19.2",
      "mandala": 1,
      "sukta": 19,
      "rik": 2,
      "padas": [
        {
          "text": "न॒हि दे॒वो न मर्त्यो᳚ म॒हस्तव॒ क्रतुं᳚ प॒रः ।",
          "begin": 0.0,
          "end": 7.844
        },
        {
          "text": "म॒रुद्भि᳚रग्न॒ आ ग᳚हि ॥",
          "begin": 7.844,
          "end": 11.32
        }
      ]
    },
    {
      "id": "1.19.3",
      "mandala": 1,
      "sukta": 19,
      "rik": 3,
      "padas": [
        {
          "text": "ये म॒हो रज᳚सो वि॒दुर्विश्वे᳚ दे॒वासो᳚ अ॒द्रुहः᳚ ।",
          "begin": 0.0,
          "end": 4.738
        },
        {
          "text": "म॒रुद्भि᳚रग्न॒ आ ग᳚हि ॥",
          "begin": 4.738,
          "end": 12.28
        }
      ]
    },
    {
      "id": "1.19.4",
      "mandala": 1,
      "sukta": 19,
      "rik": 4,
      "padas": [
        {
          "text": "य उ॒ग्रा अ॒र्कमा᳚नृ॒चुरना᳚धृष्टास॒ ओज᳚सा ।",
          "begin": 0.0,
          "end": 8.338
        },
        {
          "text": "म॒रुद्भि᳚रग्न॒ आ ग᳚हि ॥",
          "begin": 8.338,
          "end": 11.76
        }
      ]
    },
    {
      "id": "1.19.5",
      "mandala": 1,
      "sukta": 19,
      "rik": 5,
      "padas": [
        {
          "text": "ये शु॒भ्रा घो॒रव᳚र्पसः सुक्ष॒त्रासो᳚ रि॒शाद᳚सः ।",
          "begin": 0.0,
          "end": 7.878
        },
        {
          "text": "म॒रुद्भि᳚रग्न॒ आ ग᳚हि ॥",
          "begin": 7.878,
          "end": 11.32
        }
      ]
    },
    {
      "id": "1.19.6",
      "mandala": 1,
      "sukta": 19,
      "rik": 6,
      "padas": [
        {
          "text": "ये नाक॒स्याधि᳚ रोच॒ने दि॒वि दे॒वास॒ आस᳚ते ।",
          "begin": 0.0,
          "end": 6.902
        },
        {
          "text": "म॒रुद्भि᳚रग्न॒ आ ग᳚हि ॥",
          "begin": 6.902,
          "end": 10.44
        }
      ]
    },
    {
      "id": "1.19.7",
      "mandala": 1,
      "sukta": 19,
      "rik": 7,
      "padas": [
        {
          "text": "य ई॒ङ्खय᳚न्ति॒ पर्व᳚तान्ति॒रः स᳚मु॒द्रम᳚र्ण॒वम् ।",
          "begin": 0.0,
          "end": 8.148
        },
        {
          "text": "म॒रुद्भि᳚रग्न॒ आ ग᳚हि ॥",
          "begin": 8.148,
          "end": 11.72
        }
      ]
    },
    {
      "id": "1.19.8",
      "mandala": 1,
      "sukta": 19,
      "rik": 8,
      "padas": [
        {
          "text": "आ ये त॒न्वन्ति᳚ र॒श्मिभि᳚स्ति॒रः स᳚मु॒द्रमोज᳚सा ।",
          "begin": 0.0,
          "end": 5.11
        },
        {
          "text": "म॒रुद्भि᳚रग्न॒ आ ग᳚हि ॥",
          "begin": 5.11,
          "end": 10.4
        }
      ]
    },
    {
      "id": "1.19.9",
      "mandala": 1,
      "sukta": 19,
      "rik": 9,
      "padas": [
        {
          "text": "अ॒भि त्वा᳚ पू॒र्वपी᳚तये सृ॒जामि᳚ सो॒म्यं मधु᳚ ।",
          "begin": 0.0,
          "end": 6.873
        },
        {
          "text": "म॒रुद्भि᳚रग्न॒ आ ग᳚हि ॥",
          "begin": 6.873,
          "end": 13.48
        }
      ]
    }
  ]
}