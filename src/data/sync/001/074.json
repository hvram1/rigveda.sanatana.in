{
  "mandala": 1,
  "sukta": 74,
  "verses": [
    {
      "id": "1.74.1",
      "mandala": 1,
      "sukta": 74,
      "rik": 1,
      "padas": [
        {
          "text": "उ॒प॒प्र॒यन्तो᳚ अध्व॒रं मन्त्रं᳚ वोचेमा॒ग्नये᳚ ।",
          "begin": 0.0,
          "end": 9.397
        },
        {
          "text": "आ॒रे अ॒स्मे च᳚ शृण्व॒ते ॥",
          "begin": 9.397,
          "end": 13.76
        }
      ]
    },
    {
      "id": "1.74.2",
      "mandala": 1,
      "sukta": 74,
      "rik": 2,
      "padas": [
        {
          "text": "यः स्नीहि᳚तीषु पू॒र्व्यः सं᳚जग्मा॒नासु᳚ कृ॒ष्टिषु᳚ ।",
          "begin": 0.0,
          "end": 6.987
        },
        {
          "text": "अर᳚क्षद्दा॒शुषे॒ गय᳚म् ॥",
          "begin": 6.987,
          "end": 11.0
        }
      ]
    },
    {
      "id": "1.74.3",
      "mandala": 1,
      "sukta": 74,
      "rik": 3,
      "padas": [
        {
          "text": "उ॒त ब्रु᳚वन्तु ज॒न्तव॒ उद॒ग्निर्वृ᳚त्र॒हाज᳚नि ।",
          "begin": 0.0,
          "end": 6.289
        },
        {
          "text": "ध॒नं॒ज॒यो रणे᳚रणे ॥",
          "begin": 6.289,
          "end": 9.84
        }
      ]
    },
    {
      "id": "1.74.4",
      "mandala": 1,
      "sukta": 74,
      "rik": 4,
      "padas": [
        {
          "text": "यस्य᳚ दू॒तो असि॒ क्षये॒ वेषि᳚ ह॒व्यानि᳚ वी॒तये᳚ ।",
          "begin": 0.0,
          "end": 6.229
        },
        {
          "text": "द॒स्मत्कृ॒णोष्य᳚ध्व॒रम् ॥",
          "begin": 6.229,
          "end": 10.96
        }
      ]
    },
    {
      "id": "1.74.5",
      "mandala": 1,
      "sukta": 74,
      "rik": 5,
      "padas": [
        {
          "text": "तमित्सु᳚ह॒व्यम᳚ङ्गिरः सुदे॒वं स᳚हसो यहो ।",
          "begin": 0.0,
          "end": 7.14
        },
        {
          "text": "जना᳚ आहुः सुब॒र्हिष᳚म् ॥",
          "begin": 7.14,
          "end": 11.44
        }
      ]
    },
    {
      "id": "1.74.6",
      "mandala": 1,
      "sukta": 74,
      "rik": 6,
      "padas": [
        {
          "text": "आ च॒ वहा᳚सि॒ ताँ इ॒ह दे॒वाँ उप॒ प्रश᳚स्तये ।",
          "begin": 0.0,
          "end": 7.575
        },
        {
          "text": "ह॒व्या सु᳚श्चन्द्र वी॒तये᳚ ॥",
          "begin": 7.575,
          "end": 11.72
        }
      ]
    },
    {
      "id": "1.74.7",
      "mandala": 1,
      "sukta": 74,
      "rik": 7,
      "padas": [
        {
          "text": "न योरु᳚प॒ब्दिरश्व्य᳚ः शृ॒ण्वे रथ᳚स्य॒ कच्च॒न ।",
          "begin": 0.0,
          "end": 6.156
        },
        {
          "text": "यद᳚ग्ने॒ यासि᳚ दू॒त्य᳚म् ॥",
          "begin": 6.156,
          "end": 10.2
        }
      ]
    },
    {
      "id": "1.74.8",
      "mandala": 1,
      "sukta": 74,
      "rik": 8,
      "padas": [
        {
          "text": "त्वोतो᳚ वा॒ज्यह्र᳚यो॒॑भि पूर्व᳚स्मा॒दप᳚रः ।",
          "begin": 0.0,
          "end": 7.381
        },
        {
          "text": "प्र दा॒श्वाँ अ᳚ग्ने अस्थात् ॥",
          "begin": 7.381,
          "end": 12.28
        }
      ]
    },
    {
      "id": "1.74.9",
      "mandala": 1,
      "sukta": 74,
      "rik": 9,
      "padas": [
        {
          "text": "उ॒त द्यु॒मत्सु॒वीर्यं᳚ बृ॒हद᳚ग्ने विवाससि ।",
          "begin": 0.0,
          "end": 9.96
        },
        {
          "text": "दे॒वेभ्यो᳚ देव दा॒शुषे᳚ ॥",
          "begin": 9.96,
          "end": 13.72
        }
      ]
    }
  ]
}