{
  "mandala": 1,
  "sukta": 30,
  "verses": [
    {
      "id": "1.30.1",
      "mandala": 1,
      "sukta": 30,
      "rik": 1,
      "padas": [
        {
          "text": "आ व॒ इन्द्रं॒ क्रिविं᳚ यथा वाज॒यन्त᳚ः श॒तक्र᳚तुम् ।",
          "begin": 0.0,
          "end": 8.816
        },
        {
          "text": "मंहि᳚ष्ठं सिञ्च॒ इन्दु᳚भिः ॥",
          "begin": 8.816,
          "end": 12.32
        }
      ]
    },
    {
      "id": "1.30.2",
      "mandala": 1,
      "sukta": 30,
      "rik": 2,
      "padas": [
        {
          "text": "श॒तं वा॒ यः शुची᳚नां स॒हस्रं᳚ वा॒ समा᳚शिराम् ।",
          "begin": 0.0,
          "end": 8.79
        },
        {
          "text": "एदु᳚ नि॒म्नं न री᳚यते ॥",
          "begin": 8.79,
          "end": 12.76
        }
      ]
    },
    {
      "id": "1.30.3",
      "mandala": 1,
      "sukta": 30,
      "rik": 3,
      "padas": [
        {
          "text": "सं यन्मदा᳚य शु॒ष्मिण᳚ ए॒ना ह्य᳚स्यो॒दरे᳚ ।",
          "begin": 0.0,
          "end": 7.632
        },
        {
          "text": "स॒मु॒द्रो न व्यचो᳚ द॒धे ॥",
          "begin": 7.632,
          "end": 11.68
        }
      ]
    },
    {
      "id": "1.30.4",
      "mandala": 1,
      "sukta": 30,
      "rik": 4,
      "padas": [
        {
          "text": "अ॒यमु᳚ ते॒ सम᳚तसि क॒पोत᳚ इव गर्भ॒धिम् ।",
          "begin": 0.0,
          "end": 7.435
        },
        {
          "text": "वच॒स्तच्चि᳚न्न ओहसे ॥",
          "begin": 7.435,
          "end": 9.56
        }
      ]
    },
    {
      "id": "1.30.5",
      "mandala": 1,
      "sukta": 30,
      "rik": 5,
      "padas": [
        {
          "text": "स्तो॒त्रं रा᳚धानां पते॒ गिर्वा᳚हो वीर॒ यस्य᳚ ते ।",
          "begin": 0.0,
          "end": 9.152
        },
        {
          "text": "विभू᳚तिरस्तु सू॒नृता᳚ ॥",
          "begin": 9.152,
          "end": 13.36
        }
      ]
    },
    {
      "id": "1.30.6",
      "mandala": 1,
      "sukta": 30,
      "rik": 6,
      "padas": [
        {
          "text": "ऊ॒र्ध्वस्ति᳚ष्ठा न ऊ॒तये॒॑स्मिन्वाजे᳚ शतक्रतो ।",
          "begin": 0.0,
          "end": 7.784
        },
        {
          "text": "सम॒न्येषु᳚ ब्रवावहै ॥",
          "begin": 7.784,
          "end": 11.04
        }
      ]
    },
    {
      "id": "1.30.7",
      "mandala": 1,
      "sukta": 30,
      "rik": 7,
      "padas": [
        {
          "text": "योगे᳚योगे त॒वस्त᳚रं॒ वाजे᳚वाजे हवामहे ।",
          "begin": 0.0,
          "end": 8.567
        },
        {
          "text": "सखा᳚य॒ इन्द्र᳚मू॒तये᳚ ॥",
          "begin": 8.567,
          "end": 12.84
        }
      ]
    },
    {
      "id": "1.30.8",
      "mandala": 1,
      "sukta": 30,
      "rik": 8,
      "padas": [
        {
          "text": "आ घा᳚ गम॒द्यदि॒ श्रव᳚त्सह॒स्रिणी᳚भिरू॒तिभि᳚ः ।",
          "begin": 0.0,
          "end": 6.536
        },
        {
          "text": "वाजे᳚भि॒रुप᳚ नो॒ हव᳚म् ॥",
          "begin": 6.536,
          "end": 12.2
        }
      ]
    },
    {
      "id": "1.30.9",
      "mandala": 1,
      "sukta": 30,
      "rik": 9,
      "padas": [
        {
          "text": "अनु᳚ प्र॒त्नस्यौक᳚सो हु॒वे तु᳚विप्र॒तिं नर᳚म् ।",
          "begin": 0.0,
          "end": 6.704
        },
        {
          "text": "यं ते॒ पूर्वं᳚ पि॒ता हु॒वे ॥",
          "begin": 6.704,
          "end": 10.96
        }
      ]
    },
    {
      "id": "1.30.10",
      "mandala": 1,
      "sukta": 30,
      "rik": 10,
      "padas": [
        {
          "text": "तं त्वा᳚ व॒यं वि᳚श्ववा॒रा शा᳚स्महे पुरुहूत ।",
          "begin": 0.0,
          "end": 7.774
        },
        {
          "text": "सखे᳚ वसो जरि॒तृभ्य᳚ः ॥",
          "begin": 7.774,
          "end": 11.88
        }
      ]
    },
    {
      "id": "1.30.11",
      "mandala": 1,
      "sukta": 30,
      "rik": 11,
      "padas": [
        {
          "text": "अ॒स्माकं᳚ शि॒प्रिणी᳚नां॒ सोम᳚पाः सोम॒पाव्ना᳚म् ।",
          "begin": 0.0,
          "end": 9.277
        },
        {
          "text": "सखे᳚ वज्रि॒न्त्सखी᳚नाम् ॥",
          "begin": 9.277,
          "end": 14.2
        }
      ]
    },
    {
      "id": "1.30.12",
      "mandala": 1,
      "sukta": 30,
      "rik": 12,
      "padas": [
        {
          "text": "तथा॒ तद᳚स्तु सोमपा॒ः सखे᳚ वज्रि॒न्तथा᳚ कृणु ।",
          "begin": 0.0,
          "end": 8.146
        },
        {
          "text": "यथा᳚ त उ॒श्मसी॒ष्टये᳚ ॥",
          "begin": 8.146,
          "end": 12.48
        }
      ]
    },
    {
      "id": "1.30.13",
      "mandala": 1,
      "sukta": 30,
      "rik": 13,
      "padas": [
        {
          "text": "रे॒वती᳚र्नः सध॒माद॒ इन्द्रे᳚ सन्तु तु॒विवा᳚जाः ।",
          "begin": 0.0,
          "end": 8.68
        },
        {
          "text": "क्षु॒मन्तो॒ याभि॒र्मदे᳚म ॥",
          "begin": 8.68,
          "end": 12.8
        }
      ]
    },
    {
      "id": "1.30.14",
      "mandala": 1,
      "sukta": 30,
      "rik": 14,
      "padas": [
        {
          "text": "आ घ॒ त्वावा॒न्त्मना॒प्तः स्तो॒तृभ्यो᳚ धृष्णविया॒नः ।",
          "begin": 0.0,
          "end": 5.371
        },
        {
          "text": "ऋ॒णोरक्षं॒ न च॒क्र्यो᳚ः ॥",
          "begin": 5.371,
          "end": 12.32
        }
      ]
    },
    {
      "id": "1.30.15",
      "mandala": 1,
      "sukta": 30,
      "rik": 15,
      "padas": [
        {
          "text": "आ यद्दुवः᳚ शतक्रत॒वा कामं᳚ जरितॄ॒णाम् ।",
          "begin": 0.0,
          "end": 7.001
        },
        {
          "text": "ऋ॒णोरक्षं॒ न शची᳚भिः ॥",
          "begin": 7.001,
          "end": 10.92
        }
      ]
    },
    {
      "id": "1.30.16",
      "mandala": 1,
      "sukta": 30,
      "rik": 16,
      "padas": [
        {
          "text": "शश्व॒दिन्द्र॒ः पोप्रु᳚थद्भिर्जिगाय॒ नान᳚दद्भि॒ः शाश्व᳚सद्भि॒र्धना᳚नि ।",
          "begin": 0.0,
          "end": 9.659
        },
        {
          "text": "स नो᳚ हिरण्यर॒थं दं॒सना᳚वा॒न्त्स नः᳚ सनि॒ता स॒नये॒ स नो᳚॑दात् ॥",
          "begin": 9.659,
          "end": 19.68
        }
      ]
    },
    {
      "id": "1.30.17",
      "mandala": 1,
      "sukta": 30,
      "rik": 17,
      "padas": [
        {
          "text": "आश्वि᳚ना॒वश्वा᳚वत्ये॒षा या᳚तं॒ शवी᳚रया ।",
          "begin": 0.0,
          "end": 8.691
        },
        {
          "text": "गोम᳚द्दस्रा॒ हिर᳚ण्यवत् ॥",
          "begin": 8.691,
          "end": 13.0
        }
      ]
    },
    {
      "id": "1.30.18",
      "mandala": 1,
      "sukta": 30,
      "rik": 18,
      "padas": [
        {
          "text": "स॒मा॒नयो᳚जनो॒ हि वां॒ रथो᳚ दस्रा॒वम᳚र्त्यः ।",
          "begin": 0.0,
          "end": 7.632
        },
        {
          "text": "स॒मु॒द्रे अ᳚श्वि॒नेय᳚ते ॥",
          "begin": 7.632,
          "end": 11.28
        }
      ]
    },
    {
      "id": "1.30.19",
      "mandala": 1,
      "sukta": 30,
      "rik": 19,
      "padas": [
        {
          "text": "न्य१॒᳚घ्न्यस्य᳚ मू॒र्धनि᳚ च॒क्रं रथ᳚स्य येमथुः ।",
          "begin": 0.0,
          "end": 6.439
        },
        {
          "text": "परि॒ द्याम॒न्यदी᳚यते ॥",
          "begin": 6.439,
          "end": 11.12
        }
      ]
    },
    {
      "id": "1.30.20",
      "mandala": 1,
      "sukta": 30,
      "rik": 20,
      "padas": [
        {
          "text": "कस्त᳚ उषः कधप्रिये भु॒जे मर्तो᳚ अमर्त्ये ।",
          "begin": 0.0,
          "end": 6.707
        },
        {
          "text": "कं न᳚क्षसे विभावरि ॥",
          "begin": 6.707,
          "end": 9.8
        }
      ]
    },
    {
      "id": "1.30.21",
      "mandala": 1,
      "sukta": 30,
      "rik": 21,
      "padas": [
        {
          "text": "व॒यं हि ते॒ अम᳚न्म॒ह्यान्ता॒दा प᳚रा॒कात् ।",
          "begin": 0.0,
          "end": 5.27
        },
        {
          "text": "अश्वे॒ न चि᳚त्रे अरुषि ॥",
          "begin": 5.27,
          "end": 11.24
        }
      ]
    },
    {
      "id": "1.30.22",
      "mandala": 1,
      "sukta": 30,
      "rik": 22,
      "padas": [
        {
          "text": "त्वं त्येभि॒रा ग᳚हि॒ वाजे᳚भिर्दुहितर्दिवः ।",
          "begin": 0.0,
          "end": 7.021
        },
        {
          "text": "अ॒स्मे र॒यिं नि धा᳚रय ॥",
          "begin": 7.021,
          "end": 11.96
        }
      ]
    }
  ]
}