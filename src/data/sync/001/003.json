{
  "mandala": 1,
  "sukta": 3,
  "verses": [
    {
      "id": "1.3.1",
      "mandala": 1,
      "sukta": 3,
      "rik": 1,
      "padas": [
        {
          "text": "अश्वि᳚ना॒ यज्व᳚री॒रिषो॒ द्रव᳚त्पाणी॒ शुभ᳚स्पती ।",
          "begin": 0.0,
          "end": 8.93
        },
        {
          "text": "पुरु᳚भुजा चन॒स्यत᳚म् ॥",
          "begin": 8.93,
          "end": 13.04
        }
      ]
    },
    {
      "id": "1.3.2",
      "mandala": 1,
      "sukta": 3,
      "rik": 2,
      "padas": [
        {
          "text": "अश्वि᳚ना॒ पुरु᳚दंससा॒ नरा॒ शवी᳚रया धि॒या ।",
          "begin": 0.0,
          "end": 7.96
        },
        {
          "text": "धिष्ण्या॒ वन᳚तं॒ गिरः᳚ ॥",
          "begin": 7.96,
          "end": 11.88
        }
      ]
    },
    {
      "id": "1.3.3",
      "mandala": 1,
      "sukta": 3,
      "rik": 3,
      "padas": [
        {
          "text": "दस्रा᳚ यु॒वाक᳚वः सु॒ता नास᳚त्या वृ॒क्तब᳚र्हिषः ।",
          "begin": 0.0,
          "end": 8.136
        },
        {
          "text": "आ या᳚तं रुद्रवर्तनी ॥",
          "begin": 8.136,
          "end": 13.04
        }
      ]
    },
    {
      "id": "1.3.4",
      "mandala": 1,
      "sukta": 3,
      "rik": 4,
      "padas": [
        {
          "text": "इन्द्रा या᳚हि चित्रभानो सु॒ता इ॒मे त्वा॒यवः᳚ ।",
          "begin": 0.0,
          "end": 9.837
        },
        {
          "text": "अण्वी᳚भि॒स्तना᳚ पू॒तासः᳚ ॥",
          "begin": 9.837,
          "end": 15.68
        }
      ]
    },
    {
      "id": "1.3.5",
      "mandala": 1,
      "sukta": 3,
      "rik": 5,
      "padas": [
        {
          "text": "इन्द्रा या᳚हि धि॒येषि॒तो विप्र᳚जूतः सु॒ताव᳚तः ।",
          "begin": 0.0,
          "end": 8.323
        },
        {
          "text": "उप॒ ब्रह्मा᳚णि वा॒घतः᳚ ॥",
          "begin": 8.323,
          "end": 13.08
        }
      ]
    },
    {
      "id": "1.3.6",
      "mandala": 1,
      "sukta": 3,
      "rik": 6,
      "padas": [
        {
          "text": "इन्द्रा या᳚हि॒ तूतु᳚जान॒ उप॒ ब्रह्मा᳚णि हरिवः ।",
          "begin": 0.0,
          "end": 8.72
        },
        {
          "text": "सु॒ते द᳚धिष्व न॒श्चनः᳚ ॥",
          "begin": 8.72,
          "end": 12.64
        }
      ]
    },
    {
      "id": "1.3.7",
      "mandala": 1,
      "sukta": 3,
      "rik": 7,
      "padas": [
        {
          "text": "ओमा᳚सश्चर्षणीधृतो॒ विश्वे᳚ देवास॒ आ ग᳚त ।",
          "begin": 0.0,
          "end": 9.15
        },
        {
          "text": "दा॒श्वांसो᳚ दा॒शुषः᳚ सु॒तम् ॥",
          "begin": 9.15,
          "end": 14.76
        }
      ]
    },
    {
      "id": "1.3.8",
      "mandala": 1,
      "sukta": 3,
      "rik": 8,
      "padas": [
        {
          "text": "विश्वे᳚ दे॒वासो᳚ अ॒प्तुरः᳚ सु॒तमा ग᳚न्त॒ तूर्ण᳚यः ।",
          "begin": 0.0,
          "end": 9.806
        },
        {
          "text": "उ॒स्रा इ᳚व॒ स्वस᳚राणि ॥",
          "begin": 9.806,
          "end": 13.88
        }
      ]
    },
    {
      "id": "1.3.9",
      "mandala": 1,
      "sukta": 3,
      "rik": 9,
      "padas": [
        {
          "text": "विश्वे᳚ दे॒वासो᳚ अ॒स्रिध॒ एहि᳚मायासो अ॒द्रुहः᳚ ।",
          "begin": 0.0,
          "end": 10.884
        },
        {
          "text": "मेधं᳚ जुषन्त॒ वह्न᳚यः ॥",
          "begin": 10.884,
          "end": 14.76
        }
      ]
    },
    {
      "id": "1.3.10",
      "mandala": 1,
      "sukta": 3,
      "rik": 10,
      "padas": [
        {
          "text": "पा॒व॒का न॒ः सर᳚स्वती॒ वाजे᳚भिर्वा॒जिनी᳚वती ।",
          "begin": 0.0,
          "end": 8.524
        },
        {
          "text": "य॒ज्ञं व᳚ष्टु धि॒याव᳚सुः ॥",
          "begin": 8.524,
          "end": 12.8
        }
      ]
    },
    {
      "id": "1.3.11",
      "mandala": 1,
      "sukta": 3,
      "rik": 11,
      "padas": [
        {
          "text": "चो॒द॒यि॒त्री सू॒नृता᳚नां॒ चेत᳚न्ती सुमती॒नाम् ।",
          "begin": 0.0,
          "end": 9.389
        },
        {
          "text": "य॒ज्ञं द᳚धे॒ सर᳚स्वती ॥",
          "begin": 9.389,
          "end": 13.16
        }
      ]
    },
    {
      "id": "1.3.12",
      "mandala": 1,
      "sukta": 3,
      "rik": 12,
      "padas": [
        {
          "text": "म॒हो अर्ण॒ः सर᳚स्वती॒ प्र चे᳚तयति के॒तुना᳚ ।",
          "begin": 0.0,
          "end": 9.175
        },
        {
          "text": "धियो॒ विश्वा॒ वि रा᳚जति ॥",
          "begin": 9.175,
          "end": 15.88
        }
      ]
    }
  ]
}