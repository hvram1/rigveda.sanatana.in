  "verses": [
    {
      "id": "7.80.1",
      "mandala": 7,
      "sukta": 80,
      "rik": 1,
      "padas": [
        {
          "text": "प्रति॒ स्तोमे᳚भिरु॒षसं॒ वसि᳚ष्ठा गी॒र्भिर्विप्रा᳚सः प्रथ॒मा अ᳚बुध्रन् ।",
          "begin": 0.0,
          "end": 10.599
        },
        {
          "text": "वि॒व॒र्तय᳚न्तीं॒ रज᳚सी॒ सम᳚न्ते आविष्कृण्व॒तीं भुव᳚नानि॒ विश्वा᳚ ॥",
          "begin": 10.599,
          "end": 18.52
        }
      ]
    },
    {
      "id": "7.80.2",
      "mandala": 7,
      "sukta": 80,
      "rik": 2,
      "padas": [
        {
          "text": "ए॒षा स्या नव्य॒मायु॒र्दधा᳚ना गू॒ढ्वी तमो॒ ज्योति᳚षो॒षा अ᳚बोधि ।",
          "begin": 0.0,
          "end": 7.949
        },
        {
          "text": "अग्र᳚ एति युव॒तिरह्र᳚याणा॒ प्राचि᳚कित॒त्सूर्यं᳚ य॒ज्ञम॒ग्निम् ॥",
          "begin": 7.949,
          "end": 15.08
        }
      ]
    },
    {
      "id": "7.80.3",
      "mandala": 7,
      "sukta": 80,
      "rik": 3,
      "padas": [
        {
          "text": "अश्वा᳚वती॒र्गोम᳚तीर्न उ॒षासो᳚ वी॒रव᳚ती॒ः सद᳚मुच्छन्तु भ॒द्राः ।",
          "begin": 0.0,
          "end": 8.632
        },
        {
          "text": "घृ॒तं दुहा᳚ना वि॒श्वत॒ः प्रपी᳚ता यू॒यं पा᳚त स्व॒स्तिभि॒ः सदा᳚ नः ॥",
          "begin": 8.632,
          "end": 17.96
        }
      ]
    }
  ]
}