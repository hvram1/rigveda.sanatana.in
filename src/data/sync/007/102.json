      "padas": [
        {
          "text": "प॒र्जन्या᳚य॒ प्र गा᳚यत दि॒वस्पु॒त्राय᳚ मी॒ळ्हुषे᳚ ।",
          "begin": 0.0,
          "end": 7.257
        },
        {
          "text": "स नो॒ यव᳚समिच्छतु ॥",
          "begin": 7.257,
          "end": 9.84
        }
      ]
    },
    {
      "id": "7.102.2",
      "mandala": 7,
      "sukta": 102,
      "rik": 2,
      "padas": [
        {
          "text": "यो गर्भ॒मोष᳚धीनां॒ गवां᳚ कृ॒णोत्यर्व᳚ताम् ।",
          "begin": 0.0,
          "end": 5.975
        },
        {
          "text": "प॒र्जन्य᳚ः पुरु॒षीणा᳚म् ॥",
          "begin": 5.975,
          "end": 8.88
        }
      ]
    },
    {
      "id": "7.102.3",
      "mandala": 7,
      "sukta": 102,
      "rik": 3,
      "padas": [
        {
          "text": "तस्मा॒ इदा॒स्ये᳚ ह॒विर्जु॒होता॒ मधु᳚मत्तमम् ।",
          "begin": 0.0,
          "end": 6.646
        },
        {
          "text": "इळां᳚ नः सं॒यतं᳚ करत् ॥",
          "begin": 6.646,
          "end": 10.44
        }
      ]
    }
  ]
