  "verses": [
    {
      "id": "7.52.1",
      "mandala": 7,
      "sukta": 52,
      "rik": 1,
      "padas": [
        {
          "text": "आ॒दि॒त्यासो॒ अदि᳚तयः स्याम॒ पूर्दे᳚व॒त्रा व᳚सवो मर्त्य॒त्रा ।",
          "begin": 0.0,
          "end": 11.026
        },
        {
          "text": "सने᳚म मित्रावरुणा॒ सन᳚न्तो॒ भवे᳚म द्यावापृथिवी॒ भव᳚न्तः ॥",
          "begin": 11.026,
          "end": 17.44
        }
      ]
    },
    {
      "id": "7.52.2",
      "mandala": 7,
      "sukta": 52,
      "rik": 2,
      "padas": [
        {
          "text": "मि॒त्रस्तन्नो॒ वरु᳚णो मामहन्त॒ शर्म᳚ तो॒काय॒ तन᳚याय गो॒पाः ।",
          "begin": 0.0,
          "end": 7.93
        },
        {
          "text": "मा वो᳚ भुजेमा॒न्यजा᳚त॒मेनो॒ मा तत्क᳚र्म वसवो॒ यच्चय᳚ध्वे ॥",
          "begin": 7.93,
          "end": 15.64
        }
      ]
    },
    {
      "id": "7.52.3",
      "mandala": 7,
      "sukta": 52,
      "rik": 3,
      "padas": [
        {
          "text": "तु॒र॒ण्यवो॑ङ्गि᳚रसो नक्षन्त॒ रत्नं᳚ दे॒वस्य᳚ सवि॒तुरि᳚या॒नाः ।",
          "begin": 0.0,
          "end": 8.243
        },
        {
          "text": "पि॒ता च॒ तन्नो᳚ म॒हान्यज᳚त्रो॒ विश्वे᳚ दे॒वाः सम᳚नसो जुषन्त ॥",
          "begin": 8.243,
          "end": 16.64
        }
      ]
    }
  ]
}