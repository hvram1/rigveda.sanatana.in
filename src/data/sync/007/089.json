  "verses": [
    {
      "id": "7.89.1",
      "mandala": 7,
      "sukta": 89,
      "rik": 1,
      "padas": [
        {
          "text": "मो षु व᳚रुण मृ॒न्मयं᳚ गृ॒हं रा᳚जन्न॒हं ग᳚मम् ।",
          "begin": 0.0,
          "end": 7.516
        },
        {
          "text": "मृ॒ळा सु᳚क्षत्र मृ॒ळय᳚ ॥",
          "begin": 7.516,
          "end": 10.64
        }
      ]
    },
    {
      "id": "7.89.2",
      "mandala": 7,
      "sukta": 89,
      "rik": 2,
      "padas": [
        {
          "text": "यदेमि᳚ प्रस्फु॒रन्नि᳚व॒ दृति॒र्न ध्मा॒तो अ᳚द्रिवः ।",
          "begin": 0.0,
          "end": 5.63
        },
        {
          "text": "मृ॒ळा सु᳚क्षत्र मृ॒ळय᳚ ॥",
          "begin": 5.63,
          "end": 7.92
        }
      ]
    },
    {
      "id": "7.89.3",
      "mandala": 7,
      "sukta": 89,
      "rik": 3,
      "padas": [
        {
          "text": "क्रत्व᳚ः समह दी॒नता᳚ प्रती॒पं ज᳚गमा शुचे ।",
          "begin": 0.0,
          "end": 5.365
        },
        {
          "text": "मृ॒ळा सु᳚क्षत्र मृ॒ळय᳚ ॥",
          "begin": 5.365,
          "end": 7.64
        }
      ]
    },
    {
      "id": "7.89.4",
      "mandala": 7,
      "sukta": 89,
      "rik": 4,
      "padas": [
        {
          "text": "अ॒पां मध्ये᳚ तस्थि॒वांसं॒ तृष्णा᳚विदज्जरि॒तार᳚म् ।",
          "begin": 0.0,
          "end": 6.645
        },
        {
          "text": "मृ॒ळा सु᳚क्षत्र मृ॒ळय᳚ ॥",
          "begin": 6.645,
          "end": 9.32
        }
      ]
    },
    {
      "id": "7.89.5",
      "mandala": 7,
      "sukta": 89,
      "rik": 5,
      "padas": [
        {
          "text": "यत्किं चे॒दं व᳚रुण॒ दैव्ये॒ जने᳚॑भिद्रो॒हं म᳚नु॒ष्या॒३॒᳚श्चरा᳚मसि ।",
          "begin": 0.0,
          "end": 9.572
        },
        {
          "text": "अचि᳚त्ती॒ यत्तव॒ धर्मा᳚ युयोपि॒म मा न॒स्तस्मा॒देन᳚सो देव रीरिषः ॥",
          "begin": 9.572,
          "end": 19.92
        }
      ]
    }
  ]
}