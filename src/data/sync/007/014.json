{
  "mandala": 7,
  "sukta": 14,
  "verses": [
    {
      "id": "7.14.1",
      "mandala": 7,
      "sukta": 14,
      "rik": 1,
      "padas": [
        {
          "text": "स॒मिधा᳚ जा॒तवे᳚दसे दे॒वाय᳚ दे॒वहू᳚तिभिः ।",
          "begin": 0.0,
          "end": 4.102
        },
        {
          "text": "ह॒विर्भि᳚ः शु॒क्रशो᳚चिषे नम॒स्विनो᳚ व॒यं दा᳚शेमा॒ग्नये᳚ ॥",
          "begin": 4.102,
          "end": 17.04
        }
      ]
    },
    {
      "id": "7.14.2",
      "mandala": 7,
      "sukta": 14,
      "rik": 2,
      "padas": [
        {
          "text": "व॒यं ते᳚ अग्ने स॒मिधा᳚ विधेम व॒यं दा᳚शेम सुष्टु॒ती य᳚जत्र ।",
          "begin": 0.0,
          "end": 9.06
        },
        {
          "text": "व॒यं घृ॒तेना᳚ध्वरस्य होतर्व॒यं दे᳚व ह॒विषा᳚ भद्रशोचे ॥",
          "begin": 9.06,
          "end": 16.56
        }
      ]
    },
    {
      "id": "7.14.3",
      "mandala": 7,
      "sukta": 14,
      "rik": 3,
      "padas": [
        {
          "text": "आ नो᳚ दे॒वेभि॒रुप᳚ दे॒वहू᳚ति॒मग्ने᳚ या॒हि वष᳚ट्कृतिं जुषा॒णः ।",
          "begin": 0.0,
          "end": 9.379
        },
        {
          "text": "तुभ्यं᳚ दे॒वाय॒ दाश᳚तः स्याम यू॒यं पा᳚त स्व॒स्तिभि॒ः सदा᳚ नः ॥",
          "begin": 9.379,
          "end": 18.88
        }
      ]
    }
  ]
}