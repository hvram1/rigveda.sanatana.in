{
  "mandala": 7,
  "sukta": 66,
  "verses": [
    {
      "id": "7.66.1",
      "mandala": 7,
      "sukta": 66,
      "rik": 1,
      "padas": [
        {
          "text": "प्र मि॒त्रयो॒र्वरु᳚णयो॒ः स्तोमो᳚ न एतु शू॒ष्य᳚ः ।",
          "begin": 0.0,
          "end": 11.446
        },
        {
          "text": "नम᳚स्वान्तुविजा॒तयो᳚ः ॥",
          "begin": 11.446,
          "end": 15.24
        }
      ]
    },
    {
      "id": "7.66.2",
      "mandala": 7,
      "sukta": 66,
      "rik": 2,
      "padas": [
        {
          "text": "या धा॒रय᳚न्त दे॒वाः सु॒दक्षा॒ दक्ष᳚पितरा ।",
          "begin": 0.0,
          "end": 5.828
        },
        {
          "text": "अ॒सु॒र्या᳚य॒ प्रम᳚हसा ॥",
          "begin": 5.828,
          "end": 8.6
        }
      ]
    },
    {
      "id": "7.66.3",
      "mandala": 7,
      "sukta": 66,
      "rik": 3,
      "padas": [
        {
          "text": "ता नः᳚ स्ति॒पा त᳚नू॒पा वरु᳚ण जरितॄ॒णाम् ।",
          "begin": 0.0,
          "end": 4.493
        },
        {
          "text": "मित्र᳚ सा॒धय᳚तं॒ धियः᳚ ॥",
          "begin": 4.493,
          "end": 7.44
        }
      ]
    },
    {
      "id": "7.66.4",
      "mandala": 7,
      "sukta": 66,
      "rik": 4,
      "padas": [
        {
          "text": "यद॒द्य सूर॒ उदि॒ते॑ना᳚गा मि॒त्रो अ᳚र्य॒मा ।",
          "begin": 0.0,
          "end": 6.128
        },
        {
          "text": "सु॒वाति᳚ सवि॒ता भगः᳚ ॥",
          "begin": 6.128,
          "end": 8.76
        }
      ]
    },
    {
      "id": "7.66.5",
      "mandala": 7,
      "sukta": 66,
      "rik": 5,
      "padas": [
        {
          "text": "सु॒प्रा॒वीर᳚स्तु॒ स क्षय॒ः प्र नु याम᳚न्त्सुदानवः ।",
          "begin": 0.0,
          "end": 6.128
        },
        {
          "text": "ये नो॒ अंहो᳚॑ति॒पिप्र᳚ति ॥",
          "begin": 6.128,
          "end": 9.36
        }
      ]
    },
    {
      "id": "7.66.6",
      "mandala": 7,
      "sukta": 66,
      "rik": 6,
      "padas": [
        {
          "text": "उ॒त स्व॒राजो॒ अदि᳚ति॒रद᳚ब्धस्य व्र॒तस्य॒ ये ।",
          "begin": 0.0,
          "end": 5.637
        },
        {
          "text": "म॒हो राजा᳚न ईशते ॥",
          "begin": 5.637,
          "end": 8.48
        }
      ]
    },
    {
      "id": "7.66.7",
      "mandala": 7,
      "sukta": 66,
      "rik": 7,
      "padas": [
        {
          "text": "प्रति᳚ वां॒ सूर॒ उदि᳚ते मि॒त्रं गृ᳚णीषे॒ वरु᳚णम् ।",
          "begin": 0.0,
          "end": 5.606
        },
        {
          "text": "अ॒र्य॒मणं᳚ रि॒शाद᳚सम् ॥",
          "begin": 5.606,
          "end": 8.12
        }
      ]
    },
    {
      "id": "7.66.8",
      "mandala": 7,
      "sukta": 66,
      "rik": 8,
      "padas": [
        {
          "text": "रा॒या हि᳚रण्य॒या म॒तिरि॒यम᳚वृ॒काय॒ शव᳚से ।",
          "begin": 0.0,
          "end": 5.028
        },
        {
          "text": "इ॒यं विप्रा᳚ मे॒धसा᳚तये ॥",
          "begin": 5.028,
          "end": 8.4
        }
      ]
    },
    {
      "id": "7.66.9",
      "mandala": 7,
      "sukta": 66,
      "rik": 9,
      "padas": [
        {
          "text": "ते स्या᳚म देव वरुण॒ ते मि᳚त्र सू॒रिभि᳚ः स॒ह ।",
          "begin": 0.0,
          "end": 5.853
        },
        {
          "text": "इषं॒ स्व᳚श्च धीमहि ॥",
          "begin": 5.853,
          "end": 8.2
        }
      ]
    },
    {
      "id": "7.66.10",
      "mandala": 7,
      "sukta": 66,
      "rik": 10,
      "padas": [
        {
          "text": "ब॒हव॒ः सूर᳚चक्षसो॑ग्निजि॒ह्वा ऋ᳚ता॒वृधः᳚ ।",
          "begin": 0.0,
          "end": 5.283
        },
        {
          "text": "त्रीणि॒ ये ये॒मुर्वि॒दथा᳚नि धी॒तिभि॒र्विश्वा᳚नि॒ परि᳚भूतिभिः ॥",
          "begin": 5.283,
          "end": 11.48
        }
      ]
    },
    {
      "id": "7.66.11",
      "mandala": 7,
      "sukta": 66,
      "rik": 11,
      "padas": [
        {
          "text": "वि ये द॒धुः श॒रदं॒ मास॒मादह᳚र्य॒ज्ञम॒क्तुं चादृच᳚म् ।",
          "begin": 0.0,
          "end": 7.486
        },
        {
          "text": "अ॒ना॒प्यं वरु᳚णो मि॒त्रो अ᳚र्य॒मा क्ष॒त्रं राजा᳚न आशत ॥",
          "begin": 7.486,
          "end": 13.52
        }
      ]
    },
    {
      "id": "7.66.12",
      "mandala": 7,
      "sukta": 66,
      "rik": 12,
      "padas": [
        {
          "text": "तद्वो᳚ अ॒द्य म᳚नामहे सू॒क्तैः सूर॒ उदि᳚ते ।",
          "begin": 0.0,
          "end": 5.215
        },
        {
          "text": "यदोह᳚ते॒ वरु᳚णो मि॒त्रो अ᳚र्य॒मा यू॒यमृ॒तस्य᳚ रथ्यः ॥",
          "begin": 5.215,
          "end": 12.0
        }
      ]
    },
    {
      "id": "7.66.13",
      "mandala": 7,
      "sukta": 66,
      "rik": 13,
      "padas": [
        {
          "text": "ऋ॒तावा᳚न ऋ॒तजा᳚ता ऋता॒वृधो᳚ घो॒रासो᳚ अनृत॒द्विषः᳚ ।",
          "begin": 0.0,
          "end": 7.907
        },
        {
          "text": "तेषां᳚ वः सु॒म्ने सु᳚च्छ॒र्दिष्ट᳚मे नर॒ः स्याम॒ ये च᳚ सू॒रयः᳚ ॥",
          "begin": 7.907,
          "end": 14.6
        }
      ]
    },
    {
      "id": "7.66.14",
      "mandala": 7,
      "sukta": 66,
      "rik": 14,
      "padas": [
        {
          "text": "उदु॒ त्यद्द᳚र्श॒तं वपु᳚र्दि॒व ए᳚ति प्रतिह्व॒रे ।",
          "begin": 0.0,
          "end": 6.36
        },
        {
          "text": "यदी᳚मा॒शुर्वह᳚ति दे॒व एत᳚शो॒ विश्व᳚स्मै॒ चक्ष᳚से॒ अर᳚म् ॥",
          "begin": 6.36,
          "end": 12.48
        }
      ]
    },
    {
      "id": "7.66.15",
      "mandala": 7,
      "sukta": 66,
      "rik": 15,
      "padas": [
        {
          "text": "शी॒र्ष्णःशी᳚र्ष्णो॒ जग᳚तस्त॒स्थुष॒स्पतिं᳚ स॒मया॒ विश्व॒मा रजः᳚ ।",
          "begin": 0.0,
          "end": 8.353
        },
        {
          "text": "स॒प्त स्वसा᳚रः सुवि॒ताय॒ सूर्यं॒ वह᳚न्ति ह॒रितो॒ रथे᳚ ॥",
          "begin": 8.353,
          "end": 15.04
        }
      ]
    },
    {
      "id": "7.66.16",
      "mandala": 7,
      "sukta": 66,
      "rik": 16,
      "padas": [
        {
          "text": "तच्चक्षु᳚र्दे॒वहि᳚तं शु॒क्रमु॒च्चर᳚त् ।",
          "begin": 0.0,
          "end": 4.509
        },
        {
          "text": "पश्ये᳚म श॒रदः᳚ श॒तं जीवे᳚म श॒रदः᳚ श॒तम् ॥",
          "begin": 4.509,
          "end": 9.96
        }
      ]
    },
    {
      "id": "7.66.17",
      "mandala": 7,
      "sukta": 66,
      "rik": 17,
      "padas": [
        {
          "text": "काव्ये᳚भिरदा॒भ्या या᳚तं वरुण द्यु॒मत् ।",
          "begin": 0.0,
          "end": 7.206
        },
        {
          "text": "मि॒त्रश्च॒ सोम᳚पीतये ॥",
          "begin": 7.206,
          "end": 9.28
        }
      ]
    },
    {
      "id": "7.66.18",
      "mandala": 7,
      "sukta": 66,
      "rik": 18,
      "padas": [
        {
          "text": "दि॒वो धाम᳚भिर्वरुण मि॒त्रश्चा या᳚तम॒द्रुहा᳚ ।",
          "begin": 0.0,
          "end": 5.859
        },
        {
          "text": "पिब᳚तं॒ सोम᳚मातु॒जी ॥",
          "begin": 5.859,
          "end": 8.88
        }
      ]
    },
    {
      "id": "7.66.19",
      "mandala": 7,
      "sukta": 66,
      "rik": 19,
      "padas": [
        {
          "text": "आ या᳚तं मित्रावरुणा जुषा॒णावाहु᳚तिं नरा ।",
          "begin": 0.0,
          "end": 6.967
        },
        {
          "text": "पा॒तं सोम᳚मृतावृधा ॥",
          "begin": 6.967,
          "end": 10.04
        }
      ]
    }
  ]
}